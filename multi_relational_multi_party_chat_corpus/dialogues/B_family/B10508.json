{
    "dialogue_id": "B10508",
    "dialogue_type": "Family",
    "interlocutors": [
        "りんご",
        "てばさき",
        "えのき"
    ],
    "relationship": [
        "てばさき",
        "えのき"
    ],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "りんご",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "てばさき",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "えのき",
            "text": "こんにちは！",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "りんご",
            "text": "コンビニスイーツとかたべますか？",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "えのき",
            "text": "たまに食べます",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "てばさき",
            "text": "@りんご ローソンでよく買います",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "えのき",
            "text": "今増量セールとかやってるらしいですよね",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "りんご",
            "text": "@てばさき ローソン美味しいよね",
            "mention_to": [
                "てばさき"
            ]
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "りんご",
            "text": "@えのき 増量！行かなちゃ",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "えのき",
            "text": "でもすぐ売り切れるとか…",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "てばさき",
            "text": "@りんご 今ニューデイズで四百円するマカロン売ってるのも気になってます",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "えのき",
            "text": "マカロン",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "えのき",
            "text": "美味しいけどお高いんだよね～",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "りんご",
            "text": "400円のが気になる！",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "てばさき",
            "text": "@えのき 父がめんどくさいって作ってくれない…",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "えのき",
            "text": "そりゃあめんどいよ…",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "りんご",
            "text": "お父さんがスイーツつくるの？",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "えのき",
            "text": "お父さんはパティシエじゃないんだよ",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "てばさき",
            "text": "@りんご 父は母の要請でバナナケーキとか作ります",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "えのき",
            "text": "たまに炊飯器ケーキとか作ります",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "りんご",
            "text": "お菓子作るお父さんって尊敬！",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "てばさき",
            "text": "@えのき 一番パティシエとしてパシッてるの母の癖に",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "えのき",
            "text": "そんなことない",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "りんご",
            "text": "@えのき 炊飯器やったことないです、簡単ですか？",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "えのき",
            "text": "材料合わせて炊くだけですよ",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "てばさき",
            "text": "@えのき そんなに簡単だったのか",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "りんご",
            "text": "へぇー、今度やってみようかな",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "えのき",
            "text": "レシピとかいっぱいあると思います！",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "えのき",
            "text": "@りんご お菓子とか作るんですか？",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "りんご",
            "text": "いいですね！混ぜるだけっていうのが",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "りんご",
            "text": "たまに作りますね、友達のとこ行く時とか",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "てばさき",
            "text": "@りんご どんなの作りますか？",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "えのき",
            "text": "いいですね！",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "えのき",
            "text": "私はお菓子は食べる専門です",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "りんご",
            "text": "簡単アップルパイとかです！",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "えのき",
            "text": "アップルパイ好きです！",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "えのき",
            "text": "シナモンが好き",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "りんご",
            "text": "パイ生地は買います、スーパーで",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "てばさき",
            "text": "@えのき 食べる専門っていうには母もパイシート使ってるよね",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "りんご",
            "text": "シナモン私も大好き！",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "えのき",
            "text": "パイ生地手作り難しいですもんね",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "てばさき",
            "text": "@えのき ミートパイまた食べたい",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "えのき",
            "text": "パイ生地にひき肉を炒めたのを挟んで焼くだけ。",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "りんご",
            "text": "そうなんだよね、パイシートは買った方が楽チン",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "えのき",
            "text": "何層にもしないといけないから…",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "りんご",
            "text": "ミートパイ美味しそう！手が込んでますね",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "てばさき",
            "text": "@えのき 私が作れと？私オーブンの使い方よくわかんないからやだ",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "えのき",
            "text": "パイ生地って、そのまま焼いただけでも美味しくないですか？",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "りんご",
            "text": "@てばさき 料理とかしない感じですか？",
            "mention_to": [
                "てばさき"
            ]
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "てばさき",
            "text": "@りんご しなくはないです",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "えのき",
            "text": "たまにね",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "りんご",
            "text": "今はたまにするくらいでいいと思う！",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "えのき",
            "text": "いちばん好きなのはお刺身だからね",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "えのき",
            "text": "切るだけ",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "りんご",
            "text": "いいですねー、簡単で美味しいのが一番ですよね！",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "てばさき",
            "text": "@えのき ふと思い立って作ったロールキャベツが一番手が込んでいたと思う",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "えのき",
            "text": "すごいえらいよ",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "りんご",
            "text": "@えのき 色々と手が込んでますねー、すごい！",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "えのき",
            "text": "一応こないだお刺身用に実家から柳葉もらってきたんですけど",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "てばさき",
            "text": "ロールキャベツはいいと思います、ほぼ肉なのに野菜食べた気分にもなれる",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "えのき",
            "text": "まだ研いでなくて使ってないです",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "てばさき",
            "text": "柳葉ってあの長いの？",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "えのき",
            "text": "包丁です",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "えのき",
            "text": "包丁とぎに興味があります",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "りんご",
            "text": "包丁って色々あるからねー",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "えのき",
            "text": "てばさん今度一緒に習いに行かない？",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "りんご",
            "text": "習うとこあるの？",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "てばさき",
            "text": "@えのき どうしてもっていうならついてってやらんくもないです",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "えのき",
            "text": "@りんご なんか、教えてくれるところあるらしいです",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "えのき",
            "text": "ひとりだと怖い…",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "りんご",
            "text": "へぇーそうなんだ！すごいなぁ",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "えのき",
            "text": "ストリートアカデミーっていって",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "えのき",
            "text": "いろいろ個人が教えてくれる",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "てばさき",
            "text": "@えのき 個人的には食器棚を一度整理しなおしてから研いで欲しい",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "えのき",
            "text": "包丁の研ぎ方講座もやってるところあるらしいので",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "てばさき",
            "text": "こないだ包丁床に落としかけて危なかった",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "りんご",
            "text": "食器棚整理は大変かも",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "えのき",
            "text": "食器棚、もしや買い換えたほうが…",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "えのき",
            "text": "もう20年以上使ってます",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "てばさき",
            "text": "@えのき 私より年上だったのか…",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "りんご",
            "text": "そういうのっていつのまにかなりますよね",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "えのき",
            "text": "結婚したときに最初に買ったもので",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "りんご",
            "text": "うちもタンスとかそうかも",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "えのき",
            "text": "いつの間にか年月が経ってますよね",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "りんご",
            "text": "あるあるですよ",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "えのき",
            "text": "タンスなんて何十年も普通に使えますし",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "りんご",
            "text": "タンスも地震で危ないから出したいですよね",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "えのき",
            "text": "うちの食器棚は子供たちのおかげで知らぬ間にぼろぼろになっているです",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "てばさき",
            "text": "@えのき 私たちが包丁でいたずらにつけた傷がたくさんあるよね",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "えのき",
            "text": "そうなんです…",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "りんご",
            "text": "@てばさき 包丁で？",
            "mention_to": [
                "てばさき"
            ]
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "えのき",
            "text": "子供たちの好奇心が旺盛なせいで…",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "えのき",
            "text": "三女は、ほうちょうでぎこぎこしてみたかった。と自白しました",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "りんご",
            "text": "落書きではなく？包丁ね",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "てばさき",
            "text": "@りんご 扉の淵に刃を当ててですね…",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "えのき",
            "text": "酷い話・・・",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "てばさき",
            "text": "多分上もやってるので三姉妹の共犯ですね",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "りんご",
            "text": "@てばさき こわー、でも好奇心には勝てないですね",
            "mention_to": [
                "てばさき"
            ]
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "えのき",
            "text": "@りんご さんのところはいかがですか？お子さんが小さいころ",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "りんご",
            "text": "うちはおもちゃを縛ったりしてました、ちょっとひきました",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "えのき",
            "text": "おもちゃを？",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "てばさき",
            "text": "@りんご しば…？",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "えのき",
            "text": "幼稚園くらいとかの時ですか？",
            "mention_to": []
        },
        {
            "utterance_id": 103,
            "interlocutor_id": "りんご",
            "text": "そうなんですよー、ちょっとやばいのかなって",
            "mention_to": []
        },
        {
            "utterance_id": 104,
            "interlocutor_id": "えのき",
            "text": "そんなやばい縛り方を？",
            "mention_to": []
        },
        {
            "utterance_id": 105,
            "interlocutor_id": "りんご",
            "text": "あれはウルトラマンの怪獣かな",
            "mention_to": []
        },
        {
            "utterance_id": 106,
            "interlocutor_id": "てばさき",
            "text": "@りんご いつの間にか梱包用の紐とか持ち出された感じですか？",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 107,
            "interlocutor_id": "えのき",
            "text": "怪獣を退治したかったのでは…",
            "mention_to": []
        },
        {
            "utterance_id": 108,
            "interlocutor_id": "りんご",
            "text": "どっかから紐もってきたんですよねー",
            "mention_to": []
        },
        {
            "utterance_id": 109,
            "interlocutor_id": "りんご",
            "text": "そうなのかな？",
            "mention_to": []
        }
    ],
