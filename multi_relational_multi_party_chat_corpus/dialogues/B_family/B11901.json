    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "ばなな",
            "text": "こんばんは",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "しらたき",
            "text": "こんばんは",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "つくね",
            "text": "こんばんは！",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "ばなな",
            "text": "今日は暖かくて過ごしやすかったですね",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "つくね",
            "text": "ですね！",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "しらたき",
            "text": "ですね！",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "つくね",
            "text": "とても暖かったです",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "ばなな",
            "text": "だいぶ春めいてきましたね",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "つくね",
            "text": "そうですね",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "ばなな",
            "text": "来月には桜が咲きますね",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "つくね",
            "text": "春はスキです",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "しらたき",
            "text": "1日家にこもってました！",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "ばなな",
            "text": "家ではお仕事ですか？",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "つくね",
            "text": "しらたきの実家は桜が有名です！",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "しらたき",
            "text": "ゲームが好きで",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "ばなな",
            "text": "@しらたき お家はどちらですか？",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "しらたき",
            "text": "長野県の南の方です",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "つくね",
            "text": "@ばなな は今日お仕事でしたか？",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "ばなな",
            "text": "長野県なのですね",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "しらたき",
            "text": "赤い桜が咲きます",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "ばなな",
            "text": "はい、家で仕事を",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "ばなな",
            "text": "赤い桜なのですね",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "つくね",
            "text": "そうなんですね。",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "ばなな",
            "text": "@つくね どちらの出身ですか？",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "つくね",
            "text": "しらたきと同じです！",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "ばなな",
            "text": "同じなのですね",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "ばなな",
            "text": "長野県には何回か行きました",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "つくね",
            "text": "そうねんですね！",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "ばなな",
            "text": "登山とか",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "つくね",
            "text": "登山いいですね！",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "つくね",
            "text": "中学校で必ずやります",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "ばなな",
            "text": "だいぶ前ですけれど",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "しらたき",
            "text": "そうなんですね",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "ばなな",
            "text": "白馬とかに行きました",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "つくね",
            "text": "白馬いいですよね",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "しらたき",
            "text": "スキーとかやりますか？",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "つくね",
            "text": "行ったことないけど…",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "ばなな",
            "text": "スキーはあまり出来ないです",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "しらたき",
            "text": "私もだめです！",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "つくね",
            "text": "@ばなな はどこにお住まいなんですか？",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "ばなな",
            "text": "私は東京です",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "ばなな",
            "text": "下町です",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "しらたき",
            "text": "こないだ東京行きました！",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "ばなな",
            "text": "@しらたき どちらに行きましたか？",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "しらたき",
            "text": "海鮮が好きで築地に",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "つくね",
            "text": "海鮮美味しいですよね！",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "ばなな",
            "text": "築地に行ったのですね",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "ばなな",
            "text": "海鮮美味しいですよね",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "しらたき",
            "text": "長野はあまり海鮮が、、、",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "ばなな",
            "text": "お蕎麦が美味しいですよね",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "つくね",
            "text": "海が無い県なので憧れが",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "ばなな",
            "text": "なるほど",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "ばなな",
            "text": "海鮮は何が好きですか？",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "つくね",
            "text": "マグロがスキです",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "ばなな",
            "text": "私はサーモン",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "しらたき",
            "text": "貝類が好きです",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "つくね",
            "text": "サーモンも美味しいですよね",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "しらたき",
            "text": "特に牡蠣とか",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "ばなな",
            "text": "かき美味しいですよね",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "つくね",
            "text": "貝は苦手です…",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "ばなな",
            "text": "でも、あたってしまうので",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "しらたき",
            "text": "生牡蠣にビール最高",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "つくね",
            "text": "生だと特に",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "ばなな",
            "text": "3回具合悪くなりました",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "ばなな",
            "text": "体に合わないのかな",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "つくね",
            "text": "そうなんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "しらたき",
            "text": "私もお腹弱いです",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "ばなな",
            "text": "美味しいのに",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "つくね",
            "text": "アレルギーとかですかね？",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "しらたき",
            "text": "でも牡蠣は大丈夫です！",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "ばなな",
            "text": "アレルギーかもしれませんね",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "つくね",
            "text": "子供が少し体質持ちで",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "ばなな",
            "text": "そうなのですね",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "ばなな",
            "text": "家の子もです",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "つくね",
            "text": "熱いお湯のお風呂だと発疹が出ます",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "つくね",
            "text": "そうなんですね",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "ばなな",
            "text": "反応するのですかね",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "しらたき",
            "text": "私も辛いもの食べた日は必ずトイレから出れません！",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "つくね",
            "text": "母としては心配です",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "ばなな",
            "text": "家はピーナッツです",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "つくね",
            "text": "ピーナッツなんですね",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "ばなな",
            "text": "辛いものも、そうなりますね",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "しらたき",
            "text": "辛いもの大好きなのに",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "ばなな",
            "text": "それは辛いですね",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "しらたき",
            "text": "そうなんですよ",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "しらたき",
            "text": "毎日辛いもの食べたいのに",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "ばなな",
            "text": "辛いのは何が好きですか？",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "ばなな",
            "text": "韓国料理とか？",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "しらたき",
            "text": "好きです",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "つくね",
            "text": "韓国料理好きです",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "しらたき",
            "text": "韓国行きたいです",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "ばなな",
            "text": "何が好きですか？",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "つくね",
            "text": "独身の時よく韓国行っていました",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "ばなな",
            "text": "韓国良いですね？",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "つくね",
            "text": "ですよね",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "しらたき",
            "text": "トッポギ",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "つくね",
            "text": "お酒も美味しかったのを覚えてます",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "ばなな",
            "text": "マッコリでしたかね？",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "つくね",
            "text": "マッコリとチャミスルが美味しかったです",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "ばなな",
            "text": "飲んだことないです",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "つくね",
            "text": "焼酎です",
            "mention_to": []
        }
