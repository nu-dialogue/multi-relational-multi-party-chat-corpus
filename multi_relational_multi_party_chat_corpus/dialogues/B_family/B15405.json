    ],
    "relationship": [
        "ちくわ",
        "こんぶ"
    ],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "おおかみ",
            "text": "こんばんは〜",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "ちくわ",
            "text": "こんばんは！",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "こんぶ",
            "text": "こんばんは！",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "ちくわ",
            "text": "お寿司のネタで",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "ちくわ",
            "text": "何が好きですか？",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "おおかみ",
            "text": "話題ありがとうございます！",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "こんぶ",
            "text": "私はサーモンです！笑",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "おおかみ",
            "text": "私はマグロ！",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "おおかみ",
            "text": "一択ですね",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "ちくわ",
            "text": "自分は、ウニといくらと赤エビです。",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "ちくわ",
            "text": "笑",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "こんぶ",
            "text": "マグロも色んな種類を？",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "おおかみ",
            "text": "おそらく同世代だと思うのですが、油が大好きなんです",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "こんぶ",
            "text": "@ちくわ 贅沢か笑",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "おおかみ",
            "text": "従って好きなのは大トロですね...",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "ちくわ",
            "text": "@おおかみ マグロ一択なんすね！",
            "mention_to": [
                "おおかみ"
            ]
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "おおかみ",
            "text": "@ちくわ 高級志向ですね",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "ちくわ",
            "text": "僕も大トロすきです！",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "こんぶ",
            "text": "おお！！！大トロ",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "おおかみ",
            "text": "美味しいウニ好きです",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "こんぶ",
            "text": "一皿目から大トロいきます？",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "ちくわ",
            "text": "でも上品な中トロも好きです笑",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "おおかみ",
            "text": "@ちくわ すごくよくわかります",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "こんぶ",
            "text": "私も美味しいウニなら食べられます！笑",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "おおかみ",
            "text": "サーモンだと",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "おおかみ",
            "text": "はま寿司の炙りチーズサーモンが大好きですね",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "おおかみ",
            "text": "生はあんまり好きではなく...",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "こんぶ",
            "text": "あーー！美味しいですよね！！！",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "ちくわ",
            "text": "@こんぶ 美味しいウニってまずいウニあんの？",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "こんぶ",
            "text": "サーモンも好みありますよね",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "ちくわ",
            "text": "炙り派なんすね！",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "おおかみ",
            "text": "@ちくわ 多分磯臭いやつではないですか？",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "おおかみ",
            "text": "炙り派です",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "ちくわ",
            "text": "@おおかみ なるほど！",
            "mention_to": [
                "おおかみ"
            ]
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "こんぶ",
            "text": "@ちくわ とれたてのウニはめちゃくちゃ美味しかった",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "ちくわ",
            "text": "磯臭いのはまぁあんますね！",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "こんぶ",
            "text": "@おおかみ そうですそうです！！！",
            "mention_to": [
                "おおかみ"
            ]
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "ちくわ",
            "text": "@こんぶ 海に出て取りに行ったんか？",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "おおかみ",
            "text": "@こんぶ たまにやばいやつありますからね...",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "おおかみ",
            "text": "あと私は白子が大好きですね",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "ちくわ",
            "text": "あ！！！！！",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "こんぶ",
            "text": "@ちくわ あまさんかて笑笑",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "ちくわ",
            "text": "白子ポン酢大好きです！",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "おおかみ",
            "text": "満を持して痛風になります",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "おおかみ",
            "text": "@ちくわ おなじくです",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "ちくわ",
            "text": "まじで！",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "こんぶ",
            "text": "@おおかみ そうです！それにあたったときは、、ちょっと、、",
            "mention_to": [
                "おおかみ"
            ]
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "ちくわ",
            "text": "白子ポン酢すきです笑",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "おおかみ",
            "text": "ですよね...",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "こんぶ",
            "text": "おお！趣味あいそうですね！笑",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "ちくわ",
            "text": "笑",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "こんぶ",
            "text": "みんなで居酒屋でも行きたいですね！笑",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "こんぶ",
            "text": "@おおかみ お酒のまれます？",
            "mention_to": [
                "おおかみ"
            ]
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "おおかみ",
            "text": "@こんぶ ですね、なんか一致してる話題な感じしてます笑",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "ちくわ",
            "text": "妻はサーモンなら１０貫ぐらい食べますよ！笑",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "おおかみ",
            "text": "@こんぶ はい！飲みますよ",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "こんぶ",
            "text": "@おおかみ ね！そんな気しますー！",
            "mention_to": [
                "おおかみ"
            ]
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "おおかみ",
            "text": "@ちくわ それはそれで怖い",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "こんぶ",
            "text": "@おおかみ あら！じゃあ、みんなで！！笑",
            "mention_to": [
                "おおかみ"
            ]
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "ちくわ",
            "text": "僕はハイボール派です笑",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "おおかみ",
            "text": "私はビールからのハイボールですね",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "こんぶ",
            "text": "@ちくわ さすがに１０はきつすぎるわ笑",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "おおかみ",
            "text": "@こんぶ さんは？",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "こんぶ",
            "text": "私もビールからのジンジャーハイを",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "ちくわ",
            "text": "@おおかみ 笑　僕もその流れ！",
            "mention_to": [
                "おおかみ"
            ]
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "おおかみ",
            "text": "お二人は似てるんですね笑",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "ちくわ",
            "text": "@こんぶ さすがにか。笑",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "おおかみ",
            "text": "二人で夜飲んだりされるんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "こんぶ",
            "text": "たまにのみますよ〜〜",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "おおかみ",
            "text": "いいですね",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "こんぶ",
            "text": "あんまり晩酌はしないですが",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "ちくわ",
            "text": "ほろよいですけど",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "こんぶ",
            "text": "たまーに！！",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "ちくわ",
            "text": "僕自身毎日飲まないので！",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "こんぶ",
            "text": "ほろよいはジュースだよね笑",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "おおかみ",
            "text": "お子さんが寝た後とかにですね",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "ちくわ",
            "text": "そうすそうす！！",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "こんぶ",
            "text": "そうですそうです！",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "こんぶ",
            "text": "夜な夜な、、笑",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "おおかみ",
            "text": "@こんぶ いろんな味があっていいですよね",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "おおかみ",
            "text": "あれはあれで",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "こんぶ",
            "text": "スルメをあてにして笑",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "ちくわ",
            "text": "でもガッツリは飲まないです！",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "おおかみ",
            "text": "スルメいいですね",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "こんぶ",
            "text": "@おおかみ そうなんですよ！軽い感覚でのみます",
            "mention_to": [
                "おおかみ"
            ]
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "ちくわ",
            "text": "エイヒレとか食べたくなります",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "こんぶ",
            "text": "私チータラ好きです",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "おおかみ",
            "text": "まぁ私はお酒が次の日残る派なので、あんまり飲まないです",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "おおかみ",
            "text": "@こんぶ チーたら炙ると美味しいらしいですよ",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "こんぶ",
            "text": "次の日残るのはきついですね",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "おおかみ",
            "text": "Xで見ました",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "ちくわ",
            "text": "@おおかみ 結構残るタイプなんすね！",
            "mention_to": [
                "おおかみ"
            ]
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "ちくわ",
            "text": "えーー炙ったら溶けないんすか？",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "こんぶ",
            "text": "@おおかみ 炙りチータラ？絶対美味しい！！！",
            "mention_to": [
                "おおかみ"
            ]
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "おおかみ",
            "text": "@ちくわ 大学生の時とかは日々死んでました",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "こんぶ",
            "text": "それはいっぱい飲むからですか？笑",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "ちくわ",
            "text": "@おおかみ まぁそんなもんすよ！",
            "mention_to": [
                "おおかみ"
            ]
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "ちくわ",
            "text": "大学は！笑",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "こんぶ",
            "text": "程よくのんだら、残らない？",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "おおかみ",
            "text": "@ちくわ 少し膨らんで美味しいらしいですよ",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "ちくわ",
            "text": "長い時間飲むからとかやない？",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "おおかみ",
            "text": "@こんぶ そうですね、いっぱい飲むから",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "こんぶ",
            "text": "@ちくわ 今度やってみよーーー",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 103,
            "interlocutor_id": "ちくわ",
            "text": "@こんぶ やってみよ！",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 104,
            "interlocutor_id": "おおかみ",
            "text": "絶対美味しいと思います",
            "mention_to": []
        },
        {
            "utterance_id": 105,
            "interlocutor_id": "こんぶ",
            "text": "程よくには抑えられないやつですね笑",
            "mention_to": []
        },
        {
            "utterance_id": 106,
            "interlocutor_id": "ちくわ",
            "text": "いっぱい飲んだらやばいすよねー",
            "mention_to": []
        },
        {
            "utterance_id": 107,
            "interlocutor_id": "おおかみ",
            "text": "そうなんです",
            "mention_to": []
        }
