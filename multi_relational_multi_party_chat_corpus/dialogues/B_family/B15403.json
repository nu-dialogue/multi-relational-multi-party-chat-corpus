    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "おおかみ",
            "text": "こんばんは",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "こんぶ",
            "text": "こんばんは〜",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "ちくわ",
            "text": "こんばんは",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "おおかみ",
            "text": "何か趣味にされてることとかありますか？",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "ちくわ",
            "text": "趣味かーー",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "こんぶ",
            "text": "それが、今趣味なくて〜〜",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "こんぶ",
            "text": "これといったものが、、、",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "ちくわ",
            "text": "適度な運動！笑",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "おおかみ",
            "text": "おふたりともないんですね",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "ちくわ",
            "text": "趣味ではないですけど！",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "おおかみ",
            "text": "@ちくわ 運動されてるんですね",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "ちくわ",
            "text": "適度に！",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "こんぶ",
            "text": "適度に笑",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "おおかみ",
            "text": "でも体動かすってストレス発散ですもんね",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "こんぶ",
            "text": "しいていうなら、YouTube鑑賞でしょか笑",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "ちくわ",
            "text": "身体痛めてますけどね笑",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "こんぶ",
            "text": "それって趣味かな笑",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "ちくわ",
            "text": "YouTube鑑賞してます",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "おおかみ",
            "text": "@こんぶ お、何を見るんですか？",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "おおかみ",
            "text": "立派な趣味です笑",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "ちくわ",
            "text": "YouTube鑑賞してます！",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "こんぶ",
            "text": "たしかに！ちょっと動くだけでストレス発散なりますよね",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "おおかみ",
            "text": "@こんぶ そうなんですよね",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "ちくわ",
            "text": "これは趣味に入るんすね！",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "こんぶ",
            "text": "@おおかみ 平成フラミンゴ？コムドットとか！すきです",
            "mention_to": [
                "おおかみ"
            ]
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "こんぶ",
            "text": "よかった〜笑ありがとうございます",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "おおかみ",
            "text": "@こんぶ 二つのも全く見たことないですね...",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "ちくわ",
            "text": "@おおかみ なんか見たことあります？",
            "mention_to": [
                "おおかみ"
            ]
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "ちくわ",
            "text": "YouTubeで！",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "こんぶ",
            "text": "@おおかみ YouTubeなにかみます？",
            "mention_to": [
                "おおかみ"
            ]
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "おおかみ",
            "text": "@ちくわ ホラー系のゲーム実況が好きです",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "こんぶ",
            "text": "おお！！！それは、見たことない！笑",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "ちくわ",
            "text": "なんか聞いたことあります！",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "おおかみ",
            "text": "誰というのはないんですけど",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "こんぶ",
            "text": "そんなのも、やってるんですね！！！",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "ちくわ",
            "text": "ゲームとか実況してくれてるのをみるやつ！",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "おおかみ",
            "text": "私がゲーム全くできないんですよ",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "おおかみ",
            "text": "@ちくわ そうです、それです",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "おおかみ",
            "text": "だから見てやった気になってます",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "ちくわ",
            "text": "僕もゲームしません笑",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "こんぶ",
            "text": "ゲームできないから、解説をみて、攻略？",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "おおかみ",
            "text": "@ちくわ それは素晴らしいと思います",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "こんぶ",
            "text": "私もゲームしません笑",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "こんぶ",
            "text": "携帯ゲームくらい笑",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "おおかみ",
            "text": "旦那が本当にゲーマーなので",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "ちくわ",
            "text": "@おおかみ 飽き性なんで！笑",
            "mention_to": [
                "おおかみ"
            ]
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "こんぶ",
            "text": "旦那さんが！！！",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "おおかみ",
            "text": "@こんぶ 余裕で深夜2時とかまでやってるんで",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "ちくわ",
            "text": "なら夜はゲームばっかりすか？",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "ちくわ",
            "text": "うわ！",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "ちくわ",
            "text": "そんなに？",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "ちくわ",
            "text": "仕事に影響でないんすか？",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "おおかみ",
            "text": "@ちくわ うるさくないからいいんですが...",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "こんぶ",
            "text": "@おおかみ えーー！それはそれで、すごすぎる",
            "mention_to": [
                "おおかみ"
            ]
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "おおかみ",
            "text": "在宅民です",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "こんぶ",
            "text": "あれですか、ゲーム用の椅子とかある？",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "おおかみ",
            "text": "@こんぶ 朝寝坊するとブチギレます",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "おおかみ",
            "text": "@こんぶ はい、あります！",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "おおかみ",
            "text": "そこそこ高いみたいです",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "ちくわ",
            "text": "なんかそういう特別な部屋があるんすか？",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "ちくわ",
            "text": "寝坊、、",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "こんぶ",
            "text": "@おおかみ ゲームしてたのは自分なのに？笑",
            "mention_to": [
                "おおかみ"
            ]
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "ちくわ",
            "text": "ブチギレ",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "おおかみ",
            "text": "@ちくわ うち2LDKなのですが、1つの部屋は旦那部屋です",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "ちくわ",
            "text": "イメージできますね笑",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "ちくわ",
            "text": "@おおかみ え！すごい！",
            "mention_to": [
                "おおかみ"
            ]
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "こんぶ",
            "text": "高そうー！！！専用イスは",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "ちくわ",
            "text": "旦那さん部屋とかあるんすね笑",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "おおかみ",
            "text": "@こんぶ そうなんです、普通に寝坊する...",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "おおかみ",
            "text": "まぁ私も趣味がないので部屋が必要ないんです",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "ちくわ",
            "text": "他の部屋にしなくてよかったんすね！",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "こんぶ",
            "text": "@おおかみ あらら",
            "mention_to": [
                "おおかみ"
            ]
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "おおかみ",
            "text": "まぁおくものもあんまりないから良いかなと",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "こんぶ",
            "text": "でも、趣味ないからって部屋を与えちゃうなんて逆にすごいです〜",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "ちくわ",
            "text": "それで寝坊したら",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "おおかみ",
            "text": "@ちくわ こもってやってくれれば静かですし",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "ちくわ",
            "text": "そりゃキレますね笑",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "ちくわ",
            "text": "@おおかみ そういう考えなんすね！",
            "mention_to": [
                "おおかみ"
            ]
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "こんぶ",
            "text": "たしかに！こっちもこっちで自分1人の空間で良きです",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "おおかみ",
            "text": "@こんぶ 私は大体リビングで携帯いじってるだけなので",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "ちくわ",
            "text": "1人の時間すね！",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "おおかみ",
            "text": "@こんぶ そうなんです",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "おおかみ",
            "text": "かなり大事",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "こんぶ",
            "text": "@おおかみ お互いひとり時間ですね",
            "mention_to": [
                "おおかみ"
            ]
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "おおかみ",
            "text": "お二人はずっと一緒にいるんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "こんぶ",
            "text": "1人で色々するの苦じゃないんですね？",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "ちくわ",
            "text": "ゲームもお金かかりますよね！",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "おおかみ",
            "text": "それとも部屋は別々？",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "おおかみ",
            "text": "@こんぶ 一人が好きです笑",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "こんぶ",
            "text": "私たちは、一緒の空間しか持ってなくて笑",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "こんぶ",
            "text": "各々の部屋はありません笑",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "ちくわ",
            "text": "子ども部屋はあるけど笑",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "こんぶ",
            "text": "けど、各々違うことしてたり、",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "おおかみ",
            "text": "@こんぶ それで何もないならいい夫婦かなって思います！",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "こんぶ",
            "text": "はたまた、一緒のテレビをみたり！です",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "おおかみ",
            "text": "仲良しですね",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "ちくわ",
            "text": "平日も夜は仕事に出たりもあるんですよ！",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "おおかみ",
            "text": "@ちくわ こども部屋は必須ですね",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "こんぶ",
            "text": "@おおかみ そうですね〜今のところは、お互い問題なさそうです",
            "mention_to": [
                "おおかみ"
            ]
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "おおかみ",
            "text": "えっ",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "おおかみ",
            "text": "夜勤あるんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "ちくわ",
            "text": "@おおかみ ですよね！",
            "mention_to": [
                "おおかみ"
            ]
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "ちくわ",
            "text": "@おおかみ ま！そんな感じです！",
            "mention_to": [
                "おおかみ"
            ]
        },
        {
            "utterance_id": 103,
            "interlocutor_id": "こんぶ",
            "text": "そうなんです〜なので、私はかなり1人時間多いです笑",
            "mention_to": []
        }
