    ],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "ばなな",
            "text": "こんばんは",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "てばさき",
            "text": "こんばんは",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "えのき",
            "text": "こんばんは",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "ばなな",
            "text": "健康のために食べているものありますか",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "ばなな",
            "text": "私は納豆です",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "てばさき",
            "text": "@ばなな 豆腐は…食べようかなと思っています",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "えのき",
            "text": "私は、サラダを食べるようにしています",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "ばなな",
            "text": "なるほど",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "えのき",
            "text": "納豆はいいですね！",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "てばさき",
            "text": "冷奴にしたらお腹膨れないかなと",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "えのき",
            "text": "大粒のが好きです納豆",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "てばさき",
            "text": "納豆は妹が好きです",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "えのき",
            "text": "好きだね",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "ばなな",
            "text": "豆腐はイソフラボンですかね",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "えのき",
            "text": "ほぼほぼ納豆で生きてるね",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "ばなな",
            "text": "豆は体に良いですね",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "てばさき",
            "text": "@ばなな そういう詳しいのはちょっとわからないです",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "ばなな",
            "text": "そんなに好きなんですか",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "えのき",
            "text": "たまご納豆ていうのが好きで",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "てばさき",
            "text": "@ばなな 好きですね",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "えのき",
            "text": "毎回リクエストされます",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "ばなな",
            "text": "たまご納豆ですか",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "てばさき",
            "text": "他の納豆との違いがわかりませんね",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "えのき",
            "text": "タレかな？",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "てばさき",
            "text": "@えのき タレかあ",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "ばなな",
            "text": "卵入っているんですかね",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "ばなな",
            "text": "タレの味が好きなのかしら",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "えのき",
            "text": "たまごっぽい感じ？なんですかね",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "えのき",
            "text": "ひきわり納豆も好きで",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "ばなな",
            "text": "そうなんですね",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "てばさき",
            "text": "@えのき ひきわりは美味しい",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "えのき",
            "text": "よく、納豆まきを作れと言われます",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "えのき",
            "text": "手巻き寿司の練習してます",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "ばなな",
            "text": "そうなりますよね",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "てばさき",
            "text": "なぜ自分で作ろうとしないのか謎です",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "ばなな",
            "text": "手巻き良いですね",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "えのき",
            "text": "@てばさき f",
            "mention_to": [
                "てばさき"
            ]
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "ばなな",
            "text": "手巻きなら作れそう",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "えのき",
            "text": "ねぎとろ巻きが好きだよね",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "えのき",
            "text": "てばさんは",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "ばなな",
            "text": "ネギトロも良いですね",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "てばさき",
            "text": "@えのき　ネギトロうまい",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "ばなな",
            "text": "@てばさき 恵方巻食べましたか",
            "mention_to": [
                "てばさき"
            ]
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "ばなな",
            "text": "家は少し",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "てばさき",
            "text": "@ばなな 食べました！",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "えのき",
            "text": "なんか立派なの食べてたよね",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "ばなな",
            "text": "作りましたか",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "てばさき",
            "text": "海鮮系のを母と半分こしました",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "えのき",
            "text": "スーパーで買ってきて",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "ばなな",
            "text": "海鮮高いですよね",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "えのき",
            "text": "お高いから半分ずつ…",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "ばなな",
            "text": "なるほど",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "えのき",
            "text": "分け合って…",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "てばさき",
            "text": "@えのき ごめん、値段はみてなかった",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "えのき",
            "text": "てばさんは、すごく海鮮が好きだよね",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "てばさき",
            "text": "@えのき ビバ鮮魚",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "ばなな",
            "text": "そうなんですね",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "えのき",
            "text": "とりあえずなにか海鮮丼を買っていけば喜んでくれる",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "てばさき",
            "text": "@えのき 喜びます、しかしちょろいわけではないですからね",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "ばなな",
            "text": "好きなのあると良いですね",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "えのき",
            "text": "のですごく楽です",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "てばさき",
            "text": "@えのき 楽とは",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "ばなな",
            "text": "家の子に何でも良いと言われると",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "ばなな",
            "text": "困ります",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "えのき",
            "text": "なんでもいいは困りますよね",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "てばさき",
            "text": "@ばなな 私は肉か魚って言います",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "ばなな",
            "text": "そうなんですよ",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "えのき",
            "text": "何でもいいって言ったのに文句言われるとさらに困りますよね",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "ばなな",
            "text": "@てばさき それは良いです",
            "mention_to": [
                "てばさき"
            ]
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "ばなな",
            "text": "そうそう",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "てばさき",
            "text": "自分が食べたい気分のものであれば、何でもいい",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "えのき",
            "text": "三女は肉しか言わないですね",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "ばなな",
            "text": "文句言われると",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "ばなな",
            "text": "お肉好きなのですね",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "えのき",
            "text": "大好きですね",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "てばさき",
            "text": "@えのき ひき肉フライパンにひっくり返して焼いただけのもので満足する安い妹です",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "ばなな",
            "text": "それは簡単",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "えのき",
            "text": "最近は、モヤシ炒めを多く付け合わせています",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "えのき",
            "text": "肉ばかりになるので",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "ばなな",
            "text": "なるほど",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "ばなな",
            "text": "家の子も肉ばかり",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "えのき",
            "text": "@ばなな 献立に困ったらどうしますか？",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "ばなな",
            "text": "@えのき クックパッドとか",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "ばなな",
            "text": "検索しますね",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "えのき",
            "text": "検索しちゃいますよね",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "てばさき",
            "text": "@ばなな いいですね、クックパッド",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "ばなな",
            "text": "そうなんです",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "ばなな",
            "text": "考えてもどうにもならなくて",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "てばさき",
            "text": "私は冷蔵庫など備蓄を探します",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "えのき",
            "text": "そうね。あるものから",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "ばなな",
            "text": "@てばさき 料理好きですか",
            "mention_to": [
                "てばさき"
            ]
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "てばさき",
            "text": "ツナ缶とご飯があればマヨ足してツナマヨできます",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "えのき",
            "text": "ちょろい",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "ばなな",
            "text": "ツナマヨ良いですね",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "てばさき",
            "text": "@えのき ちょろくはないとあらかじめ言ったはず…",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "えのき",
            "text": "なにかのフリかと…",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "ばなな",
            "text": "おにぎりもツナマヨ好きですか",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "てばさき",
            "text": "@えのき ちょろくないという事実を述べただけ",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "てばさき",
            "text": "@ばなな ツナマヨ好きです",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "えのき",
            "text": "@ばなな おにぎりは何が好きですか？",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "ばなな",
            "text": "@えのき 好きなおにぎりの具は",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "てばさき",
            "text": "コンビニのツナマヨでハズレ引き当てたことありません",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "ばなな",
            "text": "同じになりましたね",
            "mention_to": []
        },
        {
            "utterance_id": 103,
            "interlocutor_id": "ばなな",
            "text": "私は鮭です",
            "mention_to": []
        },
        {
            "utterance_id": 104,
            "interlocutor_id": "えのき",
            "text": "鮭いいですね！",
            "mention_to": []
        },
        {
            "utterance_id": 105,
            "interlocutor_id": "ばなな",
            "text": "@えのき 何ですか",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 106,
            "interlocutor_id": "てばさき",
            "text": "@ばなな ファミマの鮭ハラス美味しいですよ",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 107,
            "interlocutor_id": "えのき",
            "text": "おにぎりは、塩結びかなあ…自分で作る",
            "mention_to": []
        },
        {
            "utterance_id": 108,
            "interlocutor_id": "えのき",
            "text": "好きな塩かげんにできるので",
            "mention_to": []
        }
