            "text": "おはようございます！",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "にんじん",
            "text": "おはようございます！",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "たまねぎ",
            "text": "おはようございます！",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "ポテト",
            "text": "最近したプチ贅沢って何かありますか？",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "たまねぎ",
            "text": "@ポテト うーん。",
            "mention_to": [
                "ポテト"
            ]
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "たまねぎ",
            "text": "なんだろー",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "にんじん",
            "text": "@ポテト 先日行った旅行かな？",
            "mention_to": [
                "ポテト"
            ]
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "ポテト",
            "text": "旅行！",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "ポテト",
            "text": "ご家族で？",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "にんじん",
            "text": "日帰りですがアウトレットに！",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "たまねぎ",
            "text": "@ポテト そうです！",
            "mention_to": [
                "ポテト"
            ]
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "ポテト",
            "text": "アウトレット！楽しそう！",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "ポテト",
            "text": "見て回るだけでも楽しいやつですね！",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "にんじん",
            "text": "@ポテト そうなんですー！",
            "mention_to": [
                "ポテト"
            ]
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "にんじん",
            "text": "そこで衝動買いしました！",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "たまねぎ",
            "text": "パジャマです！笑",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "ポテト",
            "text": "@たまねぎ パジャマ！",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "たまねぎ",
            "text": "ジェラートピケの！笑",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "ポテト",
            "text": "あ～～～～～！いいやつだ！",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "にんじん",
            "text": "私も欲しかったけど笑",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "にんじん",
            "text": "旦那に買いました！",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "にんじん",
            "text": "もこもこ！",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "ポテト",
            "text": "@にんじん だんなさんの？！",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "にんじん",
            "text": "そうです！",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "にんじん",
            "text": "いつも裸で寝てるので",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "ポテト",
            "text": "いいものを与えられている・・・",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "ポテト",
            "text": "え？",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "にんじん",
            "text": "あ、パンツははいてます！",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "ポテト",
            "text": "まって",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "ポテト",
            "text": "パンツはいてた・・・",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "たまねぎ",
            "text": "@にんじん おい！",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "にんじん",
            "text": "間違えた！",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "ポテト",
            "text": "動揺しましたぁ～",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "にんじん",
            "text": "笑",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "にんじん",
            "text": "これから夏が来るから",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "ポテト",
            "text": "裸族かぁ・・・ってなりました",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "にんじん",
            "text": "逆にエアコンで寒いかなーって",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "たまねぎ",
            "text": "ほぼ裸族ですけどね！",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "たまねぎ",
            "text": "むしろほんとは脱ぎたい！",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "ポテト",
            "text": "@たまねぎ なるほど・・・いや、まって冬でもパンイチ・・・",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "たまねぎ",
            "text": "娘に嫌われそうなんで仕方なくパンツは",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "たまねぎ",
            "text": "苦肉の策です！",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "ポテト",
            "text": "@たまねぎ かろうじての思いやり・・",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "にんじん",
            "text": "お腹冷えそー笑",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "にんじん",
            "text": "ということで、パジャマを笑",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "ポテト",
            "text": "お腹ひえますよ！",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "ポテト",
            "text": "@にんじん 良い買い物です！",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "たまねぎ",
            "text": "お腹強いんですよー",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "ポテト",
            "text": "でも、脱ごうとしているたまねぎさん・・・",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "ポテト",
            "text": "@たまねぎ お腹強いとかの問題じゃないやい・・・！",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "たまねぎ",
            "text": "え？！",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "たまねぎ",
            "text": "笑",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "ポテト",
            "text": "服を着るのです・・・笑",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "たまねぎ",
            "text": "でも、あのパジャマ気持ちいいのでちゃんと着ます！",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "ポテト",
            "text": "よかった・・・",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "ポテト",
            "text": "@にんじん 良かったですね・・・",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "にんじん",
            "text": "良かった笑",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "たまねぎ",
            "text": "汗っかきなんでねー",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "ポテト",
            "text": "でも脱いだらにんじんさんのものになるかも・・・",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "たまねぎ",
            "text": "寝ながら脱ぐこともしばしば",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "ポテト",
            "text": "ワンチャン・・・",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "にんじん",
            "text": "あ、そうね！",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "ポテト",
            "text": "あるな・・・",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "にんじん",
            "text": "私も着たいとこだけど身長差が半端なくて",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "ポテト",
            "text": "もう、貰っちゃいましょ・・・脱ぎますよ・・・時間の問題だぁ・・・",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "にんじん",
            "text": "旦那のは全くむりです！笑",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "ポテト",
            "text": "@にんじん おっと！",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "ポテト",
            "text": "物理的に無理だった・・・",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "にんじん",
            "text": "@ポテト そーなんですー",
            "mention_to": [
                "ポテト"
            ]
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "にんじん",
            "text": "でかすぎて",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "たまねぎ",
            "text": "オーバーサイズ可愛いけどね、",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "ポテト",
            "text": "@にんじん 身長差相当ですか？",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "ポテト",
            "text": "@たまねぎ それはたしかに・・・",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "にんじん",
            "text": "はい！旦那185.私152",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "たまねぎ",
            "text": "頭一個分ですね！",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "たまねぎ",
            "text": "一緒に買い物してると見失います！",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "ポテト",
            "text": "さ・・・さんじゅっせんち・・・",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "たまねぎ",
            "text": "下見るといる！",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "たまねぎ",
            "text": "カートだけ動いてます！",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "ポテト",
            "text": "@たまねぎ 見失わないで・・・！",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "にんじん",
            "text": "失礼笑",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "たまねぎ",
            "text": "真下にいられるとちょっと笑",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "ポテト",
            "text": "カートだけ動いてるは笑う・・・笑",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "にんじん",
            "text": "そんなに見えなくなるもん？？",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "ポテト",
            "text": "たしかに！",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "たまねぎ",
            "text": "急に消える！",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "ポテト",
            "text": "巨人の発言・・・",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "たまねぎ",
            "text": "やめてくれる？",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "にんじん",
            "text": "@ポテト 巨人笑",
            "mention_to": [
                "ポテト"
            ]
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "ポテト",
            "text": "@たまねぎ 急に消えてるわけじゃない・・・！笑",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "にんじん",
            "text": "近いだけ笑",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "たまねぎ",
            "text": "そうなのかー",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "ポテト",
            "text": "おじいちゃんの眼鏡眼鏡と同じ原理・・・",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "たまねぎ",
            "text": "@ポテト 確かに笑",
            "mention_to": [
                "ポテト"
            ]
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "にんじん",
            "text": "灯台もと暗し笑",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "ポテト",
            "text": "@にんじん にんじんさんも首が疲れそう・・・",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "にんじん",
            "text": "@ポテト そうなんですー！",
            "mention_to": [
                "ポテト"
            ]
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "にんじん",
            "text": "立って話したりハグしたりすると首持ってかれる",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "ポテト",
            "text": "首持ってかれる笑",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "ポテト",
            "text": "やばい",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "たまねぎ",
            "text": "ちっちゃいから思いきし抱きしめると死にそうになってます笑",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "にんじん",
            "text": "いつか殺される気がしてます笑",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "ポテト",
            "text": "@たまねぎ 死にそうと笑いはくっつけちゃダメ！笑",
            "mention_to": [
                "たまねぎ"
            ]
        }
