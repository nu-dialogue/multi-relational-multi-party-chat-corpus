        },
        {
            "utterance_id": 1,
            "interlocutor_id": "こんぶ",
            "text": "こんばんは",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "ちくわ",
            "text": "こんばんは！",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "おおかみ",
            "text": "そちらの地元で有名なものとかありますか？",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "こんぶ",
            "text": "んー！なんだろ！",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "おおかみ",
            "text": "どこお住まいでしたっけ",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "ちくわ",
            "text": "長野です！",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "こんぶ",
            "text": "@ちくわ なにかな？",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "こんぶ",
            "text": "長野ですよー！",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "ちくわ",
            "text": "@こんぶ 長野だとおやきとか？",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "こんぶ",
            "text": "あ！そうだね！！",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "おおかみ",
            "text": "@ちくわ おやき、おいしいですね",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "こんぶ",
            "text": "おやき美味しい！！",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "ちくわ",
            "text": "この辺だと、、、なんだろ？",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "こんぶ",
            "text": "色んな味があってね！",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "ちくわ",
            "text": "あ！",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "こんぶ",
            "text": "この辺はーー",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "ちくわ",
            "text": "蕎麦！",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "こんぶ",
            "text": "ん？",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "ちくわ",
            "text": "信州そば！",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "おおかみ",
            "text": "へー！",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "こんぶ",
            "text": "あーー！そうだの！蕎麦もだ",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "おおかみ",
            "text": "聞いたことありますね",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "こんぶ",
            "text": "美味しい蕎麦いっぱい",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "ちくわ",
            "text": "美味しいですよー！",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "おおかみ",
            "text": "お水が美味しいイメージありますもんね",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "ちくわ",
            "text": "こしがあって！",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "おおかみ",
            "text": "長野というとライチョウなんですよね",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "こんぶ",
            "text": "そう！こしがあって！",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "ちくわ",
            "text": "あ、そうですね！",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "おおかみ",
            "text": "私の中だと",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "こんぶ",
            "text": "あー！お菓子ですよね？",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "ちくわ",
            "text": "県鳥ですね",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "おおかみ",
            "text": "いや鳥そのものです笑",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "こんぶ",
            "text": "鳥のほうか笑",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "こんぶ",
            "text": "お菓子もありますよね！笑",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "おおかみ",
            "text": "私の大学の先輩が雷鳥の保護活動しててw",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "おおかみ",
            "text": "お菓子もありますね",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "こんぶ",
            "text": "えー！すごい！",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "ちくわ",
            "text": "おおー凄いですね",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "おおかみ",
            "text": "コスパ悪いことしてて",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "ちくわ",
            "text": "そうなんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "こんぶ",
            "text": "というと？",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "おおかみ",
            "text": "ライチョウを追っかけ回して近くから獲物に狙われないようにしてるという",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "おおかみ",
            "text": "保護班みたいなことしてます",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "ちくわ",
            "text": "なるほど！",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "こんぶ",
            "text": "そんな活動があるんですか！！",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "おおかみ",
            "text": "ダーウィンが来たにもそれが放映されて、何してるんだろうと思ってみてました",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "こんぶ",
            "text": "長野？で？",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "ちくわ",
            "text": "でも雷鳥も数が少ないと聞きます",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "おおかみ",
            "text": "そうです",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "おおかみ",
            "text": "乗鞍岳でやってます",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "おおかみ",
            "text": "だいぶ少ないみたいですね...",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "こんぶ",
            "text": "あーー！！そうなんですね！",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "ちくわ",
            "text": "なので保護活動してくださることが",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "おおかみ",
            "text": "警戒心が薄いらしくすぐ獲物に捕まっちゃうみたいで、、",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "ちくわ",
            "text": "有難いですね！",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "こんぶ",
            "text": "@ちくわ ね！そうらしいね！",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "おおかみ",
            "text": "@ちくわ と、いうことらしいですね",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "こんぶ",
            "text": "知らなかったですー！",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "おおかみ",
            "text": "宣伝でした笑",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "ちくわ",
            "text": "宣伝！笑",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "こんぶ",
            "text": "情報ありがとうございます！笑",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "ちくわ",
            "text": "素晴らしい宣伝です！",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "おおかみ",
            "text": "長野といえば勝手にやっぱりスキーのイメージですかね...",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "おおかみ",
            "text": "スノボもですが",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "こんぶ",
            "text": "まあ、そうですね〜",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "こんぶ",
            "text": "私はスノボを！",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "ちくわ",
            "text": "そうですね！ウインタースポーツ盛んですね",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "おおかみ",
            "text": "長野オリンピックのイメージからですねきっと。",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "こんぶ",
            "text": "たしかに！それはありますね！",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "ちくわ",
            "text": "@おおかみ それありますねー！",
            "mention_to": [
                "おおかみ"
            ]
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "おおかみ",
            "text": "お二人で行かれるんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "おおかみ",
            "text": "お子様も連れて",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "こんぶ",
            "text": "まだ行ったことはないです笑",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "おおかみ",
            "text": "あ、そうなんですか",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "ちくわ",
            "text": "子供が小さいので！",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "こんぶ",
            "text": "一緒に行こうとしたら、私が妊娠しました笑",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "おおかみ",
            "text": "@こんぶ めでたいことです！",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "ちくわ",
            "text": "@こんぶ ソリしに行きたいね！",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "こんぶ",
            "text": "そう！娘はまだデビューできず、、笑",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "こんぶ",
            "text": "@ちくわ ねー！ソリ行きたい！！",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "こんぶ",
            "text": "@おおかみ ありがとうございます〜！",
            "mention_to": [
                "おおかみ"
            ]
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "おおかみ",
            "text": "@こんぶ ソリいいですね",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "こんぶ",
            "text": "大人でも楽しいやつ！！！",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "おおかみ",
            "text": "お子さんが乗ってたら絶対に可愛い",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "ちくわ",
            "text": "今のソリって",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "ちくわ",
            "text": "ハンドルとか付いてるみたいで",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "こんぶ",
            "text": "乗ってるの可愛いですよね〜",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "こんぶ",
            "text": "えーー！そうなの！！",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "ちくわ",
            "text": "操縦できる！笑",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "おおかみ",
            "text": "@ちくわ すごい、、",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "こんぶ",
            "text": "そんなソリあるんだ！！！",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "おおかみ",
            "text": "進化してますね",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "ちくわ",
            "text": "あと、スキー場だとタイヤに乗って滑る！",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "こんぶ",
            "text": "@ちくわ それ！欲しい！！笑",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "おおかみ",
            "text": "タイヤに乗って、、？",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "こんぶ",
            "text": "あーー！タイヤね！見たことある！",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "おおかみ",
            "text": "全然知らなかったです",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "ちくわ",
            "text": "@こんぶ 欲しいよね！見つけたら買おう！",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "おおかみ",
            "text": "発展してるなぁ",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "こんぶ",
            "text": "@ちくわ わーい、わーい！",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "ちくわ",
            "text": "@おおかみ タイヤめっちゃ早いです！笑",
            "mention_to": [
                "おおかみ"
            ]
        },
        {
            "utterance_id": 103,
            "interlocutor_id": "こんぶ",
            "text": "ね！発展してますよね〜",
            "mention_to": []
        },
        {
            "utterance_id": 104,
            "interlocutor_id": "おおかみ",
            "text": "@ちくわ そうしましょう",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 105,
