    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "うさぎ",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "にんじん",
            "text": "こんにちは！",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "たまねぎ",
            "text": "こんにちは！",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "うさぎ",
            "text": "ベットとかわれてますか？",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "うさぎ",
            "text": "ペット",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "にんじん",
            "text": "@うさぎ 犬飼ってました！",
            "mention_to": [
                "うさぎ"
            ]
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "うさぎ",
            "text": "@にんじん 過去形ですか？",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "にんじん",
            "text": "5年ほど前に亡くなりまして",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "うさぎ",
            "text": "@にんじん うわーペットロス大丈夫でしたか？",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "たまねぎ",
            "text": "@うさぎ チワワでした",
            "mention_to": [
                "うさぎ"
            ]
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "うさぎ",
            "text": "@たまねぎ チワワかわいい",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "にんじん",
            "text": "@うさぎ 今はもう大丈夫ですが",
            "mention_to": [
                "うさぎ"
            ]
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "にんじん",
            "text": "当時は寂しくて",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "たまねぎ",
            "text": "白いチワワです",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "うさぎ",
            "text": "@にんじん ですよね、周りにもいます",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "うさぎ",
            "text": "@たまねぎ 白！いいですね",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "にんじん",
            "text": "でも、子供にはいい経験かなと",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "うさぎ",
            "text": "お子さんも悲しがったんですね",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "にんじん",
            "text": "そうですねー",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "うさぎ",
            "text": "命の教育ですね",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "にんじん",
            "text": "噛まれてましたけど笑",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "うさぎ",
            "text": "嬉しいと噛むんですよね",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "にんじん",
            "text": "いや、マジガミでした笑",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "にんじん",
            "text": "でも、歯がなかったから痛くなくて笑",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "うさぎ",
            "text": "この間、12人襲われてましたよね？",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "うさぎ",
            "text": "怖いですよね",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "にんじん",
            "text": "@うさぎ え！そーなんですか？？",
            "mention_to": [
                "うさぎ"
            ]
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "うさぎ",
            "text": "土佐犬？",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "うさぎ",
            "text": "逃げ出して",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "たまねぎ",
            "text": "@うさぎ 昔ヘビの殺人もありましたね！",
            "mention_to": [
                "うさぎ"
            ]
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "うさぎ",
            "text": "塀乗り越えて",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "にんじん",
            "text": "@うさぎ えー",
            "mention_to": [
                "うさぎ"
            ]
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "うさぎ",
            "text": "しかも狂犬病注射してなかったらしく",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "にんじん",
            "text": "@うさぎ うんうん",
            "mention_to": [
                "うさぎ"
            ]
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "うさぎ",
            "text": "@たまねぎ へび？！",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "うさぎ",
            "text": "入れ違いごめんなさい、噛まれた小学生の親が怒ってました",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "たまねぎ",
            "text": "@うさぎ 病気とか大丈夫なんですかね？",
            "mention_to": [
                "うさぎ"
            ]
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "にんじん",
            "text": "責任取れないならかっちゃだめですよね",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "うさぎ",
            "text": "@たまねぎ 狂犬病発症するとら90%の致死率",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "うさぎ",
            "text": "だから怖がられてるらしく",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "たまねぎ",
            "text": "そりゃ怖い",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "うさぎ",
            "text": "日本ではほぼ絶滅らしいですが、",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "にんじん",
            "text": "@うさぎ 詳しいですね！",
            "mention_to": [
                "うさぎ"
            ]
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "うさぎ",
            "text": "@たまねぎ 蛇の殺人って日本ですか？",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "たまねぎ",
            "text": "@うさぎ そーです！",
            "mention_to": [
                "うさぎ"
            ]
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "うさぎ",
            "text": "えー知らないです",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "たまねぎ",
            "text": "おじさんのうちにお泊まり行ったら",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "たまねぎ",
            "text": "ヘビ逃げて",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "うさぎ",
            "text": "うんうん",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "たまねぎ",
            "text": "小学生2人",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "うさぎ",
            "text": "2人も？",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "たまねぎ",
            "text": "締め殺してしまったみたいです",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "うさぎ",
            "text": "ぎゃーーー",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "にんじん",
            "text": "@たまねぎ 丸呑みかと思った！",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "うさぎ",
            "text": "最近ですか？",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "たまねぎ",
            "text": "@うさぎ いや、結構前です！",
            "mention_to": [
                "うさぎ"
            ]
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "うさぎ",
            "text": "@にんじん もはや映画ですよね",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "たまねぎ",
            "text": "ニュースになってて",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "うさぎ",
            "text": "アナコンダ？",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "にんじん",
            "text": "@うさぎ アナコンダのやつもありましたね！",
            "mention_to": [
                "うさぎ"
            ]
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "うさぎ",
            "text": "そこまで大蛇じゃないのかな？",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "にんじん",
            "text": "奥さん丸呑みのやつ",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "にんじん",
            "text": "海外だったけど",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "うさぎ",
            "text": "奥さんでしたか？！",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "うさぎ",
            "text": "いやー",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "うさぎ",
            "text": "怖い",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "うさぎ",
            "text": "丸呑み怖すぎ",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "たまねぎ",
            "text": "海外はなんでも規格外だすね！",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "にんじん",
            "text": "恐ろしい！",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "うさぎ",
            "text": "映画になると余計",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "うさぎ",
            "text": "ジョーズとか昔怖くて",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "うさぎ",
            "text": "今はそんなにですが",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "たまねぎ",
            "text": "俺はターミネーターが怖かったです",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "にんじん",
            "text": "私はエイリアン",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "うさぎ",
            "text": "@たまねぎ 見てましたーー",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "うさぎ",
            "text": "エイリアン！",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "にんじん",
            "text": "トラウマになりますよね！",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "うさぎ",
            "text": "オカルトものもね",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "にんじん",
            "text": "あぁいう映画って！",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "うさぎ",
            "text": "映画どんな系統みますか？",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "たまねぎ",
            "text": "割となんでも見ますけどホラーとか好きですね！",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "にんじん",
            "text": "私はジブリです！",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "うさぎ",
            "text": "@たまねぎ だから詳しいんですね",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "うさぎ",
            "text": "@にんじん ジブリいいですよね",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "うさぎ",
            "text": "テーマパークありますよね？",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "にんじん",
            "text": "ありますね！",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "うさぎ",
            "text": "行かれましたか？",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "たまねぎ",
            "text": "行ったことないです！",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "うさぎ",
            "text": "予約制でしたっけ？",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "にんじん",
            "text": "@うさぎ 今どこもそうですよね！",
            "mention_to": [
                "うさぎ"
            ]
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "うさぎ",
            "text": "ジブリの森？でしたか？",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "たまねぎ",
            "text": "@うさぎ どこにありましたっけ？",
            "mention_to": [
                "うさぎ"
            ]
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "うさぎ",
            "text": "@たまねぎ 私も今思ってて東京、愛知こんがらがってます",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "うさぎ",
            "text": "ジブリ、何がお好きですか？",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "にんじん",
            "text": "@うさぎ トトロです",
            "mention_to": [
                "うさぎ"
            ]
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "うさぎ",
            "text": "トトロ！定番ですね",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "にんじん",
            "text": "かわいいですね！",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "にんじん",
            "text": "話も分かりやすいし",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "うさぎ",
            "text": "ママさんコーラスで歌いました",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "たまねぎ",
            "text": "@うさぎ おー",
            "mention_to": [
                "うさぎ"
            ]
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "うさぎ",
            "text": "子供達大喜び",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "うさぎ",
            "text": "下手なのに笑",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "たまねぎ",
            "text": "魔女宅もいい歌ですよね",
            "mention_to": []
        },
        {
            "utterance_id": 103,
            "interlocutor_id": "たまねぎ",
            "text": "@うさぎ 気持ちです！",
            "mention_to": [
                "うさぎ"
            ]
        },
        {
            "utterance_id": 104,
            "interlocutor_id": "うさぎ",
            "text": "実写版もありましたよね？",
            "mention_to": []
        },
        {
            "utterance_id": 105,
            "interlocutor_id": "にんじん",
            "text": "@うさぎ ありましたね！",
            "mention_to": [
                "うさぎ"
            ]
        }
