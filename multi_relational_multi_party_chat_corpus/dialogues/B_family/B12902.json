        },
        {
            "utterance_id": 40,
            "interlocutor_id": "いるか",
            "text": "強引に笑",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "たまご",
            "text": "楽しかった！",
