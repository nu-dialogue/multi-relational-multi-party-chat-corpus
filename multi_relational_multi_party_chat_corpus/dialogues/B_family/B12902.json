        },
        {
            "utterance_id": 48,
            "interlocutor_id": "いるか",
            "text": "監督的な！",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "たまご",
            "text": "大学でも何個か作りました",
