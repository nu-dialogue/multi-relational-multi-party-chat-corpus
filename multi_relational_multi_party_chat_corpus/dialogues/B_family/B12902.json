        },
        {
            "utterance_id": 51,
            "interlocutor_id": "いるか",
            "text": "すごい！",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "たまご",
            "text": "出る側も良いけど作る側もたのしい",
