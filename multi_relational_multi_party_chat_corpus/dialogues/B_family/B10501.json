    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "りんご",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "えのき",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "てばさき",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "りんご",
            "text": "宜しくお願いします",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "えのき",
            "text": "お願いします",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "てばさき",
            "text": "よろしくお願いします！",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "りんご",
            "text": "今日は風がすごいですね",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "てばさき",
            "text": "そうですね",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "えのき",
            "text": "春一番みたいですね",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "えのき",
            "text": "ニュースで言ってました",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "りんご",
            "text": "そうみたいですね",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "えのき",
            "text": "すごい強風ですよね",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "てばさき",
            "text": "昨日は暖かかったです",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "りんご",
            "text": "春一番なのに今日は寒くなってしまって",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "りんご",
            "text": "昨日の暖かさ4月ですよね",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "えのき",
            "text": "晴れてはいるのに、寒いんですね",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "えのき",
            "text": "昨日は暖かくてよかったです",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "てばさき",
            "text": "これぞ三寒四温",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "りんご",
            "text": "本当ですね！",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "えのき",
            "text": "でも風が強くて、自転車で煽られそうになってしまいました・・・",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "りんご",
            "text": "自転車はやばいですね",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "えのき",
            "text": "車道のすぐ脇を走っているので、突風が怖いです",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "りんご",
            "text": "@てばさき 自転車通学ですか？",
            "mention_to": [
                "てばさき"
            ]
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "てばさき",
            "text": "歩いている時の強風はなかなかに楽しいですけどね",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "えのき",
            "text": "@りんご 通勤です。普段、自転車には乗りますか？",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "てばさき",
            "text": "@りんご 電車です！",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "りんご",
            "text": "自転車もってないです",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "えのき",
            "text": "すみません、てばさんは電車です",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "えのき",
            "text": "そうなんですね",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "りんご",
            "text": "そうなんですね！",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "りんご",
            "text": "電車の中って暑くないですか？",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "てばさき",
            "text": "@りんご 暖房効きすぎるとそうなりますよね",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "えのき",
            "text": "@りんご 普段は車ですか？",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "りんご",
            "text": "そうなんです！",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "りんご",
            "text": "田舎なのでほぼ車です",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "えのき",
            "text": "そうなんですね",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "てばさき",
            "text": "@りんご 田舎は車必須ってよく効きます",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "りんご",
            "text": "@えのき車のりますか？",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "えのき",
            "text": "うちは、車を暗転できないので・・・どこでも自転車です",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "えのき",
            "text": "ペーパードライバーなんです",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "りんご",
            "text": "でも自転車のが快適な時ありますよね",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "てばさき",
            "text": "かろうじて田舎住まいの姉と祖母は運転できます！",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "えのき",
            "text": "駐車場の心配が不要ですね",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "りんご",
            "text": "そうなんです、駐車場問題あります",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "えのき",
            "text": "都会だと、お金もかかりますし・・・",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "りんご",
            "text": "駐車場代を払うのもね",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "えのき",
            "text": "あと、一方通行が・・・よくわからない・・・",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "てばさき",
            "text": "母と一緒に自転車乗ると速過ぎてよく置いてかれそうになります",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "りんご",
            "text": "そんなに早いの？",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "えのき",
            "text": "速いですかね・・・",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "りんご",
            "text": "すごい速さなのでは？",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "えのき",
            "text": "一応20km以下を目安に走っていますが",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "てばさき",
            "text": "@えのき 警察にいつかお世話になると思ってます",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "えのき",
            "text": "そんなことないよ！",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "りんご",
            "text": "えーそんなに！",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "えのき",
            "text": "一時停止でも一応止まってるし…",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "えのき",
            "text": "周りをよく見てるし…ヘルメットもかぶってる！",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "りんご",
            "text": "自転車も一応車両ですからね",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "てばさき",
            "text": "あと自分だけ電動自転車の時は坂道でも容赦ない",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "えのき",
            "text": "坂道は一気に行きたい",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "りんご",
            "text": "すごいですね！電動なんですね",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "てばさき",
            "text": "@えのき やめて",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "えのき",
            "text": "電動なので、10km20km行っちゃいます※距離",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "りんご",
            "text": "そんな早いんだぁ",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "てばさき",
            "text": "@えのき 無限の体力",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "えのき",
            "text": "自転車の遠乗りは、6時間くらいで帰るようにしてる",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "えのき",
            "text": "一応",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "てばさき",
            "text": "おかしいな、若さは私の方があるはずなのに",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "りんご",
            "text": "えー遠乗り？自転車で？",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "りんご",
            "text": "@えのき どこまでいくんですか？",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "えのき",
            "text": "最近ドラクエウォークをやってて…",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "えのき",
            "text": "ゲームの中の目的地があるので、そこまで行きます",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "てばさき",
            "text": "１キロごとにあるスポット片っ端から巡ってますね",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "りんご",
            "text": "なるほど！ポケモンゴーみたいなやつ？",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "えのき",
            "text": "そんな感じです",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "てばさき",
            "text": "@りんご 合ってます",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "りんご",
            "text": "よかったです、間違ってなくて",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "えのき",
            "text": "@りんご りんごさんは何かゲームとかやりますか？",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "りんご",
            "text": "ゲームとかあまりしないんです",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "えのき",
            "text": "普段、何か趣味とかやります？",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "りんご",
            "text": "温泉周りなんです",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "りんご",
            "text": "地味",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "えのき",
            "text": "いいですね！",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "てばさき",
            "text": "温泉！いいですねぇ",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "えのき",
            "text": "よかったところありますか？参考にしたいです～",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "りんご",
            "text": "@てばさき @えのき 行きますか？",
            "mention_to": [
                "てばさき",
                "えのき"
            ]
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "てばさき",
            "text": "熱海とか草津とか鬼怒川とかしか知りませんし、行ったこともないですね",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "えのき",
            "text": "北海道に住んでいた時は結構気軽に行ってました",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "えのき",
            "text": "温泉地に住んでいたので",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "えのき",
            "text": "普段入る銭湯が温泉",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "りんご",
            "text": "北海道！温泉地？いあなぁ",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "えのき",
            "text": "北海道は温泉多いんですよね",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "りんご",
            "text": "そうなんだ",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "てばさき",
            "text": "あそこって温泉地なんだ…",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "りんご",
            "text": "さすがに北海道は行ったことないかも",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "えのき",
            "text": "そうだよ。あのへんの旅館みんな温泉だったんだよ実は",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "てばさき",
            "text": "まあ北海道は活火山多いらしいし",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "えのき",
            "text": "有珠山とか結構最近噴火してるし",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "りんご",
            "text": "なるほど！北海道なら景色もいいですね",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "えのき",
            "text": "車は必須ですけどね！",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "りんご",
            "text": "ですね、車ないとあの距離はね",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "えのき",
            "text": "いちいち距離が遠いんですよね・・・",
            "mention_to": []
        }
