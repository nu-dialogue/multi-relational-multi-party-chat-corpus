        "こんぶ"
    ],
    "relationship": [
        "ちくわ",
        "こんぶ"
    ],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "いるか",
            "text": "おはようございます！",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "ちくわ",
            "text": "おはようございます！",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "こんぶ",
            "text": "おはようございます！",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "ちくわ",
            "text": "今無性に飲みたいものあります？",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "ちくわ",
            "text": "ぼくは、、牛乳です！",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "こんぶ",
            "text": "最近は白湯にハマってる",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "いるか",
            "text": "@ちくわ 急に炭酸が飲みたくなります！",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "いるか",
            "text": "疲れてるときに！",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "こんぶ",
            "text": "炭酸！！！わかります！！！",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "ちくわ",
            "text": "白湯！",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "ちくわ",
            "text": "炭酸わかります！！",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "こんぶ",
            "text": "ぐびぐびっと",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "ちくわ",
            "text": "コーラとか",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "こんぶ",
            "text": "私たちも、炭酸は常備してます",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "いるか",
            "text": "白湯！健康的！",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "ちくわ",
            "text": "最近家でも缶のコーラ買ってます",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "いるか",
            "text": "@こんぶ わかります～！うちもです！",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "ちくわ",
            "text": "@こんぶ 寒いから余計に白湯飲んでるよね！",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "こんぶ",
            "text": "@いるか 白湯もけっこうグビグビいけちゃいますよー！",
            "mention_to": [
                "いるか"
            ]
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "いるか",
            "text": "あれ、どのくらいの温度で飲むんですか？？",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "こんぶ",
            "text": "@ちくわ そうそう！ぬるいくらいまで冷めるとよき",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "ちくわ",
            "text": "白湯をぐびぐび、、",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "いるか",
            "text": "なるほど！！！",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "こんぶ",
            "text": "えーー私は、ゆっても猫舌なので",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "ちくわ",
            "text": "氷いれてるときあるよね！",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "こんぶ",
            "text": "けっこう冷ましちゃいます",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "こんぶ",
            "text": "あ、バレてる笑",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "ちくわ",
            "text": "めちゃ猫舌やね！",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "いるか",
            "text": "猫舌！",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "ちくわ",
            "text": "白湯の意味ーーってな",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "こんぶ",
            "text": "そう！どうしてもすぐ飲みたい時は、氷で微調整です",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "こんぶ",
            "text": "ほんとそれ",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "いるか",
            "text": "お湯に氷はもう笑",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "いるか",
            "text": "そして、牛乳も分かります・・・！",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "ちくわ",
            "text": "朝の白湯は体にいいもんね",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "こんぶ",
            "text": "ただの水ですね",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "いるか",
            "text": "@こんぶ 笑",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "こんぶ",
            "text": "牛乳好き家族で、すーぐなくなります",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "ちくわ",
            "text": "僕牛乳の消費量全国で上位にいます",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "ちくわ",
            "text": "自分で確信持ってるやつ",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "いるか",
            "text": "レベルが違う・・・！",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "こんぶ",
            "text": "あ、勝手な推測ですよ！",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "いるか",
            "text": "一週間でどのくらい消費します？？",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "ちくわ",
            "text": "めちゃくちゃ牛乳飲むんすよ！",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "こんぶ",
            "text": "まだまだ成長期なのでしょうか？",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "こんぶ",
            "text": "えーー４本？",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "いるか",
            "text": "@こんぶ 成長期笑",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "ちくわ",
            "text": "身長止まってます、、、",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "こんぶ",
            "text": "@いるか 大きくなぁれ〜〜",
            "mention_to": [
                "いるか"
            ]
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "ちくわ",
            "text": "1週間で4本はいってるかー",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "こんぶ",
            "text": "私や娘も飲むんですが",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "いるか",
            "text": "ほとんどをちくわさんが？",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "こんぶ",
            "text": "旦那が大半をしめてますね！",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "ちくわ",
            "text": "ご飯中でも牛乳飲めるくらいですよ！",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "いるか",
            "text": "おぉ～",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "こんぶ",
            "text": "牛乳も安くないじゃないですか！！",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "ちくわ",
            "text": "大半しめてるか、、",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "いるか",
            "text": "@こんぶ そう！分かります！",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "いるか",
            "text": "最近少しまた高くなった！",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "こんぶ",
            "text": "一本200円くらいはするのにーー",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "ちくわ",
            "text": "でも、近くのスーパーで",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "こんぶ",
            "text": "@いるか ね！ですよね！！！",
            "mention_to": [
                "いるか"
            ]
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "ちくわ",
            "text": "半額のクーポン出るんですよ！",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "こんぶ",
            "text": "そう！半額のクーポンが狙い目です！",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "いるか",
            "text": "@ちくわ 半額は大きい！",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "ちくわ",
            "text": "@こんぶ その日はしっかり買ってるよね！",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "ちくわ",
            "text": "ありがたいです。消費者としては",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "いるか",
            "text": "間違いなくかってしまう！",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "こんぶ",
            "text": "そう！違う店舗のも登録して",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "こんぶ",
            "text": "店舗またいで買ってます！",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "ちくわ",
            "text": "牛乳は飲まれます？",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "いるか",
            "text": "でも、賞味期限がまたそんな長くないんですよね・・・",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "こんぶ",
            "text": "あーー！たしかに！！！",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "いるか",
            "text": "うちも・・・週４～５本無くなります・・・",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "ちくわ",
            "text": "賞味期限ねー",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "ちくわ",
            "text": "気にしないくらい早く飲むので、、",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "こんぶ",
            "text": "お！！！うちらと一緒ですね！！",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "ちくわ",
            "text": "何も考えてなかったです",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "いるか",
            "text": "@ちくわ それはいいこと・・・！",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "こんぶ",
            "text": "そうなんですよ、賞味期限前に飲んでしまいます",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "ちくわ",
            "text": "@いるか いいことにしときましょう！",
            "mention_to": [
                "いるか"
            ]
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "ちくわ",
            "text": "@いるか 何人家族ですか？",
            "mention_to": [
                "いるか"
            ]
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "こんぶ",
            "text": "賞味期限て美味しくのめる期間？てこと？",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "ちくわ",
            "text": "@こんぶ たぶん、、",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "いるか",
            "text": "@ちくわ ３人家族です！",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "ちくわ",
            "text": "一緒ですね！",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "ちくわ",
            "text": "僕らも3人家族です！",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "こんぶ",
            "text": "@いるか 一緒です！3人！",
            "mention_to": [
                "いるか"
            ]
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "いるか",
            "text": "@こんぶ 消費期限が一番危ないやつ！",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "こんぶ",
            "text": "牛乳苦手な人も多くいますからね〜",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "ちくわ",
            "text": "3人でもわちゃわちゃです！",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "いるか",
            "text": "お、いっしょですね！",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "こんぶ",
            "text": "その中でも、私たちは",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "こんぶ",
            "text": "牛乳消費家族ですね！！！",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "ちくわ",
            "text": "1人2人増えただけで賑やかになりますよね！",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "いるか",
            "text": "@ちくわ わちゃわちゃ、可愛いですね笑",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "ちくわ",
            "text": "牛乳消費家族、、",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "ちくわ",
            "text": "おもしろい！",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "こんぶ",
            "text": "牛乳常備消費家族！",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "ちくわ",
            "text": "@いるか まだ一歳なのですごいです。",
            "mention_to": [
                "いるか"
            ]
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "こんぶ",
            "text": "早口言葉になりそう",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "ちくわ",
            "text": "動きが！！",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "ちくわ",
            "text": "新しい早口言葉やね！",
            "mention_to": []
        }
