    ],
    "relationship": [
        "にんじん",
        "たまねぎ"
    ],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "くじら",
            "text": "おはようございます",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "にんじん",
            "text": "おはようございます！",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "たまねぎ",
            "text": "おはようございます！",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "くじら",
            "text": "今日は夕飯何にしますか",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "にんじん",
            "text": "@くじら まだ決めてないですー！",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "たまねぎ",
            "text": "カレーがいいなぁ",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "くじら",
            "text": "悩みますよね",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "くじら",
            "text": "ひき肉があるのですけれど",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "にんじん",
            "text": "カレー楽チンー",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "たまねぎ",
            "text": "ひき肉料理？？",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "くじら",
            "text": "ひき肉でなんの料理が好きですか？",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "にんじん",
            "text": "うちはハンバーグかそぼろ丼ですね！",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "くじら",
            "text": "この間は麻婆豆腐にしました",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "にんじん",
            "text": "昨日のご飯そぼろ丼でした！",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "くじら",
            "text": "美味しそうですね",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "たまねぎ",
            "text": "@くじら 麻婆豆腐もひき肉だー",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "くじら",
            "text": "ハンバーグ、手がべたべたになるのが悩みで",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "にんじん",
            "text": "はいはい",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "にんじん",
            "text": "私爪が長いので手袋しちゃいます",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "くじら",
            "text": "手袋ですね",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "にんじん",
            "text": "終わったら捨てるだけ",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "くじら",
            "text": "しばらく手がギトギトになるので困ってました",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "たまねぎ",
            "text": "こねまくりますもんね！",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "くじら",
            "text": "そうなんですよ",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "くじら",
            "text": "ハンバーグのソースは何が好きですか？",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "にんじん",
            "text": "@くじら ケチャップとソースまぜたやつかなー",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "くじら",
            "text": "@にんじん 同じですね",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "にんじん",
            "text": "美味しいですよねー",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "くじら",
            "text": "美味しいですね",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "くじら",
            "text": "今日はハンバーグにしようかな",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "たまねぎ",
            "text": "@くじら お！決まりましたね",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "くじら",
            "text": "お子さんの好きな料理は？",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "たまねぎ",
            "text": "@くじら 唐揚げ！",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "くじら",
            "text": "家もです",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "くじら",
            "text": "毎日でもたぶん大丈夫そう",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "たまねぎ",
            "text": "間違い無いですよね！",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "にんじん",
            "text": "そーそー",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "くじら",
            "text": "ケンタッキーも好きで",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "にんじん",
            "text": "何個あげてもすぐなくなる",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "くじら",
            "text": "月に一度はケンタッキー食べてます",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "たまねぎ",
            "text": "@くじら あ！わかります！",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "にんじん",
            "text": "@くじら そんなにーー？",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "くじら",
            "text": "@たまねぎ たまねぎさんも？",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "くじら",
            "text": "あと何かあるとマック行きたいになりますね",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "たまねぎ",
            "text": "この前食べたばっかりです！",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "くじら",
            "text": "そうでしたか",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "にんじん",
            "text": "@くじら マックは家の近くだからすぐ行きます笑",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "くじら",
            "text": "近くにあったら行きますね",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "にんじん",
            "text": "行っちゃいますね！",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "くじら",
            "text": "家は少し歩かないとないので",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "にんじん",
            "text": "はい",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "にんじん",
            "text": "ポテト中毒やばいです",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "くじら",
            "text": "そうですよね",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "たまねぎ",
            "text": "無性に食べたくなるやつー",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "くじら",
            "text": "ポテトは止まらない",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "にんじん",
            "text": "妊娠中とか特に",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "くじら",
            "text": "あのしょっぱいのが良いのですよね",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "たまねぎ",
            "text": "お昼はマックかなーー",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "くじら",
            "text": "お昼、マック良いですね",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "にんじん",
            "text": "逆に朝マック好きじゃないです",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "たまねぎ",
            "text": "ポテトないしね",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "たまねぎ",
            "text": "朝からポテト大丈夫なのにー",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "くじら",
            "text": "なるほど朝はなかったですね",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "にんじん",
            "text": "そうなんですよ",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "くじら",
            "text": "昔はハッピーセット目当てに行ってました",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "にんじん",
            "text": "@くじら そーそー！",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "にんじん",
            "text": "おもちゃ欲しかった！",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "くじら",
            "text": "でも、たいして遊ばなかったな",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "たまねぎ",
            "text": "クオリティ高いしね",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "にんじん",
            "text": "うちは最近もらってません",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "くじら",
            "text": "もうハッピーでは足りなくて",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "にんじん",
            "text": "コマーシャルで見るとすごく欲しいけど",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "たまねぎ",
            "text": "@くじら 確かに足りなくなる笑",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "くじら",
            "text": "ＣＭ見たら欲しくなりそうですね",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "くじら",
            "text": "今では私より食べますし",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "たまねぎ",
            "text": "@くじら ですよね？",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "にんじん",
            "text": "ほうほう",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "くじら",
            "text": "はい、食べますね",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "くじら",
            "text": "野菜はちっともですけれど",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "にんじん",
            "text": "野菜おいしいのー",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "くじら",
            "text": "きれいにキャベツの千切りだけ残します",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "にんじん",
            "text": "あら！",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "くじら",
            "text": "お子さんは野菜食べますか？",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "にんじん",
            "text": "賢い笑",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "にんじん",
            "text": "野菜好きです！",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "にんじん",
            "text": "豚しゃぶサラダよくたべます！",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "くじら",
            "text": "千切りたいへんなんだよというのですけれど",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "くじら",
            "text": "豚しゃぶ、良いですね",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "くじら",
            "text": "ポン酢で食べますか？",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "たまねぎ",
            "text": "@くじら 千切り大変だよー",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "にんじん",
            "text": "@くじら はい！",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "にんじん",
            "text": "あとはドレッシング！",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "くじら",
            "text": "千切りはたいへんですよね",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "くじら",
            "text": "ポン酢なら食べるかしら",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "にんじん",
            "text": "ポン酢も好き嫌いありますよねー",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "たまねぎ",
            "text": "ゆずぽんおいしー",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "くじら",
            "text": "酸っぱいから",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "にんじん",
            "text": "そー酸っぱい",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "くじら",
            "text": "家はソース好きなのかなあ",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "くじら",
            "text": "キャベツにはソースかけてたかと",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "にんじん",
            "text": "あー",
            "mention_to": []
        }
