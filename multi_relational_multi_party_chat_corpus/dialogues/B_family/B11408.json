    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "ぱんだ",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "つみれ",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "たまご",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "ぱんだ",
            "text": "最近のマイブームとかありますか？",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "ぱんだ",
            "text": "私は温活です！",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "ぱんだ",
            "text": "とにかく冷やさないようにしてます！",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "つみれ",
            "text": "温活！",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "つみれ",
            "text": "どんな対策してるんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "ぱんだ",
            "text": "@つみれ 温活です！",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "たまご",
            "text": "たまにサウナ入ります",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "ぱんだ",
            "text": "まず白湯ですね",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "ぱんだ",
            "text": "@たまご サウナ！行くんですか？",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "つみれ",
            "text": "私も白湯は持ち歩いてます！",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "ぱんだ",
            "text": "@つみれ すごーい！",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "ぱんだ",
            "text": "全くの白湯ですか？",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "つみれ",
            "text": "全くの白湯です！",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "つみれ",
            "text": "水と白湯のどちらか",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "たまご",
            "text": "たまにですけど",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "つみれ",
            "text": "水筒に入れて持ってます",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "ぱんだ",
            "text": "白湯って、水道水をガスで鍋で15分以上沸騰させる水を白湯って言うらしいです",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "ぱんだ",
            "text": "@つみれ えらい！意識して白湯ですか？",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "つみれ",
            "text": "私、お湯のことならなんでも白湯っていうと思ってたんですけど、",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "ぱんだ",
            "text": "私は全く味のしない飲み物が無理でして",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "つみれ",
            "text": "友人に違うって教わりました",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "ぱんだ",
            "text": "@つみれ おお！",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "ぱんだ",
            "text": "で、どうしてますか？",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "ぱんだ",
            "text": "やっぱり鍋で沸かす？",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "ぱんだ",
            "text": "@たまご サウナ、私はのぼせます！",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "たまご",
            "text": "@ぱんだ いいですね",
            "mention_to": [
                "ぱんだ"
            ]
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "ぱんだ",
            "text": "私は白湯にナツメを入れてます",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "たまご",
            "text": "のぼせちゃうんですね",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "つみれ",
            "text": "@ぱんだ いえ、ウォーターサーバーのお湯を持っていきます笑",
            "mention_to": [
                "ぱんだ"
            ]
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "つみれ",
            "text": "白湯でないですね",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "ぱんだ",
            "text": "@たまご サウナはスーパー銭湯的なところで入りますか？",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "つみれ",
            "text": "お湯を持ち歩いてますが正解",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "ぱんだ",
            "text": "@つみれ なるほど！",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "つみれ",
            "text": "@ぱんだ ナツメ？",
            "mention_to": [
                "ぱんだ"
            ]
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "ぱんだ",
            "text": "でも、結果的に内臓温まれば良いわけですからね！",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "たまご",
            "text": "@ぱんだ そういうとこで入ります",
            "mention_to": [
                "ぱんだ"
            ]
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "つみれ",
            "text": "@ぱんだ そうですね！",
            "mention_to": [
                "ぱんだ"
            ]
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "つみれ",
            "text": "サウナも温活になるんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "ぱんだ",
            "text": "@たまご ですよね！サウナ好きの友人のママはご主人にサウナ付きの家を建ててもらってました！",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "ぱんだ",
            "text": "電気代半端ない！",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "たまご",
            "text": "すごい！！",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "たまご",
            "text": "家にほしいですl",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "つみれ",
            "text": "@ぱんだ 家にサウナ？富豪ですか？",
            "mention_to": [
                "ぱんだ"
            ]
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "ぱんだ",
            "text": "@たまご 今はその家売ってしまい",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "ぱんだ",
            "text": "スーパー銭湯に毎日通っているようです。",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "たまご",
            "text": "@つみれ 本当に",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "ぱんだ",
            "text": "@たまご 不動産会社の社長さんでした",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "たまご",
            "text": "それもありです",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "つみれ",
            "text": "お家売っちゃったんですね",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "ぱんだ",
            "text": "余りにもお金のかかる家だったようで",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "たまご",
            "text": "すごすぎます",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "ぱんだ",
            "text": "今時じゃない！って売ったようです",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "ぱんだ",
            "text": "売って正解ですね",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "ぱんだ",
            "text": "電気代かなり高いですもの",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "つみれ",
            "text": "普通じゃ、とても住めないですね！",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "たまご",
            "text": "すごすぎる！",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "たまご",
            "text": "電気代すごいです",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "つみれ",
            "text": "まず建てちゃったのがすごいですけど",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "ぱんだ",
            "text": "家もほぼオール電化ですが",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "ぱんだ",
            "text": "電気代半端ないです",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "つみれ",
            "text": "@ぱんだ うちもです！",
            "mention_to": [
                "ぱんだ"
            ]
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "ぱんだ",
            "text": "@つみれ 確かに！ですね",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "つみれ",
            "text": "特に冬がやばいですよね？",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "ぱんだ",
            "text": "@つみれ そうなんですね！",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "たまご",
            "text": "暖房が",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "ぱんだ",
            "text": "@つみれ そうそう！",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "ぱんだ",
            "text": "石油ストーブと併用してます！",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "つみれ",
            "text": "暖房使ったら電気代一気に跳ね上がります",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "ぱんだ",
            "text": "床暖とかありますか？",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "つみれ",
            "text": "床暖はないです！",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "たまご",
            "text": "ないですー",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "ぱんだ",
            "text": "家も！",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "ぱんだ",
            "text": "だから寒いんですよねー",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "つみれ",
            "text": "蓄熱暖房１つだけです",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "ぱんだ",
            "text": "家の中で白い息が出ちゃう！",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "ぱんだ",
            "text": "蓄熱暖房！",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "たまご",
            "text": "床あったかいの最高",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "つみれ",
            "text": "うちも寒いですよ！",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "ぱんだ",
            "text": "寒すぎて、温活始めたのもあります",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "ぱんだ",
            "text": "若い時は冷えなんて気にしなかったのに",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "たまご",
            "text": "案外外より寒い時あります",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "ぱんだ",
            "text": "@たまご 家も！",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "ぱんだ",
            "text": "夏は外より熱い",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "つみれ",
            "text": "体の中から温める作戦ですね",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "ぱんだ",
            "text": "@つみれ そうなんですよ！",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "ぱんだ",
            "text": "でも脂肪が邪魔してて",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "たまご",
            "text": "ですよね",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "ぱんだ",
            "text": "中々温まりません！",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "ぱんだ",
            "text": "脂肪が冷えると冷たくて",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "つみれ",
            "text": "@ぱんだ 足とか冷え冷えです",
            "mention_to": [
                "ぱんだ"
            ]
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "つみれ",
            "text": "私も脂肪の塊なので",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "ぱんだ",
            "text": "@つみれ 私はやばいです！脂肪！",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "ぱんだ",
            "text": "ホッカイロ貼るんですが",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "ぱんだ",
            "text": "カイロの温かさ感じないくらい！",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "ぱんだ",
            "text": "脂肪が冷え冷え！",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "たまご",
            "text": "貼りまくるしかないです",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "つみれ",
            "text": "@ぱんだ 体が冷え切ってるんですね！",
            "mention_to": [
                "ぱんだ"
            ]
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "ぱんだ",
            "text": "深層部まで温かさが伝わらない！",
            "mention_to": []
        }
