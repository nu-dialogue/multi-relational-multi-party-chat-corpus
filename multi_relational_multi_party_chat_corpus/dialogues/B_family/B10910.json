    "relationship": [
        "えのき",
        "てばさき"
    ],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "ポテト",
            "text": "おはようございます！",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "えのき",
            "text": "おはようございます",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "てばさき",
            "text": "おはようございます",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "えのき",
            "text": "@ポテト 今日は何時に起床されましたか？",
            "mention_to": [
                "ポテト"
            ]
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "ポテト",
            "text": "@えのき ５時半です・・・",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "えのき",
            "text": "早いですね！",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "ポテト",
            "text": "早く寝てしまって・・・",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "えのき",
            "text": "何時に寝ました？",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "てばさき",
            "text": "@ポテト 私は7時半に叩き起こされたのに…",
            "mention_to": [
                "ポテト"
            ]
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "ポテト",
            "text": "へへ・・・１時ちょい前です・・・",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "えのき",
            "text": "え？",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "えのき",
            "text": "午前1時？",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "えのき",
            "text": "ではなく",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "ポテト",
            "text": "@てばさき ７時半！それはタタキ起こされるじかん！",
            "mention_to": [
                "てばさき"
            ]
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "ポテト",
            "text": "深夜１時ですね・・・",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "えのき",
            "text": "早く寝てなくないですか？",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "てばさき",
            "text": "@えのき  いや、逆に午後1時からならめちゃくちゃぐっすりの人では…",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "ポテト",
            "text": "@えのき 私も打ちながらあれってなりました",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "えのき",
            "text": "確かにそれだとねすぎだけれども",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "えのき",
            "text": "夜更かしの部類では？",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "ポテト",
            "text": "いつも、２時とかなので・・・",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "てばさき",
            "text": "私なら多分できますが。一時に寝て次の朝までぐっすり",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "えのき",
            "text": "@ポテト 夜行性なほうですか？",
            "mention_to": [
                "ポテト"
            ]
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "ポテト",
            "text": "@てばさき ぐっすりねられるの、才能ですよ！",
            "mention_to": [
                "てばさき"
            ]
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "えのき",
            "text": "@てばさき てばさん、ほぼほぼ一日寝てるから・・・",
            "mention_to": [
                "てばさき"
            ]
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "ポテト",
            "text": "完全夜行性です！",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "てばさき",
            "text": "@えのき 昼行性の人間見たことない",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "ポテト",
            "text": "てばさきさん、本当はねこちゃんなのでは・・・",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "えのき",
            "text": "私もそう思ってました・・・",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "てばさき",
            "text": "@ポテト 猫になりたいですねぇ",
            "mention_to": [
                "ポテト"
            ]
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "ポテト",
            "text": "それなら、寝てても仕方ない・・・",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "えのき",
            "text": "てばさん、赤ちゃんの頃、1日20時間くらい寝てましたよ・・・",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "えのき",
            "text": "育てやすい子！",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "ポテト",
            "text": "@えのき いいこ！",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "ポテト",
            "text": "うち、きっかり２時間ごとに起きましたね・・・",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "てばさき",
            "text": "昔は寝てて褒められ手たっぽいのに…今となっては",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "えのき",
            "text": "あまりにも寝すぎて体重減って怒られるくらい寝ていた新生児でした",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "えのき",
            "text": "@",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "えのき",
            "text": "@ポテト 普通は2時間ごととかに起こされますよね！",
            "mention_to": [
                "ポテト"
            ]
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "ポテト",
            "text": "@えのき めちゃくちゃ寝てる！健康を害するぐらいに！",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "ポテト",
            "text": "ですです・・・起こされる辛さ・・・",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "えのき",
            "text": "無理にでも起こして授乳してください。って言われるほど寝てた・・・",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "えのき",
            "text": "今でも寝てるけれども・・・",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "ポテト",
            "text": "@えのき すごい・・・",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "えのき",
            "text": "@ポテト でも長女の時は、2か月くらいは全然まとめて寝られなかったですね～",
            "mention_to": [
                "ポテト"
            ]
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "てばさき",
            "text": "@えのき 私は！何もしないを！しているのです！",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "えのき",
            "text": "すばらしい！！？",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "ポテト",
            "text": "@えのき ですよね～・・・",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "えのき",
            "text": "@てばさき こうぺんちゃんとかすきだよね",
            "mention_to": [
                "てばさき"
            ]
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "てばさき",
            "text": "あの愛らしい全肯定botは愛されるべきです",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "ポテト",
            "text": "こうぺんちゃん、友人が好きで集めてますね！",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "えのき",
            "text": "すべてを肯定してくれる！！",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "ポテト",
            "text": "サインつきの本かってました、こないだ！",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "てばさき",
            "text": "@えのき 母ももっと褒めてくれていいのよ？",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "てばさき",
            "text": "生きてるだけでお",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "えのき",
            "text": "私もそういう存在でありたいものです",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "えのき",
            "text": "@ポテト サイン本、いいですね!",
            "mention_to": [
                "ポテト"
            ]
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "えのき",
            "text": "私は、子供たちはみんな、生きているだけでいいと、思ってはいるよ",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "ポテト",
            "text": "@えのき ですね・・・息をして生きているだけで褒められたい・・・",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "えのき",
            "text": "一応",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "てばさき",
            "text": "こうペンちゃんは「布団の中から出たくない",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "ポテト",
            "text": "一応、つけちゃった",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "てばさき",
            "text": "という曲が好きです",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "ポテト",
            "text": "@てばさき てばさきさんの歌かな？",
            "mention_to": [
                "てばさき"
            ]
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "てばさき",
            "text": "PVが可愛い",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "ポテト",
            "text": "え、見てみます！",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "えのき",
            "text": "てばさんの魂の叫びを具現化してくれている歌ですね",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "ポテト",
            "text": "ソウルシャウト・・・",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "えのき",
            "text": "打首獄門同好会",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "ポテト",
            "text": "夏に外に出たくなっていうこうぺんちゃんのはみました",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "ポテト",
            "text": "シャウトしていた・・・",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "てばさき",
            "text": "米の歌とか魚の歌とかもこうペンちゃんはいませんが素晴らしいと思います",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "えのき",
            "text": "打首獄門同好会の歌？",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "てばさき",
            "text": "そう",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "ポテト",
            "text": "@てばさき こうぺんちゃんいない？？？",
            "mention_to": [
                "てばさき"
            ]
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "ポテト",
            "text": "聞いてみます！",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "てばさき",
            "text": "日本の米は世界一ッ！",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "ポテト",
            "text": "叫んじゃった",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "えのき",
            "text": "もしかして、人間椅子とかも好きなのでは・・・",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "てばさき",
            "text": "？",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "てばさき",
            "text": "なぜ急に江戸川さん？",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "ポテト",
            "text": "江戸川乱歩？？",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "えのき",
            "text": "いや、似たようなベクトルのバンドで",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "ポテト",
            "text": "バンド名！",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "ポテト",
            "text": "調べてみよう・・・",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "えのき",
            "text": "三島由紀夫の命、売りますドラマ版の主題歌も歌ってた",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "てばさき",
            "text": "ドラマ関連は疎いもので",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "ポテト",
            "text": "@えのき タイトルが気になりすぎる・・・",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "えのき",
            "text": "すごい、原作も面白いらしいですよ。ドラマしか見たことないですが",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "ポテト",
            "text": "三島由紀夫の命、売ります・・・",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "えのき",
            "text": "三島由紀夫さん、今でも色褪せないですよね",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "ポテト",
            "text": "原作は小説とかですか？漫画？",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "てばさき",
            "text": "三島由紀夫って金閣寺のひとか、聞いたことはありました",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "えのき",
            "text": "三島由紀夫さんなので、小説です",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "ポテト",
            "text": "三島由紀夫さん自身の小説！",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "えのき",
            "text": "かなり古いんですよね",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "ポテト",
            "text": "なるほどー！",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "えのき",
            "text": "1968年ごろに連載されてたらしいです",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "ポテト",
            "text": "結構前ですね！",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "えのき",
            "text": "私まだ生まれてません、くらいの年代",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "てばさき",
            "text": "古い…",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "ポテト",
            "text": "原作は探したらよめそうですね・・・",
            "mention_to": []
        }
