    ],
    "relationship": [
        "にんじん",
        "たまねぎ"
    ],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "カンガルー",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "にんじん",
            "text": "こんにちは！",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "たまねぎ",
            "text": "こんにちは！",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "カンガルー",
            "text": "毎日の飲み物？何か決まってますか？",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "カンガルー",
            "text": "なんていうんだろー",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "たまねぎ",
            "text": "断然コーヒー！",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "カンガルー",
            "text": "常に飲んでるもの！",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "にんじん",
            "text": "私はほうじ茶かなー",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "たまねぎ",
            "text": "ブラックコーヒー",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "カンガルー",
            "text": "@たまねぎ コーヒーは缶？",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "たまねぎ",
            "text": "@カンガルー 妻がいれてくれます！",
            "mention_to": [
                "カンガルー"
            ]
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "たまねぎ",
            "text": "それを持って出勤です！",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "カンガルー",
            "text": "@にんじん ほうじ茶好きです",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "にんじん",
            "text": "@カンガルー おいしー",
            "mention_to": [
                "カンガルー"
            ]
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "カンガルー",
            "text": "@たまねぎ 優しいですね！",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "にんじん",
            "text": "ちょいめんどい！笑",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "にんじん",
            "text": "ホットでいれて氷いれてアイスコーヒーにするから！",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "カンガルー",
            "text": "@にんじん 私もお茶は香ばしいやつが好きです",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "たまねぎ",
            "text": "いつもありがとう！",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "にんじん",
            "text": "ほうじ茶あったかいのとか神！",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "にんじん",
            "text": "@たまねぎ いえいえー",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "カンガルー",
            "text": "@たまねぎ 感謝の気持ちを伝えるの大事です",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "たまねぎ",
            "text": "そうですよね！",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "たまねぎ",
            "text": "普段言えてないかも",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "にんじん",
            "text": "言ってくれてるよー",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "にんじん",
            "text": "さりげなく笑",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "カンガルー",
            "text": "ご飯も美味しいとか言ってもらえず",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "たまねぎ",
            "text": "@カンガルー 旦那さんに？",
            "mention_to": [
                "カンガルー"
            ]
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "カンガルー",
            "text": "当たり前のように食べられると泣けます",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "にんじん",
            "text": "@カンガルー そりゃ悲しい",
            "mention_to": [
                "カンガルー"
            ]
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "カンガルー",
            "text": "@たまねぎ そうそう",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "たまねぎ",
            "text": "ずっとコンビニ弁当だったんで",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "カンガルー",
            "text": "だからたまにこちらから聞きますけどー",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "たまねぎ",
            "text": "作ってくれることに感謝ですよー",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "にんじん",
            "text": "@カンガルー 聞くんだ！",
            "mention_to": [
                "カンガルー"
            ]
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "カンガルー",
            "text": "@たまねぎ ホントですよね！",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "にんじん",
            "text": "私料理嫌いなんですー",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "にんじん",
            "text": "家事で一番嫌い",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "カンガルー",
            "text": "@にんじん 聞くし、感想言ってくれなきゃ作る気になれないとも言います",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "たまねぎ",
            "text": "@カンガルー ちゃんと言えるのはえらい！",
            "mention_to": [
                "カンガルー"
            ]
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "カンガルー",
            "text": "私は家事で料理が1番好きですよ",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "にんじん",
            "text": "@カンガルー 答えてくれましたか？",
            "mention_to": [
                "カンガルー"
            ]
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "にんじん",
            "text": "@カンガルー うらやましー",
            "mention_to": [
                "カンガルー"
            ]
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "にんじん",
            "text": "必須科目ですからね！",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "にんじん",
            "text": "料理好きならどんなに楽しいかと",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "カンガルー",
            "text": "@にんじん 感想言ってる。と。",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "にんじん",
            "text": "うんうん",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "たまねぎ",
            "text": "作れないからなー",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "カンガルー",
            "text": "聞こえねー…っ伝わんなきゃ意味ねー…と呟いてます",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "たまねぎ",
            "text": "@カンガルー 聞こえてない！！！",
            "mention_to": [
                "カンガルー"
            ]
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "カンガルー",
            "text": "まぁもう結婚して20年",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "にんじん",
            "text": "そりゃ意味ない笑",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "たまねぎ",
            "text": "@カンガルー おー",
            "mention_to": [
                "カンガルー"
            ]
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "たまねぎ",
            "text": "長い！",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "にんじん",
            "text": "うちの旦那さんは一歳の子に",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "カンガルー",
            "text": "出会ってから何年だろ？ってわからないくらいです",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "カンガルー",
            "text": "多分30年くらい？",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "にんじん",
            "text": "ご飯食べさせてくれるんですが",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "たまねぎ",
            "text": "@カンガルーすごい！",
            "mention_to": [
                "カンガルー"
            ]
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "カンガルー",
            "text": "間違えた27年か",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "にんじん",
            "text": "一口一口おいしーって言いながらあげてくれる！",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "たまねぎ",
            "text": "@カンガルー 僕の生まれた歳ですねー",
            "mention_to": [
                "カンガルー"
            ]
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "カンガルー",
            "text": "@にんじん さん　それ最高！",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "にんじん",
            "text": "軽く洗脳笑",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "カンガルー",
            "text": "@たまねぎ マジですか",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "たまねぎ",
            "text": "27歳です！",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "たまねぎ",
            "text": "笑",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "カンガルー",
            "text": "笑っちゃいます",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "カンガルー",
            "text": "もうそんなかー…",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "にんじん",
            "text": "素敵ですねー",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "にんじん",
            "text": "うちらはバツ同士なので",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "カンガルー",
            "text": "29歳で結婚しました",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "カンガルー",
            "text": "7年お付き合いして",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "たまねぎ",
            "text": "@カンガルー 健全！",
            "mention_to": [
                "カンガルー"
            ]
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "にんじん",
            "text": "うんうん",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "カンガルー",
            "text": "@にんじん なるほど！本物の愛を見つけたんですね！",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "にんじん",
            "text": "@カンガルー そうなんです！",
            "mention_to": [
                "カンガルー"
            ]
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "カンガルー",
            "text": "いいなー",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "にんじん",
            "text": "こんなにも価値観合う人がいるのかと",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "たまねぎ",
            "text": "俺もそう思うー",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "カンガルー",
            "text": "私は本物かどうかわかりませーを",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "カンガルー",
            "text": "なるほど！",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "カンガルー",
            "text": "相思相愛",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "にんじん",
            "text": "喧嘩もないです！",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "たまねぎ",
            "text": "イラッと来ない",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "カンガルー",
            "text": "私も喧嘩ないです",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "カンガルー",
            "text": "違う意味で",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "たまねぎ",
            "text": "@カンガルー あー",
            "mention_to": [
                "カンガルー"
            ]
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "カンガルー",
            "text": "喧嘩しない様に我慢してます",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "にんじん",
            "text": "忍耐的な？",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "にんじん",
            "text": "我慢かー",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "カンガルー",
            "text": "ですです",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "にんじん",
            "text": "貯まりませんか？",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "カンガルー",
            "text": "もー喧嘩している体力ありません",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "カンガルー",
            "text": "たまりますよー",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "にんじん",
            "text": "私は前の旦那は話にならなかったので",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "にんじん",
            "text": "諦めました",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "たまねぎ",
            "text": "俺は喧嘩ばっかりー",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "カンガルー",
            "text": "@にんじん そうなんですね！",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "にんじん",
            "text": "こっちの話聞いてるのかなー？",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "にんじん",
            "text": "理解してんのかなー？",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "にんじん",
            "text": "みたいな",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "たまねぎ",
            "text": "歩み寄ってるのにねー",
            "mention_to": []
        },
        {
            "utterance_id": 103,
            "interlocutor_id": "カンガルー",
            "text": "なるほどー",
            "mention_to": []
        }
