    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "すじこ",
            "text": "こんばんは",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "こんぶ",
            "text": "こんばんは！",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "ちくわ",
            "text": "こんばんは",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "ちくわ",
            "text": "ちくわです！",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "すじこ",
            "text": "最近物忘れが激しい",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "こんぶ",
            "text": "こんぶです！",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "すじこ",
            "text": "すじこです",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "ちくわ",
            "text": "物忘れ、、、",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "こんぶ",
            "text": "物忘れが、、、",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "ちくわ",
            "text": "どういった時に？",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "こんぶ",
            "text": "何を忘れましたか？",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "すじこ",
            "text": "妻の言ったことは",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "すじこ",
            "text": "ほぼ忘れます",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "ちくわ",
            "text": "あー！",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "こんぶ",
            "text": "笑ってしまいました",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "ちくわ",
            "text": "何か頼まれた時とかですか？",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "すじこ",
            "text": "そうですね",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "こんぶ",
            "text": "それは、完全に響いてないという事ですね",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "すじこ",
            "text": "いや",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "ちくわ",
            "text": "お！",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "ちくわ",
            "text": "女目線！",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "すじこ",
            "text": "めちゃくちゃ真剣に",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "すじこ",
            "text": "聞いてるんです",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "こんぶ",
            "text": "あ、失礼しました",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "ちくわ",
            "text": "まぁそうですよね！笑",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "すじこ",
            "text": "が",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "ちくわ",
            "text": "でもなんか頼まれたりして、",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "こんぶ",
            "text": "真剣さ伝わりました",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "ちくわ",
            "text": "他のことをやっていると、忘れちゃいますよ！",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "すじこ",
            "text": "秒で忘れるタイプです",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "ちくわ",
            "text": "なるほど、、",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "こんぶ",
            "text": "なるほど、、、",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "ちくわ",
            "text": "それは頻繁に？",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "こんぶ",
            "text": "メモするとか？",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "ちくわ",
            "text": "メモね、、",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "すじこ",
            "text": "それも考えました",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "ちくわ",
            "text": "男はあんまそういうのしないよなー",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "すじこ",
            "text": "というか実行に移しました",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "ちくわ",
            "text": "お！！！",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "こんぶ",
            "text": "おお！！移したんですね",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "こんぶ",
            "text": "その成果は？",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "すじこ",
            "text": "するとなんと！",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "ちくわ",
            "text": "なんと！！",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "こんぶ",
            "text": "なんとなんと！！！",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "ちくわ",
            "text": "なんと？",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "すじこ",
            "text": "抜けたものは絶対に出来ない！",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "ちくわ",
            "text": "忘れたってことですか？",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "ちくわ",
            "text": "笑",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "すじこ",
            "text": "メモし忘れると絶望的です",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "ちくわ",
            "text": "あー！",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "こんぶ",
            "text": "メモをし忘れたということですね",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "ちくわ",
            "text": "そういうことか！",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "こんぶ",
            "text": "ん？その時はって",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "こんぶ",
            "text": "聞きながらメモしました？",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "こんぶ",
            "text": "それとも",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "すじこ",
            "text": "妻のなかではあまり頼んでないらしい",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "こんぶ",
            "text": "思い出してからのメモ？",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "ちくわ",
            "text": "頼まれてはないけど、",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "すじこ",
            "text": "聞きながらメモしてるんですが",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "ちくわ",
            "text": "何かしようとしてるんですね！",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "こんぶ",
            "text": "聞きながらですよね！！、",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "こんぶ",
            "text": "それでも、どこか抜けちゃうんですね",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "ちくわ",
            "text": "頼まれごとって具体的に",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "ちくわ",
            "text": "どんなんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "ちくわ",
            "text": "僕もよく妻に",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "こんぶ",
            "text": "気になります！！！",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "すじこ",
            "text": "あとから付け加えてきやがるんです",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "ちくわ",
            "text": "頼まれますけど",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "こんぶ",
            "text": "どんな事頼まれますか？",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "ちくわ",
            "text": "追加があるんですね！",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "こんぶ",
            "text": "きやがるという言い方に笑ってしまいました",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "ちくわ",
            "text": "ちょっと愚痴のような、、笑",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "ちくわ",
            "text": "笑っちゃいました！",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "すじこ",
            "text": "@こんぶ ほんとたいしたことじゃないんです",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "こんぶ",
            "text": "@すじこ 追加！！！それは難しそう",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "ちくわ",
            "text": "@すじこ 内心くそーーとか思ってたり？",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "すじこ",
            "text": "@ちくわ 愚痴っぽくなっちゃいましたね笑",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "こんぶ",
            "text": "大したことじゃなくても頼まれるということは、、、",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "ちくわ",
            "text": "はい！笑っちゃいました！",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "ちくわ",
            "text": "忘れたりした時は",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "ちくわ",
            "text": "奥さんになんか言われます？",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "こんぶ",
            "text": "奥様は信頼してくれてるということには〜",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "こんぶ",
            "text": "ならないですか？",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "すじこ",
            "text": "@ちくわ このくらいだったらと自分を信じて失敗します",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "ちくわ",
            "text": "@すじこ そうなんですね！",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "すじこ",
            "text": "めっちゃ怒られます",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "ちくわ",
            "text": "めちゃ怒られるんですね！笑",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "こんぶ",
            "text": "@すじこ ドンマイです",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "ちくわ",
            "text": "でも頻繁に忘れたら",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "ちくわ",
            "text": "怒ってきますよね！",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "こんぶ",
            "text": "女の人は、いつ期待しちゃうんですが",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "こんぶ",
            "text": "つい期待しちゃうんですが",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "すじこ",
            "text": "洗濯やら掃除やら炊事やら買い物やら色々ですね",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "こんぶ",
            "text": "期待ってよくないですよね",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "こんぶ",
            "text": "えーー！！！",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "こんぶ",
            "text": "それは、けっこう沢山頼まれてますね",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "ちくわ",
            "text": "奥さんからは手伝ってほしいという",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "ちくわ",
            "text": "ヘルプですね！",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "こんぶ",
            "text": "失礼ながら、、、共働きですか？",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "すじこ",
            "text": "@こんぶ 期待を盛大に裏切ります",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "すじこ",
            "text": "@ちくわ そうです！",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "こんぶ",
            "text": "@すじこ そうですよね、期待ってやっぱりよくない",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "ちくわ",
            "text": "共働きだったら？",
            "mention_to": []
        },
        {
            "utterance_id": 103,
            "interlocutor_id": "こんぶ",
            "text": "期待しちゃうから、こっちも怒りになっちゃうんですよ",
            "mention_to": []
        },
        {
            "utterance_id": 104,
            "interlocutor_id": "ちくわ",
            "text": "大変そうですー",
            "mention_to": []
        },
        {
            "utterance_id": 105,
            "interlocutor_id": "すじこ",
            "text": "@こんぶ 共働きですが妻はパートです",
            "mention_to": [
                "こんぶ"
            ]
        }
