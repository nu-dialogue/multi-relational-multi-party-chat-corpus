        {
            "utterance_id": 1,
            "interlocutor_id": "たまねぎ",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "にんじん",
            "text": "こんにちは！",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "りんご",
            "text": "今日は寒いですね",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "たまねぎ",
            "text": "さむいですねー",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "にんじん",
            "text": "そうですね！",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "りんご",
            "text": "風邪ひきそうです",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "にんじん",
            "text": "ほんとそれです",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "りんご",
            "text": "@にんじん 大丈夫ですか？体調とか",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "にんじん",
            "text": "風邪よりも花粉症が…",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "たまねぎ",
            "text": "辛そうですー",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "りんご",
            "text": "@にんじん 花粉症は辛い！",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "にんじん",
            "text": "先週なりました！",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "にんじん",
            "text": "鼻詰まるー",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "りんご",
            "text": "@たまねぎ 花粉症は大丈夫ですか？",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "たまねぎ",
            "text": "全然大丈夫なんですよー",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "たまねぎ",
            "text": "辛さがわかってあげられない",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "りんご",
            "text": "@にんじん 先週暖かかったから",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "にんじん",
            "text": "うらやましー",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "にんじん",
            "text": "@りんご そうなんです",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "にんじん",
            "text": "急に来ました",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "りんご",
            "text": "私もなんか花粉症きた感じです！",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "たまねぎ",
            "text": "アウトレット行った時だよねー",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "にんじん",
            "text": "@りんご 同じですね！",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "りんご",
            "text": "@にんじん 本当、突然ですね",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "にんじん",
            "text": "@たまねぎ そーそー",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "にんじん",
            "text": "暖かかったからねー",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "にんじん",
            "text": "@りんご 突然なんですよー",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "にんじん",
            "text": "風邪かと思うけど",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "りんご",
            "text": "思いました、風邪かなって",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "たまねぎ",
            "text": "いいティッシュ必須！",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "にんじん",
            "text": "@たまねぎ 鼻荒れるからね！",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "りんご",
            "text": "鼻がいつもでますよね",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "にんじん",
            "text": "@りんご 風邪かと思う人多い",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "にんじん",
            "text": "あと喉も痛い",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "りんご",
            "text": "気づくと鼻が垂れてる",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "にんじん",
            "text": "水みたいに",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "たまねぎ",
            "text": "うんー",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "たまねぎ",
            "text": "つらそー",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "りんご",
            "text": "やっぱり？喉も痛いよね",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "にんじん",
            "text": "@りんご いたい",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "りんご",
            "text": "咳もでちゃうんです！",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "にんじん",
            "text": "そうなんですよー",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "にんじん",
            "text": "花粉症ですね！",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "りんご",
            "text": "なのでずーと風邪かなと",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "たまねぎ",
            "text": "今年からですか？",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "にんじん",
            "text": "デビュー？？",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "りんご",
            "text": "やっぱりそうですか、今年からなんです、はい",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "にんじん",
            "text": "あ、いらっしゃい！",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "にんじん",
            "text": "笑",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "りんご",
            "text": "@にんじん デビューしました",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "たまねぎ",
            "text": "でも、歳取ると治るんだよね？",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "りんご",
            "text": "@たまねぎ デビューまだですかー、いいなぁ",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "たまねぎ",
            "text": "そういってるとなりそー",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "りんご",
            "text": "@たまねぎ えー本当に？",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "たまねぎ",
            "text": "@りんご みたいですよー",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "たまねぎ",
            "text": "だから花粉症っていうと若いねーって",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "にんじん",
            "text": "そーそー",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "にんじん",
            "text": "言われるー",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "りんご",
            "text": "期待したいけど、もうそれなりに歳なんだけどなあ",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "にんじん",
            "text": "いやいや！",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "にんじん",
            "text": "恐らく60.70の話かと！",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "たまねぎ",
            "text": "@りんご さん若いんですよ！",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "りんご",
            "text": "@にんじん えー！まだまだやないかい！",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "にんじん",
            "text": "まだまだですよー",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "にんじん",
            "text": "付き合い長くなりますね",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "りんご",
            "text": "@たまねぎ 長いですねー",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "りんご",
            "text": "今日は雨だからまだましかな",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "たまねぎ",
            "text": "コロナでマスクしてた時は和らいでたのに",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "にんじん",
            "text": "@りんご 雨の日の方がやばいですよー",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "にんじん",
            "text": "@たまねぎ マスクしても変わらん！",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "りんご",
            "text": "@にんじん そうなんですか？",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "にんじん",
            "text": "雨で花粉がおちる",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "たまねぎ",
            "text": "飛ばないけどおちる",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "りんご",
            "text": "こ、こわい！",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "りんご",
            "text": "お洗濯物も部屋干し？",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "たまねぎ",
            "text": "日焼けも一緒かー",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "にんじん",
            "text": "@りんご うちはそうですれ",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "にんじん",
            "text": "でも、基本的に花粉のせいではなくて",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "りんご",
            "text": "@にんじん そうなりますねー",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "にんじん",
            "text": "薪ストーブだからですね",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "にんじん",
            "text": "夜洗濯します！",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "りんご",
            "text": "えー薪ストーブ？すごい！！！",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "たまねぎ",
            "text": "乾燥防止",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "りんご",
            "text": "暖かいですよねー",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "にんじん",
            "text": "暖かいですよー",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "りんご",
            "text": "勝浦の実家がそうです！",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "たまねぎ",
            "text": "違いがハッキリ",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "たまねぎ",
            "text": "@りんご おー",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "たまねぎ",
            "text": "薪いいですよねー",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "りんご",
            "text": "めっちゃ暖かいですよね",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "にんじん",
            "text": "芯からあったまる",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "にんじん",
            "text": "暑いくらい！",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "にんじん",
            "text": "真夏の方が長袖着てる",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "りんご",
            "text": "@たまねぎ 本当にすごいですよね、あれ",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "たまねぎ",
            "text": "冬は楽しみです",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "たまねぎ",
            "text": "うちからでれないー",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "りんご",
            "text": "暑い！確かに\nお家全体暖かくなりますね",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "にんじん",
            "text": "半袖でいられますからねー",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "りんご",
            "text": "動きやすいですね\n半袖になれるくらいですね",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "たまねぎ",
            "text": "外出る時困りますけどね",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "りんご",
            "text": "子供なんか半袖でも暑い感じになりますね",
            "mention_to": []
