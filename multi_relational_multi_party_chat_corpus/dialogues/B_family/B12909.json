    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "いるか",
            "text": "おはようございます！",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "つみれ",
            "text": "おはようございます",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "たまご",
            "text": "おはようございます",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "いるか",
            "text": "おふたりって、料理とか結構されますか？",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "たまご",
            "text": "全然できないです",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "いるか",
            "text": "@たまご 全くです？",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "たまご",
            "text": "はいー",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "たまご",
            "text": "ギリ作れるのが",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "たまご",
            "text": "パスタくらいです",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "いるか",
            "text": "おっと～！これはつみれさん心配では・・・",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "たまご",
            "text": "カルボナーラとか！",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "いるか",
            "text": "@たまご あ、でもパスタつくれるのすごい！",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "つみれ",
            "text": "心配すぎなんです",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "いるか",
            "text": "しかもカルボナーラ！",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "つみれ",
            "text": "バイトで覚えたパスタだけは美味しいですよ",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "いるか",
            "text": "@つみれ カルボナーラ作ってますよ！！！",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "つみれ",
            "text": "家でも作ってくれました",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "たまご",
            "text": "パスタだけ得意です",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "いるか",
            "text": "あ！バイトで！でも凄い！実生活に活かせる技術！",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "たまご",
            "text": "もっと覚えないと",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "つみれ",
            "text": "@たまご 得意ではありません",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "いるか",
            "text": "あれっ！",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "いるか",
            "text": "雲行きが怪しい！",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "たまご",
            "text": "違うんかい",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "つみれ",
            "text": "一人暮らしが心配で",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "たまご",
            "text": "料理覚えないと",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "いるか",
            "text": "@たまご 簡単なのからいきましょ！",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "つみれ",
            "text": "料理男子になって欲しいです",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "たまご",
            "text": "目玉焼きとか",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "たまご",
            "text": "生姜焼きとか",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "いるか",
            "text": "@つみれ 料理男子！",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "つみれ",
            "text": "@たまご まずはそこから",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "たまご",
            "text": "うどんも極めたい",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "いるか",
            "text": "目玉焼きから生姜焼きのレベルの違いがすごい",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "いるか",
            "text": "@たまご うどんですか？",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "たまご",
            "text": "レベルアップしていきます",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "たまご",
            "text": "うどんです",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "たまご",
            "text": "うどん大好きなので",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "いるか",
            "text": "製麺・・・から？",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "いるか",
            "text": "その勢いがありそうで怖いです",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "たまご",
            "text": "そこからじゃないです",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "つみれ",
            "text": "小麦粉からだ・・・",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "いるか",
            "text": "@たまご 良かった・・・",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "いるか",
            "text": "うどん屋さんになるのかと・・・",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "たまご",
            "text": "職人になりたいわけじゃないです",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "いるか",
            "text": "@たまご 料理としてのうどんですね！",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "たまご",
            "text": "そうです！",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "いるか",
            "text": "うどんかぁ",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "つみれ",
            "text": "将来、讃岐に移住しますとか言われたらどうしよう",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "たまご",
            "text": "毎日うどんでも良いくらい好きです",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "たまご",
            "text": "ガチすぎる",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "いるか",
            "text": "@つみれ 急に笑俺、うどん道を突き詰めたい！",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "たまご",
            "text": "そこまで行ったら職人",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "いるか",
            "text": "ですね笑",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "つみれ",
            "text": "親としては・・・見守ります",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "いるか",
            "text": "@たまご 本当にお好きなんですね！",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "たまご",
            "text": "まずは赤いきつねを超えるとこから",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "いるか",
            "text": "ん？",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "たまご",
            "text": "@いるか 好きです！",
            "mention_to": [
                "いるか"
            ]
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "いるか",
            "text": "確かに美味しいですけど・・・ん？",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "いるか",
            "text": "赤いきつね・・・ライバル・・・",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "たまご",
            "text": "カップうどんに勝ちます",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "つみれ",
            "text": "@いるか ちょっと痛い子です",
            "mention_to": [
                "いるか"
            ]
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "いるか",
            "text": "@つみれ 笑",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "いるか",
            "text": "でも、すごくイイコなのは伝わってきます！",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "つみれ",
            "text": "ありがとうございます涙",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "つみれ",
            "text": "@いるか お料理されますか？",
            "mention_to": [
                "いるか"
            ]
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "いるか",
            "text": "@つみれ 毎日基本的に料理をしているので",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "たまご",
            "text": "凄い！",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "いるか",
            "text": "他人の作ったご飯を食べたくてしょうがないです。毎日",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "つみれ",
            "text": "わかりますーーーー！",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "いるか",
            "text": "こないだ、主人がカレーを作るというから期待したら",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "いるか",
            "text": "野菜は切って、肉は準備しといてと言われて",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "いるか",
            "text": "違う、そうじゃない・・・！ってなりました",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "つみれ",
            "text": "？？？",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "たまご",
            "text": "煮るだけですね",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "いるか",
            "text": "そう",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "つみれ",
            "text": "そうですね",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "いるか",
            "text": "後味付け？",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "いるか",
            "text": "なんの苦労もしてなくない？？？ってなりました",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "つみれ",
            "text": "一番大変なとこお任せ笑",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "たまご",
            "text": "切るとこから大変ですよね",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "いるか",
            "text": "そうなんですよ～！！！",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "たまご",
            "text": "あんまし意味がなくなっちゃうー",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "つみれ",
            "text": "切るとこまでやったら自分で全部やりますね",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "いるか",
            "text": "そこ～！そこからやってほしいの～！って心で叫んでました",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "いるか",
            "text": "@つみれ ほんとそれですよ！",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "いるか",
            "text": "このまま作れる・・・ってなりました",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "いるか",
            "text": "むしろ私が離れる意味？？ってなりました",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "つみれ",
            "text": "ご主人に直接言わないんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "たまご",
            "text": "料理番組みたいに切ったのはこちらです",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "いるか",
            "text": "@たまご そうです！笑",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "たまご",
            "text": "って感じですね",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "つみれ",
            "text": "なるほど！料理番組",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "つみれ",
            "text": "差し替えまーす！ですね",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "いるか",
            "text": "@つみれ せっかくのやる気をつぶすのはしのびなく・・・",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "つみれ",
            "text": "@いるか 優しい！",
            "mention_to": [
                "いるか"
            ]
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "つみれ",
            "text": "いい奥さんですね",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "いるか",
            "text": "アシスタントいるか、です",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "つみれ",
            "text": "見習います！",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "いるか",
            "text": "@つみれ でも、ストレスはマックスです！笑",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "たまご",
            "text": "アシスタントの方が手際が良い番組",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "つみれ",
            "text": "私なら絶対行っちゃうから笑",
            "mention_to": []
        },
        {
            "utterance_id": 103,
            "interlocutor_id": "つみれ",
            "text": "言っちゃう",
            "mention_to": []
        },
        {
            "utterance_id": 104,
            "interlocutor_id": "いるか",
            "text": "なので、言えるつみれさんうらやましいです！",
            "mention_to": []
        }
