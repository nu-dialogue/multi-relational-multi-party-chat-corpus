    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "くじら",
            "text": "おはようございます",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "つくね",
            "text": "おはようございます",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "しらたき",
            "text": "おはようございます",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "くじら",
            "text": "好きな外国料理は何ですか？",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "くじら",
            "text": "イタリア料理とか中華とか色々ありますね",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "つくね",
            "text": "イタリアン好きです！",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "つくね",
            "text": "パスタとか",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "つくね",
            "text": "よく食べます",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "くじら",
            "text": "パスタはよく食べます",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "しらたき",
            "text": "俺は韓国料理ですね",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "くじら",
            "text": "お昼はパスタが多いかと",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "つくね",
            "text": "@くじら 作るのも手軽なので",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "つくね",
            "text": "頻繁に作ります！",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "くじら",
            "text": "@しらたき 韓国料理の何が好きですか？",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "しらたき",
            "text": "キムチが好きです",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "しらたき",
            "text": "焼肉とかも！",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "くじら",
            "text": "キムチですね",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "しらたき",
            "text": "辛いのが好きで",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "くじら",
            "text": "辛いの得意なのですね",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "つくね",
            "text": "@しらたき 辛いの好きだけど、お腹弱いよね",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "しらたき",
            "text": "@つくね それがネック！",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "くじら",
            "text": "それはそれは",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "しらたき",
            "text": "@くじら CoCo壱のカレーも結構辛いの食べます",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "しらたき",
            "text": "でもお腹が弱くて",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "しらたき",
            "text": "次の日休みじゃないと激辛は食べられないです笑",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "くじら",
            "text": "おなか弱くても辛いのは食べたいんですね",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "くじら",
            "text": "確かに",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "つくね",
            "text": "@しらたき ペヤングの激辛好きだよね笑",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "しらたき",
            "text": "@くじら そうなんです",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "しらたき",
            "text": "体と心がバラバラ笑",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "くじら",
            "text": "ペヤングの激辛？",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "しらたき",
            "text": "@つくね ペヤングの激辛はうまい！",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "しらたき",
            "text": "@くじら よくYouTuberとかの企画で",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "くじら",
            "text": "何か辛いのをかけるんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "しらたき",
            "text": "すっごい辛いペヤング食べたりしてるの見て",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "くじら",
            "text": "テレビでもやってます？",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "しらたき",
            "text": "@くじら YouTubeですね",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "しらたき",
            "text": "ソースが辛いです",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "くじら",
            "text": "海外で食べてみてくださいってやっていたような",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "つくね",
            "text": "@くじら 私は一口でギブアップです！",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "しらたき",
            "text": "@くじら あ！そういうのです",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "くじら",
            "text": "辛すぎでほとんどの人が駄目なようでした",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "しらたき",
            "text": "@くじら 本当に辛いです",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "くじら",
            "text": "辛いの食べたら、汗出ませんか？",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "しらたき",
            "text": "しゃっくりも止まらない笑",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "しらたき",
            "text": "汗も出ます",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "くじら",
            "text": "しゃっくりも！",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "しらたき",
            "text": "そのペヤングは冷や汗が出ます",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "しらたき",
            "text": "辛すぎて",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "くじら",
            "text": "体がびっくりしているとか",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "つくね",
            "text": "辛すぎてビックリしてますね",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "くじら",
            "text": "@つくね 辛いの好きですか？",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "くじら",
            "text": "私はほどほどが良いです",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "つくね",
            "text": "@くじら 食べますがそんなに辛いのは無理です笑",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "しらたき",
            "text": "@くじら ほどほどが一番美味しい",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "しらたき",
            "text": "辛すぎて唇も舌も麻痺します",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "くじら",
            "text": "辛すぎるともう味わえなくて",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "しらたき",
            "text": "歯医者の麻酔かけた後みたいな",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "つくね",
            "text": "@くじら 味わえないですよね",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "くじら",
            "text": "それは辛い",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "つくね",
            "text": "せっかくの料理を…",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "しらたき",
            "text": "@くじら 牛乳飲んだりして緩和させます",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "つくね",
            "text": "@しらたき 牛乳の消費やばいよね笑",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "くじら",
            "text": "牛乳で緩和ですね",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "しらたき",
            "text": "@つくね 結構飲むよね笑",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "くじら",
            "text": "家は私がコーヒーに入れるか",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "しらたき",
            "text": "@くじら 牛乳かヨーグルトですね",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "くじら",
            "text": "夫が青汁に入れるかぐらい",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "つくね",
            "text": "@くじら 青汁に牛乳ですか！？",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "つくね",
            "text": "え？美味しんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "くじら",
            "text": "その方が飲みやすいようで",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "くじら",
            "text": "私は飲んでません",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "しらたき",
            "text": "牛乳に青汁…見た目は抹茶オレ！",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "つくね",
            "text": "@くじら そうなんですね",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "つくね",
            "text": "青汁飲んだことないです",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "くじら",
            "text": "抹茶オレなら飲みたいです",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "つくね",
            "text": "@しらたき 抹茶オレなら私も飲める",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "くじら",
            "text": "青汁は苦い感じが",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "しらたき",
            "text": "@くじら 間違えて飲みそうですね",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "しらたき",
            "text": "抹茶オレと笑",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "くじら",
            "text": "確かに色味が似てますから",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "つくね",
            "text": "@くじら 苦いと聞きますが体に良さそうです！",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "くじら",
            "text": "体には良いですよねきっと",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "くじら",
            "text": "ヤクルト１０００が気になっています",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "くじら",
            "text": "飲んだことありますか？",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "つくね",
            "text": "ヤクルト1000飲んだことないです",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "しらたき",
            "text": "普通のヤクルトと何が違うの？",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "くじら",
            "text": "普通のヤクルトより高くて",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "くじら",
            "text": "倍ぐらい違った気が",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "つくね",
            "text": "@くじら 普通のヤクルトも高いと思うのに",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "つくね",
            "text": "それ以上なんですね！？",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "くじら",
            "text": "ストレス緩和とか",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "くじら",
            "text": "効くのかなと",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "しらたき",
            "text": "@くじら なるほど！ストレス緩和は気になる",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "つくね",
            "text": "緩和させたいですー",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "くじら",
            "text": "この間買ってみる？となりましたけど",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "くじら",
            "text": "高いから続かないねって",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "つくね",
            "text": "続けるってなると金額がですね",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "くじら",
            "text": "そうなんです",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "しらたき",
            "text": "できれば継続した方がいいよね",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "くじら",
            "text": "継続しないと効くかもわからないですね",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "つくね",
            "text": "そうですよね！",
            "mention_to": []
        }
