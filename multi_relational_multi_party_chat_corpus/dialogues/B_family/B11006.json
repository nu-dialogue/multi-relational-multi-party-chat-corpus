        "こんぶ",
        "ちくわ"
    ],
    "relationship": [
        "こんぶ",
        "ちくわ"
    ],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "ポテト",
            "text": "こんにちは！",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "こんぶ",
            "text": "こんには",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "ちくわ",
            "text": "こんにちは。",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "こんぶ",
            "text": "こんにちは！",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "ポテト",
            "text": "今日も寒いですね～",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "ちくわ",
            "text": "雪降ってますよー！",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "ちくわ",
            "text": "そちらは降ってますか？",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "こんぶ",
            "text": "寒いです〜！雪です",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "ポテト",
            "text": "えっ！雪！",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "ポテト",
            "text": "@ちくわ こちらは曇りです！",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "こんぶ",
            "text": "こっちは朝起きたら真っ白でした",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "ちくわ",
            "text": "今シーズン結構降ります！",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "ポテト",
            "text": "雨は降りそう・・・",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "ちくわ",
            "text": "@ポテト 雪降らないだけましですよー！",
            "mention_to": [
                "ポテト"
            ]
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "ポテト",
            "text": "結構降ってるんですね！",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "こんぶ",
            "text": "@ポテト 雨だけでとどまってほしいですね",
            "mention_to": [
                "ポテト"
            ]
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "こんぶ",
            "text": "牡丹雪です！",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "ポテト",
            "text": "雪も時々ならよいのですが・・・",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "こんぶ",
            "text": "ぼたぼた降ってます！",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "ちくわ",
            "text": "車に少し積もってる感じです！",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "ポテト",
            "text": "@こんぶ ボタン雪！",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "こんぶ",
            "text": "でも、あまり道路とかに積もる感じじゃないですよ",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "ポテト",
            "text": "@こんぶ そうなんですね！",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "ちくわ",
            "text": "積もる感じだったらやばいけど",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "こんぶ",
            "text": "@ポテト 大きなかたまりで降ってるかんじです",
            "mention_to": [
                "ポテト"
            ]
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "こんぶ",
            "text": "積もると雪払いが大変なんでね、、",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "ちくわ",
            "text": "まだ前よりかはましです！",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "ポテト",
            "text": "水っぽい感じかな？？",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "こんぶ",
            "text": "そうだね！前よりかは！",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "こんぶ",
            "text": "そうですね！とけてきます！",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "ポテト",
            "text": "@ちくわ 前はもっとですか？",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "ちくわ",
            "text": "@ポテト 前はすごかったです！",
            "mention_to": [
                "ポテト"
            ]
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "こんぶ",
            "text": "前はどんどん積もってって",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "こんぶ",
            "text": "本当に真っ白だったね！！",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "ちくわ",
            "text": "夜に雪かきしました！",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "ポテト",
            "text": "うわぁ",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "こんぶ",
            "text": "雪かき必須でした！！！",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "ちくわ",
            "text": "道路も真っ白で！",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "こんぶ",
            "text": "そうそう！道路が",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "ポテト",
            "text": "夜に雪かき！",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "ちくわ",
            "text": "みんなゆっくり運転してました！",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "ポテト",
            "text": "こわい～",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "こんぶ",
            "text": "今回は大丈夫そうですが、、",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "ちくわ",
            "text": "@こんぶ 子供が寝てから雪かきしたね！",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "ポテト",
            "text": "前は大変でしたね・・・",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "こんぶ",
            "text": "私は雪国出身なんですが",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "ちくわ",
            "text": "今回は大丈夫だと思う！",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "こんぶ",
            "text": "運転は怖いです",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "ちくわ",
            "text": "大雪降ったことあります？",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "ポテト",
            "text": "雪の降り方でもうわかりそう・・・",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "こんぶ",
            "text": "ね！今回は大丈夫そう！",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "ちくわ",
            "text": "そちらの地域は！",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "ポテト",
            "text": "@ちくわ 数年前とかのレベルです",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "こんぶ",
            "text": "どこにお住まいですか？",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "ポテト",
            "text": "@こんぶ 千葉です！",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "こんぶ",
            "text": "雪嬉しい〜きれい〜くらいのレベルがいいですね！",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "ちくわ",
            "text": "@ポテト そうなんですねー！",
            "mention_to": [
                "ポテト"
            ]
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "こんぶ",
            "text": "千葉なんですね！",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "ポテト",
            "text": "なので、あまり降らないですね～",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "ちくわ",
            "text": "思いっきり振られて",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "こんぶ",
            "text": "今季はふりませんでしたっけ？",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "ちくわ",
            "text": "運転怖くなるのいやですねー！",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "ポテト",
            "text": "@こんぶ 今回一回くらいですね・・・",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "こんぶ",
            "text": "2月ですか？",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "ちくわ",
            "text": "多分その時に長野とか結構降ったときかな？",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "ポテト",
            "text": "運転怖くなるの本当に怖いですね～",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "ポテト",
            "text": "@こんぶ 二月かな？",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "こんぶ",
            "text": "車社会なんで、、運転がね〜",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "ちくわ",
            "text": "@ポテト スタッドレス履いててもこわいです！",
            "mention_to": [
                "ポテト"
            ]
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "ポテト",
            "text": "@ちくわ 多分そうだと思います！",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "ちくわ",
            "text": "ですよね！",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "こんぶ",
            "text": "@ポテト あの時ですね！私たちが夜に雪かきした日です！",
            "mention_to": [
                "ポテト"
            ]
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "ポテト",
            "text": "スタッドレスでも・・・！",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "ちくわ",
            "text": "ここ最近は東京とかでも",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "ちくわ",
            "text": "普通に雪降りますもんね！",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "こんぶ",
            "text": "ドキドキしながら運転してます",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "ポテト",
            "text": "ですね～",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "こんぶ",
            "text": "チラチラふって綺麗ぐらいがいいね！",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "ポテト",
            "text": "運転本当に怖そうで・・・",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "ちくわ",
            "text": "細い道とかは除雪車がいないんですよ！",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "ポテト",
            "text": "@こんぶ ですね～",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "ポテト",
            "text": "@ちくわ そうなんですか！",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "ちくわ",
            "text": "だから雪が溶けて凍ってしまうんすよ！",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "こんぶ",
            "text": "凍るのが1番こわいです！！！",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "ポテト",
            "text": "東北とか北海道は、道路から水が出てますよね",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "ちくわ",
            "text": "昔は雪だーとか思っていたのに、",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "ポテト",
            "text": "アイスバーン怖い・・・",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "ちくわ",
            "text": "大人になったら嫌になりそう!!!",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "ポテト",
            "text": "@ちくわ わかります・・・",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "こんぶ",
            "text": "そうですね！アイスバーン！！！",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "ポテト",
            "text": "アイスバーン、歩きも怖い・・・",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "こんぶ",
            "text": "でも、なんだから、雪降るとワクワクする自分もいます",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "ちくわ",
            "text": "この辺は道路から水は出てないですねー！",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "こんぶ",
            "text": "歩き！！たしかに！！！",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "こんぶ",
            "text": "歩くのも慎重になりますよね！",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "こんぶ",
            "text": "ね！今のところは大丈夫だね！",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "ポテト",
            "text": "@こんぶ それもわかります！わくわくはする！！",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "こんぶ",
            "text": "@ポテト わかりますかーー！！！",
            "mention_to": [
                "ポテト"
            ]
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "こんぶ",
            "text": "そうなんですよ！！おっ！！て",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "ちくわ",
            "text": "雪遊びは楽しいけどね！",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "こんぶ",
            "text": "子どもと遊べればいんですけど〜",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "ポテト",
            "text": "たしかに～",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "ちくわ",
            "text": "でも思いっきり降ったあとがねーー",
            "mention_to": []
        },
        {
            "utterance_id": 103,
            "interlocutor_id": "こんぶ",
            "text": "きっと今日の雪はただびしょ濡れなるだけです〜笑",
            "mention_to": []
        }
