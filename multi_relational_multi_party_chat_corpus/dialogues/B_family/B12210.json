    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "ばなな",
            "text": "こんばんは",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "えのき",
            "text": "こんばんは",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "てばさき",
            "text": "こんばんは",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "ばなな",
            "text": "今度は三連休ですね",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "えのき",
            "text": "そうですね",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "ばなな",
            "text": "どこか出かけますか",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "てばさき",
            "text": "@ばなな そうですね",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "えのき",
            "text": "2月は三連休が2回もあって…",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "えのき",
            "text": "てばさんどこか行く？",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "てばさき",
            "text": "@えのき 私は現在毎日が日曜日状態なので",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "ばなな",
            "text": "そうなんですよね",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "えのき",
            "text": "私は仕事！",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "ばなな",
            "text": "休みが多いです",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "てばさき",
            "text": "@えのき じゃあ私も仕事…？",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "ばなな",
            "text": "@えのき お仕事なのですね",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "えのき",
            "text": "いや、パートなんで…",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "てばさき",
            "text": "@えのき せち辛え",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "えのき",
            "text": "てばさんもパートする？",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "えのき",
            "text": "親子で働いてる人いるよ",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "てばさき",
            "text": "@えのき やだあ、火傷したくない",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "えのき",
            "text": "わがままだなあ",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "ばなな",
            "text": "@えのき 調理の仕事ですか",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "えのき",
            "text": "そうです、ファミレスのキッチンで",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "えのき",
            "text": "唐揚げあげたりしてます",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "ばなな",
            "text": "たいへんですよね",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "えのき",
            "text": "すぐ火傷します",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "てばさき",
            "text": "@ばなな  この間家でいかさばく練習してました",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "えのき",
            "text": "イカね…",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "ばなな",
            "text": "@てばさき そうなんですね",
            "mention_to": [
                "てばさき"
            ]
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "えのき",
            "text": "最近はブリをさばいてます",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "てばさき",
            "text": "@ばなな 今日はブリカマ持ち帰ってきました",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "ばなな",
            "text": "火傷、辛いですね",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "えのき",
            "text": "季節によってメニューがすぐ変わるので",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "ばなな",
            "text": "ぶりかまですか。",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "ばなな",
            "text": "なるほど",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "ばなな",
            "text": "メニューが次々と",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "てばさき",
            "text": "ブリカマ焼くと生臭いので悲しくなります",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "えのき",
            "text": "換気扇、回すよ・・・",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "てばさき",
            "text": "@えのき 換気扇が間に合わない現状",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "ばなな",
            "text": "確かに匂いがするかも",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "えのき",
            "text": "@ばなな お魚は好きですか？",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "ばなな",
            "text": "@えのき 好きです",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "ばなな",
            "text": "今はぶりとか",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "えのき",
            "text": "旬ですね",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "てばさき",
            "text": "@ばなな お魚焼いたのと煮たのと干したのと生どれが好きですか？",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "ばなな",
            "text": "さばもよく食べます",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "ばなな",
            "text": "悩みますね",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "えのき",
            "text": "私は干物が好きかな…",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "てばさき",
            "text": "@えのき 生こそ至高",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "ばなな",
            "text": "家では焼くのが多くて",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "てばさき",
            "text": "焼いたのも好きです",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "ばなな",
            "text": "干物も良いですね",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "てばさき",
            "text": "@ばなな 秋は焼きさんま食べたくなります",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "えのき",
            "text": "鯖の一夜干しとか",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "えのき",
            "text": "秋はさんまだね",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "ばなな",
            "text": "@てばさき 食べたくなりますね",
            "mention_to": [
                "てばさき"
            ]
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "えのき",
            "text": "去年は買わなかった気がするけどさんま…",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "ばなな",
            "text": "家もです",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "てばさき",
            "text": "@えのき 母買うって言ってたのに",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "ばなな",
            "text": "高かった",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "えのき",
            "text": "すみません…",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "ばなな",
            "text": "しかも細くて",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "てばさき",
            "text": "@えのき 待ってたら冬になってた",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "えのき",
            "text": "スーパーで、サーモン刺が安くて…",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "ばなな",
            "text": "それは残念",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "てばさき",
            "text": "@えのき それはとても嬉しかったし美味しかった",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "えのき",
            "text": "ていうか、私がサーモンのほうが好きだから…",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "てばさき",
            "text": "@えのき それはそう",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "ばなな",
            "text": "サーモンは皆喜びます",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "えのき",
            "text": "大きい固まりで買ってきてたらふく食べてしまいます",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "ばなな",
            "text": "そうですよね",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "てばさき",
            "text": "@えのき 今家で魚食べられるの母と私しかいないのにね",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "えのき",
            "text": "てばさんもご相伴にあずかっているではないですか",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "てばさき",
            "text": "@えのき ごちになりました！",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "ばなな",
            "text": "家も私だけ",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "えのき",
            "text": "他の家族は刺身嫌いだから…",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "えのき",
            "text": "家族と食の好みがちがうの大変ですよね",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "てばさき",
            "text": "父は耐性ないし、妹は好き嫌いがヘドバン並みに激しすぎる",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "ばなな",
            "text": "刺身嫌いな人いるのですね",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "ばなな",
            "text": "皆好きかと",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "えのき",
            "text": "妹は好き嫌いが多くて…",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "てばさき",
            "text": "@ばなな お肉も食べれない人いますし、そんなもんですよ",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "ばなな",
            "text": "家の子もです",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "えのき",
            "text": "主人は、小さいころから生ものが食べられません",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "ばなな",
            "text": "なるほど",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "てばさき",
            "text": "私は私で生野菜苦手です",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "えのき",
            "text": "てばさん…",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "ばなな",
            "text": "色々ありますね",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "てばさき",
            "text": "野菜が出たら鍋に突っ込みます",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "ばなな",
            "text": "@てばさき 生でなければ",
            "mention_to": [
                "てばさき"
            ]
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "ばなな",
            "text": "食べられるのですね",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "えのき",
            "text": "生野菜のサラダが出せなくて面倒です",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "てばさき",
            "text": "芯までクッタクタのしなしなになればいけます",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "ばなな",
            "text": "それはたいへんです",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "えのき",
            "text": "@ばなな 好き嫌いはありますか？",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "ばなな",
            "text": "私はあまりないです",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "えのき",
            "text": "私も、ゲテモノ以外はほぼ食べられます",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "えのき",
            "text": "いなごは無理でしたが…",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "えのき",
            "text": "文化の違いが…",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "てばさき",
            "text": "@えのき だけどカエルの卵飲んでませんでした？",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "ばなな",
            "text": "ええ～",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "えのき",
            "text": "あれちがうよ、チアシードだよ",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "ばなな",
            "text": "びっくりしました",
            "mention_to": []
        },
        {
            "utterance_id": 103,
            "interlocutor_id": "えのき",
            "text": "美味しいよチアシード",
            "mention_to": []
        },
        {
            "utterance_id": 104,
            "interlocutor_id": "ばなな",
            "text": "チアシードでしたか",
            "mention_to": []
        },
        {
            "utterance_id": 105,
            "interlocutor_id": "てばさき",
            "text": "@えのき 私にカエルの卵といってビビらせてたの覚えてますよ",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 106,
            "interlocutor_id": "えのき",
            "text": "小さいころの遺恨が…",
            "mention_to": []
        },
        {
            "utterance_id": 107,
            "interlocutor_id": "ばなな",
            "text": "それはそれは",
            "mention_to": []
        },
        {
            "utterance_id": 108,
            "interlocutor_id": "えのき",
            "text": "こんなところで…",
            "mention_to": []
        }
