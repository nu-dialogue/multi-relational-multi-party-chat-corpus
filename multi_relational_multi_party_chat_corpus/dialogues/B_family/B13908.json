    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "いるか",
            "text": "おはようございます！",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "ちくわ",
            "text": "おはようございます。",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "こんぶ",
            "text": "おはようございます",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "こんぶ",
            "text": "宝くじあたったら何どうします？",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "こんぶ",
            "text": "どーーんと！あたったら",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "いるか",
            "text": "宝くじ！",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "ちくわ",
            "text": "5億設定にする？",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "こんぶ",
            "text": "いいね！",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "いるか",
            "text": "@こんぶ なんだろう・・・",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "こんぶ",
            "text": "5億やばい、、、",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "いるか",
            "text": "５億！しかも！",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "ちくわ",
            "text": "5億当選したとして、、",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "こんぶ",
            "text": "夢すぎる",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "ちくわ",
            "text": "税金払わないといけないよね、、",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "こんぶ",
            "text": "真面目笑",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "ちくわ",
            "text": "笑",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "こんぶ",
            "text": "そうだね、税金か",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "いるか",
            "text": "@ちくわ いくら取られるのか・・・",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "ちくわ",
            "text": "貯金とかしたいのなら",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "ちくわ",
            "text": "仕事は辞めない",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "こんぶ",
            "text": "いくらくらいなんですかね、、",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "こんぶ",
            "text": "まあ、そうだね〜〜",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "ちくわ",
            "text": "結構取られる気がする",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "ちくわ",
            "text": "まずは、",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "こんぶ",
            "text": "残り少ない人生なら辞めるかもしれないけど笑",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "ちくわ",
            "text": "旅行だな。笑",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "こんぶ",
            "text": "これからと考えると、やめられない",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "いるか",
            "text": "旅行いいですね！",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "ちくわ",
            "text": "@こんぶ せやな。",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "こんぶ",
            "text": "あとは、家のローンを完済して",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "ちくわ",
            "text": "それ！",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "ちくわ",
            "text": "あ！",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "いるか",
            "text": "めっちゃいいホテル泊りたいです！",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "ちくわ",
            "text": "車買う！",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "こんぶ",
            "text": "あーー！いいですね！！！",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "ちくわ",
            "text": "いいホテルもあり！！",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "いるか",
            "text": "@ちくわ 車！",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "こんぶ",
            "text": "ホテルはグレードアップして",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "いるか",
            "text": "@こんぶ そうそう",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "ちくわ",
            "text": "プール付き、サウナ付き！",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "こんぶ",
            "text": "飛行機のるなら、ファーストクラスで！！！笑",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "いるか",
            "text": "@こんぶ もう絶対条件ですね！",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "ちくわ",
            "text": "飛行機使うのなら、、",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "こんぶ",
            "text": "@ちくわ 車も一括かな！笑",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "ちくわ",
            "text": "専用のジェット機を予約する",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "こんぶ",
            "text": "@いるか もう、全部いいのにします！！！",
            "mention_to": [
                "いるか"
            ]
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "こんぶ",
            "text": "専用ジェットやばすぎ",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "いるか",
            "text": "@ちくわ レベルが違う！",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "ちくわ",
            "text": "車も一括やね！",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "ちくわ",
            "text": "レベル、、笑",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "ちくわ",
            "text": "ジェット機から降りても誰も迎えてくれないのにね。",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "こんぶ",
            "text": "それでも、残るのかな？笑",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "ちくわ",
            "text": "笑",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "いるか",
            "text": "車は一気に減りそうですね！",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "ちくわ",
            "text": "車は最高でも500万くらいやない？",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "こんぶ",
            "text": "車たしかに〜何の車にするかでも違いますよね〜",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "ちくわ",
            "text": "そんなフェラーリとか買っても家族乗れないし笑",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "いるか",
            "text": "@ちくわ いい車を・・・！",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "いるか",
            "text": "確かに笑",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "こんぶ",
            "text": "あ、一応、ファミリーカーで考えてるのか笑",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "ちくわ",
            "text": "国産の車でいいやつぐらいやったら500から700やね！",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "いるか",
            "text": "なんかもう長いやつを",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "こんぶ",
            "text": "2台持ちもありだけど",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "ちくわ",
            "text": "@こんぶ 一応考えてます",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "こんぶ",
            "text": "後での、維持費を考えたら〜",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "ちくわ",
            "text": "維持費は高いねー",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "いるか",
            "text": "そうですよ！何台もかえる！",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "いるか",
            "text": "あ～～～現実が～～～～",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "こんぶ",
            "text": "車そんなに好きでもないから、一台でも満足かーー",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "こんぶ",
            "text": "ねーー！現実みたら、、",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "いるか",
            "text": "そう考えると",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "ちくわ",
            "text": "一台で十分",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "こんぶ",
            "text": "5億とはいえ、、なくなることを考えたら、、",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "いるか",
            "text": "大きい買い物って難しいですね",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "ちくわ",
            "text": "そうすよね！",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "こんぶ",
            "text": "そうなりますよね〜〜",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "こんぶ",
            "text": "ほんとそうですよね！！！",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "ちくわ",
            "text": "なんか色々細々したの買いますよねー",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "いるか",
            "text": "@ちくわ ですです",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "ちくわ",
            "text": "でも最近、2億？",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "こんぶ",
            "text": "@ちくわ たしかにーーー",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "ちくわ",
            "text": "当選した人が見つからなかったニュースみました？",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "いるか",
            "text": "なんか、ドーン！と買えるものってなんだろう",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "こんぶ",
            "text": "え！！！見てない",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "いるか",
            "text": "@ちくわ え！",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "いるか",
            "text": "日本ですか？？？",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "こんぶ",
            "text": "ドーンと買えるもの、、",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "ちくわ",
            "text": "なんかヤフーニュースで見たような",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "ちくわ",
            "text": "@いるか 多分日本です！",
            "mention_to": [
                "いるか"
            ]
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "こんぶ",
            "text": "当選者気付いてない？",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "いるか",
            "text": "当選者・・・私だったらどうしよう・・・笑",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "こんぶ",
            "text": "家？マンション？",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "ちくわ",
            "text": "どーなんやろね！笑",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "ちくわ",
            "text": "ドーンと買うのなら家か！",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "こんぶ",
            "text": "買ったんですね！！！宝くじ！！",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "いるか",
            "text": "@こんぶ 家！！！",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "いるか",
            "text": "@こんぶ 季節ものは買います！",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "いるか",
            "text": "年末ジャンボとか！",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "こんぶ",
            "text": "私たちまだ挑戦したことなくて〜",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "ちくわ",
            "text": "昔、家族でスクラッチしてました笑",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "こんぶ",
            "text": "とかって、密かに買ってたりして、ちくわさん笑",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "いるか",
            "text": "@こんぶ 宝くじですか？",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "ちくわ",
            "text": "親が買ってきたのを削って",
            "mention_to": []
        },
        {
            "utterance_id": 103,
            "interlocutor_id": "こんぶ",
            "text": "@いるか そうなんですよーー！！！",
            "mention_to": [
                "いるか"
            ]
        }
