        },
        {
            "utterance_id": 1,
            "interlocutor_id": "こんぶ",
            "text": "こんばんは！",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "ちくわ",
            "text": "こんばんは！",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "こんぶ",
            "text": "この後は何をされますか？",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "こんぶ",
            "text": "私たちはテレビを見ながら",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "ちくわ",
            "text": "僕らは、録画のドラマかYouTubeみます！",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "こんぶ",
            "text": "洗濯がおわるの待ちです！",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "ばなな",
            "text": "そうなのですね",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "ちくわ",
            "text": "あ！洗濯が残ってた、、、",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "こんぶ",
            "text": "日課です！",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "ばなな",
            "text": "録画は何を？",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "ばなな",
            "text": "洗濯これからなのですね",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "こんぶ",
            "text": "木梨憲武さんの",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "こんぶ",
            "text": "春がきたら？春になったら？",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "ちくわ",
            "text": "新空港占拠のドラマとあとなんだったけ？",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "こんぶ",
            "text": "とかみてます",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "ちくわ",
            "text": "そうそう！",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "こんぶ",
            "text": "そうそう！それも！",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "ばなな",
            "text": "色々ありますね",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "ちくわ",
            "text": "洗濯待ってまーす！",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "こんぶ",
            "text": "@ばなな 夜に洗濯派です",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "ちくわ",
            "text": "家が薪ストーブなので",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "ちくわ",
            "text": "夜のうちに干そうとなり！",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "こんぶ",
            "text": "@ばなな 何かドラマみられますか？",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "ばなな",
            "text": "薪ストーブあるのですね",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "こんぶ",
            "text": "洗濯が加湿にもなるんですよ！",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "こんぶ",
            "text": "そうなんです！",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "ばなな",
            "text": "@こんぶ 今見ているのはグレイトギフトです",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "ちくわ",
            "text": "@ばなな 新築なので、付けました！",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "ばなな",
            "text": "あとはさよならマエストロ",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "こんぶ",
            "text": "さよならマエストロ！",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "こんぶ",
            "text": "知ってます！",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "こんぶ",
            "text": "録画してまだみてないです！",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "ちくわ",
            "text": "溜まっていくばかりで",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "ちくわ",
            "text": "結局見ないという、、",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "ばなな",
            "text": "確かに溜まります",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "こんぶ",
            "text": "リアルタイムでドラマはなかなか見れません",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "ちくわ",
            "text": "@ばなな いつも何時には就寝してるんですか？",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "ばなな",
            "text": "@ちくわ わかります",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "ちくわ",
            "text": "僕らはなんだかんだ0時です！",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "ばなな",
            "text": "@ちくわ ０時近くです",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "こんぶ",
            "text": "@ばなな そうですよね！！！",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "ちくわ",
            "text": "あ！",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "ちくわ",
            "text": "一緒ですね！",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "こんぶ",
            "text": "私たちも同じくらいです！",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "ばなな",
            "text": "何故か遅くなります",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "ちくわ",
            "text": "一階のリビングで寝てしまい",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "こんぶ",
            "text": "わかります！",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "ちくわ",
            "text": "起きたら2時とかあります！",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "ばなな",
            "text": "リビングで",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "こんぶ",
            "text": "@ばなな 朝は何時起きですか？",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "ちくわ",
            "text": "眠い中、2階に上がるのは大変です！",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "ばなな",
            "text": "@こんぶ ６時３０分ぐらいです",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "こんぶ",
            "text": "そうそう！大変",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "ちくわ",
            "text": "僕らは、娘が起こしてくれます！",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "こんぶ",
            "text": "娘が目覚ましがわりだよね！",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "ちくわ",
            "text": "僕らも6時半とかだね！",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "こんぶ",
            "text": "そうそう！",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "こんぶ",
            "text": "@ばなな 一緒くらいです！",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "ばなな",
            "text": "娘さんが起こしてくれるなんて",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "ばなな",
            "text": "羨ましいです",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "ちくわ",
            "text": "休日はまだ寝てたいのに起こしてきます！",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "ばなな",
            "text": "それは辛いですね",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "こんぶ",
            "text": "ほんとそう〜！",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "ばなな",
            "text": "昔はそうだったかも",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "ちくわ",
            "text": "でも可愛いんですよー！",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "こんぶ",
            "text": "子どもがいると、ゆっくり寝てるなんて、無理ですね",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "ばなな",
            "text": "可愛いですよね",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "ちくわ",
            "text": "朝から活発です！",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "こんぶ",
            "text": "可愛いですね",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "ばなな",
            "text": "お子さん小さいとゆっくりは無理ですね",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "ちくわ",
            "text": "今はもう寝てくれています！",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "こんぶ",
            "text": "やっぱりそうですよね",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "ばなな",
            "text": "それは良いですね。",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "こんぶ",
            "text": "今は、自分も体力勝負です！",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "こんぶ",
            "text": "早めに寝かしつけて",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "ばなな",
            "text": "家はなかなか寝なかったです",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "ちくわ",
            "text": "また夜中に一回起きてくると思います！",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "こんぶ",
            "text": "あとは、大人のゆっくり時間にしてます！",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "ちくわ",
            "text": "そうなんですね！",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "ばなな",
            "text": "それはたいへん",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "ばなな",
            "text": "熟睡できないですね",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "こんぶ",
            "text": "そうなんですよ",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "ばなな",
            "text": "息子は朝も弱くて",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "こんぶ",
            "text": "でも、旦那は熟睡",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "ちくわ",
            "text": "なんかルーティン？みたいな感じです！",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "こんぶ",
            "text": "起きるのは私です",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "ばなな",
            "text": "なるほど",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "こんぶ",
            "text": "朝起きるの苦手だと",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "ちくわ",
            "text": "朝も弱いんですね！",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "こんぶ",
            "text": "起こすんですね！",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "ばなな",
            "text": "夜強く朝に弱いです",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "ちくわ",
            "text": "起こすのも大変そうですね！",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "こんぶ",
            "text": "起こしても、わかってる〜とか言って、布団もぐりこんで",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "こんぶ",
            "text": "なかなか起きないやつ",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "ばなな",
            "text": "はい、５回起こしても起きないです",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "こんぶ",
            "text": "起こしてるのに、逆に怒られるみたいな！",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "ちくわ",
            "text": "そして、時間がギリギリになるやつ",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "ばなな",
            "text": "そうなんです",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "こんぶ",
            "text": "5回！！！",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "こんぶ",
            "text": "思わず笑ってしまいました",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "ばなな",
            "text": "いらっとします",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "ちくわ",
            "text": "ちゃんと起こしてよ！とか言われたり！",
            "mention_to": []
        },
        {
            "utterance_id": 103,
            "interlocutor_id": "こんぶ",
            "text": "それは、ほんと大変だ",
            "mention_to": []
        },
        {
            "utterance_id": 104,
            "interlocutor_id": "ちくわ",
            "text": "僕らもそうなるのかなー？",
            "mention_to": []
        },
        {
            "utterance_id": 105,
            "interlocutor_id": "ばなな",
            "text": "前に霧吹きで水掛けました",
            "mention_to": []
        },
        {
            "utterance_id": 106,
            "interlocutor_id": "こんぶ",
            "text": "@ばなな それでも遅刻はしませんか？",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 107,
            "interlocutor_id": "こんぶ",
            "text": "ナイスですね！！！",
            "mention_to": []
        },
        {
            "utterance_id": 108,
            "interlocutor_id": "ちくわ",
            "text": "霧吹きはナイスです！",
            "mention_to": []
        }
