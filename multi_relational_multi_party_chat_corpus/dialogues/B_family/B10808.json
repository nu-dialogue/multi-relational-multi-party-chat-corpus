        {
            "utterance_id": 38,
            "interlocutor_id": "たまご",
            "text": "けど2月でそこまでででした",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "りんご",
            "text": "ディズニーはなんかきれいでワクワクになりますね",
