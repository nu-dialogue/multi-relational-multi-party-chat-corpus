        },
        {
            "utterance_id": 22,
            "interlocutor_id": "たまご",
            "text": "一回行った",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "りんご",
            "text": "お母さん知らなかったようです",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "たまご",
            "text": "2月くらいにー",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "つみれ",
            "text": "びっくりです",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "りんご",
            "text": "@たまご パレードとかみました？",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "たまご",
            "text": "見ましたー",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "りんご",
            "text": "@たまご お母さんも連れて行ってあげてくださいー",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "たまご",
            "text": "パレード綺麗で好きです",
