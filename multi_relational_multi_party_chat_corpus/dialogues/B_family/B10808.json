        {
            "utterance_id": 35,
            "interlocutor_id": "つみれ",
            "text": "@りんご 信じて待ちます",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "りんご",
            "text": "@たまご 混んでましたか？",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "たまご",
            "text": "混んでました",
