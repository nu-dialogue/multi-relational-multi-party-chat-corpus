        },
        {
            "utterance_id": 1,
            "interlocutor_id": "つみれ",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "たまご",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "うさぎ",
            "text": "うさぎです。よろしくお願いします",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "つみれ",
            "text": "つみれです。",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "たまご",
            "text": "たまごです！",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "たまご",
            "text": "お願いします",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "つみれ",
            "text": "よろしくお願いします",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "うさぎ",
            "text": "お二人は親子でいらっしゃるんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "たまご",
            "text": "そうです！",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "つみれ",
            "text": "そうです！親子なんです。",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "うさぎ",
            "text": "私にも高校生の娘がいます！",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "たまご",
            "text": "子です！",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "たまご",
            "text": "そうなんですね！",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "うさぎ",
            "text": "今日はバレンタインですね",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "つみれ",
            "text": "高校生のお子さんがいらっしゃるんですね",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "つみれ",
            "text": "バレンタインですね",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "うさぎ",
            "text": "はい、週末、友チョコ作りで大変でした",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "たまご",
            "text": "チョコの季節です",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "うさぎ",
            "text": "あっ、チョコ失敗してクッキーになってましたが",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "たまご",
            "text": "クッキーになったんですか！！",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "つみれ",
            "text": "娘さんチョコ作ったんですね",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "うさぎ",
            "text": "はい、笑えますよね",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "うさぎ",
            "text": "学校禁止ですがこっそりみんな持ってきてるらしいです",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "たまご",
            "text": "クッキーも美味しいんで良いです！！",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "たまご",
            "text": "青春ですねー",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "つみれ",
            "text": "そうですそうです！",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "うさぎ",
            "text": "チョコ買われたりしましたか？",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "つみれ",
            "text": "お友達同士楽しいですよね",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "たまご",
            "text": "買ってないですー",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "うさぎ",
            "text": "我が家は自分にご褒美として買っちゃいます",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "たまご",
            "text": "貰いたい側です",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "つみれ",
            "text": "私、昨日まで２泊3日の仕事でそれどころではなかったです。",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "たまご",
            "text": "それも良いですね〜",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "うさぎ",
            "text": "つみれさん、お疲れ様です！",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "たまご",
            "text": "お疲れ様！",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "うさぎ",
            "text": "お泊まりですか？",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "つみれ",
            "text": "そうです！",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "たまご",
            "text": "大変だ",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "うさぎ",
            "text": "大変ですね",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "つみれ",
            "text": "静岡まで行ってきました",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "うさぎ",
            "text": "寒いですし",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "たまご",
            "text": "遠い！",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "うさぎ",
            "text": "静岡ならまだそこまで寒さは？",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "つみれ",
            "text": "静岡も思ってたより寒かったです",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "うさぎ",
            "text": "あーそうなんですね！",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "つみれ",
            "text": "底冷えする感じで",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "うさぎ",
            "text": "転勤族なので大阪にいたときとかよく新幹線で通ってました",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "たまご",
            "text": "太平洋側の方がまだ大丈夫な寒さですか",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "たまご",
            "text": "大阪にいたんですか",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "つみれ",
            "text": "転勤族は大変ですね。",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "つみれ",
            "text": "大阪羨ましい",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "うさぎ",
            "text": "はい、仙台、広島、高松、大阪にいました",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "うさぎ",
            "text": "旅行ですね笑",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "たまご",
            "text": "北から南まですごい",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "うさぎ",
            "text": "なぜか北海道、九州はなかったです",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "つみれ",
            "text": "本州メインなんですね",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "うさぎ",
            "text": "たまたまですね",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "うさぎ",
            "text": "本当は海外もあるので",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "たまご",
            "text": "全部ご飯が美味しいところ！",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "つみれ",
            "text": "本当に！",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "たまご",
            "text": "すごいですね！",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "うさぎ",
            "text": "牡蠣が広島、仙台有名なのに主人がダメで",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "うさぎ",
            "text": "友達とランチで食べてました",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "たまご",
            "text": "牡蠣美味しそう〜",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "うさぎ",
            "text": "味が微妙に違いました",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "つみれ",
            "text": "ご主人嫌いなんですね。。。",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "うさぎ",
            "text": "主人はあたったらしくて",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "つみれ",
            "text": "もったいない、私大好きです！",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "つみれ",
            "text": "私も2度当たってますが、やめられません",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "うさぎ",
            "text": "私もあたったことあるけど立ち直りました",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "うさぎ",
            "text": "つみれさん、同志！",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "つみれ",
            "text": "ですよね！",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "たまご",
            "text": "まだ免れてます",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "うさぎ",
            "text": "たまごさんは大丈夫？",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "たまご",
            "text": "牡蠣好きです！",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "つみれ",
            "text": "あの辛さは。。。最強ですよね。",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "たまご",
            "text": "怖いー",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "うさぎ",
            "text": "生牡蠣、カキフライ両方好きですか？",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "つみれ",
            "text": "家族みんな好きです！",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "たまご",
            "text": "全部好きです！！",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "うさぎ",
            "text": "牡蠣鍋やりたいです！",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "うさぎ",
            "text": "土手鍋？",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "つみれ",
            "text": "いいですね〜",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "たまご",
            "text": "鍋もうまそう〜",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "うさぎ",
            "text": "お味噌つけません？",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "つみれ",
            "text": "味噌系だ！",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "うさぎ",
            "text": "はい、実家でやります",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "たまご",
            "text": "良いですね〜",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "うさぎ",
            "text": "主人抜きで笑笑",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "うさぎ",
            "text": "ついでに蟹も。。",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "たまご",
            "text": "最高だ！！",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "うさぎ",
            "text": "ちょっとビールとかも？",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "つみれ",
            "text": "最高だ！",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "うさぎ",
            "text": "冬はいいですよね",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "つみれ",
            "text": "ご主人抜きで笑",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "たまご",
            "text": "食べたくなってきました",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "うさぎ",
            "text": "出張おおいので",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "うさぎ",
            "text": "わかります！私も食べたくなってきました",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "つみれ",
            "text": "出張を狙って！",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "たまご",
            "text": "頭の中では食べてます",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "うさぎ",
            "text": "こういうときにないんですよねー",
            "mention_to": []
        }
