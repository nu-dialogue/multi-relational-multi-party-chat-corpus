{
    "dialogue_id": "B12202",
    "dialogue_type": "Family",
    "interlocutors": [
        "ばなな",
        "えのき",
        "てばさき"
    ],
    "relationship": [
        "えのき",
        "てばさき"
    ],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "ばなな",
            "text": "おはようございます。",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "えのき",
            "text": "おはようございます",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "てばさき",
            "text": "おはようございます",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "えのき",
            "text": "来週は雪の予報ですね",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "えのき",
            "text": "寒くなりそうです",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "ばなな",
            "text": "そうですよね",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "てばさき",
            "text": "@えのき よし、絶対に外出ない",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "ばなな",
            "text": "寒いのは嫌ですね",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "えのき",
            "text": "私、通勤に自転車を使っているので、雪はつらいです・・・",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "てばさき",
            "text": "@えのき それはそれとして積もったなら遊びたい",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "えのき",
            "text": "ええー",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "てばさき",
            "text": "森羅万象の雪をサクサク踏むのです",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "ばなな",
            "text": "雪遊びですね",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "えのき",
            "text": "子供は気楽でいいなあ",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "えのき",
            "text": "自転車が使えないと、バスになるし",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "えのき",
            "text": "バスだと、自転車より時間がかかっちゃうし",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "えのき",
            "text": "みたいなことしか考えない",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "ばなな",
            "text": "@えのき どちらにお住まいですか？",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "てばさき",
            "text": "関東です",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "えのき",
            "text": "です",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "ばなな",
            "text": "同じですね",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "えのき",
            "text": "基本的に雪が降らないので、降ると大変ですよね",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "ばなな",
            "text": "そうですよね",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "ばなな",
            "text": "道は凍りますし",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "てばさき",
            "text": "電車がよく止まります、雪が降ると",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "ばなな",
            "text": "交通機関が駄目になりますね",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "えのき",
            "text": "@ばなな さんは、ふだん自転車ですか？車？電車？",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "えのき",
            "text": "うちは車がないので自転車か電車ですが…",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "ばなな",
            "text": "私は家で仕事しています",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "えのき",
            "text": "おでかけされないんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "てばさき",
            "text": "@ばなな 在宅いいですよね　ぬくぬく",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "ばなな",
            "text": "しますよ",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "えのき",
            "text": "てばさんは、運動のためにも外に出たほうがいいと思う",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "ばなな",
            "text": "運転は出来なくて",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "えのき",
            "text": "私もペーパードライバーで運転できません",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "えのき",
            "text": "免許証は身分証明書です",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "ばなな",
            "text": "普段は歩きが多いです",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "てばさき",
            "text": "私は無免許なので、これまでも、これからも",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "えのき",
            "text": "@てばさき さんは、取らないほうがいいね、免許",
            "mention_to": [
                "てばさき"
            ]
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "ばなな",
            "text": "@てばさき 免許取らないですか？",
            "mention_to": [
                "てばさき"
            ]
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "てばさき",
            "text": "訓練でも私に運転させてみろ、事故る",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "ばなな",
            "text": "ええ～",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "えのき",
            "text": "人には向き不向きが…",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "ばなな",
            "text": "そんなことないかと",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "えのき",
            "text": "てばさんは、とても向いていない…",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "ばなな",
            "text": "@てばさき 何か特技は？",
            "mention_to": [
                "てばさき"
            ]
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "てばさき",
            "text": "車は死角が多すぎるのです…生身でもいろんなところにぶつけるのに",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "えのき",
            "text": "あと、都会は車が多くて、運転大変そう←田舎育ち",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "てばさき",
            "text": "@ばなな 特技ですか…",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "えのき",
            "text": "てばさんの特技・・・",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "えのき",
            "text": "お昼寝！？",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "ばなな",
            "text": "何か好きなこととか",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "てばさき",
            "text": "@えのき 惰眠は至福です",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "ばなな",
            "text": "昼寝良いですね",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "えのき",
            "text": "だと思ってた",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "ばなな",
            "text": "どこでも眠れるのかしら",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "ばなな",
            "text": "夫がそうです",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "えのき",
            "text": "どこでも寝られるのは特技ですよね！",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "てばさき",
            "text": "小さい頃は歩きながら友達の服を掴んで目を閉じてました",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "えのき",
            "text": "歩きながら眠れるの？",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "ばなな",
            "text": "それはすごい",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "てばさき",
            "text": "目を閉じれば意外と脳は休まる",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "えのき",
            "text": "おともだちに迷惑かけてない！？",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "てばさき",
            "text": "@えのき 迷惑以外の何者でもないのでは？",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "えのき",
            "text": "母心配になってきた…",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "ばなな",
            "text": "歩きながらは特技ですね",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "てばさき",
            "text": "まああの頃の自分はとても愛嬌がよかったので",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "えのき",
            "text": "@ばなな さんは、何か得意なことはありますか？",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "ばなな",
            "text": "@えのき ピアノ弾くことですかね",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "ばなな",
            "text": "音楽が好きです",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "えのき",
            "text": "ピアノ弾かれるんですね！",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "えのき",
            "text": "すごい！",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "てばさき",
            "text": "@ばなな ピアノ！魔王とか弾けますか？",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "ばなな",
            "text": "20年ほど習っていました",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "えのき",
            "text": "音楽いいですね！",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "ばなな",
            "text": "魔王は弾いたことないです",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "えのき",
            "text": "ピアノ…弾けたら楽しそう",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "えのき",
            "text": "どんな曲が得意ですか？",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "ばなな",
            "text": "昔はショパンとか弾いていました",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "えのき",
            "text": "ストリートピアノとか弾いたりしますか？",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "てばさき",
            "text": "@えのき あれ弾けたらかっこいい",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "ばなな",
            "text": "ストリートは恥ずかしいですね",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "えのき",
            "text": "YouTubeでよく見ますよね",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "てばさき",
            "text": "ショパン…子犬のワルツとか幻想即興曲とかでしたっけ？",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "ばなな",
            "text": "今はそんなに弾けなくて",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "えのき",
            "text": "そうなんですね",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "えのき",
            "text": "私は楽器一切弾けないですが",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "えのき",
            "text": "おうちにピアノはあるんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "ばなな",
            "text": "あります",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "えのき",
            "text": "いいですね！",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "てばさき",
            "text": "こちらにもキーボードありますよ、今は使えてませんが",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "ばなな",
            "text": "@てばさき 何か弾いたことは？",
            "mention_to": [
                "てばさき"
            ]
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "えのき",
            "text": "すみません…キーボード、物置になってて",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "えのき",
            "text": "小学生くらい？のころに",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "ばなな",
            "text": "そうでしたか",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "ばなな",
            "text": "キーボードは皆欲しがりますね",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "えのき",
            "text": "遠い記憶すぎてすみません",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "てばさき",
            "text": "@ばなな ブラックジャックの月光花とかキーボードに収録されていたので弾きました！",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "えのき",
            "text": "@てばさき へー",
            "mention_to": [
                "てばさき"
            ]
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "ばなな",
            "text": "@てばさき すごいですね",
            "mention_to": [
                "てばさき"
            ]
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "えのき",
            "text": "キーボード自体に曲が入ってるんだね",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "えのき",
            "text": "今だと、適宜ダウンロードして弾けそう",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "ばなな",
            "text": "入っていますね",
            "mention_to": []
        },
        {
            "utterance_id": 103,
            "interlocutor_id": "えのき",
            "text": "音楽は、聞く専門ですね",
            "mention_to": []
        },
        {
            "utterance_id": 104,
            "interlocutor_id": "てばさき",
            "text": "聖者の行進とか蜂が飛ぶんとかも入ってましたよ",
            "mention_to": []
        },
        {
            "utterance_id": 105,
            "interlocutor_id": "ばなな",
            "text": "@えのき 何を聴きますか？",
            "mention_to": [
                "えのき"
            ]
        }
