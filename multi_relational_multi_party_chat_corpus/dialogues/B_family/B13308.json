        "つくね",
        "しらたき"
    ],
    "relationship": [
        "つくね",
        "しらたき"
    ],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "コアラ",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "つくね",
            "text": "こんにちはー！",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "しらたき",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "コアラ",
            "text": "今年は絶対これしたいって何かありますか？",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "コアラ",
            "text": "あっ、悩みますか？",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "つくね",
            "text": "USJ行きたいです",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "しらたき",
            "text": "USJ!",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "コアラ",
            "text": "@つくね あっ、行かれたことない？",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "コアラ",
            "text": "お二人とも！",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "しらたき",
            "text": "行きたいね",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "つくね",
            "text": "@コアラ 1回ありますが",
            "mention_to": [
                "コアラ"
            ]
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "つくね",
            "text": "マリオができる前だったので",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "つくね",
            "text": "もう1回行きたいです",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "コアラ",
            "text": "@つくね あーマリオ楽しいですよ！",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "しらたき",
            "text": "そーそーマリオね",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "コアラ",
            "text": "一昨年行きました",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "しらたき",
            "text": "@コアラ いーなー",
            "mention_to": [
                "コアラ"
            ]
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "コアラ",
            "text": "高校生の娘も大盛り上がりでした",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "つくね",
            "text": "@コアラ いーなー！",
            "mention_to": [
                "コアラ"
            ]
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "コアラ",
            "text": "あとはハリポタエリア",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "つくね",
            "text": "ハリポタの杖は前行った時に",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "コアラ",
            "text": "ただお盆だったので地獄の暑さでした",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "つくね",
            "text": "買いました！",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "つくね",
            "text": "混みますよね",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "コアラ",
            "text": "@つくね あの杖！",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "しらたき",
            "text": "ハリポタ混んでて乗れなかった",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "コアラ",
            "text": "@しらたき 次乗ってください",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "コアラ",
            "text": "いまそこまでじゃないかも？",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "コアラ",
            "text": "とにかく暑くて",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "つくね",
            "text": "@コアラ 3つも買いました笑",
            "mention_to": [
                "コアラ"
            ]
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "コアラ",
            "text": "40℃近かったので",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "しらたき",
            "text": "@コアラ 乗ります！",
            "mention_to": [
                "コアラ"
            ]
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "つくね",
            "text": "@コアラ えー40度はしんどいですね",
            "mention_to": [
                "コアラ"
            ]
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "しらたき",
            "text": "並んででも！",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "つくね",
            "text": "並ぶし",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "コアラ",
            "text": "@つくね まだあるなら持ってけますね",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "つくね",
            "text": "@コアラ そうなんですが、",
            "mention_to": [
                "コアラ"
            ]
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "しらたき",
            "text": "バタービール不味かった！",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "つくね",
            "text": "子供がちいさかったので",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "コアラ",
            "text": "一応ファストパスみたいなの追加で買いましたー",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "つくね",
            "text": "先端が…ダメかも",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "コアラ",
            "text": "@しらたき わかるー甘くないですか？",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "つくね",
            "text": "@コアラ あれ高くないですか！？",
            "mention_to": [
                "コアラ"
            ]
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "しらたき",
            "text": "@コアラ 甘すぎます！",
            "mention_to": [
                "コアラ"
            ]
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "コアラ",
            "text": "@つくね はい、とんでもなかったです",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "つくね",
            "text": "しかも1人ずつですよね？",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "コアラ",
            "text": "でもおそらく最初で最後だったので",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "しらたき",
            "text": "ほんとにビールならいいのにな！",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "つくね",
            "text": "いくらあったらいいんだろ…",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "コアラ",
            "text": "確か3人で10万近くとかでした",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "つくね",
            "text": "ぎゃー！まじですか！？",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "コアラ",
            "text": "それなのにジュラシックパークは乗れませんでした",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "つくね",
            "text": "やばいですね",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "コアラ",
            "text": "お盆で1番高かったので",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "しらたき",
            "text": "諭吉10人、、、",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "つくね",
            "text": "@コアラ ダイナソーのやつですね！",
            "mention_to": [
                "コアラ"
            ]
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "コアラ",
            "text": "ホテルは泊まりますか？",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "つくね",
            "text": "@コアラ 基本1泊はします！",
            "mention_to": [
                "コアラ"
            ]
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "コアラ",
            "text": "@つくね それです！あれつけなきゃよかったー",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "つくね",
            "text": "そうなんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "コアラ",
            "text": "うちもパーク前のユニバーサルホテルかな？泊まりました",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "つくね",
            "text": "色々選べますよね",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "つくね",
            "text": "おー！羨ましい！",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "しらたき",
            "text": "泊まらなきゃ死んじゃう",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "コアラ",
            "text": "@つくね とにかくマリオだけとか絶対のものをつけるとか？",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "つくね",
            "text": "公式ホテル泊まってみたいでさ",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "コアラ",
            "text": "あとは最低限でもいいかも？",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "つくね",
            "text": "@コアラ なるほど！いっぱい選べるから",
            "mention_to": [
                "コアラ"
            ]
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "コアラ",
            "text": "@つくね いつ頃予定してますか？",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "つくね",
            "text": "どれ買えばいいか",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "つくね",
            "text": "迷います…",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "コアラ",
            "text": "@つくね あとはマリオのウォッチ？なんかありますよね？",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "つくね",
            "text": "@コアラ 秋頃の予定ですね！",
            "mention_to": [
                "コアラ"
            ]
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "コアラ",
            "text": "忘れましたが",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "つくね",
            "text": "@コアラ あれも結構しますよね？",
            "mention_to": [
                "コアラ"
            ]
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "コアラ",
            "text": "パークじゃなくて事前にメルカリでもいいかも",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "コアラ",
            "text": "たくさん出てますよー",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "つくね",
            "text": "まじですか！？",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "コアラ",
            "text": "はい",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "つくね",
            "text": "見てみます！",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "コアラ",
            "text": "ぜひ！",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "コアラ",
            "text": "もったいないですよ",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "つくね",
            "text": "何回も行けるわけではないので",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "コアラ",
            "text": "ガイドとかも",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "つくね",
            "text": "その辺は中古でもいいかな",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "コアラ",
            "text": "すぐ出てるし",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "しらたき",
            "text": "@コアラ ユニバの絶叫系乗れますか？",
            "mention_to": [
                "コアラ"
            ]
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "つくね",
            "text": "そうなんですね！？",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "コアラ",
            "text": "一回行かれてるなら場所わかりますものね",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "つくね",
            "text": "メルカリすごい笑",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "コアラ",
            "text": "@しらたき のれないですー",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "コアラ",
            "text": "乗りますか？",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "つくね",
            "text": "@コアラ でも数年前なので",
            "mention_to": [
                "コアラ"
            ]
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "つくね",
            "text": "迷いそうです",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "しらたき",
            "text": "同じく乗れません！",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "コアラ",
            "text": "なにしろダイナソー棄権したので",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "しらたき",
            "text": "足ふらふらのやつ！笑",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "コアラ",
            "text": "絶叫系のファストパス外せばそこまで高くないはず",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "つくね",
            "text": "この前トラブルで宙ぶらりんでしたよね？",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "コアラ",
            "text": "マリオはマストですよね",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "しらたき",
            "text": "つくねは乗れるね！",
            "mention_to": []
        }
