    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "ばなな",
            "text": "こんばんは",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "つくね",
            "text": "こんばんは",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "しらたき",
            "text": "こんばんは",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "ばなな",
            "text": "好きな食べ物は？",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "しらたき",
            "text": "ラーメンとカレーです",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "ばなな",
            "text": "良いですね",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "つくね",
            "text": "@しらたき 子供じゃん！笑",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "ばなな",
            "text": "寒い日はラーメン食べたくなります",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "つくね",
            "text": "私はパスタとかグラタンが好きです",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "しらたき",
            "text": "あったまりますね",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "ばなな",
            "text": "グラタンも良いですね",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "しらたき",
            "text": "@ばなな さんは？",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "つくね",
            "text": "作るのがめんどくさいですが",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "つくね",
            "text": "美味しいです",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "ばなな",
            "text": "@しらたき 甘いのが好きで",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "ばなな",
            "text": "あんこ好きです",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "つくね",
            "text": "@ばなな お汁粉すきです！！",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "ばなな",
            "text": "今日はおはぎを食べました",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "しらたき",
            "text": "@つくね は、粒あんだめだよね！",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "ばなな",
            "text": "お汁粉も良いですね",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "つくね",
            "text": "息子も娘もおはぎ大好きです！",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "ばなな",
            "text": "粒あん駄目ですか？",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "つくね",
            "text": "@しらたき つぶあんは歯に挟まる！",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "つくね",
            "text": "あの皮が",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "つくね",
            "text": "@ばなな 食べられますが",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "ばなな",
            "text": "それが嫌なのですね",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "つくね",
            "text": "あんまり好きじゃないです笑",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "しらたき",
            "text": "歯に挟まるのはエノキ！",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "ばなな",
            "text": "こしあんの方がなめらかですね",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "つくね",
            "text": "@しらたき えのきね！確かに挟まる！",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "ばなな",
            "text": "@しらたき エノキ、わかります",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "しらたき",
            "text": "冬は鍋も好きです",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "つくね",
            "text": "@ばなな ですよね！あんころ餅とか大好きです！",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "ばなな",
            "text": "鍋も良いですね",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "つくね",
            "text": "@しらたき 鍋したいねー",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "しらたき",
            "text": "鍋のエノキはに挟まる",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "ばなな",
            "text": "鍋は何が好きですか？",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "つくね",
            "text": "@しらたき しかもとれないよね",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "しらたき",
            "text": "キムチ！",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "つくね",
            "text": "@ばなな 鍋は塩系ととんこつ系が好きです",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "ばなな",
            "text": "キムチ鍋ですね",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "ばなな",
            "text": "塩もさっぱりしていて美味しそう",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "つくね",
            "text": "@ばなな でも、基本キムチになります笑",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "つくね",
            "text": "大体子供もキムチ一択で",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "つくね",
            "text": "中々私の意見は通らずです",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "しらたき",
            "text": "でも鍋食べた後片付けが大変",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "ばなな",
            "text": "お子さんもキムチ食べられるのですね",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "つくね",
            "text": "@ばなな 締めのリゾットが好きでして",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "ばなな",
            "text": "リゾット良いですね",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "つくね",
            "text": "鍋はほとんど親が食べてます",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "つくね",
            "text": "@ばなな チーズたくさんで美味しいです！",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "ばなな",
            "text": "家は麺を入れたりします",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "つくね",
            "text": "麺もいいですねー！",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "ばなな",
            "text": "@つくね チーズたくさん、美味しそうですね",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "しらたき",
            "text": "締めは鍋の種類にもよりますね",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "ばなな",
            "text": "確かにそうですよね",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "ばなな",
            "text": "家はこの間、坦坦鍋にして",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "つくね",
            "text": "@ばなな ちょっとくどいですが",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "つくね",
            "text": "美味しいです！",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "ばなな",
            "text": "締めに担々麺にしました",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "つくね",
            "text": "@ばなな 坦々鍋！美味しそう！",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "しらたき",
            "text": "坦々鍋は食べた事ないなー",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "ばなな",
            "text": "美味しかったです",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "つくね",
            "text": "冬はご飯に困ったら鍋にできるので",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "つくね",
            "text": "結構楽です笑",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "ばなな",
            "text": "そうですよね",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "つくね",
            "text": "義実家から白菜を毎年たくさんもらうので",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "つくね",
            "text": "余計に鍋率が上がります！",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "しらたき",
            "text": "@つくね 美味しいから何度でもオッケー",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "ばなな",
            "text": "良いですね",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "ばなな",
            "text": "白菜、よく食べますよ",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "つくね",
            "text": "@しらたき 確かに！今週は鍋かな？",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "しらたき",
            "text": "よろしく！",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "ばなな",
            "text": "楽しみですね",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "つくね",
            "text": "@ばなな 鍋にはやっぱり白菜ですよね！",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "つくね",
            "text": "@しらたき え！？休みだよね？笑",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "つくね",
            "text": "@しらたき 当番です！",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "ばなな",
            "text": "@つくね 白菜は入れますね",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "つくね",
            "text": "@ばなな 白菜は入れます！他に何入れますか？",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "しらたき",
            "text": "タラいれるー",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "ばなな",
            "text": "あとは豆腐とキノコですかね",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "ばなな",
            "text": "@しらたき 鱈もいれるのですね",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "つくね",
            "text": "@ばなな 焼き豆腐外せないです！",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "つくね",
            "text": "ネギも美味しい！",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "ばなな",
            "text": "確かに美味しいですよね",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "しらたき",
            "text": "牡蠣と白滝も！",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "ばなな",
            "text": "焼き豆腐はすき焼きの時に使います",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "つくね",
            "text": "@ばなな すき焼き！！",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "しらたき",
            "text": "すき焼き食べたい",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "つくね",
            "text": "今すごく食べたいです！",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "つくね",
            "text": "@しらたき 今週はすき焼きかな！？",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "ばなな",
            "text": "お二人ともすき焼きすきなのですね？",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "しらたき",
            "text": "好きです",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "ばなな",
            "text": "私もすき焼き食べたくて",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "つくね",
            "text": "@ばなな 好きです！美味しいですよねー！",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "ばなな",
            "text": "ずっと食べてないので",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "つくね",
            "text": "でも生卵食べれないので",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "しらたき",
            "text": "牛肉が高くて手が出しにくい",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "つくね",
            "text": "結構しょっぱいです笑",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "ばなな",
            "text": "牛肉が高くてですよね",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "つくね",
            "text": "@しらたき 豚で十分！笑",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "ばなな",
            "text": "豚肉の所もあるんですよね？",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "つくね",
            "text": "@ばなな 地域柄ですかね？",
            "mention_to": [
                "ばなな"
            ]
        }
