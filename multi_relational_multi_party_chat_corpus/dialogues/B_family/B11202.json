        },
        {
            "utterance_id": 27,
            "interlocutor_id": "ポテト",
            "text": "たまごさん逃げて・・・！",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "たまご",
            "text": "逃げます",
