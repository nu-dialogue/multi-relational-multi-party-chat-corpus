        },
        {
            "utterance_id": 29,
            "interlocutor_id": "ポテト",
            "text": "でも半袖は心配になる",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "たまご",
            "text": "布団かけると暑くて",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "つみれ",
            "text": "毛布でぐるぐる巻にしてやります",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "たまご",
            "text": "こわ",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "ポテト",
            "text": "やばい",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "ポテト",
            "text": "逃げられないやつ",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "たまご",
            "text": "もう少し寒くなったら",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "たまご",
            "text": "厚着します",
