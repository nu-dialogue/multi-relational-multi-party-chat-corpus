        },
        {
            "utterance_id": 3,
            "interlocutor_id": "ポテト",
            "text": "寒さ対策で何かやっていることありますか？",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "ポテト",
            "text": "私は、電子レンジであたためるゆたんぽが気になってます！",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "つみれ",
            "text": "厚着するくらいです",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "つみれ",
            "text": "@ポテト 湯たんぽ！",
            "mention_to": [
                "ポテト"
            ]
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "たまご",
            "text": "あんまり寒さ対策をしてないですー",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "ポテト",
            "text": "@つみれ 厚着で・・・しのげますか・・・？？？？？？",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "たまご",
            "text": "湯たんぽあったかそう",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "ポテト",
            "text": "@たまご 寒いの大丈夫な人ですか？",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "たまご",
            "text": "結構強いです",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "つみれ",
            "text": "@ポテト 昔ほどは寒くないので。",
            "mention_to": [
                "ポテト"
            ]
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "たまご",
            "text": "半袖でまだ寝てます",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "ポテト",
            "text": "いいなぁ・・・",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "ポテト",
            "text": "え",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "ポテト",
            "text": "半袖？",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "つみれ",
            "text": "@たまご やめて",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "ポテト",
            "text": "南半球にお住まいかな？？",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "たまご",
            "text": "やめます",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "ポテト",
            "text": "季節違う？",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "たまご",
            "text": "長袖着ます",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "つみれ",
            "text": "頭の中夏なんでしょうね",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "たまご",
            "text": "毛布があったかいのです",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "ポテト",
            "text": "バレましたね・・・つみれさんに・・・",
