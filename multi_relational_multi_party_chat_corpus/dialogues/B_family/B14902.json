        },
        {
            "utterance_id": 76,
            "interlocutor_id": "ぺんぎん",
            "text": "なるなるね！",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "たまご",
            "text": "後ろから見ると",
