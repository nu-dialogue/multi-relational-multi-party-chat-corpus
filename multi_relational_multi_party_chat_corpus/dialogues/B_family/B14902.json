        },
        {
            "utterance_id": 64,
            "interlocutor_id": "ぺんぎん",
            "text": "飲み物は注意！",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "つみれ",
            "text": "そんなに？",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "たまご",
            "text": "そう",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "ぺんぎん",
            "text": "やばいよね",
            "mention_to": []
