        {
            "utterance_id": 3,
            "interlocutor_id": "ぺんぎん",
            "text": "@たまご 学生さんですか？",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "たまご",
            "text": "学生です",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "たまご",
            "text": "4月から社会人ですが",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "たまご",
            "text": "まだギリ学生です",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "ぺんぎん",
            "text": "そうなんですね！春休みだ",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "たまご",
            "text": "そうです",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "ぺんぎん",
            "text": "この春休みは楽しまないとですね",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "つみれ",
            "text": "春休みって響きいいですよね",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "ぺんぎん",
            "text": "羨ましい",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "たまご",
            "text": "最後の春休みなんで楽しみます",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "つみれ",
            "text": "羨ましい",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "つみれ",
            "text": "人生最後の春休み！",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "ぺんぎん",
            "text": "@たまご そうだね、ここは楽しんだ方がいい！",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "たまご",
            "text": "学生だけですもんね",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "たまご",
            "text": "春休みは",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "ぺんぎん",
            "text": "@たまご 働くと時間ないから",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "たまご",
            "text": "そうですよねー",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "ぺんぎん",
            "text": "@つみれ 大変ですよねー",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "たまご",
            "text": "時間あるうちに何かしないと",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "ぺんぎん",
            "text": "どんなことしたいです？",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "つみれ",
            "text": "@ぺんぎん 本当に大変！",
            "mention_to": [
                "ぺんぎん"
            ]
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "たまご",
            "text": "遊ぶこともですけど",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "ぺんぎん",
            "text": "@つみれ 色々とねー",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "たまご",
            "text": "映画見るとか",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "たまご",
            "text": "どこか行くとかしたいです",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "ぺんぎん",
            "text": "映画はどんなのみる？",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "ぺんぎん",
            "text": "@つみれ 映画みますか？",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "たまご",
            "text": "洋画系見ます",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "つみれ",
            "text": "@ぺんぎん 私はあまり見ないんです",
            "mention_to": [
                "ぺんぎん"
            ]
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "ぺんぎん",
            "text": "今は何やってるのかな？",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "ぺんぎん",
            "text": "@つみれ 私も今は映画館に行かないですね",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "たまご",
            "text": "いまも結構色々やってます",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "ぺんぎん",
            "text": "春休みだから色々はじってるのかもね",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "たまご",
            "text": "映画館で観るのとテレビです観るの結構違いますよね",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "つみれ",
            "text": "@ぺんぎん ゆっくり映画館へってなかなか時間が取れないですよね",
            "mention_to": [
                "ぺんぎん"
            ]
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "たまご",
            "text": "そうだと思います",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "つみれ",
            "text": "私、映画館で見ると酔っちゃって",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "ぺんぎん",
            "text": "@たまご 最近のって匂いとか動いたりするよね",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "たまご",
            "text": "4DXってやつです",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "ぺんぎん",
            "text": "@つみれ アトラクションみたいのだと余計かも音もすごいし",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "たまご",
            "text": "風とか水とか",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "ぺんぎん",
            "text": "@たまご そうそう、一回みた",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "つみれ",
            "text": "@ぺんぎん 厳しいですね〜",
            "mention_to": [
                "ぺんぎん"
            ]
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "たまご",
            "text": "激しすぎてやばいです",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "ぺんぎん",
            "text": "ワイルドスピードかな",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "つみれ",
            "text": "風が吹いたり？",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "ぺんぎん",
            "text": "そうなんです！",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "ぺんぎん",
            "text": "風きたりするんです",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "つみれ",
            "text": "えーー！！",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "つみれ",
            "text": "ディズニーランドみたい",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "ぺんぎん",
            "text": "しかも席が動く",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "たまご",
            "text": "すごい風吹く",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "ぺんぎん",
            "text": "あれ、アトラクションですよ",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "たまご",
            "text": "本当にそうです",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "つみれ",
            "text": "席も動くんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "ぺんぎん",
            "text": "そうなんです！",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "たまご",
            "text": "アクション系観るとすごいです",
