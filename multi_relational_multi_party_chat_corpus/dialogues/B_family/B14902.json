        },
        {
            "utterance_id": 61,
            "interlocutor_id": "ぺんぎん",
            "text": "びっくりしました！",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "たまご",
            "text": "たまに落ちそうになる",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "つみれ",
            "text": "映画館みんなで揺れたりしてるってことですか？",
