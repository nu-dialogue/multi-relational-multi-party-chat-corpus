        },
        {
            "utterance_id": 74,
            "interlocutor_id": "ぺんぎん",
            "text": "笑飛んでいきます！",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "たまご",
            "text": "席で作ってるみたいになる",
