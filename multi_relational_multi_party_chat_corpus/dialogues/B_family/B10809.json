            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "りんご",
            "text": "@つみれ かわいくて",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "つみれ",
            "text": "@りんご 本当にそうなりますね！",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "りんご",
            "text": "@たまご モフモフです",
