        },
        {
            "utterance_id": 85,
            "interlocutor_id": "りんご",
            "text": "@つみれ そうなりますよね、出かけも心配になるし",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "つみれ",
            "text": "そうなんですよ！",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "りんご",
            "text": "お留守番カメラとかつけたいと思っちゃう笑",
