        },
        {
            "utterance_id": 94,
            "interlocutor_id": "りんご",
            "text": "@たまご いやー可愛すぎる",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "つみれ",
            "text": "@りんご ずっと寝てるみたいです",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "たまご",
            "text": "ついてきます",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "りんご",
            "text": "@たまご いかないでーって思ってますよね",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "たまご",
            "text": "思ってるとうれしい",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "りんご",
            "text": "@つみれ でもきっと家族いないからつまらなくて寝てるのかもしれないですよね",
            "mention_to": [
                "つみれ"
            ]
        },
