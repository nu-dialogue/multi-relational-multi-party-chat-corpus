            ]
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "りんご",
            "text": "@つみれ 子供以上にかわいい",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "りんご",
            "text": "@たまご ごめんなさい",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "つみれ",
            "text": "@りんご 柴は愛らしくて可愛いですね",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "たまご",
            "text": "モフモフ撫でたくなる",
