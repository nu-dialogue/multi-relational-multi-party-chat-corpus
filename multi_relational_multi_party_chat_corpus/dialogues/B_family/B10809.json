        },
        {
            "utterance_id": 17,
            "interlocutor_id": "たまご",
            "text": "めちゃなでなでします",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "りんご",
            "text": "@たまご 帰るの楽しみですね",
