            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "こんぶ",
            "text": "おはようございます",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "コアラ",
            "text": "コアラです",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "コアラ",
            "text": "よろしくお願いします",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "ちくわ",
            "text": "ちくわです",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "こんぶ",
            "text": "こんぶといいます",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "ちくわ",
            "text": "よろしくお願いします",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "こんぶ",
            "text": "よろしくお願いします",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "コアラ",
            "text": "今日は雨です",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "こんぶ",
            "text": "今日は雨ですね！",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "コアラ",
            "text": "そちらもですか？",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "ちくわ",
            "text": "めちゃくちゃ降ってます",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "こんぶ",
            "text": "やはり！そちらも",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "こんぶ",
            "text": "はい！！雨と風です",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "ちくわ",
            "text": "風もあるので嫌ですねー",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "コアラ",
            "text": "はい、大きな傘持って娘は登校しました",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "こんぶ",
            "text": "隣の家の木がすごく揺れてます",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "コアラ",
            "text": "@こんぶ 気が揺れるの怖いですね",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "ちくわ",
            "text": "今週ずっと、雨だからいやですねー",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "コアラ",
            "text": "木でした！",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "ちくわ",
            "text": "@コアラ 雨の中の登校はいやですね！",
            "mention_to": [
                "コアラ"
            ]
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "こんぶ",
            "text": "@コアラ すごい高い木で細くてゆらゆらしてます",
            "mention_to": [
                "コアラ"
            ]
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "こんぶ",
            "text": "娘さん頑張って行ったんですね",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "こんぶ",
            "text": "学校は近いですか？",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "コアラ",
            "text": "@ちくわ はい、折りたたみではちょっとですよね",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "ちくわ",
            "text": "折りたたみだときつそうですよね！",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "コアラ",
            "text": "@こんぶ 電車で20分くらいです",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "ちくわ",
            "text": "あ！電車なんだーー！",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "こんぶ",
            "text": "電車通学なんですね！",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "ちくわ",
            "text": "@コアラ 高校生とかですか？",
            "mention_to": [
                "コアラ"
            ]
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "コアラ",
            "text": "はい、そこから歩いて10分です",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "コアラ",
            "text": "@ちくわ はい、高2です！",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "こんぶ",
            "text": "@コアラ またそこから歩くんですね！",
            "mention_to": [
                "コアラ"
            ]
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "ちくわ",
            "text": "そうなんですね！",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "ちくわ",
            "text": "電車通学からの徒歩はきつそう",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "こんぶ",
            "text": "でも、お友達もいれば雨の日だってなんのそのかな",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "こんぶ",
            "text": "私は岩手出身だったので",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "ちくわ",
            "text": "@コアラ 友達いたら楽しいか！",
            "mention_to": [
                "コアラ"
            ]
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "こんぶ",
            "text": "高校時代は雪の日も自転車で",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "コアラ",
            "text": "@こんぶ 自転車！",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "コアラ",
            "text": "何分くらいでしたか？",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "こんぶ",
            "text": "自転車で10分とかですかね？",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "コアラ",
            "text": "@こんぶ 近い！！",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "こんぶ",
            "text": "なので、どうしても歩くより自転車がよくて",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "コアラ",
            "text": "羨ましい",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "ちくわ",
            "text": "雪の時は自転車こわそうー",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "こんぶ",
            "text": "そうなんですよー！",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "コアラ",
            "text": "@ちくわ 確かに滑らないのかしら？",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "こんぶ",
            "text": "あ！電車からのそこからまた自転車でした！",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "こんぶ",
            "text": "転んだことあります！",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "コアラ",
            "text": "@こんぶ 駅に置いてある？",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "ちくわ",
            "text": "@コアラ どの辺の地域なんですか？",
            "mention_to": [
                "コアラ"
            ]
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "こんぶ",
            "text": "@コアラ そう！駅においてあります！",
            "mention_to": [
                "コアラ"
            ]
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "ちくわ",
            "text": "自分は、電車で1時間半で",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "ちくわ",
            "text": "そこから自転車でした",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "コアラ",
            "text": "@ちくわ 住んでいるのは川崎市ですが。娘は都内です",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "こんぶ",
            "text": "電車で1時間は遠い、、、",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "ちくわ",
            "text": "都内なんですね！",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "こんぶ",
            "text": "そうなんですね！",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "ちくわ",
            "text": "通学、通勤で雨の日の満員はやばそう",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "コアラ",
            "text": "@ちくわ １時間半！",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "ちくわ",
            "text": "よく通ったなーて思ってます。",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "こんぶ",
            "text": "満員電車は辛いですよけ",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "ちくわ",
            "text": "僕らは長野なんですけど",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "こんぶ",
            "text": "@ちくわ ほんとよく通ってたよね",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "コアラ",
            "text": "@ちくわ 本当にすごい！",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "ちくわ",
            "text": "出身は大阪で、朝の電車の",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "こんぶ",
            "text": "私たちの娘はどこの高校に行くやら",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "ちくわ",
            "text": "満員はやばかったです",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "こんぶ",
            "text": "大阪の満員はやばそう",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "ちくわ",
            "text": "@こんぶ そやねーこの辺どこも遠いからね！",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "コアラ",
            "text": "@こんぶ 娘さんいらっしゃるんですね！",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "ちくわ",
            "text": "満員やばかったよー",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "こんぶ",
            "text": "そうです！",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "ちくわ",
            "text": "暑いし。",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "こんぶ",
            "text": "一歳の娘が！",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "コアラ",
            "text": "@こんぶ いやー可愛い！",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "こんぶ",
            "text": "暑そう夏の電車クーラーきいてるとはいえね",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "ちくわ",
            "text": "可愛いですよー！",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "こんぶ",
            "text": "雨なので外遊びもできなくて",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "コアラ",
            "text": "@ちくわ たくさん写真撮ってくださいねー",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "ちくわ",
            "text": "@こんぶ 特に冬は暖房やから余計にね",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "ちくわ",
            "text": "@コアラ 撮ってますよー！",
            "mention_to": [
                "コアラ"
            ]
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "こんぶ",
            "text": "写真撮りまくります！",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "ちくわ",
            "text": "でも最近撮れてないか？",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "コアラ",
            "text": "@こんぶ 一歳の子には雨は辛い",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "こんぶ",
            "text": "そうだね！なかなかとれてないか",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "コアラ",
            "text": "外出たいですよね",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "ちくわ",
            "text": "でもちょうど、鼻水でてるんですよ！",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "こんぶ",
            "text": "外でたいですーー",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "コアラ",
            "text": "あっ、お風邪？かな？",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "こんぶ",
            "text": "もう、親も子も退屈です",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "ちくわ",
            "text": "外出れないのはねー",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "こんぶ",
            "text": "そうなんですよ、鼻風邪です",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "コアラ",
            "text": "@こんぶ ですよね！",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "こんぶ",
            "text": "熱はないんですが",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "コアラ",
            "text": "外遊びは公園ですか？",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "こんぶ",
            "text": "公園とか庭とか",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "コアラ",
            "text": "@こんぶ お庭！広そう",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "こんぶ",
            "text": "近くに土手があって三輪車でお散歩です",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "ちくわ",
            "text": "よく歩いてるよね！",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "コアラ",
            "text": "おーいいですね",
            "mention_to": []
        }
    ],
