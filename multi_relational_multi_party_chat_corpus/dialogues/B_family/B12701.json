    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "くじら",
            "text": "おはようございます",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "たまねぎ",
            "text": "おはようございます！",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "にんじん",
            "text": "おはようございます",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "くじら",
            "text": "今週はずっと雨のようですね",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "くじら",
            "text": "今は少し晴れています",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "たまねぎ",
            "text": "@くじら そうみたいですね",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "にんじん",
            "text": "こっちは止みました！",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "くじら",
            "text": "止んでいるのですね",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "くじら",
            "text": "金曜日は雪の予報も",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "にんじん",
            "text": "晴れる気配はありませんが…",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "にんじん",
            "text": "@くじら 雪ー？",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "くじら",
            "text": "また降りそうですね",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "にんじん",
            "text": "そうですね",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "くじら",
            "text": "この間の雪、たいへんではなかったですか？",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "たまねぎ",
            "text": "@くじら 雪かき3回でした！",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "くじら",
            "text": "@たまねぎ たいへんでしたね",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "くじら",
            "text": "かなり積もりましたか",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "たまねぎ",
            "text": "筋肉痛が半端ないです！",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "くじら",
            "text": "それはたいへん",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "にんじん",
            "text": "15センチは積もったんじゃないかと！",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "くじら",
            "text": "そんなに",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "くじら",
            "text": "こちらは6センチほどでした",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "にんじん",
            "text": "やっと溶けました！",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "たまねぎ",
            "text": "@くじら お住まいどの辺ですか？",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "くじら",
            "text": "@たまねぎ 東京です",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "くじら",
            "text": "なのであまり積もりません",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "にんじん",
            "text": "@くじら 東京で6センチはかなりですね！",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "くじら",
            "text": "珍しく積もりました",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "たまねぎ",
            "text": "車とか大丈夫でしたか？",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "くじら",
            "text": "車は動かさなかったので",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "くじら",
            "text": "雨ですぐに溶けましたし",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "にんじん",
            "text": "日陰は氷です！",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "たまねぎ",
            "text": "すぐ溶けるのは羨ましい",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "くじら",
            "text": "凍りますよね",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "にんじん",
            "text": "滑ります",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "くじら",
            "text": "凍って滑るのが怖いですね",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "くじら",
            "text": "歩道橋とか怖くて",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "たまねぎ",
            "text": "@くじら 階段やですね",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "くじら",
            "text": "@たまねぎ どちらにお住まいですか？",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "たまねぎ",
            "text": "長野県です！",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "くじら",
            "text": "長野だとかなり雪が積もりそうですね",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "にんじん",
            "text": "白馬の方ではないのでそんなに降らないんですが",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "くじら",
            "text": "そうなんですね",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "にんじん",
            "text": "今年もふりました！",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "くじら",
            "text": "場所によっても違うんですね",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "たまねぎ",
            "text": "こっちは信号機横ですよ！",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "にんじん",
            "text": "白馬は縦です！",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "くじら",
            "text": "ええ～横ですか",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "にんじん",
            "text": "そうなんです！",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "にんじん",
            "text": "雪が積もって壊れるみたいです！",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "くじら",
            "text": "@にんじん スキーはしますか？",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "にんじん",
            "text": "昔白馬に住んでまして",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "くじら",
            "text": "白馬ならスキーかなと",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "にんじん",
            "text": "やる機会はたくさんあったんですが",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "にんじん",
            "text": "トラウマです",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "にんじん",
            "text": "まだ5歳くらいの時に",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "くじら",
            "text": "トラウマ？",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "くじら",
            "text": "怖い思いを？",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "にんじん",
            "text": "7つ上の姉と一緒に滑ったんですが",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "にんじん",
            "text": "私は捕まってるだけなので",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "にんじん",
            "text": "ただひたすら冷たい風をあびつづけまして",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "くじら",
            "text": "それは怖そうな",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "にんじん",
            "text": "トイレにも行けず",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "にんじん",
            "text": "漏らした記憶があります",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "くじら",
            "text": "それは辛い",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "にんじん",
            "text": "姉は相変わらずスキー大好きです！",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "たまねぎ",
            "text": "それはトラウマだ笑",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "くじら",
            "text": "お姉さんはスキー好きなのですね",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "にんじん",
            "text": "よく行ってます",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "くじら",
            "text": "昔、白馬山に家族で登りました",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "たまねぎ",
            "text": "ほうほう",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "くじら",
            "text": "霧がすごくて",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "くじら",
            "text": "怖かった記憶が",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "にんじん",
            "text": "霧は怖いですよね",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "にんじん",
            "text": "運転もできない",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "くじら",
            "text": "そうなんですよ",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "くじら",
            "text": "全然見えないので",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "にんじん",
            "text": "はい",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "くじら",
            "text": "特に山では怖かったです",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "にんじん",
            "text": "登山もトラウマです笑",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "くじら",
            "text": "そうなんですね",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "にんじん",
            "text": "高山病になり辛かったです",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "くじら",
            "text": "それは辛い",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "にんじん",
            "text": "なので1度きりしか行ってません！",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "くじら",
            "text": "夫も富士山で高山病になったとか",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "たまねぎ",
            "text": "怖いー",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "くじら",
            "text": "友人がおぶってくれたそうです",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "たまねぎ",
            "text": "@くじら 優しい",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "くじら",
            "text": "ほんとうに",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "くじら",
            "text": "おぶって降りてくれるなんて",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "にんじん",
            "text": "じゃあ夫婦で登山行くことはないですね！",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "くじら",
            "text": "ないんです",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "たまねぎ",
            "text": "どっちかが好きだと困りますよね",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "くじら",
            "text": "山も怖いですからね",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "たまねぎ",
            "text": "うちはどっちもジェットコースター乗れないので",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "たまねぎ",
            "text": "富士急行くことないねーって話してました",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "くじら",
            "text": "私もです",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "にんじん",
            "text": "なんであれが楽しめるのか…",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "くじら",
            "text": "私だけ乗らずに待ってました",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "にんじん",
            "text": "わかります笑",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "にんじん",
            "text": "想像しただけで…",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "くじら",
            "text": "一緒に行く人はつまらないかも",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "たまねぎ",
            "text": "まだお化け屋敷の方が…",
            "mention_to": []
        },
        {
            "utterance_id": 103,
            "interlocutor_id": "にんじん",
            "text": "2人で行かなければOK！",
            "mention_to": []
        },
        {
            "utterance_id": 104,
            "interlocutor_id": "くじら",
            "text": "それも駄目ですね",
            "mention_to": []
        },
        {
            "utterance_id": 105,
            "interlocutor_id": "くじら",
            "text": "すぐびびってしまう",
            "mention_to": []
        }
