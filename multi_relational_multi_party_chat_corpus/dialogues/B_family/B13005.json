{
    "dialogue_id": "B13005",
    "dialogue_type": "Family",
    "interlocutors": [
        "にわとり",
        "じゃがいも",
        "くま"
    ],
    "relationship": [
        "じゃがいも",
        "くま"
    ],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "にわとり",
            "text": "こんばんはー",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "じゃがいも",
            "text": "こんばんはですー！",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "くま",
            "text": "こんばんわ",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "にわとり",
            "text": "みなさん、運動とかされてますか？",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "にわとり",
            "text": "私は全然なんです。",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "くま",
            "text": "最近ジムと契約しました",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "じゃがいも",
            "text": "@にわとり 全くしません！",
            "mention_to": [
                "にわとり"
            ]
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "くま",
            "text": "そんなには行ってません",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "じゃがいも",
            "text": "旦那はサボりがちですが行ってますよ",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "にわとり",
            "text": "@くま ジム！",
            "mention_to": [
                "くま"
            ]
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "じゃがいも",
            "text": "そこだけは尊敬に値します",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "にわとり",
            "text": "運動って普段なかなかできないんですよねー。",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "じゃがいも",
            "text": "@にわとり 変な話、体重キープできます？",
            "mention_to": [
                "にわとり"
            ]
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "くま",
            "text": "2週に一回はスノボ行くので、運動はしてますね",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "じゃがいも",
            "text": "私もう厳しいんですけど",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "にわとり",
            "text": "いや、体重キープできてない・・・厳しい・・・",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "くま",
            "text": "冬はおいしいもの多いですからね、、、",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "じゃがいも",
            "text": "マジでキープできないんですよね",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "にわとり",
            "text": "@くま かなりいってますね、スノボ。",
            "mention_to": [
                "くま"
            ]
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "くま",
            "text": "ウォーキングくらいはするといいかもですね",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "にわとり",
            "text": "年を取ると代謝が落ちて、なかなか落ちないんですう～",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "じゃがいも",
            "text": "@にわとり めっちゃ行ってますよ",
            "mention_to": [
                "にわとり"
            ]
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "くま",
            "text": "@にわとり 冬しかできないので、この時期はだいぶ行きますね",
            "mention_to": [
                "にわとり"
            ]
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "じゃがいも",
            "text": "私は亭主元気で留守がいいので",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "じゃがいも",
            "text": "本当に助かっています、ありがたやありがたや",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "にわとり",
            "text": "わはは、なるほど！",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "にわとり",
            "text": "夜中出て、",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "くま",
            "text": "毎回お土産も勝ってますからね",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "くま",
            "text": "買って",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "にわとり",
            "text": "朝スキー場につく感じですか？",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "じゃがいも",
            "text": "@くま その節はどうもありがとうございます。今後も大盤振る舞いお願いします。",
            "mention_to": [
                "くま"
            ]
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "にわとり",
            "text": "@じゃがいも おみやげうれしいですね！",
            "mention_to": [
                "じゃがいも"
            ]
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "くま",
            "text": "@にわとり 朝出て、昼前くらいについてます!",
            "mention_to": [
                "にわとり"
            ]
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "にわとり",
            "text": "なるほど！じゃあ土日はスノボ満喫ですか？",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "じゃがいも",
            "text": "@にわとり 運転して、スノボして、運転してってこの運転全部旦那がやってるんですよ",
            "mention_to": [
                "にわとり"
            ]
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "にわとり",
            "text": "おーーーー！！すばらしい",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "じゃがいも",
            "text": "しかも帰ってきて夜中の3時までゲームしてるんです",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "じゃがいも",
            "text": "イカれてません？",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "にわとり",
            "text": "運転してくれるんならいいじゃないですか！",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "じゃがいも",
            "text": "なんか体力無限すぎて怖いんですよこの人",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "くま",
            "text": "@にわとり 土日どっちか行って、片方は家ですかね",
            "mention_to": [
                "にわとり"
            ]
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "にわとり",
            "text": "えっ、寝る時間は？",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "じゃがいも",
            "text": "@にわとり 3時に寝て10時起きです",
            "mention_to": [
                "にわとり"
            ]
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "くま",
            "text": "朝はたっぷり寝ますね",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "じゃがいも",
            "text": "犬の世話は私がしております",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "にわとり",
            "text": "あっ、7時間は確保！いいですね！",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "にわとり",
            "text": "ワンちゃん、遊んでほしいかもー。",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "くま",
            "text": "@じゃがいも 朝犬に餌あげてる時もあるやん",
            "mention_to": [
                "じゃがいも"
            ]
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "じゃがいも",
            "text": "@くま あっでもこの間の新潟、大変だったよね。",
            "mention_to": [
                "くま"
            ]
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "にわとり",
            "text": "えさやりは大事ですね！",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "くま",
            "text": "@じゃがいも 渋滞はやばかった",
            "mention_to": [
                "じゃがいも"
            ]
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "じゃがいも",
            "text": "@くま ありがとうございやす",
            "mention_to": [
                "くま"
            ]
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "にわとり",
            "text": "あっ地震で？",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "じゃがいも",
            "text": "いやいや、自然渋滞で友達二人が帰れなくてうちに泊まりました",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "じゃがいも",
            "text": "びっくりしません？",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "くま",
            "text": "連休の渋滞に当たったんですよね",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "にわとり",
            "text": "あらら・・・・自然渋滞で？！？！",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "じゃがいも",
            "text": "事故渋滞ではなく自然渋滞",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "くま",
            "text": "交通量多すぎたんでしょうね",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "じゃがいも",
            "text": "しかも三連休の中日",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "にわとり",
            "text": "あっ、なるほど、連休の・・・・！そんなにこんでたんだ～",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "にわとり",
            "text": "うおおおお　それはすごい",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "くま",
            "text": "さすがに疲れましたね",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "じゃがいも",
            "text": "あれはすごかった、流石に私も驚きました",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "じゃがいも",
            "text": "@にわとり しかもそのあと毛布かけないで寝て",
            "mention_to": [
                "にわとり"
            ]
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "じゃがいも",
            "text": "風邪をひきました、彼は",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "にわとり",
            "text": "えーーーーー！！",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "じゃがいも",
            "text": "何してんねんっていうw",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "にわとり",
            "text": "それは寒いでしょうに・・・",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "くま",
            "text": "たまたまお昼食べてなかったので、空腹で瀕死でした",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "くま",
            "text": "家が意外とあったかいんですよね",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "にわとり",
            "text": "あらら、弱り目祟り目・・・・",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "じゃがいも",
            "text": "@にわとり 寝袋は2個合ったんですけど友達に貸して死んでましたね",
            "mention_to": [
                "にわとり"
            ]
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "じゃがいも",
            "text": "夜中は寒いわ",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "にわとり",
            "text": "おおお・・・寝袋もなしで・・・・",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "じゃがいも",
            "text": "@くま まぁ新潟方面はあんまり行かない方がいいね",
            "mention_to": [
                "くま"
            ]
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "くま",
            "text": "@じゃがいも 3個あるから足りなかったわけではない",
            "mention_to": [
                "じゃがいも"
            ]
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "じゃがいも",
            "text": "@くま じゃ何してんのよww",
            "mention_to": [
                "くま"
            ]
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "にわとり",
            "text": "ねぶくろだけだとさむいことありますよね。",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "くま",
            "text": "連休中に行くのはやめようかな",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "じゃがいも",
            "text": "それはそうかも",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "にわとり",
            "text": "インナーシェルフないと。",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "くま",
            "text": "暖房くらいは入れとけば良かったかな",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "じゃがいも",
            "text": "確かになぁ",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "じゃがいも",
            "text": "そしたら友達は暑かったかもね",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "じゃがいも",
            "text": "定かではないですけど",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "にわとり",
            "text": "冷えはよくないですよね！",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "くま",
            "text": "細いから大丈夫でしょ",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "じゃがいも",
            "text": "私はお布団でぬくぬく寝れたので全く問題ありません",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "くま",
            "text": "きっとちょうど良かったよ",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "くま",
            "text": "俺もそっち行けばよかったか",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "にわとり",
            "text": "わはは、お布団最強ですよ！",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "じゃがいも",
            "text": "@にわとり ほんまに結構ガタイいいんですけど、こういう人が風邪引くのが怖い",
            "mention_to": [
                "にわとり"
            ]
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "じゃがいも",
            "text": "@くま いえ、一人で布団を占領できたので結構です",
            "mention_to": [
                "くま"
            ]
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "にわとり",
            "text": "それはありますね、普段元気な人が寝込むと大変。",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "にわとり",
            "text": "しんどそう。",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "くま",
            "text": "@じゃがいも 意外と重症かしたりする",
            "mention_to": [
                "じゃがいも"
            ]
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "にわとり",
            "text": "そうそう。油断大敵。",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "じゃがいも",
            "text": "びびっちゃうんですよね、コロナの時もすごかったですよ",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "くま",
            "text": "気温の変化もすごいから気を付けないとですね",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "じゃがいも",
            "text": "流石の私も看病しましたしね",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "にわとり",
            "text": "えっ、コロナにかかりましたか？",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "じゃがいも",
            "text": "@にわとり かかってました、もう一昨年とかですけど！",
            "mention_to": [
                "にわとり"
            ]
        },
        {
            "utterance_id": 103,
            "interlocutor_id": "くま",
            "text": "@じゃがいも 味覚は異常なくてよかったね",
            "mention_to": [
                "じゃがいも"
            ]
        }
