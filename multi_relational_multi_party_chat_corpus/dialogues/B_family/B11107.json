        },
        {
            "utterance_id": 8,
            "interlocutor_id": "ポテト",
            "text": "へ～！",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "たまねぎ",
            "text": "欲しいものをあげたい",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "ポテト",
            "text": "@たまねぎサプライズ、どんな感じですか？",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "ポテト",
            "text": "急にくれる・・・？",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "たまねぎ",
            "text": "ん！って渡されます！",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "ポテト",
            "text": "笑",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "にんじん",
            "text": "笑",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "にんじん",
            "text": "サプライズだけど、サプライズじゃないやつ",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "たまねぎ",
            "text": "嬉しいです！",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "たまねぎ",
            "text": "センスが同じなのであげてももらっても嬉しいです",
