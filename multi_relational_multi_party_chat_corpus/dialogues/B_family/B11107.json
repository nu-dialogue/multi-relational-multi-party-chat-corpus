        },
        {
            "utterance_id": 18,
            "interlocutor_id": "にんじん",
            "text": "確かに",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "ポテト",
            "text": "好みがわかっているからこそできるやつですね",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "たまねぎ",
            "text": "@ポテト ですね！",
            "mention_to": [
                "ポテト"
            ]
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "にんじん",
            "text": "サプライズの方がやる気出る！",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "にんじん",
            "text": "バレンタインとか貰えるーって思われると",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "ポテト",
            "text": "やる気！笑",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "にんじん",
            "text": "あげたくなくなる！",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "ポテト",
            "text": "@にんじん あ～わかるかも",
            "mention_to": [
                "にんじん"
