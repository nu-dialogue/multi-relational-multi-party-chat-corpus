{
    "dialogue_id": "B15306",
    "dialogue_type": "Family",
    "interlocutors": [
        "カンガルー",
        "たまねぎ",
        "にんじん"
    ],
    "relationship": [
        "たまねぎ",
        "にんじん"
    ],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "カンガルー",
            "text": "おはようございます",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "たまねぎ",
            "text": "おはようございます！",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "にんじん",
            "text": "おはようございます",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "カンガルー",
            "text": "今朝雪が積もってました",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "たまねぎ",
            "text": "@カンガルー そうなんですか？？",
            "mention_to": [
                "カンガルー"
            ]
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "カンガルー",
            "text": "窓開けたら",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "にんじん",
            "text": "@カンガルー 地域はどこですかー？",
            "mention_to": [
                "カンガルー"
            ]
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "カンガルー",
            "text": "東京です",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "たまねぎ",
            "text": "@カンガルー えーー",
            "mention_to": [
                "カンガルー"
            ]
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "にんじん",
            "text": "@カンガルー 東京降ったんですね！",
            "mention_to": [
                "カンガルー"
            ]
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "カンガルー",
            "text": "真っ白で",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "たまねぎ",
            "text": "こちらは長野県ですが",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "たまねぎ",
            "text": "数日前に降っただけで",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "カンガルー",
            "text": "一瞬、北国に来ているかと錯覚しました",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "にんじん",
            "text": "@カンガルー 素敵ですけどね！",
            "mention_to": [
                "カンガルー"
            ]
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "カンガルー",
            "text": "@たまねぎ 長野なんですね",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "たまねぎ",
            "text": "@カンガルー そうです！",
            "mention_to": [
                "カンガルー"
            ]
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "カンガルー",
            "text": "東京は雪が降るとパニックになりますが",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "にんじん",
            "text": "@カンガルー たしかに",
            "mention_to": [
                "カンガルー"
            ]
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "にんじん",
            "text": "スタッドレスじゃないですよね？",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "カンガルー",
            "text": "逆にワクワクもしちゃいます",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "たまねぎ",
            "text": "よくニュースでみる",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "たまねぎ",
            "text": "@カンガルー 笑",
            "mention_to": [
                "カンガルー"
            ]
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "たまねぎ",
            "text": "少しわかるー！",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "カンガルー",
            "text": "スタッドレスですよ",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "にんじん",
            "text": "@カンガルー そーなんですか？",
            "mention_to": [
                "カンガルー"
            ]
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "にんじん",
            "text": "東京の方は買わないイメージ！",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "カンガルー",
            "text": "いや普通の家庭はスタッドレスではないと思いますが",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "たまねぎ",
            "text": "うんうん",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "カンガルー",
            "text": "ウチは主人の仕事関係でこの時期はスタッドレスなんです",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "にんじん",
            "text": "あー",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "にんじん",
            "text": "じゃあ無敵ですね！",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "カンガルー",
            "text": "突然の雪にも対応しなければならなくて",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "たまねぎ",
            "text": "滑り知らず！",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "たまねぎ",
            "text": "ほうほう",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "カンガルー",
            "text": "まさに今日みたいな日",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "にんじん",
            "text": "突然ですね！",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "カンガルー",
            "text": "はい",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "たまねぎ",
            "text": "雪かきするほど？",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "カンガルー",
            "text": "でもスタッドレスも",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "カンガルー",
            "text": "無敵ではないですよね？",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "カンガルー",
            "text": "アイスバーンには対応できない？",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "にんじん",
            "text": "まー確かに",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "にんじん",
            "text": "でも、慣れると無敵！",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "カンガルー",
            "text": "@たまねぎ 雪かきは必要無さそうです！",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "たまねぎ",
            "text": "四駆で走れば！",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "カンガルー",
            "text": "@にんじん 慣れるとかあるんですね！",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "たまねぎ",
            "text": "@カンガルー こっちは降ると何回もかかなきゃだから",
            "mention_to": [
                "カンガルー"
            ]
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "にんじん",
            "text": "慣れますよー",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "にんじん",
            "text": "降りたては怖いけど",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "にんじん",
            "text": "後つっこまれる確率が高くなる",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "たまねぎ",
            "text": "そーそー",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "カンガルー",
            "text": "@たまねぎ 四駆にすればいいのか",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "たまねぎ",
            "text": "@カンガルー ですです",
            "mention_to": [
                "カンガルー"
            ]
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "カンガルー",
            "text": "@にんじん 突っ込まれたら泣きますね",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "にんじん",
            "text": "こっちの人たちギリギリまで変えないから",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "たまねぎ",
            "text": "なんだかんだ降らない年も多いしね！",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "にんじん",
            "text": "１１月に大雪降って",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "にんじん",
            "text": "その時はあせったー",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "たまねぎ",
            "text": "すぐ変えた笑",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "カンガルー",
            "text": "今年は暖冬でしたが12月にはスタッドレスに変えました",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "たまねぎ",
            "text": "のにそこから降らない！",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "にんじん",
            "text": "@カンガルー 早めの対策！",
            "mention_to": [
                "カンガルー"
            ]
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "カンガルー",
            "text": "11月に大雪降ったんですね",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "たまねぎ",
            "text": "@カンガルー これはやばいと変えたのに",
            "mention_to": [
                "カンガルー"
            ]
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "にんじん",
            "text": "@たまねぎ ねー",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "カンガルー",
            "text": "東京は２月に大雪でした",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "にんじん",
            "text": "そこから晴れ続き",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "カンガルー",
            "text": "交通というか流通が麻痺してました",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "たまねぎ",
            "text": "@カンガルー 東京で雪みたことないなー",
            "mention_to": [
                "カンガルー"
            ]
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "にんじん",
            "text": "@カンガルー そうなりますよねー",
            "mention_to": [
                "カンガルー"
            ]
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "カンガルー",
            "text": "@たまねぎ 何年かに一度は大雪になり",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "にんじん",
            "text": "割とびちゃびちゃな雪ですよね",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "たまねぎ",
            "text": "滑るやつ",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "カンガルー",
            "text": "子供達は近所の公園にソリを持って出かけますよ",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "にんじん",
            "text": "@カンガルー おー",
            "mention_to": [
                "カンガルー"
            ]
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "にんじん",
            "text": "やることは一緒だ笑",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "たまねぎ",
            "text": "楽しいのは子供だけー",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "カンガルー",
            "text": "そうなんですね",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "たまねぎ",
            "text": "雪かきで腕ぱんぱん",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "カンガルー",
            "text": "いや、東京の人は大人も楽しむ！かも？",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "にんじん",
            "text": "@カンガルー 笑",
            "mention_to": [
                "カンガルー"
            ]
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "にんじん",
            "text": "そーか",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "カンガルー",
            "text": "２月の大雪の時は",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "カンガルー",
            "text": "家族で夜、デッカい雪だるま作りました",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "たまねぎ",
            "text": "@カンガルー 素敵ー",
            "mention_to": [
                "カンガルー"
            ]
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "カンガルー",
            "text": "凄く楽しかったですよ",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "にんじん",
            "text": "雪合戦も楽しいですよねー",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "にんじん",
            "text": "本気になる！",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "たまねぎ",
            "text": "久しくやってないなー",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "カンガルー",
            "text": "でも翌日から信じられないくらい晴れて暖かくて",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "にんじん",
            "text": "うんうん",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "カンガルー",
            "text": "すぐに雪だるま溶けました",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "にんじん",
            "text": "確かに",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "にんじん",
            "text": "溶けちゃう",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "カンガルー",
            "text": "娘が悲しがってました",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "たまねぎ",
            "text": "あら…",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "カンガルー",
            "text": "雪合戦は絶対ですね！",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "たまねぎ",
            "text": "絶対なんだ笑",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "にんじん",
            "text": "笑",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "カンガルー",
            "text": "娘は学校で雪合戦大会だったようです",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "にんじん",
            "text": "えー",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "にんじん",
            "text": "そんなことまでしてくれるのか",
            "mention_to": []
        }
