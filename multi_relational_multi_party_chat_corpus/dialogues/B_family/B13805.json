        },
        {
            "utterance_id": 77,
            "interlocutor_id": "たまご",
            "text": "カード系マジック見ちゃいます",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "くじら",
            "text": "マジック出来たらかっこいいですね",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "たまご",
            "text": "チラシにポケットみたいなの作る感じです",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "つみれ",
            "text": "@たまご 破っても戻るチラシって作れるんだね",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "くじら",
            "text": "@たまご なるほどそんな仕掛けが",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "たまご",
            "text": "作れました",
