        },
        {
            "utterance_id": 83,
            "interlocutor_id": "くじら",
            "text": "破っても戻るの実際に見てみたいです",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "たまご",
            "text": "今度また作ってみたいです",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "たまご",
            "text": "マジックも趣味かもしれませn",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "たまご",
            "text": "しれません",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "くじら",
            "text": "@つみれお雛様近いですけれど、何か工作しますか？",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "つみれ",
            "text": "@たまご マジックショーを開催する際にはくじらさんをご招待だね",
            "mention_to": [
