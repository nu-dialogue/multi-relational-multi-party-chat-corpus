        },
        {
            "utterance_id": 2,
            "interlocutor_id": "たまご",
            "text": "こんばんは",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "くじら",
            "text": "趣味は何ですか？",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "たまご",
            "text": "映画鑑賞です！",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "つみれ",
            "text": "工作が好きなんです",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "くじら",
            "text": "@たまご 最近、面白かった映画あります？",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "たまご",
            "text": "グランツーリスモという映画が",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "たまご",
            "text": "面白かったです",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "くじら",
            "text": "それは車の話ですか？",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "たまご",
            "text": "そうです",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "たまご",
            "text": "レース系の話です",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "つみれ",
            "text": "@たまご レースとかのお話？",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "つみれ",
            "text": "そうなんだ！",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "くじら",
            "text": "レースの話なんですね",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "たまご",
            "text": "ゲームが上手い人が",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "つみれ",
            "text": "＠どんなところが面白いの？",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "たまご",
            "text": "実際のレースにでる話で",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "たまご",
            "text": "ノンフィクションの話です",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "たまご",
            "text": "レースの緊迫感とリアルさが面白いです",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "つみれ",
            "text": "ゲームと本物のレースは全然違う気がするけど・・・",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "つみれ",
            "text": "しかもノンフィクション？",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "たまご",
            "text": "そう",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "くじら",
            "text": "面白そうですね。",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "たまご",
            "text": "ほんとに行けんのかみたいな話",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "くじら",
            "text": "レースでハラハラドキドキしそうです",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "たまご",
            "text": "面白いです",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "たまご",
            "text": "めちゃドキドキします",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "くじら",
            "text": "昔はテレビでF１は見てましたけれど",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "つみれ",
            "text": "私、画面で酔ってしまいそうです・・・",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "くじら",
            "text": "今はさっぱり",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "たまご",
            "text": "@くじら そうなんですね！",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "たまご",
            "text": "車のレース面白いです",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "くじら",
            "text": "もう相当前です。",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "つみれ",
            "text": "F1",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "くじら",
            "text": "今、F1ってやっているんですよね？",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "つみれ",
            "text": "友人も好きでよく話は聞きました",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "たまご",
            "text": "けど早すぎてえぐいです",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "つみれ",
            "text": "最近はあまり聞かないですね",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "たまご",
            "text": "多分やっていると思います",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "つみれ",
            "text": "やってはいそうですよね",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "くじら",
            "text": "テレビでよくやっていたのに見なくなりました",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "くじら",
            "text": "@つみれ 工作は何を作ってますか？",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "つみれ",
            "text": "@くじら 深夜によくやってましたよね？",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "たまご",
            "text": "テレビでは全然みないです",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "たまご",
            "text": "深夜帯か",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "くじら",
            "text": "@つみれ 深夜にやってましたね",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "たまご",
            "text": "生で見ても面白いかも",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "つみれ",
            "text": "@くじら 工作、画用紙で色々作ったりしてました。",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "くじら",
            "text": "@つみれ 何を作るのが好きです？",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "つみれ",
            "text": "七五三の千歳あめの袋をおしゃれに作ったり",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "たまご",
            "text": "すごい",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "つみれ",
            "text": "アクセサリーとかも作ります",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "くじら",
            "text": "千歳あめの袋！",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "くじら",
            "text": "千歳飴の袋、作れるんですね",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "つみれ",
            "text": "飛び出すメッセージカードどか",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "つみれ",
            "text": "千歳あめの袋作れますよ",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "たまご",
            "text": "すごい工作力",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "くじら",
            "text": "@つみれ メッセージカード良いですね",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "くじら",
            "text": "飛び出すの作ってみたいです",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "たまご",
            "text": "サプライズ感えぐいよ",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "つみれ",
            "text": "お誕生日とか結婚式とかに作って送ってました",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "たまご",
            "text": "たしかに作ってみたいです",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "くじら",
            "text": "手作りのカードもらえたら嬉しい",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "つみれ",
            "text": "@たまご 一緒に作りますか？",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "たまご",
            "text": "今度作る",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "くじら",
            "text": "良いですね親子でぜひ",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "たまご",
            "text": "飛び出す系",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "くじら",
            "text": "親子で一緒に作るの良いなあ",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "つみれ",
            "text": "親子で工作、なかなかないですからね",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "たまご",
            "text": "昔学校で",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "たまご",
            "text": "マジックの道具作った気がします",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "くじら",
            "text": "@たまご どんなの作りました？",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "くじら",
            "text": "マジックすごいですね",
