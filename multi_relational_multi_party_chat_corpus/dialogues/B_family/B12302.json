    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "つくね",
            "text": "こんばんは",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "しらたき",
            "text": "こんばんは",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "すじこ",
            "text": "こんばんは",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "つくね",
            "text": "今日息子の野球の卒団式があったんですが",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "つくね",
            "text": "人の子なのにうるうるきちゃいました！",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "つくね",
            "text": "歳かなと…",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "すじこ",
            "text": "分かります！",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "しらたき",
            "text": "涙腺が",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "しらたき",
            "text": "ボロボロー",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "つくね",
            "text": "@すじこ 卒業シーズンは涙もろくなりますね",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "すじこ",
            "text": "子ども出来てから涙腺が弱くなりました",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "つくね",
            "text": "@すじこ 私もです！",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "つくね",
            "text": "卒園式はだいぶ号泣でした笑",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "しらたき",
            "text": "@つくね 久しぶりに泣いた！",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "すじこ",
            "text": "@つくね ほほえましかったり",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "つくね",
            "text": "@しらたき だよね！笑",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "つくね",
            "text": "@すじこ お子さんおいくつですか？",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "すじこ",
            "text": "@つくね 11と10と今年年長です",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "つくね",
            "text": "@すじこ 同じですね！",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "しらたき",
            "text": "3人！",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "しらたき",
            "text": "習い事とかされてます？",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "つくね",
            "text": "うちも10歳と7歳です！",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "つくね",
            "text": "@すじこ ちなみにしらたきと夫婦です！",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "すじこ",
            "text": "@しらたき 上2人がバスケをしてます",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "しらたき",
            "text": "かっこいいー",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "つくね",
            "text": "@すじこ いいですねー！",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "つくね",
            "text": "私もバスケ部でした笑",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "しらたき",
            "text": "未来の花道ですかね！",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "しらたき",
            "text": "うちは未来の大谷目指してます！",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "すじこ",
            "text": "@しらたき 女の子なんです",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "しらたき",
            "text": "すいません",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "すじこ",
            "text": "@つくね すごい偶然！",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "つくね",
            "text": "@すじこ ですね！",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "つくね",
            "text": "10歳の女の子は",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "しらたき",
            "text": "@つくね バスケは分からないよー",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "すじこ",
            "text": "@しらたき 未来の大谷カッコイイ",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "つくね",
            "text": "だいぶませてますよね笑",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "つくね",
            "text": "@しらたき ダムダムしてシュート！",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "しらたき",
            "text": "それくらい知ってます！",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "すじこ",
            "text": "恋バナとかしますもんね",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "つくね",
            "text": "@すじこ 娘の恋バナとか聞きたいです！",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "つくね",
            "text": "でも、クラスに男子が",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "つくね",
            "text": "6人しかいなくて…",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "つくね",
            "text": "好きとかなさそうなんです…",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "すじこ",
            "text": "父は聞きたくない！笑",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "しらたき",
            "text": "同じくです！",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "つくね",
            "text": "@すじこ 確かに！笑",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "つくね",
            "text": "そのうち彼氏とか…笑",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "すじこ",
            "text": "@つくね それはそれで心配かも",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "つくね",
            "text": "@すじこ 親の心配は尽きませんね笑",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "しらたき",
            "text": "かっこいい彼氏出来るといいなー",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "つくね",
            "text": "@しらたき 確かに！",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "すじこ",
            "text": "@しらたき どういう表情してたら正解なのか分からないですよね？",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "つくね",
            "text": "ちゃんと見定めないとね！",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "すじこ",
            "text": "見定め大事！",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "しらたき",
            "text": "自分人見知り激しいから",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "つくね",
            "text": "@すじこ ですよね！でも家に連れてくるとなると",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "つくね",
            "text": "緊張しますよね笑",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "しらたき",
            "text": "彼氏と上手くはなせるかな",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "すじこ",
            "text": "でもどこまで突っ込んでいいのやら",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "つくね",
            "text": "@しらたき 多分大丈夫でしょ笑",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "つくね",
            "text": "@すじこ 娘さんと仲良い方ですか？",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "しらたき",
            "text": "まだ彼氏とか想像できない",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "すじこ",
            "text": "最近の子は親に会いたがるとか",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "つくね",
            "text": "@しらたき そうだね…",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "つくね",
            "text": "@すじこ そうみたいですね笑",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "つくね",
            "text": "自分の時は合わせたくなかったな…",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "すじこ",
            "text": "@つくね 長女は嫌がりますが次女はハグしてくれます！",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "つくね",
            "text": "@すじこ いいじゃないですかー！",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "しらたき",
            "text": "最近チューもしてくれない",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "つくね",
            "text": "長女さんはやっぱり年頃って感じですね！",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "すじこ",
            "text": "想像したくもないです笑",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "つくね",
            "text": "@しらたき 嫌がられてるよね…",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "しらたき",
            "text": "@すじこ ハグもない！",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "つくね",
            "text": "@しらたき それな！",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "しらたき",
            "text": "切ない、、、",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "しらたき",
            "text": "でもお風呂は",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "つくね",
            "text": "@すじこ 先日のバレンタインはもらいました？",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "すじこ",
            "text": "受け入れなければ",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "しらたき",
            "text": "一緒に入る",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "すじこ",
            "text": "現実を",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "しらたき",
            "text": "@すじこ 大丈夫です",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "しらたき",
            "text": "すでに受け入れてます",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "しらたき",
            "text": "加齢臭とか！笑",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "すじこ",
            "text": "バスケの子の交換で僕には無しでしたね笑",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "つくね",
            "text": "@すじこ そうなんですね…寂しい…",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "つくね",
            "text": "@しらたき うちも無かったよね？笑",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "すじこ",
            "text": "@しらたき お風呂一緒に羨ましい！",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "しらたき",
            "text": "唯一の娘とのスキンシップが",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "しらたき",
            "text": "風呂です！",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "すじこ",
            "text": "加齢臭は家族全員から猛攻を受けてます笑",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "しらたき",
            "text": "裸の付き合いです！",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "つくね",
            "text": "@しらたき でもお風呂の時間めっちゃ早いよね笑",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "すじこ",
            "text": "@しらたき お風呂での会話って結構貴重ですよね！",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "しらたき",
            "text": "最近足の臭いがやばい",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "つくね",
            "text": "@しらたき 水虫疑惑！",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "しらたき",
            "text": "@つくね やめろ！",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "つくね",
            "text": "@しらたき 病院へ！",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "しらたき",
            "text": "水虫じゃない！",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "すじこ",
            "text": "靴履いてる時間長いですか？",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "しらたき",
            "text": "仕事で12時間",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "しらたき",
            "text": "履いてます",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "すじこ",
            "text": "おおう",
            "mention_to": []
        },
        {
            "utterance_id": 103,
            "interlocutor_id": "しらたき",
            "text": "汗だくで",
            "mention_to": []
        },
        {
            "utterance_id": 104,
            "interlocutor_id": "つくね",
            "text": "@すじこ だいぶキツイです！笑",
            "mention_to": [
                "すじこ"
            ]
        }
