    ],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "うさぎ",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "たまねぎ",
            "text": "こんにちは！",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "にんじん",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "うさぎ",
            "text": "春はどこか旅行とかいきますか？",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "にんじん",
            "text": "ユニバーサル行きたいです",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "うさぎ",
            "text": "おととし行きました！",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "たまねぎ",
            "text": "行ったことないんです",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "うさぎ",
            "text": "私もはじめてでした",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "たまねぎ",
            "text": "どうでしたか？",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "うさぎ",
            "text": "すごかったです！",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "うさぎ",
            "text": "乗り物怖いのが多くて",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "たまねぎ",
            "text": "はいはい",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "うさぎ",
            "text": "ジェットコースター大丈夫ですか？",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "たまねぎ",
            "text": "私はダメです",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "うさぎ",
            "text": "私もなんです",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "にんじん",
            "text": "僕は好きですね",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "うさぎ",
            "text": "にんじんさんは平気なんですね！",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "にんじん",
            "text": "楽しいです",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "たまねぎ",
            "text": "理解できないです笑",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "うさぎ",
            "text": "お盆のときだったので、追加で乗れるファストパスみたいなの買いました",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "うさぎ",
            "text": "高かったです",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "たまねぎ",
            "text": "待ち時間は減りますけどね",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "うさぎ",
            "text": "でもジュラシックパークライドとか結局乗れず",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "うさぎ",
            "text": "見たら怖すぎて断念",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "にんじん",
            "text": "はい",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "にんじん",
            "text": "怖いんだ",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "うさぎ",
            "text": "無駄にしました",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "うさぎ",
            "text": "いや、にんじんさんなら大丈夫なのかも？",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "にんじん",
            "text": "僕なんでもいけますね",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "うさぎ",
            "text": "事前にYouTubeとかで雰囲気見るといいかもです",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "たまねぎ",
            "text": "あー確かに",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "うさぎ",
            "text": "ディズニーとかよりは怖いですね",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "たまねぎ",
            "text": "断然ディズニー派です！",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "うさぎ",
            "text": "ミニオンズとかハリポタは楽しめました",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "うさぎ",
            "text": "ディズニー、アナ雪とかできますよね！",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "たまねぎ",
            "text": "一番下がマリオとミニオンハマってまして",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "うさぎ",
            "text": "新エリア",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "うさぎ",
            "text": "お子さんおいくつですか？",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "たまねぎ",
            "text": "1歳半にしてマリオデビューしました！",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "うさぎ",
            "text": "きゃーかわいい！",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "たまねぎ",
            "text": "しかも映画から入ったので",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "うさぎ",
            "text": "マリオも楽しめますね",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "たまねぎ",
            "text": "ヨッシーは知りません",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "うさぎ",
            "text": "うちもSwitchで娘とやってます",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "うさぎ",
            "text": "映画も行きました",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "たまねぎ",
            "text": "ヨッシーはでてこないですよね",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "うさぎ",
            "text": "意外と面白かったですよね",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "うさぎ",
            "text": "ヨッシーたしかに、、、",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "にんじん",
            "text": "大人が面白い",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "うさぎ",
            "text": "映画はお二人で連れて行く感じですか？",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "たまねぎ",
            "text": "私は寝ちゃいます",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "たまねぎ",
            "text": "旦那が連れて行きます！",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "うさぎ",
            "text": "いいパパさんですね！",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "にんじん",
            "text": "いえいえ",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "にんじん",
            "text": "自分が見たいだけです笑",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "たまねぎ",
            "text": "そーそー",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "うさぎ",
            "text": "確かにちょっとお母さんは休みたいですよね",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "うさぎ",
            "text": "その間だけでも",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "たまねぎ",
            "text": "映画館行くと寝ちゃうからもったいなくて",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "うさぎ",
            "text": "映画館近いんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "たまねぎ",
            "text": "映画館は遠いです！",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "たまねぎ",
            "text": "30〜40くらいかな",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "うさぎ",
            "text": "おーお車とかで？",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "にんじん",
            "text": "はい！",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "うさぎ",
            "text": "ちょっとした遠出ですね",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "にんじん",
            "text": "ですね！",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "うさぎ",
            "text": "だいたいショッピングモールとかありますよね？",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "たまねぎ",
            "text": "ありますあります",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "うさぎ",
            "text": "昔、転勤族であちこち住んでいたので",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "たまねぎ",
            "text": "そうなんですね",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "うさぎ",
            "text": "広島、高松、大阪",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "にんじん",
            "text": "すごい",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "うさぎ",
            "text": "どこもそんな感じでした",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "たまねぎ",
            "text": "慣れるまで旅行ですね",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "うさぎ",
            "text": "映画終わるとご飯食べてショッピングして帰るという",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "たまねぎ",
            "text": "そーですね",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "うさぎ",
            "text": "期間限定だったし、若かったので笑",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "うさぎ",
            "text": "ここ10年はもう地元です",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "たまねぎ",
            "text": "もうないですか？",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "うさぎ",
            "text": "あっても単身赴任してもらいます",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "にんじん",
            "text": "確かに",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "うさぎ",
            "text": "転勤とかはあまりない感じですか？",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "たまねぎ",
            "text": "ないですね",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "うさぎ",
            "text": "いいですね！",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "たまねぎ",
            "text": "あっても1日とか",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "うさぎ",
            "text": "1日？",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "うさぎ",
            "text": "出張？",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "うさぎ",
            "text": "ではなく？",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "にんじん",
            "text": "すこーし遠くに行く感じです",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "にんじん",
            "text": "出張ですね",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "うさぎ",
            "text": "あっ、そういうことですね",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "うさぎ",
            "text": "ライフプランがありますものね",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "たまねぎ",
            "text": "でも憧れます",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "うさぎ",
            "text": "いやーもう引越し慣れも困りますよ",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "たまねぎ",
            "text": "いろんな先で美味しいもの食べるの",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "うさぎ",
            "text": "それは確かに！",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "たまねぎ",
            "text": "物も増えなさそう",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "うさぎ",
            "text": "私、仙台にも最初いて、広島にもいて牡蠣の産地でした",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "たまねぎ",
            "text": "牡蠣いいなぁ",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "たまねぎ",
            "text": "大好きです",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "うさぎ",
            "text": "でも主人が牡蠣だめで",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "にんじん",
            "text": "僕もです",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "うさぎ",
            "text": "友達と食べに行きました",
            "mention_to": []
        },
        {
            "utterance_id": 103,
            "interlocutor_id": "にんじん",
            "text": "煮ても焼いても",
            "mention_to": []
        }
