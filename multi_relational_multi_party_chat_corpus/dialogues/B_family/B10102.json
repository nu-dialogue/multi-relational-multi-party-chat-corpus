    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "うさぎ",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "にんじん",
            "text": "こんにちは！",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "たまねぎ",
            "text": "こんにちは！",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "うさぎ",
            "text": "今日バレンタインですね",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "にんじん",
            "text": "あ！そうですね！",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "うさぎ",
            "text": "うちは娘が友チョコならぬクッキー焼いて大変でした",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "うさぎ",
            "text": "台所がめちゃくちゃ",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "たまねぎ",
            "text": "娘が作る＝母ですよね",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "うさぎ",
            "text": "いやもう手を出しませんでした",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "にんじん",
            "text": "僕は食べる専門ですね",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "うさぎ",
            "text": "にんじんさん甘党ですか？",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "にんじん",
            "text": "はい！お酒飲んだら食べられませんが",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "うさぎ",
            "text": "主人も甘いのも辛いのも大好きです",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "にんじん",
            "text": "辛いのも好きです！",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "うさぎ",
            "text": "もしかして激辛もいけますか？",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "にんじん",
            "text": "いけます！",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "うさぎ",
            "text": "うちもタバスコとかかけちゃう人です",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "にんじん",
            "text": "次の日やばいですが！",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "うさぎ",
            "text": "次の日、困りますよね",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "たまねぎ",
            "text": "私は出産して弱くなりました！",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "うさぎ",
            "text": "たまねぎさんは辛いのは？",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "たまねぎ",
            "text": "前まで好きで2人でよく食べたのに",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "うさぎ",
            "text": "あっ、体質変わりますよね",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "うさぎ",
            "text": "お子さんはおいくつですか？",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "にんじん",
            "text": "残り物は食べてます",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "たまねぎ",
            "text": "一番下が1歳半です！",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "うさぎ",
            "text": "うわー可愛い！",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "うさぎ",
            "text": "えっとよちよち歩き？",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "たまねぎ",
            "text": "可愛くてしょうがないです",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "たまねぎ",
            "text": "今はしっかり歩けてますよー",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "うさぎ",
            "text": "1番下ということは何人もいらっしゃる？",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "たまねぎ",
            "text": "5人です",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "うさぎ",
            "text": "えええええー",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "うさぎ",
            "text": "ごめんなさい、驚きました",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "にんじん",
            "text": "大家族です笑",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "うさぎ",
            "text": "私の周りでは友達1人だけいました",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "たまねぎ",
            "text": "大家族？",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "うさぎ",
            "text": "ちょうど真ん中で、兄弟姉妹全員いますって",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "たまねぎ",
            "text": "おー",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "にんじん",
            "text": "5人だー",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "うさぎ",
            "text": "結婚式がすごかったです",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "たまねぎ",
            "text": "あ、確かに",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "にんじん",
            "text": "田舎は多いかもしれません",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "うさぎ",
            "text": "上のご兄弟のご家族合わせたらすごかった",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "たまねぎ",
            "text": "お正月とかすごそうですね！",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "うさぎ",
            "text": "だから自然の中で大家族、いいですねー",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "うさぎ",
            "text": "もしかして移住とか？ではなくですか？",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "たまねぎ",
            "text": "ずっと地元です！",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "うさぎ",
            "text": "地元の方たちも大家族多いですか？",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "たまねぎ",
            "text": "でも、近くにコンビニもあるし都会ですよ！笑",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "うさぎ",
            "text": "コンビニ必要ですよね",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "うさぎ",
            "text": "関東ですか？",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "たまねぎ",
            "text": "そうです！",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "うさぎ",
            "text": "うちは主人が栃木に住んでたことがあるんです",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "たまねぎ",
            "text": "餃子の街ですね！",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "うさぎ",
            "text": "宇都宮餃子です",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "うさぎ",
            "text": "そうそう",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "たまねぎ",
            "text": "おいしいですよね！",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "にんじん",
            "text": "あと猿！",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "うさぎ",
            "text": "たまねぎさん、食事の支度大変じゃないですか？",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "たまねぎ",
            "text": "食事は毎日が合宿です！",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "うさぎ",
            "text": "にんじんさんも協力的な気がしますが",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "にんじん",
            "text": "家事は教えこまれました！",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "うさぎ",
            "text": "大鍋？",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "たまねぎ",
            "text": "鍋は２つ！",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "うさぎ",
            "text": "二つ！",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "うさぎ",
            "text": "すごい",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "たまねぎ",
            "text": "カレーも鍋は２つ！",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "うさぎ",
            "text": "クラクラしますね",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "にんじん",
            "text": "たまにお隣さんも来るから、みっつ！",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "うさぎ",
            "text": "1番上のお子さんはおいくつなんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "たまねぎ",
            "text": "18です！",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "うさぎ",
            "text": "おー大人ですね",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "うさぎ",
            "text": "娘と近いです",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "うさぎ",
            "text": "成人式もそろそろ？",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "たまねぎ",
            "text": "就職してくれたので楽ちんです",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "うさぎ",
            "text": "うわーそれは親孝行",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "たまねぎ",
            "text": "ですよねー",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "うさぎ",
            "text": "でも面倒みてくれそうですね",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "うさぎ",
            "text": "下の兄弟",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "たまねぎ",
            "text": "2人目がみてくれます！",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "たまねぎ",
            "text": "やはり女の子だなーって",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "うさぎ",
            "text": "高校生とかですか？",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "たまねぎ",
            "text": "二番目は中３です！",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "うさぎ",
            "text": "高校受験？",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "たまねぎ",
            "text": "はい！",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "にんじん",
            "text": "この前前期試験でした！",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "うさぎ",
            "text": "そろそろですか？",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "うさぎ",
            "text": "学生大変ですよね",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "たまねぎ",
            "text": "もう合否はわかりました！",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "うさぎ",
            "text": "あっ、もう？",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "たまねぎ",
            "text": "前期なので！",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "うさぎ",
            "text": "あーそうなんですね",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "たまねぎ",
            "text": "お弁当が心配です",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "うさぎ",
            "text": "じゃああとは学生生活を楽しむ感じですか？",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "たまねぎ",
            "text": "そうですね！",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "うさぎ",
            "text": "給食だったんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "たまねぎ",
            "text": "そうですよー！",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "たまねぎ",
            "text": "お弁当だったんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "うさぎ",
            "text": "はい、ただ中にコンビニもあってので",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "にんじん",
            "text": "え！",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "うさぎ",
            "text": "あっ、うちは私立なんです",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "うさぎ",
            "text": "中高一貫",
            "mention_to": []
