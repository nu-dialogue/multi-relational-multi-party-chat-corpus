        {
            "utterance_id": 95,
            "interlocutor_id": "ちくわ",
            "text": "おおいんだーー！",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "にわとり",
            "text": "あー、稲葉さんの実家はね、",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "こんぶ",
            "text": "@ちくわ 知りたすぎて笑",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "ちくわ",
            "text": "え。",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "にわとり",
            "text": "津山ってところで、北のほうなんですが、",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "こんぶ",
            "text": "え！！",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "ちくわ",
            "text": "知ってるんだ笑",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "こんぶ",
            "text": "知ってるんですね！！！！！さすが",
            "mention_to": []
        },
        {
            "utterance_id": 103,
            "interlocutor_id": "にわとり",
            "text": "本人から「実家には迷惑かけないでね」ってお知らせされてるらしいですよ。",
            "mention_to": []
        },
        {
            "utterance_id": 104,
            "interlocutor_id": "ちくわ",
            "text": "まぁ地元の人とかだとわかるんすね！",
            "mention_to": []
        },
        {
            "utterance_id": 105,
            "interlocutor_id": "こんぶ",
            "text": "あーーそりゃそうですよね〜",
            "mention_to": []
        },
        {
            "utterance_id": 106,
            "interlocutor_id": "こんぶ",
            "text": "地元民も誇らしいでしょうけども",
            "mention_to": []
        },
        {
            "utterance_id": 107,
            "interlocutor_id": "にわとり",
            "text": "でも、駅前にビーズのでっかい看板がたってます！",
