    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "うさぎ",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "てばさき",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "えのき",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "えのき",
            "text": "もうすぐバレンタインですね！",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "うさぎ",
            "text": "明日バレンタインですね",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "うさぎ",
            "text": "あっかぶりましたね！",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "えのき",
            "text": "チョコレート買いましたか？",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "うさぎ",
            "text": "買いました！",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "うさぎ",
            "text": "えのきさんも？",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "てばさき",
            "text": "私はまだですね",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "えのき",
            "text": "私も池袋西武で買ってきちゃいました！",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "うさぎ",
            "text": "おーっ、どんなのですか？",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "てばさき",
            "text": "そういえば言ってましたね、池袋",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "えのき",
            "text": "チョコレートフェスティバルやってたので",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "うさぎ",
            "text": "あー楽しい！",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "えのき",
            "text": "お高いラインナップが多くてほぼ買えなかったですが",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "うさぎ",
            "text": "わかります、チョコ高いですよね",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "えのき",
            "text": "100個入りのお手頃なのを買いました",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "てばさき",
            "text": "お土産という名のおこぼれもらいました",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "うさぎ",
            "text": "えっ？！そんなに！",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "うさぎ",
            "text": "お得",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "えのき",
            "text": "職場にも持っていきます",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "うさぎ",
            "text": "喜ばれますね",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "えのき",
            "text": "あと家族にも分けます",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "てばさき",
            "text": "美味しかったです",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "うさぎ",
            "text": "てばさきさんもご主人様も喜びますね！",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "えのき",
            "text": "ブルガリとかとか高すぎて、ほぼブランド代？って感じでしたよ",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "うさぎ",
            "text": "うちは娘が昨日手作り大変でした",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "てばさき",
            "text": "手作りですか！？",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "えのき",
            "text": "手作りすごい！",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "うさぎ",
            "text": "友チョコならぬ友クッキー",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "うさぎ",
            "text": "チョコ失敗しました笑",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "えのき",
            "text": "残念",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "てばさき",
            "text": "チョコクッキーとかにアレンジした感じですか？",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "うさぎ",
            "text": "はい、トリュフ作ったらなんかヘンテコで",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "えのき",
            "text": "トリュフむずかしそう・・・",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "うさぎ",
            "text": "親のお腹に入ってます",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "えのき",
            "text": "味は大丈夫でしたか？",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "うさぎ",
            "text": "味は美味しかったのですが、見た目が、、",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "えのき",
            "text": "残念なことに…",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "うさぎ",
            "text": "てばさきさんは大学では配ったりはなく？",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "てばさき",
            "text": "それでも三月にお返しを請求されそうですね",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "えのき",
            "text": "大学はもう休みです",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "えのき",
            "text": "ね",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "てばさき",
            "text": "私はもう大学の授業終わったので",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "うさぎ",
            "text": "あっ、そうなんですねー",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "うさぎ",
            "text": "大学休み長かったですねー",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "えのき",
            "text": "2か月も春休みとか長すぎですよね",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "うさぎ",
            "text": "確かにバイトとかはされますか？",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "てばさき",
            "text": "勉強があるので、バイトはしてないです",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "うさぎ",
            "text": "えらい！！！",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "うさぎ",
            "text": "レポートとかですか？",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "てばさき",
            "text": "簿記二級受けるんです",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "えのき",
            "text": "今度こそ受かってほしい・・・",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "うさぎ",
            "text": "うわーすごい！",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "てばさき",
            "text": "もう本番を模試にはしたくない",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "えのき",
            "text": "本試験を模試代わりにしないで欲しい・・・",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "うさぎ",
            "text": "娘の英検準一級と同じですね",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "うさぎ",
            "text": "あと数点",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "てばさき",
            "text": "数点とか可愛いレベルではないですね、あと数十点",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "えのき",
            "text": "親が出す受験料のことも考えてあげてください",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "てばさき",
            "text": "すいませんでした",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "うさぎ",
            "text": "受かる気がしないって言ってます",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "うさぎ",
            "text": "えのきさん、お気持ちわかります",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "てばさき",
            "text": "受かる気だけしてます！",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "うさぎ",
            "text": "てばさきさん、前向きでいいですね！",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "えのき",
            "text": "長女は絶対受かる確信があるまで受けないタイプなのに・・・",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "うさぎ",
            "text": "簿記検定料お高いんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "てばさき",
            "text": "いくらでしたっけ…",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "てばさき",
            "text": "五千円？",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "えのき",
            "text": "2級4,720円だって",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "うさぎ",
            "text": "英検1万近くですよー",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "えのき",
            "text": "高い！",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "うさぎ",
            "text": "もう2回受けててあと2回申し込んでます",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "えのき",
            "text": "大富豪じゃないですか！",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "うさぎ",
            "text": "もうほんとやめてほしい",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "てばさき",
            "text": "もう2回申し込んでるんですか！？",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "えのき",
            "text": "笑うしかないですよね・・・",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "うさぎ",
            "text": "はい、争奪戦です",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "えのき",
            "text": "大変なんですね",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "うさぎ",
            "text": "5月から問題変わるから",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "うさぎ",
            "text": "値上げもするし怒",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "えのき",
            "text": "最近なんでも値上げですよね",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "うさぎ",
            "text": "ほんとですよね！",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "てばさき",
            "text": "値上げ大変ですよね",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "えのき",
            "text": "家計が大変です",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "うさぎ",
            "text": "お菓子とか少ないですよねー",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "てばさき",
            "text": "お気に入りのジャーキーが五百円近くになって驚きました",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "うさぎ",
            "text": "袋軽くないですか？",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "えのき",
            "text": "だんだん少なくなっていくんですよね",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "うさぎ",
            "text": "わかります！",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "えのき",
            "text": "カントリーマアムとか・・・",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "うさぎ",
            "text": "それそれ！",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "うさぎ",
            "text": "好きなのに",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "えのき",
            "text": "2040年問題",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "てばさき",
            "text": "どこかのクッキーはいつかマーブルサイズになると予想",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "うさぎ",
            "text": "マーブル？",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "うさぎ",
            "text": "ダイエットにはいいけど？？",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "えのき",
            "text": "小さいチョコ？",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "えのき",
            "text": "そんなサイズに…",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "てばさき",
            "text": "それです",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "うさぎ",
            "text": "舐めて終わりとか？辛い",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "えのき",
            "text": "つらい",
            "mention_to": []
        }
