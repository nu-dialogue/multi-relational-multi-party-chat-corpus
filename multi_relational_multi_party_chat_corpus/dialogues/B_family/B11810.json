{
    "dialogue_id": "B11810",
    "dialogue_type": "Family",
    "interlocutors": [
        "ばなな",
        "ちくわ",
        "こんぶ"
    ],
    "relationship": [
        "ちくわ",
        "こんぶ"
    ],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "ばなな",
            "text": "こんばんは",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "ちくわ",
            "text": "こんばんは！",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "こんぶ",
            "text": "こんばんは。",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "ばなな",
            "text": "今、肩がパンパンです",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "こんぶ",
            "text": "あら。",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "こんぶ",
            "text": "それはなぜですか？",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "ちくわ",
            "text": "え？なんかあったんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "ばなな",
            "text": "肩こりがひどくて",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "ちくわ",
            "text": "あー仕事疲れとかです？",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "こんぶ",
            "text": "肩こりなんですね。",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "ちくわ",
            "text": "仕事がデスクワークとか？",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "ばなな",
            "text": "同じ姿勢でいるからですかね？",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "ちくわ",
            "text": "肩こりって結構しぶといですよね！",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "こんぶ",
            "text": "マッサージとかいきますか？",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "ばなな",
            "text": "@ちくわ デスクワークです",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "ちくわ",
            "text": "姿勢は結構大事ですよ！",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "こんぶ",
            "text": "デスクワークすごいです！",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "ちくわ",
            "text": "@ばなな デスクワークだと長時間座ってたりするとなりますよね！",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "ばなな",
            "text": "@こんぶ マッサージはあまり行ってないです",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "こんぶ",
            "text": "私は動いてないとなんだか\n落ち着かないです",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "ばなな",
            "text": "そうなのですね",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "ちくわ",
            "text": "僕も動いときたいです。",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "こんぶ",
            "text": "@ばなな あったかい湯船に浸かるのはどうでしょう",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "ちくわ",
            "text": "でも、久しぶりにマッサージいきたいなー",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "ばなな",
            "text": "そうですね。",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "こんぶ",
            "text": "夫婦そろって落ち着きのない",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "こんぶ",
            "text": "私もいきたい！",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "ばなな",
            "text": "温泉でも入りたいです",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "ちくわ",
            "text": "@ばなな お灸とかいいですよ！",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "こんぶ",
            "text": "でも、近くにない気がする",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "こんぶ",
            "text": "温泉！",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "ばなな",
            "text": "お灸ですか",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "こんぶ",
            "text": "いいですね！私も入りたいです！",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "ばなな",
            "text": "したことないんです",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "ちくわ",
            "text": "@こんぶ いきたいねーー！",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "こんぶ",
            "text": "私もしたことない！",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "こんぶ",
            "text": "ね！行きたい！",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "ちくわ",
            "text": "膝を痛めたりしていた時に",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "こんぶ",
            "text": "お灸は少しドキドキします",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "ちくわ",
            "text": "市販のお灸を買って治療していました！",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "ばなな",
            "text": "自分でするのですね",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "こんぶ",
            "text": "市販のがあるんだ！",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "ちくわ",
            "text": "シールタイプなので、誰でも着けやすいですよ！",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "こんぶ",
            "text": "誰でもできやすいのならいいね！",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "ちくわ",
            "text": "ライターで火をつけて何分間行うやつです！",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "ばなな",
            "text": "シールがあるんですね",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "こんぶ",
            "text": "効果はあるの？",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "ちくわ",
            "text": "ドラッグストアに売っていますよ！",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "ちくわ",
            "text": "効果はあった！",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "こんぶ",
            "text": "そうなんだ！",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "ちくわ",
            "text": "身体を温めることがいいみたいですよ！",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "ばなな",
            "text": "火をつけるのは何か怖い感じです",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "こんぶ",
            "text": "自分でできて効果があるなら！",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "こんぶ",
            "text": "それはいいね！",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "こんぶ",
            "text": "@ばなな わかります",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "こんぶ",
            "text": "なんか、ドキドキしそうです",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "ちくわ",
            "text": "@ばなな 中学生の時に母親につけてもらってたんですが、",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "ちくわ",
            "text": "すね毛燃やされました！",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "ばなな",
            "text": "それはたいへん",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "こんぶ",
            "text": "面白すぎる",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "ばなな",
            "text": "痛くなかったですか？",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "ちくわ",
            "text": "母親は笑っていました",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "ばなな",
            "text": "笑っている場合では",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "ちくわ",
            "text": "@ばなな あつっっ！！てなりました！",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "こんぶ",
            "text": "火傷した？",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "ちくわ",
            "text": "火傷してたかも？少し！",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "ちくわ",
            "text": "でもライターで付けるから",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "ばなな",
            "text": "@ちくわ たいへんでしたね",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "こんぶ",
            "text": "大惨事にならなくてよかった",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "ちくわ",
            "text": "ちょっと炙っちゃうんですよ！",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "こんぶ",
            "text": "なるほど！そうなんだ",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "ちくわ",
            "text": "すね毛を！",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "ちくわ",
            "text": "でも今は、脱毛しているので",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "ばなな",
            "text": "そうだったのですね",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "ちくわ",
            "text": "大丈夫です！",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "こんぶ",
            "text": "そうなんですよ！",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "ばなな",
            "text": "それは良かったです",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "ちくわ",
            "text": "母親に付けられたとしても、、",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "こんぶ",
            "text": "旦那は足を脱毛してて",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "こんぶ",
            "text": "美意識に目覚めてます！",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "ばなな",
            "text": "素晴らしい",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "こんぶ",
            "text": "私の方が焦ります",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "ちくわ",
            "text": "今の男子は美意識ですよ！",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "ばなな",
            "text": "家の夫は全然",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "こんぶ",
            "text": "意識低くて",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "こんぶ",
            "text": "それぐらいがいいですよ！",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "ばなな",
            "text": "@ちくわ 化粧水も使っているのですか",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "こんぶ",
            "text": "旦那がやられると、私も美意識高めないとって思ってます",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "ちくわ",
            "text": "たまたま家の近くに脱毛のお店があったんで！",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "ちくわ",
            "text": "@ばなな はい！！",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "こんぶ",
            "text": "使ってるよね！",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "ちくわ",
            "text": "無印の！",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "ちくわ",
            "text": "時々、バックもしてます！",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "こんぶ",
            "text": "そうそう！無印の！",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "ばなな",
            "text": "無印のは良いですか？",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "こんぶ",
            "text": "大容量ボトル！",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "ちくわ",
            "text": "でも娘が産まれてからあんまりつけてないです！",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "ばなな",
            "text": "パックも",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "ちくわ",
            "text": "結構いいみたいですよ！",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "ちくわ",
            "text": "パックもです！！！",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "ばなな",
            "text": "段々、美意識低下しています",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "ばなな",
            "text": "駄目ですね",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "こんぶ",
            "text": "わかります",
            "mention_to": []
        },
        {
            "utterance_id": 103,
            "interlocutor_id": "ちくわ",
            "text": "美意識て考えると",
            "mention_to": []
        },
        {
            "utterance_id": 104,
            "interlocutor_id": "ちくわ",
            "text": "次は、、、",
            "mention_to": []
        },
        {
            "utterance_id": 105,
            "interlocutor_id": "こんぶ",
            "text": "もう、どうでも良く思えたり",
            "mention_to": []
        },
        {
            "utterance_id": 106,
            "interlocutor_id": "こんぶ",
            "text": "家ではすっぴんでいることが多いです",
            "mention_to": []
        },
        {
            "utterance_id": 107,
            "interlocutor_id": "ちくわ",
            "text": "鼻毛ワックスで一気に抜こうかと！",
            "mention_to": []
        }
    ],
