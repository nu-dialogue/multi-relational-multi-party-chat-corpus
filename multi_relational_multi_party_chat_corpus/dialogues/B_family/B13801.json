    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "くじら",
            "text": "こんばんは",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "たまご",
            "text": "こんばんは",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "つみれ",
            "text": "こんばんは",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "くじら",
            "text": "今日は天気が良かったけど",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "くじら",
            "text": "風が冷たくて",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "たまご",
            "text": "はい風が強かったです",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "つみれ",
            "text": "ほんと、風が冷たくて寒かったです",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "たまご",
            "text": "寒かったです",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "つみれ",
            "text": "ここのところ寒いですね",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "くじら",
            "text": "寒かったですねー",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "くじら",
            "text": "@つみれ 寒い日続いていますね",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "たまご",
            "text": "厚着しないと大変です",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "つみれ",
            "text": "ほんとに！",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "くじら",
            "text": "今日、靴下に貼るカイロを買おうか悩んで",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "くじら",
            "text": "買ったことあります？",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "たまご",
            "text": "ありますねー",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "たまご",
            "text": "買ったことないです",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "つみれ",
            "text": "買ったことあります！",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "つみれ",
            "text": "結構良いですよ！",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "くじら",
            "text": "@つみれ 良いんですね",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "くじら",
            "text": "今日は結局買わなかったです",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "つみれ",
            "text": "でも、長い時間はもたない感じかな",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "たまご",
            "text": "靴があったかいのいいなー",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "つみれ",
            "text": "@くじら 今度試してみてください",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "くじら",
            "text": "長時間は効かないんですね",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "たまご",
            "text": "僕もつぁってみたい",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "くじら",
            "text": "今度試してみます",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "くじら",
            "text": "足元から冷えますね",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "たまご",
            "text": "寝るときも足寒いです",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "くじら",
            "text": "@たまご 確かに",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "つみれ",
            "text": "足元寒いとほんと寒く感じますね",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "くじら",
            "text": "寝る時は何もしてなくて",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "くじら",
            "text": "以前は湯たんぽしてました",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "くじら",
            "text": "でも、蹴っ飛ばしてしまうので",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "つみれ",
            "text": "湯たんぽ良いですね",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "たまご",
            "text": "湯たんぽ何回か使ったことあります",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "つみれ",
            "text": "@くじら 足元からなくなってるパターンですね",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "くじら",
            "text": "そうなんですー",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "くじら",
            "text": "いつの間にかない・・・",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "つみれ",
            "text": "@たまご 私が用意したやつね",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "つみれ",
            "text": "@くじら そうですね、いつの間にかいなくなってる",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "たまご",
            "text": "そうー",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "くじら",
            "text": "靴下履いて寝るのも好きではなくて",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "たまご",
            "text": "落ちてますね",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "くじら",
            "text": "家でも靴下すぐ脱いでしまいます",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "つみれ",
            "text": "@くじら 最初気持ち悪かったんですけど",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "くじら",
            "text": "@つみれ なれます？",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "つみれ",
            "text": "慣れたら履かないと寒くて",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "たまご",
            "text": "逆にね",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "つみれ",
            "text": "逆に",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "くじら",
            "text": "なるほど慣れれば大丈夫かな",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "たまご",
            "text": "僕は脱がないとダメ派です",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "くじら",
            "text": "@たまご 私も素足が好きで",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "たまご",
            "text": "家帰った瞬間脱ぎます",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "つみれ",
            "text": "寝る時靴下履くのあまり良くないって聞いたこともあります",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "たまご",
            "text": "えー",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "たまご",
            "text": "そうなのか",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "くじら",
            "text": "@つみれ そうなんです？",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "たまご",
            "text": "色々説ありますね",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "くじら",
            "text": "蒸れるのかな？",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "つみれ",
            "text": "うる覚えなので定かではないです笑",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "くじら",
            "text": "早く暖かくなってほしいです",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "くじら",
            "text": "ほどよい暖かさに",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "つみれ",
            "text": "足の裏で体温調節するからだったかな。。。",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "つみれ",
            "text": "そうですね、早く暖かくなって欲しい",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "くじら",
            "text": "@つみれ なるほど、それはありそうです",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "つみれ",
            "text": "定かではないです笑",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "くじら",
            "text": "春になったらお出かけしたい所あります？",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "たまご",
            "text": "桜見とか！",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "たまご",
            "text": "花見でした",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "くじら",
            "text": "@たまご 桜は見たいです！",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "つみれ",
            "text": "花見は",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "つみれ",
            "text": "春と言ったら！って感じで良いですね",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "くじら",
            "text": "そうですよね",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "たまご",
            "text": "夜桜も綺麗で",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "たまご",
            "text": "程よい暖かさが好きです",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "くじら",
            "text": "@たまご 夜桜、行きました",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "たまご",
            "text": "@くじら いいですねー",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "くじら",
            "text": "ライトアップされると、また綺麗で",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "つみれ",
            "text": "夜の桜綺麗ですね",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "くじら",
            "text": "幻想的ですよね",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "たまご",
            "text": "そうです",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "つみれ",
            "text": "ほんと短い期間だけですからね",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "たまご",
            "text": "綺麗です",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "くじら",
            "text": "短いですよね。",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "つみれ",
            "text": "儚い感じもいいですよね",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "くじら",
            "text": "最近は卒業式の時に咲いていますね",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "くじら",
            "text": "入学式にはもう散ってしまって",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "つみれ",
            "text": "早く咲くこと多くなりましたね",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "たまご",
            "text": "一瞬で終わりますよね",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "つみれ",
            "text": "入学式と桜って感じだったのに",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "くじら",
            "text": "だんだん咲くの早くなりました",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "つみれ",
            "text": "暖冬の影響ですかね？",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "くじら",
            "text": "@つみれ きっとそうですね",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "たまご",
            "text": "もっと長く咲いて欲しいです",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "つみれ",
            "text": "ここ最近の寒さは暖冬には程遠い気がしますが",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "つみれ",
            "text": "@たまご そうだね、もっと長く咲けばいいね",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "くじら",
            "text": "暖冬といっても寒いです。",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "つみれ",
            "text": "@くじら ほんとに！",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "くじら",
            "text": "桜も色々種類があるので、しばらくは楽しめますね",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "たまご",
            "text": "寒い時期も短いのかも",
            "mention_to": []
        }
