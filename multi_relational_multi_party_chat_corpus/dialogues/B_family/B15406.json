        "こんぶ",
        "ちくわ"
    ],
    "relationship": [
        "こんぶ",
        "ちくわ"
    ],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "おおかみ",
            "text": "こんばんは！",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "こんぶ",
            "text": "こんばんは！",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "ちくわ",
            "text": "こんばんは",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "おおかみ",
            "text": "今日の夕飯はなんでしたか？",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "こんぶ",
            "text": "今日は、ラーメンでした！",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "こんぶ",
            "text": "あとは、残り物笑",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "おおかみ",
            "text": "@こんぶ あら外ですか？",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "ちくわ",
            "text": "ラーメンでした！！",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "こんぶ",
            "text": "スーパーに売ってるやつです！笑",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "おおかみ",
            "text": "あ、家で！",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "ちくわ",
            "text": "美味しかった〜",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "こんぶ",
            "text": "@おおかみ そう家で！笑",
            "mention_to": [
                "おおかみ"
            ]
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "おおかみ",
            "text": "あれは最強に美味しいですのね",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "こんぶ",
            "text": "とんこつです！笑",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "おおかみ",
            "text": "最近思うのがあまりにカップ麺の塩分高くてびっくりしてます",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "こんぶ",
            "text": "今回買ったのは当たりでした！笑",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "こんぶ",
            "text": "@ちくわ ね！おいしかったねー",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "ちくわ",
            "text": "@こんぶ 確かにー！",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "ちくわ",
            "text": "うまかった",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "おおかみ",
            "text": "@こんぶ いいですね",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "おおかみ",
            "text": "私も豚骨好きですよ",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "こんぶ",
            "text": "カップ麺はやばいですよね〜",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "おおかみ",
            "text": "あれなんであんなに高いのか...",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "こんぶ",
            "text": "だからこそ、美味しいのですが、、",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "ちくわ",
            "text": "カップ麺のクオリティが高いですよね",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "おおかみ",
            "text": "分かります",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "こんぶ",
            "text": "@おおかみ つい豚骨に惹かれましたー",
            "mention_to": [
                "おおかみ"
            ]
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "おおかみ",
            "text": "最近のカップ麺クオリティはすごいです",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "こんぶ",
            "text": "体に悪いものって、ほんと美味しい笑",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "こんぶ",
            "text": "つい食べちゃいます",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "おおかみ",
            "text": "私は蒙古タンメンのカップラーメンがずっと好きです",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "こんぶ",
            "text": "あーー！！！",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "ちくわ",
            "text": "@おおかみ ですよね！色んな味もあって悩みます！",
            "mention_to": [
                "おおかみ"
            ]
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "おおかみ",
            "text": "@こんぶ つい食べますよね",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "こんぶ",
            "text": "私は辛いのが苦手で、、",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "ちくわ",
            "text": "@おおかみ 蒙古タンメン！うまいですよね！",
            "mention_to": [
                "おおかみ"
            ]
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "こんぶ",
            "text": "悩む悩む〜〜",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "ちくわ",
            "text": "辛いけど笑",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "おおかみ",
            "text": "@ちくわ そうなんです、辛いけど高校生くらいから食べてます",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "こんぶ",
            "text": "辛いけどうまい！！の舌になりたいな〜",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "おおかみ",
            "text": "ただ北極ラーメンは無理ですね",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "こんぶ",
            "text": "辛いと辛いしか感じなくて笑",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "ちくわ",
            "text": "@おおかみ 高校生の時から！？",
            "mention_to": [
                "おおかみ"
            ]
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "おおかみ",
            "text": "1回食べましたけどあまりに辛いとしか感じなかった",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "こんぶ",
            "text": "北極も聞いたことあります！",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "ちくわ",
            "text": "そんな前からあるんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "おおかみ",
            "text": "@ちくわ そうなんです！",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "こんぶ",
            "text": "へえー！そうなんですね！！",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "おおかみ",
            "text": "多分かれこれ10年前からあるはずですね",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "ちくわ",
            "text": "@おおかみ 知らなかったー！",
            "mention_to": [
                "おおかみ"
            ]
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "こんぶ",
            "text": "そんなに前から！！！",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "おおかみ",
            "text": "高校帰りの公園で友達と食べてました",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "ちくわ",
            "text": "セブンでよくみるようになってから",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "こんぶ",
            "text": "もっと最近かと思いました！！！",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "おおかみ",
            "text": "今思うとよく太らなかったなと",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "ちくわ",
            "text": "まだ数年くらいな気が…",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "こんぶ",
            "text": "たしかにたしかに！",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "こんぶ",
            "text": "私もそんな気がしてた！",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "おおかみ",
            "text": "@ちくわ 私もそう思うんですけど、確かに高校生から食べてたんです",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "こんぶ",
            "text": "高校生の頃はほんとよく食べますよね笑",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "おおかみ",
            "text": "写真に残ってました",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "おおかみ",
            "text": "なんかあの時の代謝が欲しいですね",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "こんぶ",
            "text": "写真に！！！笑",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "こんぶ",
            "text": "それは、たしかな証拠だ！！！笑",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "おおかみ",
            "text": "なんかえもかったです、、",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "こんぶ",
            "text": "エモいですね〜〜",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "おおかみ",
            "text": "ちなみに私は生姜焼きでした",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "ちくわ",
            "text": "だんだん代謝も悪くなってきますよねー",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "おおかみ",
            "text": "@ちくわ 間違いなく...",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "ちくわ",
            "text": "まだまだ若いですが笑",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "こんぶ",
            "text": "代謝ほんとに、、わるく、、なって、、",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "ちくわ",
            "text": "年かなと思う時があります！",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "おおかみ",
            "text": "露骨に悪くなりますよね",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "こんぶ",
            "text": "生姜焼きは！夕飯ですね！！",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "こんぶ",
            "text": "年を実感するときもあります笑",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "おおかみ",
            "text": "@こんぶ 私ではなく旦那が作りましたが",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "おおかみ",
            "text": "私は全く料理できず、、",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "こんぶ",
            "text": "@おおかみ おお！！料理男子ですね",
            "mention_to": [
                "おおかみ"
            ]
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "こんぶ",
            "text": "あ！そうなんですかーー！",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "おおかみ",
            "text": "@ちくわ 同じくです、、",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "こんぶ",
            "text": "旦那さんやってくれるなら、いいですねーー！",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "ちくわ",
            "text": "俺も料理しますー！",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "こんぶ",
            "text": "そうそう！",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "おおかみ",
            "text": "おっ！",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "ちくわ",
            "text": "@こんぶ ねっ！笑",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "おおかみ",
            "text": "@こんぶ 良い旦那さん...",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "こんぶ",
            "text": "こちらも料理男子です！笑",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "おおかみ",
            "text": "料理男子は重宝したほうがいいです",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "こんぶ",
            "text": "助かりますよね〜できる男の人笑",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "ちくわ",
            "text": "一応料理男子でーす！",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "おおかみ",
            "text": "本当に助かるから...",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "こんぶ",
            "text": "いや〜ほんとそう思います！！！",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "おおかみ",
            "text": "@こんぶ 褒められたがってますよ",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "ちくわ",
            "text": "今後も頑張ります！",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "こんぶ",
            "text": "@ちくわ ありがとうございまーす",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "ちくわ",
            "text": "@おおかみ ほめてあげて！",
            "mention_to": [
                "おおかみ"
            ]
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "ちくわ",
            "text": "そして俺もほめて！",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "こんぶ",
            "text": "よっ！いつもありがとう！！！",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "こんぶ",
            "text": "いいぞ！いいぞ！料理男子",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "ちくわ",
            "text": "@こんぶ お！さんきゅー！",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "こんぶ",
            "text": "褒め方下手くそ笑",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "おおかみ",
            "text": "あ、これ持ち上げてるやつかな",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "ちくわ",
            "text": "頑張りまーす！！",
            "mention_to": []
        },
        {
            "utterance_id": 103,
            "interlocutor_id": "こんぶ",
            "text": "単純すぎて笑",
            "mention_to": []
        },
        {
            "utterance_id": 104,
            "interlocutor_id": "おおかみ",
            "text": "@こんぶ でも嬉しそうです",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 105,
            "interlocutor_id": "こんぶ",
            "text": "うん！よろしくーー！",
            "mention_to": []
        },
        {
            "utterance_id": 106,
            "interlocutor_id": "ちくわ",
            "text": "@こんぶ いやいや！感謝の言葉が一番大事！",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 107,
            "interlocutor_id": "こんぶ",
            "text": "本当そうだよね！！！",
            "mention_to": []
        }
