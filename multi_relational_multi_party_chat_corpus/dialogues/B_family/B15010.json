    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "じゃがいも",
            "text": "こんばんはー",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "ちくわ",
            "text": "こんばんはー",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "こんぶ",
            "text": "こんばんは〜！！",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "じゃがいも",
            "text": "コロナかインフルになりました？",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "じゃがいも",
            "text": "今年！",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "こんぶ",
            "text": "今年はまだ何も！！！",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "じゃがいも",
            "text": "今年ってかこの冬ですね笑",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "ちくわ",
            "text": "今年はなってないです！",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "こんぶ",
            "text": "我が家はセーフです笑",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "ちくわ",
            "text": "@じゃがいも かかりました？",
            "mention_to": [
                "じゃがいも"
            ]
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "こんぶ",
            "text": "@じゃがいも なったんですか？？",
            "mention_to": [
                "じゃがいも"
            ]
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "じゃがいも",
            "text": "@こんぶ お子さんいると大変ですもんね、よかった！",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "じゃがいも",
            "text": "ならなかったんですが",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "じゃがいも",
            "text": "昨日喉に違和感覚えて",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "こんぶ",
            "text": "そうなんですよ、、胃腸炎にはなりました",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "じゃがいも",
            "text": "めっちゃ風邪薬飲みました",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "こんぶ",
            "text": "あら！！！",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "こんぶ",
            "text": "その効果は？",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "じゃがいも",
            "text": "@こんぶ めっちゃしんどくないですか？胃腸炎",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "ちくわ",
            "text": "@じゃがいも おお！コロナとかだったんですか？",
            "mention_to": [
                "じゃがいも"
            ]
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "じゃがいも",
            "text": "@こんぶ 花粉だったのかも...ってかんじですね",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "じゃがいも",
            "text": "なんか今日には治りました笑",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "こんぶ",
            "text": "@じゃがいも うつって、ゲーしてピーでました",
            "mention_to": [
                "じゃがいも"
            ]
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "ちくわ",
            "text": "今日！？笑",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "ちくわ",
            "text": "だいぶリアルタイム！笑",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "じゃがいも",
            "text": "@こんぶ 胃腸炎はめっちゃ大変ですよね",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "こんぶ",
            "text": "花粉ならばよかった、、",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "こんぶ",
            "text": "花粉も辛いですが、、、",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "じゃがいも",
            "text": "@ちくわ なんか喉に違和感というか...ガサガサ感",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "ちくわ",
            "text": "健康が一番！",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "じゃがいも",
            "text": "嫌な感じしました、昨日は笑",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "じゃがいも",
            "text": "私も胃腸炎の中仕事した時あって",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "こんぶ",
            "text": "@じゃがいも 風邪症状の中で、吐くのが辛いですよね、、",
            "mention_to": [
                "じゃがいも"
            ]
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "ちくわ",
            "text": "@じゃがいも イガイガするの嫌ですよねー",
            "mention_to": [
                "じゃがいも"
            ]
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "じゃがいも",
            "text": "こんなに辛いとは思ってなくて、ビビりましたね",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "こんぶ",
            "text": "喉のイガイガいやだーーー",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "じゃがいも",
            "text": "@こんぶ 間違い無いです",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "ちくわ",
            "text": "大人になってからの病気って",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "ちくわ",
            "text": "結構しんどいですよね",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "こんぶ",
            "text": "わかるーーー",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "じゃがいも",
            "text": "@ちくわ めちゃ分かりますわ",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "ちくわ",
            "text": "@こんぶ ただの風邪でもしんどい",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "こんぶ",
            "text": "大人になってからの病気がしんどい。。",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "じゃがいも",
            "text": "まぁ仕事休めるラッキーみたいな気持ちはどこかあります笑",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "こんぶ",
            "text": "なんででしょうか？？",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "じゃがいも",
            "text": "正直w",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "ちくわ",
            "text": "@じゃがいも 確かに！",
            "mention_to": [
                "じゃがいも"
            ]
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "ちくわ",
            "text": "でも元気で休みたい笑",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "じゃがいも",
            "text": "@こんぶ 謎現象ですよね",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "こんぶ",
            "text": "そうなりますよね笑笑",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "こんぶ",
            "text": "元気で休みたい笑",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "じゃがいも",
            "text": "旦那がコロナになって、昔に！その時に濃厚接触者で在宅になった時がマジでハッピーでした",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "こんぶ",
            "text": "@じゃがいも 子どもは熱でも元気なのに、。",
            "mention_to": [
                "じゃがいも"
            ]
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "じゃがいも",
            "text": "こんなこと会社に言ったら終わりますけど笑",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "こんぶ",
            "text": "それは、ハッピーですね！！！",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "ちくわ",
            "text": "あーそれはハッピーですね！",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "じゃがいも",
            "text": "めちゃハッピーでしたね",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "こんぶ",
            "text": "今は家族がコロナになったらどうなりますか？？",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "ちくわ",
            "text": "旦那さんだけがしんどい笑",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "じゃがいも",
            "text": "今は多分インフルと同じなので、普通に出社かなと！",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "ちくわ",
            "text": "@こんぶ 普通に出勤とかしていいんじゃない？",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "ちくわ",
            "text": "5類だし！",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "じゃがいも",
            "text": "@ちくわ 旦那はマジで死んでしまたね",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "こんぶ",
            "text": "今は、そうなるんだもんね！！！",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "ちくわ",
            "text": "@じゃがいも ですよねー！",
            "mention_to": [
                "じゃがいも"
            ]
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "じゃがいも",
            "text": "大人で40度の熱で、飯が喉通ってなかったです。喉が痛すぎて",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "こんぶ",
            "text": "@じゃがいも 旦那さんお気の毒。。",
            "mention_to": [
                "じゃがいも"
            ]
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "ちくわ",
            "text": "@じゃがいも 40度はしんどいー",
            "mention_to": [
                "じゃがいも"
            ]
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "こんぶ",
            "text": "40度！！！！！",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "じゃがいも",
            "text": "まぁ珍しく可哀想でしたね、、、",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "こんぶ",
            "text": "何日で治りました？",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "こんぶ",
            "text": "長引きました？？",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "ちくわ",
            "text": "@じゃがいも 労ってあげてください！",
            "mention_to": [
                "じゃがいも"
            ]
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "じゃがいも",
            "text": "5日かかりました",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "じゃがいも",
            "text": "@ちくわ その時は本領発揮しました笑",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "こんぶ",
            "text": "@ちくわ 労わってあげてください。",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "ちくわ",
            "text": "後遺症残るって聞きますがどーなんでしょ？",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "じゃがいも",
            "text": "ただ私も濃厚接触者であんまり外にいけないという...",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "ちくわ",
            "text": "@じゃがいも さすがです！！",
            "mention_to": [
                "じゃがいも"
            ]
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "こんぶ",
            "text": "5日かーーしんどすぎる。",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "じゃがいも",
            "text": "@ちくわ 人によりますね！",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "じゃがいも",
            "text": "友達は味覚消えたって本当に言ってました",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "ちくわ",
            "text": "@じゃがいも それもしんどいですね、、、外に出れないの",
            "mention_to": [
                "じゃがいも"
            ]
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "こんぶ",
            "text": "味覚！！！",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "ちくわ",
            "text": "味覚消えるのも嫌ですね…",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "じゃがいも",
            "text": "@ちくわ もう材料なくなったら全デリバリーですね笑",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "こんぶ",
            "text": "やっぱり、そうなると怖いですやね",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "ちくわ",
            "text": "@じゃがいも ウーバーイーツ？笑",
            "mention_to": [
                "じゃがいも"
            ]
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "こんぶ",
            "text": "ウーバーですか？",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "じゃがいも",
            "text": "お二人はコロナにもならず？",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "こんぶ",
            "text": "私たちも去年？一昨年？コロナに！",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "じゃがいも",
            "text": "謎に旦那が出前館派なんですよw",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "こんぶ",
            "text": "出前館！！！",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "じゃがいも",
            "text": "@こんぶ 大丈夫でした？",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "こんぶ",
            "text": "私らのとこらは、、、",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "ちくわ",
            "text": "@じゃがいも かかりましたー",
            "mention_to": [
                "じゃがいも"
            ]
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "こんぶ",
            "text": "ウーバーありません笑",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "こんぶ",
            "text": "私がなって、娘がなって、旦那が、、",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "じゃがいも",
            "text": "@こんぶ あれ、どこですか？",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "こんぶ",
            "text": "みんなで一気に",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "ちくわ",
            "text": "出前館もウーバーイーツもないので羨ましい！",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "こんぶ",
            "text": "長野県です！",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "じゃがいも",
            "text": "長野か...",
            "mention_to": []
        },
        {
            "utterance_id": 103,
            "interlocutor_id": "こんぶ",
            "text": "ほんとそうですね！！！",
            "mention_to": []
        },
        {
            "utterance_id": 104,
            "interlocutor_id": "じゃがいも",
            "text": "@こんぶ みんなで一気にはきつい",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 105,
            "interlocutor_id": "ちくわ",
            "text": "@こんぶ デリバリーがそもそもないよね",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 106,
            "interlocutor_id": "こんぶ",
            "text": "ウーバー憧れですーー",
            "mention_to": []
        }
