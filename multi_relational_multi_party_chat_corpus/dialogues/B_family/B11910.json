        "つくね"
    ],
    "relationship": [
        "しらたき",
        "つくね"
    ],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "ばなな",
            "text": "こんばんは",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "しらたき",
            "text": "こんばんは",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "つくね",
            "text": "こんばんは！",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "ばなな",
            "text": "家電で欲しい物ありますか？",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "しらたき",
            "text": "シャークの掃除機が欲しい",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "ばなな",
            "text": "私は炭酸つくるのが気になっています",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "つくね",
            "text": "新しい冷蔵庫と冷凍庫が欲しいです",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "ばなな",
            "text": "シャークの掃除機ですか",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "つくね",
            "text": "@ばなな 炭酸つくるのですか！？",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "ばなな",
            "text": "家で炭酸飲めたらなって",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "ばなな",
            "text": "たまに炭酸水が飲みたくなって",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "つくね",
            "text": "ただの炭酸ってことですよね？",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "つくね",
            "text": "味無くて飲めないです笑",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "ばなな",
            "text": "ジュースでも作れるみたいです",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "しらたき",
            "text": "レモンサワー作るのにあると便利だね",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "つくね",
            "text": "そうなんですか！？",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "つくね",
            "text": "初めてしりました！",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "ばなな",
            "text": "色んな炭酸作れるみたいです",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "つくね",
            "text": "いいですねー！",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "つくね",
            "text": "ウォーターサーバーは",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "つくね",
            "text": "いつか欲しいなと思ってます",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "ばなな",
            "text": "そうなんですね",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "ばなな",
            "text": "あると便利ですよ",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "つくね",
            "text": "お湯すぐ欲しい時に",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "つくね",
            "text": "便利かなーと",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "しらたき",
            "text": "@つくね 家狭くて",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "ばなな",
            "text": "そうなんです",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "しらたき",
            "text": "置けないよー",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "ばなな",
            "text": "お湯がすぐ出ます",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "つくね",
            "text": "@しらたき 仕方ない！",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "ばなな",
            "text": "今はコンパクトなのもありますよ",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "つくね",
            "text": "@ばなな 持ってるんですか！？",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "ばなな",
            "text": "家にあります",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "つくね",
            "text": "羨ましい！！",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "しらたき",
            "text": "羨ましい！",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "つくね",
            "text": "水入れて浄水するやつ？",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "ばなな",
            "text": "1度始めると便利で",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "ばなな",
            "text": "水のパックをセットして使っています",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "つくね",
            "text": "水買ってるんですね",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "ばなな",
            "text": "そうなんです",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "つくね",
            "text": "ランニングコストを考えると",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "つくね",
            "text": "手が出せないです",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "つくね",
            "text": "でも、災害時とか",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "つくね",
            "text": "水あるといいって聞きました",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "ばなな",
            "text": "災害時にはあると良いですよね",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "しらたき",
            "text": "@つくね 近くに川流れてるよ",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "つくね",
            "text": "いつ災害あるかわからないですからね…",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "つくね",
            "text": "@しらたき 飲めないじゃん！",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "ばなな",
            "text": "そうなんですよね",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "つくね",
            "text": "氾濫したらそれどころじゃない！",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "しらたき",
            "text": "いつ地震来てもおかしくないね",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "ばなな",
            "text": "家も近く川があります",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "つくね",
            "text": "@ばなな 今年は年始から地震や火事とかがあったので",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "つくね",
            "text": "怖いですよね",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "ばなな",
            "text": "そうですよね",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "ばなな",
            "text": "年明けからたいへんでしたね",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "ばなな",
            "text": "防災グッズ揃えてないのですけれど",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "ばなな",
            "text": "揃えていますか？",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "つくね",
            "text": "そうですね。子供たちもビックリしてました！",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "つくね",
            "text": "あんまり備えてないです",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "しらたき",
            "text": "オール電化なので",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "しらたき",
            "text": "電気とまると",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "ばなな",
            "text": "オール電化なのですね",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "しらたき",
            "text": "やばいですね！",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "つくね",
            "text": "@しらたき ソーラーつけた方がいい！",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "ばなな",
            "text": "確かに電気が止まると",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "つくね",
            "text": "蓄電しておく！",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "しらたき",
            "text": "カセットコンロを買おう！",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "つくね",
            "text": "@しらたき そうだね！ガスはあるから",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "つくね",
            "text": " @ばなな 電気、水道が止まるのが一番大変ですよね",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "ばなな",
            "text": "そうですよね",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "ばなな",
            "text": "計画停電体験した時たいへんでした",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "つくね",
            "text": "計画停電の時は大変ですよね",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "つくね",
            "text": "忘れてるとビックリします！",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "ばなな",
            "text": "部屋が真っ暗になって",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "ばなな",
            "text": "そうなんですよ",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "しらたき",
            "text": "今って計画的停電あるの？",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "ばなな",
            "text": "料理が途中になった時もありました",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "つくね",
            "text": "帯電してるのとかも一気に消えるし",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "つくね",
            "text": "@しらたき あるよ！",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "つくね",
            "text": "家にいないから知らないだけで笑",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "しらたき",
            "text": "@つくね うちもあったっけ？",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "つくね",
            "text": "@ばなな それはいたいですね",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "つくね",
            "text": "@しらたき あったよ！工事とかでも停電するじゃん",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "つくね",
            "text": "1時間とか！",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "しらたき",
            "text": "全然覚えて無い",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "ばなな",
            "text": "トイレも済ませておかないとですし",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "つくね",
            "text": "冷蔵庫も全部電源止まるから",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "つくね",
            "text": "結構大変だよ笑",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "ばなな",
            "text": "工事でも確かに停電ありますね",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "つくね",
            "text": "@ばなな そう！トイレも電気ですもんね！",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "ばなな",
            "text": "そうなんですよ",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "つくね",
            "text": "ライフラインの確保は必須ですね",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "ばなな",
            "text": "何でも電気で動いているんですね",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "つくね",
            "text": "今更ですが、そうなんですよね",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "しらたき",
            "text": "やっぱりソーラーのせるか",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "つくね",
            "text": "電気は大事です！",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "つくね",
            "text": "@しらたき そうなのよ！ソーラー乗せるの",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "つくね",
            "text": "考えてもいいかも！",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "ばなな",
            "text": "電気は大事ですね",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "ばなな",
            "text": "自家発電出来たら良いです",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "つくね",
            "text": "@ばなな 自家発電できたら確かにいいですよね！",
            "mention_to": [
                "ばなな"
            ]
        }
