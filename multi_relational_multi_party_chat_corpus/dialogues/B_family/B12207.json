    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "ばなな",
            "text": "こんばんは",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "えのき",
            "text": "こんばんは",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "てばさき",
            "text": "こんばんは",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "えのき",
            "text": "@ばなな お酒は飲まれますか？",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "ばなな",
            "text": "夕飯は食べましたか",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "えのき",
            "text": "はい",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "ばなな",
            "text": "@えのき お酒は少し",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "えのき",
            "text": "ごはんとともにまだ浅い時間ですが、ちょっと一杯ひっかけてしまいました…",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "ばなな",
            "text": "良いですね",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "てばさき",
            "text": "@ばなな メイドバイ父のお好み焼きでしたー",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "えのき",
            "text": "クラフトフルーツと書かれた梅酒が美味しそうだったので、つい",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "ばなな",
            "text": "飲めるのは羨ましい",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "えのき",
            "text": "普段はそんなに飲まないんですが",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "えのき",
            "text": "梅酒が飲みたくなり",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "てばさき",
            "text": "@えのき クラフトフルーツだと美味しくなるの？",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "ばなな",
            "text": "梅酒好きです",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "えのき",
            "text": "職場でもたまに飲み会やります",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "えのき",
            "text": "@ばなな 飲み会とかありますか？",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "てばさき",
            "text": "梅はしゅっぱいイメージしかない",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "えのき",
            "text": "そうだね",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "ばなな",
            "text": "飲み会、ほとんどないです",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "えのき",
            "text": "コロナもありましたしね～",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "ばなな",
            "text": "飲み会では梅酒飲みますよ",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "えのき",
            "text": "甘くて美味しいですしね！",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "ばなな",
            "text": "確かにコロナですっかり",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "てばさき",
            "text": "@えのき 酸っぱくはないのか",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "えのき",
            "text": "前に職場の飲み会があったんですけど",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "えのき",
            "text": "飲み会は自転車で行くので、お酒は飲まないんですが",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "ばなな",
            "text": "飲みやすいです",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "えのき",
            "text": "てばさんはまだ飲めない年だからね",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "ばなな",
            "text": "そうでしたか",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "ばなな",
            "text": "これからですね",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "てばさき",
            "text": "来年の今頃やる成人式ですら私が引っ掛けるのはソフトドリンクなのだ…",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "えのき",
            "text": "早生まれだから…",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "ばなな",
            "text": "来年、成人式ですか",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "てばさき",
            "text": "@ばなな そうです",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "えのき",
            "text": "@てばさき 20になったら、何か飲んでみたいお酒はありますか？",
            "mention_to": [
                "てばさき"
            ]
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "てばさき",
            "text": "@えのき カルーアミルクとかいうの気になってる",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "えのき",
            "text": "実は飲んだことない",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "ばなな",
            "text": "@てばさき 甘いやつですね",
            "mention_to": [
                "てばさき"
            ]
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "えのき",
            "text": "甘くておいしそうな、予感はしている",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "てばさき",
            "text": "イチゴミルク味とか、コーヒー牛乳味の酒の存在は気になってる感じです",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "ばなな",
            "text": "コーヒー牛乳みたいな",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "えのき",
            "text": "来年の成人式、楽しみだね",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "えのき",
            "text": "美容室ってもう予約取らなくていいのかな？",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "えのき",
            "text": "心配になってきた…",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "ばなな",
            "text": "甘くて酔ったことに気が付かないかも",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "てばさき",
            "text": "@えのき 知らぬ",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "ばなな",
            "text": "着物着るのですか",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "えのき",
            "text": "結構みんな着物着てますね、女の子は",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "てばさき",
            "text": "@ばなな 姉のを着る予定です",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "えのき",
            "text": "男の子はほぼスーツですが",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "ばなな",
            "text": "私は成人式では着なかったです",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "てばさき",
            "text": "首周りのもふもふはつけてみたいです",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "えのき",
            "text": "私も着なかったです～",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "ばなな",
            "text": "@てばさき お姉さんのがあるのですね",
            "mention_to": [
                "てばさき"
            ]
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "てばさき",
            "text": "@ばなな あるのです",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "ばなな",
            "text": "着物着たら歩きにくそうと思って",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "えのき",
            "text": "長女は着物にこだわっていたので・・・",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "えのき",
            "text": "お高いのを買わされました・・・",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "てばさき",
            "text": "こだわってましたね",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "ばなな",
            "text": "そうでしたか",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "えのき",
            "text": "そうなんです",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "ばなな",
            "text": "買ったのですね",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "てばさき",
            "text": "お母さんの着てた着物も可愛かった",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "えのき",
            "text": "あー",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "えのき",
            "text": "猫の",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "てばさき",
            "text": "なんならあれ着たいかも",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "ばなな",
            "text": "えのきさんも着たのですね",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "えのき",
            "text": "猫の結城紬を着ました",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "ばなな",
            "text": "猫の柄",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "てばさき",
            "text": "@ばなな 記念写真の時に",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "ばなな",
            "text": "可愛いですね",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "えのき",
            "text": "猫のは来年おねえちゃんが着るみたいだよ",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "てばさき",
            "text": "@えのき 何のために",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "ばなな",
            "text": "記念写真ですね",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "えのき",
            "text": "結城紬を着たいからだよ",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "てばさき",
            "text": "@えのき 無念",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "えのき",
            "text": "何かにかこつけて",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "ばなな",
            "text": "着物良いですね",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "えのき",
            "text": "長女は着付けもがんばって勉強してます！",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "てばさき",
            "text": "@えのき お姉さま結構多才",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "ばなな",
            "text": "着付けも",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "えのき",
            "text": "着物が好きらしいです…",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "ばなな",
            "text": "自分で着れたら素敵",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "えのき",
            "text": "結城紬推しらしいです…",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "ばなな",
            "text": "こだわりがあるのですね",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "てばさき",
            "text": "あれは一度やってみたいです、襷掛け",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "えのき",
            "text": "おそうじする時にするやつ？",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "てばさき",
            "text": "@えのき かっこいい",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "えのき",
            "text": "あれの構造がどうなっているかよくわからない…",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "ばなな",
            "text": "襷がけ、何か格好いいですよね",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "てばさき",
            "text": "@えのき 私もわからない",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "えのき",
            "text": "ふわっとしてる",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "えのき",
            "text": "イメージで語ってる",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "ばなな",
            "text": "私もわかりません",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "てばさき",
            "text": "@ばなな 最近で着物着る機会とかありますか？",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "ばなな",
            "text": "全くないですね",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "えのき",
            "text": "なかなかないですよね",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "ばなな",
            "text": "そうなんです",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "えのき",
            "text": "着物って、保管も大変だし…",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "えのき",
            "text": "うちは全部おばあちゃん任せですよ",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "てばさき",
            "text": "@えのき いい服って保存大変なの多い",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 103,
            "interlocutor_id": "ばなな",
            "text": "保管が確かにたいへんそう",
            "mention_to": []
        }
