        },
        {
            "utterance_id": 2,
            "interlocutor_id": "しらたき",
            "text": "こんばんは",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "つくね",
            "text": "@ばなな 動物はすきですか？",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "ばなな",
            "text": "動物はあまり",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "つくね",
            "text": "そうなんですね",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "しらたき",
            "text": "え！",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "ばなな",
            "text": "@つくね 好きですか？",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "つくね",
            "text": "大好きです",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "つくね",
            "text": "特に猫が",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "つくね",
            "text": "好きです！",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "しらたき",
            "text": "私も大好きです",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "ばなな",
            "text": "そうなのですね",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "ばなな",
            "text": "@しらたき 猫好きですか",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "しらたき",
            "text": "大好きです",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "つくね",
            "text": "息子はゴリラとモンキーが好きで",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "しらたき",
            "text": "飼ってます",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "つくね",
            "text": "よく動物園に連れて行きました",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "ばなな",
            "text": "動物園はどちらに？",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "つくね",
            "text": "上野動物園に",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "つくね",
            "text": "行きました",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "しらたき",
            "text": "私をおいて",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "ばなな",
            "text": "そうなのですね",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "ばなな",
            "text": "それさ寂しい",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "しらたき",
            "text": "寂しい",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "つくね",
            "text": "しらたきは仕事でした",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "ばなな",
            "text": "そうでしたか",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "つくね",
            "text": "結構よくあります",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "ばなな",
            "text": "@つくね パンダは見ましたか？",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "つくね",
            "text": "見ました！",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "つくね",
            "text": "すごく可愛かったです",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "ばなな",
            "text": "良いですね",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "しらたき",
            "text": "いいなー",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "つくね",
            "text": "ぬいぐるみも買いました",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "ばなな",
            "text": "混んでいませんでしたか？",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "つくね",
            "text": "すごく混んでて",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "つくね",
            "text": "桜も咲いていたので",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "しらたき",
            "text": "いいなー",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "つくね",
            "text": "お花見客が多かったです",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "ばなな",
            "text": "良い時期だったのですね",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "つくね",
            "text": "そうですね",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "つくね",
            "text": "@ばなな は上野とか行かれますか？",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "ばなな",
            "text": "行きます",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "ばなな",
            "text": "電車でわりと近くて",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "つくね",
            "text": "そうなんですね！",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "ばなな",
            "text": "３０分ほどで行けます",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "つくね",
            "text": "それは近いですね",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "しらたき",
            "text": "長野からは遠くて",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "ばなな",
            "text": "なので時々行きます",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "しらたき",
            "text": "滅多に行けない",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "ばなな",
            "text": "長野からは遠いですね",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "ばなな",
            "text": "電車で来ますか？",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "つくね",
            "text": "上野は鳩のイメージが強いです",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "つくね",
            "text": "電車か車ですね",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "ばなな",
            "text": "確かにはとがたくさんいますね",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "ばなな",
            "text": "家は長野は車で行きます",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "つくね",
            "text": "ですよね！",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "つくね",
            "text": "車なんですね",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "ばなな",
            "text": "夫がドライブ好きで",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "つくね",
            "text": "いいですね！",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "しらたき",
            "text": "いいですね！",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "つくね",
            "text": "私は運転嫌いです",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "しらたき",
            "text": "車好きとか？",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "つくね",
            "text": "長距離は眠くなります",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "ばなな",
            "text": "夫は大好きです",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "ばなな",
            "text": "金沢まで行ったことあります",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "しらたき",
            "text": "そうなんですね！",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "つくね",
            "text": "金沢！",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "つくね",
            "text": "いいですね！",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "ばなな",
            "text": "@つくね 金沢行きましたか？",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "つくね",
            "text": "1回石川県には行った事あります",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "つくね",
            "text": "息子がウルトラマンが好きで",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "しらたき",
            "text": "運転してきました",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "つくね",
            "text": "ウルトラマンスタジアムに行きました",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "ばなな",
            "text": "知らなかったです",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "しらたき",
            "text": "平成から令和に変わる時に！",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "つくね",
            "text": "大興奮でした！",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "ばなな",
            "text": "どんな感じですか？",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "つくね",
            "text": "キャラクターショーが見れます",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "つくね",
            "text": "写真撮影とかもあって",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "ばなな",
            "text": "なるほど",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "つくね",
            "text": "握手もしてもらいました",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "つくね",
            "text": "まだ2歳だったので",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "ばなな",
            "text": "喜びますね",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "つくね",
            "text": "とっても喜んでました！",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "ばなな",
            "text": "家の子はトーマスとか好きでした",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "つくね",
            "text": "トーマスハマりますよね！",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "つくね",
            "text": "トーマスランドとか行きました？",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "ばなな",
            "text": "皆はまるのでしょうね",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "ばなな",
            "text": "行きましたよ",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "つくね",
            "text": "プラレール、トミカは通り道ですね",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "しらたき",
            "text": "ゴードン最高！",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "ばなな",
            "text": "確かに",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "ばなな",
            "text": "皆通る道ですね",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "しらたき",
            "text": "富士急ハイランド",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "つくね",
            "text": "トーマスランド富士急ですよね？",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "ばなな",
            "text": "そうですよね",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "つくね",
            "text": "トミカは今でも好きです",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "しらたき",
            "text": "@ばなな さんは絶叫系乗れますか？",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "ばなな",
            "text": "駄目なんです",
