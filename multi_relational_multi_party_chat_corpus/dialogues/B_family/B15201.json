        },
        {
            "utterance_id": 1,
            "interlocutor_id": "たまねぎ",
            "text": "こんにちは！",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "にんじん",
            "text": "こんにちは！",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "ぺんぎん",
            "text": "今日は寒いですね",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "にんじん",
            "text": "@ぺんぎん そうですねー",
            "mention_to": [
                "ぺんぎん"
            ]
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "たまねぎ",
            "text": "@ぺんぎん まだまだ冬です",
            "mention_to": [
                "ぺんぎん"
            ]
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "ぺんぎん",
            "text": "昨日の暖かさがうそのよう！",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "にんじん",
            "text": "朝晩寒い",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "にんじん",
            "text": "風邪ひきそー",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "ぺんぎん",
            "text": "@たまねぎ ですねよねー、さむくて",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "たまねぎ",
            "text": "@ぺんぎん 早く春がきてほしー",
            "mention_to": [
                "ぺんぎん"
            ]
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "ぺんぎん",
            "text": "@にんじん 風邪ひかないですか？",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "にんじん",
            "text": "@ぺんぎん 今の所大丈夫です！",
            "mention_to": [
                "ぺんぎん"
            ]
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "ぺんぎん",
            "text": "@たまねぎ 本当ですよ",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "たまねぎ",
            "text": "今雪降ってます！",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "にんじん",
            "text": "積もりそー",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "ぺんぎん",
            "text": "@たまねぎ えー本当ですか？",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "たまねぎ",
            "text": "めちゃ降ってますー",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "ぺんぎん",
            "text": "@にんじん 積もりそうなくらい？",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "にんじん",
            "text": "たぶん積もる…",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "にんじん",
            "text": "雪かきしないと",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "ぺんぎん",
            "text": "@たまねぎ えー！こっちは降ってないです",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "たまねぎ",
            "text": "いいなぁー",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "ぺんぎん",
            "text": "ちなみに関東です",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "たまねぎ",
            "text": "お昼寝したら積もってるね！",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "にんじん",
            "text": "こちらは長野県です！",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "にんじん",
            "text": "雪国です！",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "にんじん",
            "text": "お昼寝しません！",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "ぺんぎん",
            "text": "@にんじん なるほど！それは雪すごそう！",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "にんじん",
            "text": "でもしんしんと、ふってると眠くなってくるー",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "たまねぎ",
            "text": "寝ちゃダメだぞー",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "ぺんぎん",
            "text": "@にんじん 雪の中お昼寝贅沢！！！",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "にんじん",
            "text": "@ぺんぎん ですよねー",
            "mention_to": [
                "ぺんぎん"
            ]
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "にんじん",
            "text": "音がしないからよく寝れる",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "たまねぎ",
            "text": "そーそー",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "たまねぎ",
            "text": "わかるけども",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "ぺんぎん",
            "text": "@にんじん 寒いしねー",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "ぺんぎん",
            "text": "お布団がよんでいるー",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "にんじん",
            "text": "ソファで毛布かけたら一撃",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "たまねぎ",
            "text": "寝たいー",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "たまねぎ",
            "text": "お昼食べたから余計眠い",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "ぺんぎん",
            "text": "猫ちゃんいたら誘われたちゃう",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "にんじん",
            "text": "@ぺんぎん それは完全にやばい",
            "mention_to": [
                "ぺんぎん"
            ]
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "たまねぎ",
            "text": "動物は寝るな",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "たまねぎ",
            "text": "見てるだけで",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "ぺんぎん",
            "text": "@たまねぎ お腹いっぱいはやばいですねー！お昼何たべたんですか？",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "たまねぎ",
            "text": "お昼は愛妻弁当ですよー",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "にんじん",
            "text": "作りましたー",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "ぺんぎん",
            "text": "@たまねぎ おー！いいですね！",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "たまねぎ",
            "text": "毎日弁当です！",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "ぺんぎん",
            "text": "うちも持たせました！愛妻？弁当",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "たまねぎ",
            "text": "@ぺんぎん ふふ",
            "mention_to": [
                "ぺんぎん"
            ]
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "たまねぎ",
            "text": "すごいですね",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "にんじん",
            "text": "毎日大変ですよね！",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "にんじん",
            "text": "朝苦手なので、昼間に作って届けます",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "ぺんぎん",
            "text": "@たまねぎ うち早いからー、昨日の夜に用意してますよ",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "たまねぎ",
            "text": "おー",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "ぺんぎん",
            "text": "@にんじん とどけてるんですか？",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "たまねぎ",
            "text": "届けてくれます",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "にんじん",
            "text": "車で10分くらいなんで",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "ぺんぎん",
            "text": "@たまねぎ 愛されてるー！",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "たまねぎ",
            "text": "てへ！",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "にんじん",
            "text": "会えるしねー",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "にんじん",
            "text": "食べてるとこ見るのスキー",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "ぺんぎん",
            "text": "@にんじん あつあつじゃないですか！",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "にんじん",
            "text": "よく食べるんですよー",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "にんじん",
            "text": "しかも、一口がでかい！",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "にんじん",
            "text": "見てて飽きない笑",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "たまねぎ",
            "text": "はずいわ！",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "にんじん",
            "text": "大食いか！みたいな",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "にんじん",
            "text": "好き嫌いないし！",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "ぺんぎん",
            "text": "@たまねぎ やっぱりたべてくれるのは嬉しいです！よ",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "にんじん",
            "text": "@ぺんぎん ですよね！",
            "mention_to": [
                "ぺんぎん"
            ]
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "にんじん",
            "text": "なんでも、食べてくれるからおもしろい",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "たまねぎ",
            "text": "なんでも食べます！",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "ぺんぎん",
            "text": "@にんじん ですよー、好き嫌いないのがいい",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "たまねぎ",
            "text": "落ちたものでも食べます！",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "にんじん",
            "text": "@たまねぎ それはだめー",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "ぺんぎん",
            "text": "@たまねぎ そ、それは",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "にんじん",
            "text": "@ぺんぎん 好き嫌いあると大変！",
            "mention_to": [
                "ぺんぎん"
            ]
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "ぺんぎん",
            "text": "@たまねぎ おなかこわすよー",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "たまねぎ",
            "text": "え？",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "たまねぎ",
            "text": "3秒ルール",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "たまねぎ",
            "text": "お腹丈夫！",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "ぺんぎん",
            "text": "@たまねぎ うちの旦那もだ！笑",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "たまねぎ",
            "text": "@ぺんぎん 落ちたの食べる？",
            "mention_to": [
                "ぺんぎん"
            ]
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "にんじん",
            "text": "そちらも？！",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "にんじん",
            "text": "やめた方がいい！",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "ぺんぎん",
            "text": "いやー、食べないなあでも旦那はたべる",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "にんじん",
            "text": "なんでかねー？",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "ぺんぎん",
            "text": "@にんじん だよね、普通たべないよね",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "にんじん",
            "text": "食べないー",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "にんじん",
            "text": "子供が落としたのとかたべてる",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "にんじん",
            "text": "外でも食べる",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "ぺんぎん",
            "text": "@にんじん なんか、自分ルール結構ある感じです",
            "mention_to": [
