    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "にわとり",
            "text": "おはようございますー！",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "こんぶ",
            "text": "おはようございます！",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "ちくわ",
            "text": "おはようございます！",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "にわとり",
            "text": "昨日外を歩いていたら、",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "にわとり",
            "text": "サクラが咲き始めてました！",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "ちくわ",
            "text": "おーー",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "ちくわ",
            "text": "はやい！",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "こんぶ",
            "text": "えーー！本当ですか！！",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "ちくわ",
            "text": "あったかいんですねー！",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "ちくわ",
            "text": "こっちはまだですね！",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "こんぶ",
            "text": "昨日はあったかかったです？",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "にわとり",
            "text": "なんかね、２日くらい前におっ、咲いてる！って思ったら、",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "にわとり",
            "text": "暖かい日があったので一気に咲いたみたいです。",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "ちくわ",
            "text": "へぇーー",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "にわとり",
            "text": "でも今日寒いから、ちょっとかわいそう・・・",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "ちくわ",
            "text": "そんな一気に咲くんだー！",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "こんぶ",
            "text": "あったかい日で一気にね！",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "ちくわ",
            "text": "寒いから萎むとか？笑",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "こんぶ",
            "text": "しぼむ？",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "ちくわ",
            "text": "一旦戻りまーす的な？笑",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "にわとり",
            "text": "そちらはまだなんですね～サクラが咲くにはまだ少し早いですよね。",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "こんぶ",
            "text": "笑",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "にわとり",
            "text": "そうそう！あ、いまのなしなし！みたいな笑",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "こんぶ",
            "text": "中旬ころの卒業式には咲きますかね〜",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "ちくわ",
            "text": "4月の頭ぐらい超えたらすかね？",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "こんぶ",
            "text": "こっちは、ほんとに",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "こんぶ",
            "text": "4月超えてかなーー",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "ちくわ",
            "text": "@にわとり そうす！笑",
            "mention_to": [
                "にわとり"
            ]
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "こんぶ",
            "text": "@ちくわ そうだね！！",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "ちくわ",
            "text": "いつもそれくらいやったかな？",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "こんぶ",
            "text": "それは、面白すぎるね〜",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "ちくわ",
            "text": "ちょうど入学式ぐらいのときかな？",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "こんぶ",
            "text": "早かったので、一旦もどりまーーす！て笑",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "にわとり",
            "text": "@こんぶ @ちくわ そうですよね、４月頭とかだよなー",
            "mention_to": [
                "ちくわ",
                "こんぶ"
            ]
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "ちくわ",
            "text": "笑",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "こんぶ",
            "text": "そうなんですよ！！なのでこちらは、まだまだだと思われます",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "ちくわ",
            "text": "自分たちで調整的な笑",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "にわとり",
            "text": "こうも温度差が激しいと",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "ちくわ",
            "text": "まぁ昨日雪降ってるんでね笑",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "にわとり",
            "text": "人間も植物もこたえますね～",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "こんぶ",
            "text": "温度差で腰痛を引き起こす人が多いと",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "こんぶ",
            "text": "昨日ニュースでやってました",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "にわとり",
            "text": "おおっ！",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "ちくわ",
            "text": "笑",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "こんぶ",
            "text": "そのおかげで旦那腰痛です",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "にわとり",
            "text": "雪降りましたか？！？！",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "こんぶ",
            "text": "あ、違うかーー笑笑",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "にわとり",
            "text": "ありゃーーーー",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "こんぶ",
            "text": "雪！昨日の昼ごろから！！！",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "こんぶ",
            "text": "降ってます！",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "にわとり",
            "text": "温度差で腰痛？！？？！",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "こんぶ",
            "text": "今は雨っぽい？",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "こんぶ",
            "text": "そう！ちゃんとみてないんですが",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "にわとり",
            "text": "えーーーー！お寒い地方なんですね！",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "ちくわ",
            "text": "@こんぶ それが原因ではない",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "こんぶ",
            "text": "腰にもきちゃうんですね、、",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "こんぶ",
            "text": "あ、違うらしいです！笑",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "にわとり",
            "text": "雪かきとかをするレベル・・ではない？",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "ちくわ",
            "text": "もう座ってるのも、だるいときあります。",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "こんぶ",
            "text": "一回雪かきもしましたが！",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "ちくわ",
            "text": "そうすね！",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "こんぶ",
            "text": "毎回ほどではないですーー",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "にわとり",
            "text": "あらららー腰はつらいですよね！",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "こんぶ",
            "text": "ずっと座ってるの私むりです〜",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "ちくわ",
            "text": "今回の雪は大丈夫だと思います！",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "こんぶ",
            "text": "いいイスにかえてもらおう！笑",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "にわとり",
            "text": "@ちくわ おお！よかった！",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "こんぶ",
            "text": "ね！今回は大丈夫そう！",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "ちくわ",
            "text": "ソファーが欲しいです、笑",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "ちくわ",
            "text": "仕事場に笑",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "にわとり",
            "text": "@こんぶ 座ってるの、意外につらいですよね。",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "こんぶ",
            "text": "仕事場にもね笑",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "ちくわ",
            "text": "よく疲れたら歩いてます！",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "にわとり",
            "text": "あーーーーソファーいいなーーー！！",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "こんぶ",
            "text": "@にわとり そうなんですよ。。なので、動いてる方が得意です笑",
            "mention_to": [
                "にわとり"
            ]
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "にわとり",
            "text": "なんか、ずーっと座ってると、",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "ちくわ",
            "text": "ガンガン動きたい！",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "こんぶ",
            "text": "でも、今の、新しい企業とかは、そーいう環境もあるとか？ないとか？",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "にわとり",
            "text": "足がだめになりそうですよね。",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "こんぶ",
            "text": "足ダメになりそうですよね！！！",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "ちくわ",
            "text": "@こんぶ なんかビーズクッションとか置いてるとこあるよね",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "にわとり",
            "text": "なるほどー！！",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "こんぶ",
            "text": "ガンガン動きたい笑",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "こんぶ",
            "text": "@ちくわ やっぱりそうだよね！！",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "ちくわ",
            "text": "疲れた時はソファーに寝る",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "にわとり",
            "text": "いいですね、ガンガン働く笑",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "こんぶ",
            "text": "そーいう職場もあるのさーー",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "にわとり",
            "text": "わーーーいいですね！",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "こんぶ",
            "text": "その姿勢見習いたいです笑",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "ちくわ",
            "text": "なんかもっと働きやすい環境欲しいです笑",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "にわとり",
            "text": "昔、大学時代に、",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "にわとり",
            "text": "泊まり込みの学生がよくソファーで寝てました・・・",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "こんぶ",
            "text": "そういう企業で働くのも憧れでした笑",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "にわとり",
            "text": "うんうん、いいですねえ。",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "こんぶ",
            "text": "泊まりこみで、、",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "にわとり",
            "text": "福利厚生大事！！",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "ちくわ",
            "text": "えーー",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "ちくわ",
            "text": "泊まり込みで！？",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "こんぶ",
            "text": "大事ですよね！！！",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "こんぶ",
            "text": "働いてたってことですか？",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "にわとり",
            "text": "そうそう、実験系だったんですが、",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "こんぶ",
            "text": "あら〜〜それは大変",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "にわとり",
            "text": "夜中寝ずに実験とかしてたんで、男子とかがね。",
            "mention_to": []
        },
        {
            "utterance_id": 103,
            "interlocutor_id": "にわとり",
            "text": "ソファーで寝てましたw",
            "mention_to": []
        },
        {
            "utterance_id": 104,
            "interlocutor_id": "こんぶ",
            "text": "教える系ですか？",
            "mention_to": []
        },
        {
            "utterance_id": 105,
            "interlocutor_id": "にわとり",
            "text": "あっ、いえいえ、私が学ぶほうだったときですー！",
            "mention_to": []
        },
        {
            "utterance_id": 106,
            "interlocutor_id": "にわとり",
            "text": "なんか実験室がたまり場になってました笑",
            "mention_to": []
        }
