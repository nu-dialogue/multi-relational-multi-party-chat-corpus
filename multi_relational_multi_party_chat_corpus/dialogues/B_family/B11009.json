    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "ポテト",
            "text": "こんにちは！",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "こんぶ",
            "text": "こんにちは〜",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "ちくわ",
            "text": "こんにちは！",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "ちくわ",
            "text": "寒い時に食べたい食べ物あります？",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "こんぶ",
            "text": "私は鍋が好きだなー！",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "ポテト",
            "text": "豚汁！",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "こんぶ",
            "text": "あ！でも、ラーメンもいいな",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "こんぶ",
            "text": "豚汁！！！好きですーー",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "ちくわ",
            "text": "豚汁！！！",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "ポテト",
            "text": "@こんぶ ラーメン美味しい！",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "ちくわ",
            "text": "@こんぶ ラーメンも分かる！",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "こんぶ",
            "text": "@ちくわ あなたは？",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "ポテト",
            "text": "なべもいいですね！",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "こんぶ",
            "text": "やっぱり、みんな熱々系ですね",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "ちくわ",
            "text": "僕は、、、、、",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "ちくわ",
            "text": "コーンスープ",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "こんぶ",
            "text": "ぼくは？",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "こんぶ",
            "text": "コーンスープ笑笑笑",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "ポテト",
            "text": "缶のやつ無性に飲みたくなります・・・！",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "ちくわ",
            "text": "それか、、、",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "ちくわ",
            "text": "アイスクリーム！！！",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "こんぶ",
            "text": "そんなのんでるのみたことないけど",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "こんぶ",
            "text": "缶のね！ありますよね！",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "ポテト",
            "text": "@ちくわ アイスクリーム！",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "ちくわ",
            "text": "時々飲むんですよ！",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "こんぶ",
            "text": "寒いからこそ食べたい的な？",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "ポテト",
            "text": "暖かい部屋で食べたいやつ！",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "こんぶ",
            "text": "へぇー知らなかった！",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "ちくわ",
            "text": "そうそう！！",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "こんぶ",
            "text": "こたつでアイスみたいな！",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "ちくわ",
            "text": "あったかいからこそ",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "ちくわ",
            "text": "アイス！",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "ちくわ",
            "text": "て感じ！",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "こんぶ",
            "text": "まあ、それ言われたら分からなくもない",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "ちくわ",
            "text": "こたつでみかんみたいな言い方すな。",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "こんぶ",
            "text": "こたつでみかんも最高",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "ポテト",
            "text": "でも、うち、結構年中アイスあるかも・・・！",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "ポテト",
            "text": "みかんもおいしい！",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "ちくわ",
            "text": "おーー！",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "こんぶ",
            "text": "あー！そうですよね！！我が家もです！",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "ちくわ",
            "text": "アイス貯蓄タイプですか？",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "こんぶ",
            "text": "我が家は、年中食べれるようにってかき氷機かいました",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "ポテト",
            "text": "@ちくわ ですです！",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "ちくわ",
            "text": "なんだかんだ、スーパーカップとか",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "ちくわ",
            "text": "買っちゃいますよね",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "こんぶ",
            "text": "夏にハマってかって！",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "ポテト",
            "text": "@こんぶ ちょっとレベルが違う！",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "ちくわ",
            "text": "それな！",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "ちくわ",
            "text": "雪解け、、なんちゃら",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "こんぶ",
            "text": "でも、結局、邪魔でしまっちゃいましよ",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "ポテト",
            "text": "@ちくわ あ～わかります",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "ちくわ",
            "text": "みたいなかき氷機ね",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "こんぶ",
            "text": "@ポテト 次の出番はまた夏です",
            "mention_to": [
                "ポテト"
            ]
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "こんぶ",
            "text": "結局そうなるという",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "ちくわ",
            "text": "今年の来年は活躍してもらおう！",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "ポテト",
            "text": "やっすいざらざらのしか出てこないのはありますね！かき氷機！",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "こんぶ",
            "text": "そう！私も実家にあったのは",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "こんぶ",
            "text": "手でゴリゴリするやつ",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "ちくわ",
            "text": "@ポテト そうなんすよ！",
            "mention_to": [
                "ポテト"
            ]
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "こんぶ",
            "text": "でも、買ったのはふわふわしたやつです",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "ポテト",
            "text": "ですか？",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "ちくわ",
            "text": "自動で、サラサラしてるやつ！",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "ポテト",
            "text": "電動ですか？",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "ちくわ",
            "text": "電動！！！",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "ポテト",
            "text": "いいやつだ！",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "こんぶ",
            "text": "そうです！",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "ちくわ",
            "text": "です！",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "こんぶ",
            "text": "わた雪なんとかってやつです！",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "ちくわ",
            "text": "でも、ドンキーで安かった！",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "ちくわ",
            "text": "なんかスイッチ変えると",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "ポテト",
            "text": "きになって買えてないです！",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "こんぶ",
            "text": "意外と家でやるならぜんぜん満足てレベルです",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "ちくわ",
            "text": "粗目とか色々",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "こんぶ",
            "text": "でも、なかなか大きくて幅とるんで",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "ちくわ",
            "text": "調整できるみたいです！",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "ポテト",
            "text": "@ちくわ ドンキにあるんだ・・・！",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "こんぶ",
            "text": "ん〜結局、倉庫いきです",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "ポテト",
            "text": "@こんぶ どのくらいの大きさですか？",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "ちくわ",
            "text": "@ポテト たまたま安くなっていたんですよ！",
            "mention_to": [
                "ポテト"
            ]
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "こんぶ",
            "text": "んー炊飯器よりも大きくて",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "ちくわ",
            "text": "縦に長いぐらいですよ",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "ポテト",
            "text": "結構大きい！",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "こんぶ",
            "text": "あ！高さがです！",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "こんぶ",
            "text": "電子レンジよりも高いですよー",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "ポテト",
            "text": "高さ！",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "こんぶ",
            "text": "スリムですが",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "ちくわ",
            "text": "@ポテト お家にはかき氷機ありますか？",
            "mention_to": [
                "ポテト"
            ]
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "ポテト",
            "text": "２リットルのペットボトルくらい？？",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "ちくわ",
            "text": "あー！",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "こんぶ",
            "text": "あとは、氷をつくるのに専用の製氷器が必要です",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "ちくわ",
            "text": "そんくらいかな！",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "こんぶ",
            "text": "あーー！そうですね！！",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "ポテト",
            "text": "@ちくわ 手動のがあります！",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "こんぶ",
            "text": "@ポテト さすがです！そのくらいです！",
            "mention_to": [
                "ポテト"
            ]
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "ちくわ",
            "text": "例えがわかりやすいですね！",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "ポテト",
            "text": "専用の製氷機、あれめんどくさくないですか？？！",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "ちくわ",
            "text": "@ポテト 手動も大変ですよね！",
            "mention_to": [
                "ポテト"
            ]
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "こんぶ",
            "text": "手動もでも楽しいですよね！",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "こんぶ",
            "text": "そう！めんどくさいんですよ！",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "ちくわ",
            "text": "カップみたいなのに水いれて",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "ちくわ",
            "text": "凍らすのでめんどいですよね！",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "こんぶ",
            "text": "専用のやつっていうのがめんどくさい",
            "mention_to": []
        }
