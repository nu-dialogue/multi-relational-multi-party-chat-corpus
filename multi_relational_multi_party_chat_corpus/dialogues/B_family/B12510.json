{
    "dialogue_id": "B12510",
    "dialogue_type": "Family",
    "interlocutors": [
        "すじこ",
        "こんぶ",
        "ちくわ"
    ],
    "relationship": [
        "こんぶ",
        "ちくわ"
    ],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "すじこ",
            "text": "こんばんは",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "こんぶ",
            "text": "こんばんは！",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "ちくわ",
            "text": "こんばんは。",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "すじこ",
            "text": "お子さんはいらっしゃいますか？",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "ちくわ",
            "text": "はい！",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "こんぶ",
            "text": "いますよ！1人です！",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "ちくわ",
            "text": "一歳の娘が",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "すじこ",
            "text": "可愛い時期ですね",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "こんぶ",
            "text": "もうすぐ一歳4ヶ月なります",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "ちくわ",
            "text": "爆走してます笑",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "すじこ",
            "text": "はいはいするくらいですかね？",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "ちくわ",
            "text": "家の中を",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "ちくわ",
            "text": "走ってます！！",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "こんぶ",
            "text": "可愛い〜とこらーー！",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "こんぶ",
            "text": "の行き来です笑",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "すじこ",
            "text": "@ちくわ 走る！笑",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "ちくわ",
            "text": "色々わかってきたのか",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "こんぶ",
            "text": "一歳前に歩いて！！",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "ちくわ",
            "text": "追いかけたら走って逃げます",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "こんぶ",
            "text": "今ではドダドタ走ってます",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "すじこ",
            "text": "@こんぶ で子どもはキャッキャ笑ってるんですね",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "こんぶ",
            "text": "@すじこ ほーんとそうです",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "ちくわ",
            "text": "@すじこ 怒られてても笑ってます",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "こんぶ",
            "text": "こちらは、白目",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "すじこ",
            "text": "たまらなく可愛いときですね",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "こんぶ",
            "text": "それの顔につい、こちらもふふって笑ってしまったりして",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "ちくわ",
            "text": "でもこの歳でそんな分かる？",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "ちくわ",
            "text": "ってぐらい言ってることやってること",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "こんぶ",
            "text": "そうなんですよ！！！",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "ちくわ",
            "text": "理解してるんですよ",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "こんぶ",
            "text": "ほんとそう！！！",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "こんぶ",
            "text": "だから、うちらの間では",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "こんぶ",
            "text": "人生2回目説が出てます",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "すじこ",
            "text": "思った以上に子どもは親の顔を見てるらしいので",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "ちくわ",
            "text": "あれは",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "ちくわ",
            "text": "まじで2回目です",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "ちくわ",
            "text": "ですよねーー",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "こんぶ",
            "text": "@すじこ ほんとそう！！！",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "ちくわ",
            "text": "親が食べてるのも欲しがるし",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "すじこ",
            "text": "凄いですよね",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "ちくわ",
            "text": "前の日はあーだったのに",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "こんぶ",
            "text": "よくみてるなーー！！！てくらい真似してきます",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "ちくわ",
            "text": "次の日にはまた成長してますからね",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "ちくわ",
            "text": "@こんぶ 真似しすぎて危ない時もあるけどね",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "こんぶ",
            "text": "成長に嬉しさとでもちょっぴり寂しさもあって",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "すじこ",
            "text": "@ちくわ 子どもの成長速度に",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "すじこ",
            "text": "親がついていけないですよね",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "ちくわ",
            "text": "なんだかんだ赤ちゃんの時期かわいいからなー",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "こんぶ",
            "text": "もう、この時期は戻ってこないのかーって思うと",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "こんぶ",
            "text": "ちょっぴりセンチメンタルです",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "ちくわ",
            "text": "@すじこ ほんまそうですよ！",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "ちくわ",
            "text": "いつの間に？",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "すじこ",
            "text": "@こんぶ そうなんですよ",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "こんぶ",
            "text": "ほんと、あっという間ですよね！",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "ちくわ",
            "text": "できるようになった？って思いますもん",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "すじこ",
            "text": "貴重な時間です",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "こんぶ",
            "text": "貴重な時間。先輩ママさんにもそれ言われます",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "こんぶ",
            "text": "今の時間を大切にって！！！",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "すじこ",
            "text": "初めての子は",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "ちくわ",
            "text": "今、むにゃむにゃ話をしてくれることが多くて可愛いです。",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "すじこ",
            "text": "親としてたくさんの初めてを",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "すじこ",
            "text": "くれる子なので",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "ちくわ",
            "text": "なるほど！",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "すじこ",
            "text": "めちゃめちゃ可愛いです",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "こんぶ",
            "text": "たしかに！！！何もかも初めてで",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "こんぶ",
            "text": "どれも忘れられません",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "ちくわ",
            "text": "@すじこ お子さんがその時期はもうデレデレでした？",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "こんぶ",
            "text": "でも、つい、、こらー！もう！！て",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "すじこ",
            "text": "@ちくわ 滑舌の悪さがまたいいんですよね",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "こんぶ",
            "text": "なっちゃう自分に日々反省です",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "すじこ",
            "text": "@ちくわ デレンデレンでした",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "ちくわ",
            "text": "@すじこ 必死に話してるのが可愛いです",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "こんぶ",
            "text": "@すじこ 言い間違いとか可愛いですよね〜",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "すじこ",
            "text": "トロトロなくらい",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "こんぶ",
            "text": "そりゃ、もう、とろけすぎだ",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "こんぶ",
            "text": "女の子です？",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "すじこ",
            "text": "@こんぶ 怒っていいんです",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "ちくわ",
            "text": "そんなにデレデレなんすね！",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "すじこ",
            "text": "躾も親の役目なので",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "こんぶ",
            "text": "@すじこ ありがたきお言葉。。",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "すじこ",
            "text": "@こんぶ 女女男です",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "こんぶ",
            "text": "ステキ組み合わせ〜！！！",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "こんぶ",
            "text": "待望の男の子でした？",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "ちくわ",
            "text": "@すじこ やっぱ性別ちがうと育て方は違います？",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "すじこ",
            "text": "うちの子は代々とうもろこしが言えませんでした",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "こんぶ",
            "text": "なんていうんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "すじこ",
            "text": "とうもころしでした",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "こんぶ",
            "text": "とうもころし？",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "こんぶ",
            "text": "やっぱり、そうなんですね！！！！",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "こんぶ",
            "text": "可愛すぎる笑",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "ちくわ",
            "text": "かわいいー",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "ちくわ",
            "text": "！",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "こんぶ",
            "text": "これから先、もっと話せるようになるのが楽しみです",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "すじこ",
            "text": "僕は女が良かったです笑",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "こんぶ",
            "text": "最近はパパママいいます",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "こんぶ",
            "text": "そうなんですね！！",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "ちくわ",
            "text": "女3人がよかったんですね",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "ちくわ",
            "text": "男の子も可愛いやないすか！",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "こんぶ",
            "text": "パパはやっぱり男の子ほしいのかと！！",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "すじこ",
            "text": "男は妻がトロトロです",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "ちくわ",
            "text": "男の子ほしいすね！笑",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "ちくわ",
            "text": "奥さん側はそうなるのか！",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "こんぶ",
            "text": "だって、息子は小さい彼氏って言いますもんね",
            "mention_to": []
        },
        {
            "utterance_id": 103,
            "interlocutor_id": "すじこ",
            "text": "初めてのパパママは嬉しいですよね！",
            "mention_to": []
        },
        {
            "utterance_id": 104,
            "interlocutor_id": "ちくわ",
            "text": "それはやばいやつ。わら",
            "mention_to": []
        }
    ],
