    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "ばなな",
            "text": "おはようございます",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "たまねぎ",
            "text": "おはようございます！",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "にんじん",
            "text": "おはようございます！",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "ばなな",
            "text": "今日も暖かいですね",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "たまねぎ",
            "text": "@ばなな そうですね！",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "にんじん",
            "text": "あったかいですれ",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "にんじん",
            "text": "雨ですけどね！",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "ばなな",
            "text": "そちらは雨ですか？",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "ばなな",
            "text": "こちらは良いお天気です",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "たまねぎ",
            "text": "@ばなな 小雨ですよー",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "にんじん",
            "text": "こっちは雨です！",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "にんじん",
            "text": "羨ましい！",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "ばなな",
            "text": "お家はどの辺りですか？",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "たまねぎ",
            "text": "長野県になります！",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "にんじん",
            "text": "寒いし山しかありません",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "ばなな",
            "text": "長野県なのですね。",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "たまねぎ",
            "text": "@ばなな どちらですか？",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "ばなな",
            "text": "私は東京です",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "にんじん",
            "text": "TOKYOですかれ",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "たまねぎ",
            "text": "@ばなな 今週末行きますよー",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "にんじん",
            "text": "結婚式で！",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "ばなな",
            "text": "@たまねぎ そうなのですね",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "たまねぎ",
            "text": "従兄弟の結婚式でいきます！",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "ばなな",
            "text": "結婚式なのですね",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "たまねぎ",
            "text": "@ばなな 楽しみです！",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "にんじん",
            "text": "ワクワクです！",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "ばなな",
            "text": "楽しみですね",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "たまねぎ",
            "text": "@ばなな ぷち旅行も兼ねてなので",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "ばなな",
            "text": "それは良いですね",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "たまねぎ",
            "text": "東京バナナ以外で何かおすすめありますか？",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "ばなな",
            "text": "色々ありますね",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "たまねぎ",
            "text": "できればチョコがいいです",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "ばなな",
            "text": "なるほど",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "にんじん",
            "text": "バレンタイン終わりましたが笑",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "ばなな",
            "text": "昨日でしたね",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "たまねぎ",
            "text": "ですね！",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "ばなな",
            "text": "@にんじん チョコもらいましたか",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "にんじん",
            "text": "@ばなな もらってないです！",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "ばなな",
            "text": "それは寂しい",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "たまねぎ",
            "text": "今年は作れませんでした",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "たまねぎ",
            "text": "去年はクッキー作りました",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "たまねぎ",
            "text": "猫の形の",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "ばなな",
            "text": "@たまねぎ 忙しかったのですね",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "たまねぎ",
            "text": "そうなんです。",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "ばなな",
            "text": "猫のは可愛いですね",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "にんじん",
            "text": "気持ちだけで",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "たまねぎ",
            "text": "量産型です！",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "ばなな",
            "text": "今度、作ってもらえると良いですね",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "にんじん",
            "text": "来年期待します！",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "ばなな",
            "text": "私は買いに行きました",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "たまねぎ",
            "text": "@ばなな　おー",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "ばなな",
            "text": "昨日、渡し忘れました",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "たまねぎ",
            "text": "@ばなな えー",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "ばなな",
            "text": "何かバタバタしていて",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "たまねぎ",
            "text": "今日渡すんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "たまねぎ",
            "text": "平日ですしね",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "ばなな",
            "text": "@たまねぎ そうですね",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "たまねぎ",
            "text": "旦那さんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "ばなな",
            "text": "@たまねぎ そうです",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "ばなな",
            "text": "あと息子と",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "にんじん",
            "text": "@ばなな 喜びますね！",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "たまねぎ",
            "text": "@ばなな 息子さんおいくつですか？",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "ばなな",
            "text": "中学１年です",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "ばなな",
            "text": "今、思春期で",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "たまねぎ",
            "text": "@ばなな まだまだかわいいですね！",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "たまねぎ",
            "text": "うん",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "ばなな",
            "text": "@たまねぎ まだ可愛いです",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "ばなな",
            "text": "でも、うるさがれるので",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "たまねぎ",
            "text": "はいはい",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "ばなな",
            "text": "ついうるさく言うからかと",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "たまねぎ",
            "text": "はい",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "ばなな",
            "text": "宿題したのとか？",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "たまねぎ",
            "text": "あー",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "ばなな",
            "text": "でも、今日は一緒にテニスする予定です",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "たまねぎ",
            "text": "@ばなな 仲良しですね",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "ばなな",
            "text": "@たまねぎ テニスだけですけれど",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "たまねぎ",
            "text": "@ばなな うちは野球をやってまして",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "たまねぎ",
            "text": "一緒にキャッチボールしますが",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "ばなな",
            "text": "野球ですか",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "たまねぎ",
            "text": "力が強すぎて",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "ばなな",
            "text": "良いですね",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "たまねぎ",
            "text": "取るのが怖いです",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "たまねぎ",
            "text": "投げても届かないし",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "ばなな",
            "text": "そんなに強いのですね",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "たまねぎ",
            "text": "ピッチャーやってたので",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "ばなな",
            "text": "すごいですね",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "ばなな",
            "text": "お子さんはいくつですか？",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "にんじん",
            "text": "@ばなな 長男は18です",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "たまねぎ",
            "text": "部活は終わりましたが",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "ばなな",
            "text": "@にんじん 大きいのですね",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "たまねぎ",
            "text": "たまにやってます",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "ばなな",
            "text": "受験生ですね",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "たまねぎ",
            "text": "うちは就職になりました",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "ばなな",
            "text": "そうでしたか",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "たまねぎ",
            "text": "とりあえず一安心です",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "ばなな",
            "text": "4月から社会人ですね",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "たまねぎ",
            "text": "@ばなな そーなんです",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "にんじん",
            "text": "@ばなな 4月に新生活です",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "ばなな",
            "text": "頑張って欲しいですね",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "たまねぎ",
            "text": "@ばなな そうですね！",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "たまねぎ",
            "text": "お子さんはお一人ですか？",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "ばなな",
            "text": "そうなんです",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "たまねぎ",
            "text": "後少しで子育て終わってしまいますね",
            "mention_to": []
        },
        {
            "utterance_id": 103,
            "interlocutor_id": "ばなな",
            "text": "何かまだ長い感じですけれど",
            "mention_to": []
        }
