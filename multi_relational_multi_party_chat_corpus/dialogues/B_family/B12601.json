    ],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "くじら",
            "text": "おはようございます",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "えのき",
            "text": "おはようございます",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "てばさき",
            "text": "おはようございます",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "えのき",
            "text": "今日は朝から雨ですね",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "くじら",
            "text": "こちらは曇っています",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "えのき",
            "text": "今日はこのあと降りそうですか？",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "てばさき",
            "text": "@くじら 雨降ってないのいいですね",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "くじら",
            "text": "雨になりそうです",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "くじら",
            "text": "もう少しで",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "くじら",
            "text": "月曜になりましたね",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "えのき",
            "text": "今週はずっと天気が悪そうですよね～",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "てばさき",
            "text": "@くじら 日曜日が終わりました",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "くじら",
            "text": "@えのき そうなんですよ",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "くじら",
            "text": "月曜日はエンジンかかりません",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "てばさき",
            "text": "@くじら 憂鬱です",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "えのき",
            "text": "@てばさき さんは、毎日が日曜日では？",
            "mention_to": [
                "てばさき"
            ]
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "えのき",
            "text": "もう春休みだし…",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "てばさき",
            "text": "@えのき それはそれとして",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "くじら",
            "text": "@てばさき 春休みですね",
            "mention_to": [
                "てばさき"
            ]
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "てばさき",
            "text": "時間の経過とは時に苦痛を与えてくるものです",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "えのき",
            "text": "@てばさき 曜日感覚ある？",
            "mention_to": [
                "てばさき"
            ]
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "てばさき",
            "text": "@えのき なきにしもあらず",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "くじら",
            "text": "@てばさき 春休みは何を？",
            "mention_to": [
                "てばさき"
            ]
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "くじら",
            "text": "お出かけは？",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "てばさき",
            "text": "@くじら 資格の勉強してます",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "えのき",
            "text": "一応がんばってるよね",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "くじら",
            "text": "資格の勉強ですね",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "てばさき",
            "text": "@えのき 一応",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "くじら",
            "text": "@てばさき 暗記は得意ですか？",
            "mention_to": [
                "てばさき"
            ]
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "てばさき",
            "text": "@くじら 苦手ですね…",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "えのき",
            "text": "苦手なんだ…",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "くじら",
            "text": "私は昔は良かったな",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "えのき",
            "text": "若いころのほうがまだ覚えられましたよね",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "くじら",
            "text": "どうしたら覚えられるのか",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "てばさき",
            "text": "理解して勉強するタイプですが、理解が及ばない…",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "えのき",
            "text": "年を取ると脳の使い方がかわってくるらしいですよ",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "くじら",
            "text": "そうなんですか",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "くじら",
            "text": "それでですかね",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "てばさき",
            "text": "今そういう啓発本ありましたね",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "くじら",
            "text": "@えのき 脳トレしてます？",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "えのき",
            "text": "すごい脳の使い方　っていう本で言ってました",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "えのき",
            "text": "特にしてないですね脳トレ",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "くじら",
            "text": "そんな本が",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "えのき",
            "text": "日々生きるだけでいっぱいいっぱいです",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "くじら",
            "text": "なかなかしないです私も",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "くじら",
            "text": "何曜日が好きですか",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "えのき",
            "text": "予定が立て込んでくるとすぐてんぱってしまいます…",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "てばさき",
            "text": "@くじら 土曜日の午後が好きです",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "えのき",
            "text": "あまり曜日しばりで生きていないですね",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "くじら",
            "text": "土曜の午後ですね",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "てばさき",
            "text": "@えのき 母上そういうところある",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "くじら",
            "text": "私は金曜の夜かな",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "えのき",
            "text": "フリーダムに生きてる",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "えのき",
            "text": "金曜の夜、はなきんですね",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "てばさき",
            "text": "@くじら 金ローとか見ます？",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "くじら",
            "text": "@てばさき たまに見ます",
            "mention_to": [
                "てばさき"
            ]
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "くじら",
            "text": "@てばさき 見てますか",
            "mention_to": [
                "てばさき"
            ]
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "てばさき",
            "text": "@くじら 金ロージブリ多いですよね",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "くじら",
            "text": "確かに多い",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "えのき",
            "text": "そんなイメージある",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "てばさき",
            "text": "@くじら 私もたまに",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "くじら",
            "text": "ジブリは人気があるからかしら",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "えのき",
            "text": "視聴率が取れるもの、というとそうなりがち…",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "えのき",
            "text": "大人の事情",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "えのき",
            "text": "無難だし…",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "てばさき",
            "text": "@えのき これだから大人は",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "えのき",
            "text": "大人ですが？",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "くじら",
            "text": "ジブリで何か好きですか？",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "てばさき",
            "text": "ジブリはポニョが好きです",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "えのき",
            "text": "懐かしい…",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "くじら",
            "text": "ポニョ可愛いです",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "えのき",
            "text": "てばさんが幼稚園児のころに流行ってたよね",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "くじら",
            "text": "歌が可愛かった",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "えのき",
            "text": "園児がみんなぽにょぽにょ歌ってました",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "てばさき",
            "text": "祖父母と一緒に映画館に見に行きました",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "てばさき",
            "text": "買ってもらったいちごみるく美味しかったです",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "えのき",
            "text": "一世を風靡してました",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "くじら",
            "text": "良いですね",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "くじら",
            "text": "映画館行ってないなあ",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "えのき",
            "text": "いちごみるくのドリンク？",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "えのき",
            "text": "飴？",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "てばさき",
            "text": "@えのき 自販機の缶",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "くじら",
            "text": "なるほど",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "くじら",
            "text": "自販機の",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "えのき",
            "text": "途中でトイレにいきたくならなかった？",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "てばさき",
            "text": "@えのき 覚えておらぬ",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "くじら",
            "text": "それありますね",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "くじら",
            "text": "うちの子がそうで",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "くじら",
            "text": "ゆっくり見られなかった",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "えのき",
            "text": "小さい子、トイレ近いですよね",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "てばさき",
            "text": "まあ映画館はあまり飲食するものではないと思います",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "くじら",
            "text": "ポップコーンもひっくり返ったことが",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "てばさき",
            "text": "いちごみるくも映画館はいる前だったし",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "えのき",
            "text": "見る前に飲むとか怖い",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "てばさき",
            "text": "@くじら 事故じゃないですか",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "えのき",
            "text": "うちも、こどもたちとアリエッティを見に行った時に",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "くじら",
            "text": "そう、たいへんでした",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "えのき",
            "text": "入れ替わり立ち代わりトイレに立たれ…",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "くじら",
            "text": "それはそれは",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "てばさき",
            "text": "@えのき その節はどうも…",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "えのき",
            "text": "内容ほぼ見られなかったですね…",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "くじら",
            "text": "話がさっぱりわからない",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "えのき",
            "text": "雰囲気だけ",
            "mention_to": []
        },
        {
            "utterance_id": 103,
            "interlocutor_id": "てばさき",
            "text": "@えのき 私は覚えてます！",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 104,
            "interlocutor_id": "えのき",
            "text": "子供は気楽でいいなあ",
            "mention_to": []
        },
        {
            "utterance_id": 105,
            "interlocutor_id": "てばさき",
            "text": "アリエッティのお父さんイケおじだった",
            "mention_to": []
        },
        {
            "utterance_id": 106,
            "interlocutor_id": "えのき",
            "text": "そんなところを…",
            "mention_to": []
        }
