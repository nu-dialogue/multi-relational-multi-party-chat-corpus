    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "ポテト",
            "text": "おはようございます！",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "たまねぎ",
            "text": "おはようございます！",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "にんじん",
            "text": "おはようございます！",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "ポテト",
            "text": "ひなまつりって、何かやる予定ありますか？",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "ポテト",
            "text": "恵方巻みたくイベント館が薄くて・・・",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "にんじん",
            "text": "@ポテト そういえばそうですね！",
            "mention_to": [
                "ポテト"
            ]
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "たまねぎ",
            "text": "毎年ちらし寿司だったかな？、",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "にんじん",
            "text": "一応やります！",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "ポテト",
            "text": "ちらし寿司！",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "たまねぎ",
            "text": "それか手巻きかな？",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "たまねぎ",
            "text": "焼肉でもいいよねー",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "ポテト",
            "text": "あ～～～手巻きもいい",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "ポテト",
            "text": "焼き・・・肉・・・？",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "にんじん",
            "text": "ひな祭り関係ない笑",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "たまねぎ",
            "text": "お祝いだから！",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "ポテト",
            "text": "食べたいモノ言ってる・・・？",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "たまねぎ",
            "text": "え？ばれた？",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "にんじん",
            "text": "リクエスト聞いてない！",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "ポテト",
            "text": "@たまねぎ 急なリクエスト笑",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "にんじん",
            "text": "ここぞとばかりに",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "たまねぎ",
            "text": "最近食べてないなーって",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "ポテト",
            "text": "お家焼肉、気軽に出来ていいですよね・・・！",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "にんじん",
            "text": "どうやら今年のひな祭りメニューはきまりそうです",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "たまねぎ",
            "text": "楽ちんですしね！",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "ポテト",
            "text": "焼肉かな・・・",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "にんじん",
            "text": "チラシと焼肉だな",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "たまねぎ",
            "text": "やったーー！",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "ポテト",
            "text": "@にんじん 両方？！",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "たまねぎ",
            "text": "@ポテト ありがとうございます！",
            "mention_to": [
                "ポテト"
            ]
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "ポテト",
            "text": "ふとっぱら・・・！",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "にんじん",
            "text": "焼肉だけだとひな祭り感ゼロ",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "ポテト",
            "text": "@たまねぎ やばい！わたしのお陰みたいにいわないで！ください！笑",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "たまねぎ",
            "text": "あざーす！",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "ポテト",
            "text": "私焼肉言ってない笑",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "たまねぎ",
            "text": "いつ言おうか悩んでたんですよー",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "ポテト",
            "text": "でも、焼肉美味しい・・・",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "ポテト",
            "text": "ずっと狙っていた・・・この時を・・・",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "たまねぎ",
            "text": "チャンスは逃さない！",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "ポテト",
            "text": "@たまねぎ たとえひな祭りとかんけいなくとも！",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "たまねぎ",
            "text": "はい！！！",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "たまねぎ",
            "text": "祭りですので！",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "ポテト",
            "text": "@たまねぎ 素直！",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "にんじん",
            "text": "いや、わかるけども！",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "ポテト",
            "text": "ん～～～～～～～たしかにぃ？？？？？？？",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "ポテト",
            "text": "怪訝な顔",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "にんじん",
            "text": "本当に食べること好きで…",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "たまねぎ",
            "text": "いいことだ！",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "ポテト",
            "text": "@にんじん い・・・いいことですよ・・・たぶん",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "にんじん",
            "text": "好き嫌いないからありがたいですが",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "ポテト",
            "text": "本人言っちゃダメ！笑",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "たまねぎ",
            "text": "アボカドはやめてね！",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "ポテト",
            "text": "@にんじん あ！それはいいですね！",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "ポテト",
            "text": "あれ、アボカドだめだった",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "にんじん",
            "text": "作りがいはあります！",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "たまねぎ",
            "text": "アボカド謎",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "たまねぎ",
            "text": "嫌いじゃないけど謎",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "ポテト",
            "text": "@たまねぎ 謎？！",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "にんじん",
            "text": "それはわかる！",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "ポテト",
            "text": "味がです？",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "にんじん",
            "text": "味がだよね？",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "たまねぎ",
            "text": "そーそー",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "ポテト",
            "text": "あ、味かぁ・・・言われてみれば・・・？",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "にんじん",
            "text": "どのポジションで食べたらいいかわからないよねー",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "たまねぎ",
            "text": "野菜っぽくないし",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "ポテト",
            "text": "@にんじん ポジション？！",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "ポテト",
            "text": "あやつ野菜では・・・",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "にんじん",
            "text": "とにかく謎なのは賛成ですね！",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "たまねぎ",
            "text": "@にんじん 体にいいのにねー",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "ポテト",
            "text": "森のバターともいわれるくらい栄養価豊富ですからね・・・！",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "たまねぎ",
            "text": "そこ！",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "たまねぎ",
            "text": "バターぽくないし！みたいな！",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "にんじん",
            "text": "屁理屈笑",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "ポテト",
            "text": "食感似てません？",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "たまねぎ",
            "text": "バター食べません！",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "ポテト",
            "text": "直接バター食べたことないですけど・・・",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "たまねぎ",
            "text": "塗ります！",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "たまねぎ",
            "text": "溶かします！",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "にんじん",
            "text": "だから屁理屈！",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "ポテト",
            "text": "@にんじん 笑",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "にんじん",
            "text": "はなしそれてるーー！やめてーーー笑",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "たまねぎ",
            "text": "戻しましょ笑",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "たまねぎ",
            "text": "恋バナします？",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "にんじん",
            "text": "大爆笑！！",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "ポテト",
            "text": "好きだな～笑",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "ポテト",
            "text": "恋バナ！",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "たまねぎ",
            "text": "気持ちは高校生笑",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "にんじん",
            "text": "でも、良くするよね！うちらも！",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "ポテト",
            "text": "@にんじん え？！",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "にんじん",
            "text": "答え合わせてきな？",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "にんじん",
            "text": "あの時どう思ってたのー？とか",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "ポテト",
            "text": "答え合わせ？？？？？？",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "たまねぎ",
            "text": "そーそー",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "ポテト",
            "text": "あーーーーー！",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "にんじん",
            "text": "いつから好きだったか？とか？",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "たまねぎ",
            "text": "初心にかえるためです！",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "たまねぎ",
            "text": "再確認というか！",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "ポテト",
            "text": "@にんじん どきどきするやつ！",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "にんじん",
            "text": "ふふふ",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "ポテト",
            "text": "え、良いですね",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "にんじん",
            "text": "ほっこりしますよねー",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "ポテト",
            "text": "うちもやってみようかな",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "にんじん",
            "text": "だんだん記憶あいまいですが笑",
            "mention_to": []
