    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "ぺんぎん",
            "text": "こんにちは！",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "たまご",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "つみれ",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "ぺんぎん",
            "text": "今日は寒いですね",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "ぺんぎん",
            "text": "体調とか大丈夫ですか？",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "つみれ",
            "text": "寒いですね",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "つみれ",
            "text": "とっても風が冷たいです",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "たまご",
            "text": "寒いです",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "つみれ",
            "text": "体調は今のところ大丈夫です",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "たまご",
            "text": "今んとこ大丈夫です",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "つみれ",
            "text": "少し前に胃腸炎にはなりました",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "ぺんぎん",
            "text": "花粉症とかは大丈夫ですか？",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "たまご",
            "text": "花粉症はやばいです",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "つみれ",
            "text": "花粉症は大丈夫なんです",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "たまご",
            "text": "鼻水が止まらないです",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "ぺんぎん",
            "text": "@つみれ 胃腸炎は大丈夫ですか？はやってますね",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "ぺんぎん",
            "text": "@たまご 花粉症なんですね",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "つみれ",
            "text": "@ぺんぎん 辛かったです",
            "mention_to": [
                "ぺんぎん"
            ]
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "つみれ",
            "text": "夜中の12時からスタートして",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "ぺんぎん",
            "text": "@つみれ 知り合いもなりました！",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "たまご",
            "text": "目の痒みは無くなってきたんですが",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "たまご",
            "text": "@ぺんぎん そうなんです",
            "mention_to": [
                "ぺんぎん"
            ]
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "つみれ",
            "text": "1時間おきにトイレに駆け込みむ状態です",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "たまご",
            "text": "きつー",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "ぺんぎん",
            "text": "@つみれ 夜中からはきつい！",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "つみれ",
            "text": "熱も高かったので",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "つみれ",
            "text": "寒気もひどくて",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "たまご",
            "text": "最悪だ",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "ぺんぎん",
            "text": "@つみれ 熱もでますよね",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "ぺんぎん",
            "text": "@つみれ 動けないですよね",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "たまご",
            "text": "動けないの大変",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "つみれ",
            "text": "4日くらいだめでした",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "ぺんぎん",
            "text": "@たまご 花粉症も辛いですね",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "たまご",
            "text": "ならないように気をつけるしかないですね",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "つみれ",
            "text": "起きたくても起きれない感じで",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "たまご",
            "text": "ながいー",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "たまご",
            "text": "花粉も結構きついです",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "つみれ",
            "text": "花粉症はもっと長い期間だから",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "ぺんぎん",
            "text": "体調は本当に大切です",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "たまご",
            "text": "外出るとすぐ鼻水が",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "つみれ",
            "text": "本当大変",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "ぺんぎん",
            "text": "こないだコロナになりました",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "つみれ",
            "text": "健康第一って思いますね",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "たまご",
            "text": "えー",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "つみれ",
            "text": "コロナ！",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "たまご",
            "text": "大変でしたね",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "つみれ",
            "text": "きつかったですか？",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "ぺんぎん",
            "text": "家族がなってうった！",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "ぺんぎん",
            "text": "つらかったです",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "つみれ",
            "text": "ああ、どうしてもそうなりますね",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "たまご",
            "text": "そうなんですね",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "ぺんぎん",
            "text": "もらってくるから",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "たまご",
            "text": "しょうがないですね",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "つみれ",
            "text": "@ぺんぎん 自宅療養でした？",
            "mention_to": [
                "ぺんぎん"
            ]
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "たまご",
            "text": "熱あがりました？",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "ぺんぎん",
            "text": "熱はそうでもなかったです",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "ぺんぎん",
            "text": "でも味覚なくなりました",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "つみれ",
            "text": "私も一昨年なったんですけど",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "ぺんぎん",
            "text": "息子は熱が40度！",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "つみれ",
            "text": "嗅覚がやられて",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "ぺんぎん",
            "text": "何食べてもわからない！",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "つみれ",
            "text": "息子さん40度かぁ",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "ぺんぎん",
            "text": "やばかったです！",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "ぺんぎん",
            "text": "@たまご コロナは大丈夫でした？",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "つみれ",
            "text": "たまごもかかりました",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "つみれ",
            "text": "寮にいたので何もしてあげられなくて",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "ぺんぎん",
            "text": "えー！心配だぁ",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "ぺんぎん",
            "text": "@たまご どうしたんですか？",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "たまご",
            "text": "一回なりました",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "ぺんぎん",
            "text": "大変だったでしょう",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "つみれ",
            "text": "@たまご 自転車で病院行ったんだよね？",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "たまご",
            "text": "そうです",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "ぺんぎん",
            "text": "@たまご 自転車で？",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "たまご",
            "text": "39℃以上で",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "たまご",
            "text": "自転車漕いで行きました",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "ぺんぎん",
            "text": "@たまご そ、それはすごい",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "ぺんぎん",
            "text": "根性あるな",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "つみれ",
            "text": "離れて暮らすとそういう時に困ります",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "たまご",
            "text": "頑張りました",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "ぺんぎん",
            "text": "@つみれ 離れて暮らしていて病気は辛いですよね",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "つみれ",
            "text": "寮だから行くにいけず",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "ぺんぎん",
            "text": "@つみれ 心配でしたね",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "ぺんぎん",
            "text": "@たまご よく頑張った！強くなったね！",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "たまご",
            "text": "自分で行けたんで良かったです",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "つみれ",
            "text": "@ぺんぎん はい・・・歯痒いですね",
            "mention_to": [
                "ぺんぎん"
            ]
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "つみれ",
            "text": "本当、そうやって成長していくんですね",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "ぺんぎん",
            "text": "@つみれ してあげれないのが辛い",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "つみれ",
            "text": "@ぺんぎん 母としては辛いの一言",
            "mention_to": [
                "ぺんぎん"
            ]
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "ぺんぎん",
            "text": "そうなんですよね、きっと",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "ぺんぎん",
            "text": "そうやって大人になるんだなぁ",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "ぺんぎん",
            "text": "@たまご でもよかったですね、よくなって！",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "つみれ",
            "text": "@ぺんぎん 親もそうやって子離れしていかないと",
            "mention_to": [
                "ぺんぎん"
            ]
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "たまご",
            "text": "よかったです",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "つみれ",
            "text": "重症化しなくてよかったです",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "ぺんぎん",
            "text": "子離れねー、本当そうかも",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "たまご",
            "text": "薬飲んだらすぐ治りました",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "ぺんぎん",
            "text": "@つみれ ホントですね、そこが一番心配です",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "つみれ",
            "text": "寮母さんもいてくれたので",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "ぺんぎん",
            "text": "@たまご 意外に体力あるんですね",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "たまご",
            "text": "頭痛と熱以外とくになかったので",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "ぺんぎん",
            "text": "@たまご 何かスポーツとかはやってた？",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "たまご",
            "text": "寝ると基本治ります",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "たまご",
            "text": "テニスくらいです",
            "mention_to": []
        }
