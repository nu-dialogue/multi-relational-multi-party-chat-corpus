    ],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "ポテト",
            "text": "おはようございます",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "ちくわ",
            "text": "おはようございます",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "こんぶ",
            "text": "おはようございます",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "ポテト",
            "text": "今日も暖かそうですね～",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "こんぶ",
            "text": "そちらは晴れてますか？",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "こんぶ",
            "text": "私の住んでいるところは",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "こんぶ",
            "text": "雨です",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "ポテト",
            "text": "晴れています！",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "ちくわ",
            "text": "晴れているんですね！",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "ちくわ",
            "text": "雨だと気持ちが下がってしまいますね！",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "こんぶ",
            "text": "@ポテト 晴れなんですね！",
            "mention_to": [
                "ポテト"
            ]
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "ポテト",
            "text": "@こんぶ どちらの方の地域ですか？",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "ポテト",
            "text": "雨は本当に気分が下がっちゃいますね～",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "こんぶ",
            "text": "@ちくわ ここの地域はなんていうんだっけ？",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "こんぶ",
            "text": "そうなんですよ〜",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "こんぶ",
            "text": "気分が下がります",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "ちくわ",
            "text": "@こんぶ 長野県です！",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "ポテト",
            "text": "長野！私は千葉です！雪が凄そう・・・",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "ちくわ",
            "text": "@ポテト 長野県です！",
            "mention_to": [
                "ポテト"
            ]
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "こんぶ",
            "text": "千葉なんですね！",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "ちくわ",
            "text": "@ポテト 雪降りましたか？",
            "mention_to": [
                "ポテト"
            ]
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "こんぶ",
            "text": "それは、あたたかそうです",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "ポテト",
            "text": "降りました！",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "ポテト",
            "text": "久々雪かきレベルでした！",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "こんぶ",
            "text": "雪かきはしましたか？",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "こんぶ",
            "text": "あ！やっぱり！！",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "ポテト",
            "text": "旦那さんがしてくれました！",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "ポテト",
            "text": "雪かき！",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "ちくわ",
            "text": "@ポテト 長野もめちゃくちゃ降りました！",
            "mention_to": [
                "ポテト"
            ]
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "こんぶ",
            "text": "そこはやっぱり",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "こんぶ",
            "text": "旦那さんの出番ですよね！",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "ポテト",
            "text": "長野は、規模が違いそうです・・・",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "ちくわ",
            "text": "@ポテト 私が頑張りました！",
            "mention_to": [
                "ポテト"
            ]
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "こんぶ",
            "text": "私たちのところも雪がすごくて",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "こんぶ",
            "text": "旦那頑張りました！",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "ポテト",
            "text": "@ちくわ さんが頑張りましたか？",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "ちくわ",
            "text": "@ポテト 頑張りましたよ！！",
            "mention_to": [
                "ポテト"
            ]
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "こんぶ",
            "text": "はい！！！",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "ポテト",
            "text": "えらい・・・！",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "こんぶ",
            "text": "@ポテト 旦那さんは",
            "mention_to": [
                "ポテト"
            ]
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "こんぶ",
            "text": "体力には自信ありますか？",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "こんぶ",
            "text": "私の旦那はジムに通ったり",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "ポテト",
            "text": "雪かき腰痛くなりますよね・・・腰痛めて帰ってきました！",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "こんぶ",
            "text": "頼りになります！",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "こんぶ",
            "text": "たしかに",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "こんぶ",
            "text": "腰にきますね。",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "ちくわ",
            "text": "@ポテト 雪かきの道具はあったんですか？",
            "mention_to": [
                "ポテト"
            ]
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "ポテト",
            "text": "@こんぶ ジム通いは結構本格的！",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "ポテト",
            "text": "スコップ？シャベル？はかろうじて！",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "こんぶ",
            "text": "@ポテト そうなんですよ〜！",
            "mention_to": [
                "ポテト"
            ]
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "こんぶ",
            "text": "そこはしっかり用意されてるんですね！",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "ちくわ",
            "text": "@ポテト 降ることなければしっかりした道具はないですよね！",
            "mention_to": [
                "ポテト"
            ]
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "こんぶ",
            "text": "降ってからの用意だと大変ですもんね。",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "ポテト",
            "text": "結構前に凄い雪が降った時に買いました！",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "こんぶ",
            "text": "さすがです！",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "ポテト",
            "text": "そう！売り場になくなっちゃうんですよ！",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "ポテト",
            "text": "みんな考えることは同じ・・・",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "こんぶ",
            "text": "みんなやっぱりその時に必要になりますもんね！",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "ちくわ",
            "text": "@こんぶ こっちは夜中に雪かきしたよね！",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "ポテト",
            "text": "夜中？！",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "こんぶ",
            "text": "うんうんそうですね！",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "こんぶ",
            "text": "そう！夜中に！",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "ポテト",
            "text": "寒くないです？？？？？",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "こんぶ",
            "text": "ずっと雪が降り続いてて",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "ちくわ",
            "text": "@ポテト 子供が寝てからです！",
            "mention_to": [
                "ポテト"
            ]
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "こんぶ",
            "text": "寒いんですけど",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "こんぶ",
            "text": "動いてると汗をかきます！",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "ポテト",
            "text": "@ちくわ 筋肉があるから大丈夫なのかな・・・",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "ちくわ",
            "text": "雪かきの量が多いから汗かきますよ！",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "ポテト",
            "text": "むしろ暑い感じですね？！",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "こんぶ",
            "text": "@ポテト 筋肉布団てやつです！！！",
            "mention_to": [
                "ポテト"
            ]
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "ちくわ",
            "text": "@ポテト 筋肉があっても大変でしたよ！",
            "mention_to": [
                "ポテト"
            ]
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "ポテト",
            "text": "大変だった！",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "こんぶ",
            "text": "大変だったんだね",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "ちくわ",
            "text": "冬でも半袖の時あります！",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "ポテト",
            "text": "@こんぶ いたわるしかないですね・・・",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "ちくわ",
            "text": "@こんぶ そんなに降ることないから久々だとね！",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "ポテト",
            "text": "あれ、半袖！",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "ちくわ",
            "text": "家が薪ストーブなんですよ！",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "こんぶ",
            "text": "@ポテト いたわるように努力します！",
            "mention_to": [
                "ポテト"
            ]
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "ちくわ",
            "text": "暖かいですよ！",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "ポテト",
            "text": "うちの旦那も結構半そでなんですが、男性あるあるなんですか・・・？",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "こんぶ",
            "text": "@ちくわ ほんとにあったかいよね",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "こんぶ",
            "text": "たしかに！",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "ポテト",
            "text": "薪ストーブ素敵！",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "こんぶ",
            "text": "男性あるあるなのかしら！",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "ちくわ",
            "text": "@ポテト 半袖の方が楽なんですよ！",
            "mention_to": [
                "ポテト"
            ]
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "こんぶ",
            "text": "楽さの問題なんだ",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "ポテト",
            "text": "楽なんですね？！",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "こんぶ",
            "text": "それでも、女子はやっぱり",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "こんぶ",
            "text": "あったかさを求めてしまう",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "ポテト",
            "text": "分かります",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "ちくわ",
            "text": "たくさん着ると邪魔なんですよ",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "こんぶ",
            "text": "動きにくいってことか！",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "ポテト",
            "text": "邪魔ときた・・・！",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "ポテト",
            "text": "己の筋肉があれば・・・",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "こんぶ",
            "text": "それでも寒さには勝てない",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "ちくわ",
            "text": "でも薪ストーブのおかげでありがたい！",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "ちくわ",
            "text": "外は寒いんですけどね",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "ポテト",
            "text": "えーいいですねぇ",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "こんぶ",
            "text": "外は寒いんだ",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "こんぶ",
            "text": "長野県では",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "ポテト",
            "text": "寒暖差凄そうですね",
            "mention_to": []
        },
        {
            "utterance_id": 103,
            "interlocutor_id": "ちくわ",
            "text": "@ポテト 千葉も寒いですよねー",
            "mention_to": [
                "ポテト"
            ]
        },
        {
            "utterance_id": 104,
            "interlocutor_id": "こんぶ",
            "text": "薪ストーブの家が流行ってます",
            "mention_to": []
        },
        {
            "utterance_id": 105,
            "interlocutor_id": "ポテト",
            "text": "流行り！？",
            "mention_to": []
        }
