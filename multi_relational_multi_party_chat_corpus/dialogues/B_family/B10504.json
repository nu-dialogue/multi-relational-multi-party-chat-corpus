    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "りんご",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "えのき",
            "text": "こんにちは！",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "てばさき",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "りんご",
            "text": "お花見とかいきますか？",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "てばさき",
            "text": "お花見ですか…",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "りんご",
            "text": "花粉症が？",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "てばさき",
            "text": "地元の小学校も桜すごいのでそれで済ませますかね",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "えのき",
            "text": "実家のほうがお花見の名所なんですが",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "りんご",
            "text": "うちの方も権現堂とかあります",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "えのき",
            "text": "菜の花畑とセットで・・・てばさんは菜の花花粉症なんですよね",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "てばさき",
            "text": "写真で見ると綺麗なんですよ、黄色と桃色のコントラスト",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "りんご",
            "text": "な、なんとお花見辛い",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "えのき",
            "text": "権現堂とか、名所ですよね。",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "えのき",
            "text": "すごい混みそうですよね",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "りんご",
            "text": "そうなんですよ、混みますね",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "てばさき",
            "text": "東京の方の桜並木も人混みがすごそう",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "りんご",
            "text": "すごそうですね",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "えのき",
            "text": "東京は・・・すごいなんてものではないのでは",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "りんご",
            "text": "コロナも解禁で多分すごいと思います",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "えのき",
            "text": "そもそも普段から混んでるし",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "てばさき",
            "text": "外国人観光客も多そうですしねぇ",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "りんご",
            "text": "人多いですよね、人に疲れちゃう",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "えのき",
            "text": "そこまで近づくのも大変そう",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "りんご",
            "text": "外国人観光バスとかすごいですよ",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "えのき",
            "text": "でもあそこ、遊歩道的にはいいですよね。",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "てばさき",
            "text": "お花見は見上げるのもいいですが、花びらの絨毯の上を歩くの楽しいです",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "えのき",
            "text": "情緒的",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "りんご",
            "text": "すごい綺麗ですよね",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "えのき",
            "text": "アジサイとか、彼岸花の時期もきれいですよね",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "りんご",
            "text": "一面赤の絨毯みたくなりますね",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "えのき",
            "text": "写真撮ってる人も多いですよね",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "りんご",
            "text": "紫陽花も最近は白が多いですよね",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "てばさき",
            "text": "なんか怖いですね…一面の彼岸花",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "りんご",
            "text": "ちょっとねー",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "えのき",
            "text": "イメージ的には怖い感じだけど・・・",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "てばさき",
            "text": "青い彼岸花紛れてないかな？",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "えのき",
            "text": "そんなのあるの？",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "りんご",
            "text": "えー青？みたことないです",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "てばさき",
            "text": "青いばらがあるなら作れるかなって",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "えのき",
            "text": "もしや、鬼滅の刃？",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "てばさき",
            "text": "そう",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "えのき",
            "text": "私鬼滅読んだことないマンです",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "りんご",
            "text": "すごい！青なんて！",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "てばさき",
            "text": "私もあまり詳しくないけど、青い彼岸花が鍵らしい",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "りんご",
            "text": "私もあんまりみてない",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "えのき",
            "text": "@りんご 漫画は読まない派ですか？",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "りんご",
            "text": "息子はハマってグッズかってます",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "えのき",
            "text": "へー",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "りんご",
            "text": "@えのき あんまり読まないですね、読んでますか？",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "えのき",
            "text": "私はどっちかというと、ドラマをよく見てます",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "えのき",
            "text": "@りんご さんは、ドラマ見ますか？",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "てばさき",
            "text": "ドラマはz軸があるからなぁ",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "りんご",
            "text": "最近は不適切みてます",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "えのき",
            "text": "いま流行ってるやつですね！",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "りんご",
            "text": "@てばさき テレビみます？",
            "mention_to": [
                "てばさき"
            ]
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "えのき",
            "text": "サブスクに来たらまとめて見ようと思ってます・・・いつか・・・",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "てばさき",
            "text": "@りんご テレビはあまり見ないですねぇ",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "てばさき",
            "text": "年末にエンタの神様とか見るくらいです",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "りんご",
            "text": "不適切は昭和ですよー",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "えのき",
            "text": "@てばさき 普段何を見てるの？",
            "mention_to": [
                "てばさき"
            ]
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "りんご",
            "text": "@てばさき 最近はみんなネットだからテレビみないよね",
            "mention_to": [
                "てばさき"
            ]
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "てばさき",
            "text": "@えのき 今は…youtube?",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "えのき",
            "text": "サブスク動画しか見ないですね・・・",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "えのき",
            "text": "YouTubeは音楽再生専用に・・・",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "りんご",
            "text": "私もそんな感じですね",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "えのき",
            "text": "@てばさき YouTubeで何を見てるの？",
            "mention_to": [
                "てばさき"
            ]
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "てばさき",
            "text": "@えのき 音楽再生ももちろんだけどゲーム実況とかアニメの切り抜きとか面白いのいっぱいあるよ",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "えのき",
            "text": "若い人は",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "りんご",
            "text": "そうなりますよね",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "りんご",
            "text": "時間足りなそう",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "えのき",
            "text": "へー",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "てばさき",
            "text": "最近study with meというジャンルを知りました",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "えのき",
            "text": "どんなの？",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "りんご",
            "text": "知りたいです！",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "てばさき",
            "text": "二、三時間勉強している動画で、視聴者も一緒に勉強している感じです",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "えのき",
            "text": "あー",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "りんご",
            "text": "へぇーそんなのあるんだ",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "えのき",
            "text": "なんかそれ、リアルでもやってた気がする。一緒に作業しましょう的な・・・",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "てばさき",
            "text": "今日は数学の勉強しますから、視聴者の皆さんも一緒にやりましょうみたいな",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "えのき",
            "text": "それのオンライン生配信版ですね",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "えのき",
            "text": "勉強の動機づけにいいね",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "てばさき",
            "text": "生配信のアーカイブとかも混ざってますね",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "りんご",
            "text": "生配信ね",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "えのき",
            "text": "それで、勉強ははかどっていますか？母より",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "てばさき",
            "text": "好きなアニメのstudy with meは見つけた",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "りんご",
            "text": "お母さん的にはそこですね",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "えのき",
            "text": "ですよね",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "えのき",
            "text": "アニメを見ながらやるの？",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "てばさき",
            "text": "キャラクターと一緒にお勉強ができるのです",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "えのき",
            "text": "へー",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "えのき",
            "text": "それで、勉強ははかどっていますか？2回目",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "りんご",
            "text": "@てばさき それはすごいですね、やる気出そう",
            "mention_to": [
                "てばさき"
            ]
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "りんご",
            "text": "お母さん厳しい！",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "てばさき",
            "text": "あと、日商簿記2級のstudy with meあったから一緒にやろうかなとは思ってるだけ",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "えのき",
            "text": "てばさん、家で見るといつもお昼寝してるから・・・お母さん心配してる",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "りんご",
            "text": "@てばさき 頑張ってー",
            "mention_to": [
                "てばさき"
            ]
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "えのき",
            "text": "簿記2級がんばって！",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "りんご",
            "text": "寝る子は育つ！",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "てばさき",
            "text": "も、もう本試験を模試にするような失態は犯したくないとは思ってる",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "えのき",
            "text": "もう育ちすぎ・・・",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "りんご",
            "text": "@てばさき それは？",
            "mention_to": [
                "てばさき"
            ]
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "てばさき",
            "text": "こんなに寝てるのに妹に身長負けてるのなぁぜなぁぜ？",
            "mention_to": []
        }
