    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "ばなな",
            "text": "こんばんは",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "こんぶ",
            "text": "こんばんは！",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "ちくわ",
            "text": "こんばんは〜",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "ばなな",
            "text": "お休みは何をして過ごしますか？",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "ちくわ",
            "text": "ジムに行って",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "ちくわ",
            "text": "体作りしてます",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "ばなな",
            "text": "すごいです",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "こんぶ",
            "text": "ジム楽しいですよ",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "ちくわ",
            "text": "歳をとると",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "ちくわ",
            "text": "身体が弱りますから",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "こんぶ",
            "text": "今から体力を",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "こんぶ",
            "text": "つけておかないとね",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "ばなな",
            "text": "からだ作りは大事ですね",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "こんぶ",
            "text": "私も見習わないと",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "こんぶ",
            "text": "私は",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "こんぶ",
            "text": "子どもがいるので",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "こんぶ",
            "text": "子どもと一緒に遊びます",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "ちくわ",
            "text": "はい。歩いたりするのとても良いですよ。",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "ちくわ",
            "text": "子どもと遊ぶのも楽しいけどね",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "ばなな",
            "text": "お子さんはまだ小さいですか？",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "こんぶ",
            "text": "はい！",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "こんぶ",
            "text": "1人います",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "こんぶ",
            "text": "一歳になりました",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "こんぶ",
            "text": "歩くのがあっという間に",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "こんぶ",
            "text": "上手になりましたよ！",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "ちくわ",
            "text": "歩き出すと大変になりそうです",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "こんぶ",
            "text": "そうそう",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "こんぶ",
            "text": "追いかけるのが大変です",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "ばなな",
            "text": "ほんとですね",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "こんぶ",
            "text": "ばななさんも",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "こんぶ",
            "text": "お子さんいらっしゃいますか？",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "ばなな",
            "text": "家も男子でたいへんでした",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "ちくわ",
            "text": "男の子なんですね",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "ばなな",
            "text": "今は中学生です",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "こんぶ",
            "text": "何人ですか？",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "ばなな",
            "text": "1人です",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "ちくわ",
            "text": "勉強、部活動に忙しそうですね！",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "こんぶ",
            "text": "今は食べ盛りですかね？",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "ばなな",
            "text": "中学生になると忙しいです",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "ばなな",
            "text": "まだそんなには",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "ばなな",
            "text": "これからですかね？",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "ちくわ",
            "text": "これからたくさん食べるようになりますね！",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "こんぶ",
            "text": "そうですよね〜",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "こんぶ",
            "text": "これから、さらに成長期に",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "ちくわ",
            "text": "思春期とかはこられましたか？",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "こんぶ",
            "text": "向かっていきますからね！",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "ばなな",
            "text": "まさに思春期です",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "こんぶ",
            "text": "私の子どもは",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "こんぶ",
            "text": "女の子なので",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "こんぶ",
            "text": "男の子のお母さん",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "こんぶ",
            "text": "尊敬します",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "こんぶ",
            "text": "男の子の気持ち",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "ちくわ",
            "text": "思春期だと",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "こんぶ",
            "text": "難しそうです",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "ちくわ",
            "text": "色々考えますよね",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "ばなな",
            "text": "女の子なのですね",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "ちくわ",
            "text": "女の子も可愛いですよね",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "ばなな",
            "text": "思春期、難しいですね",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "こんぶ",
            "text": "どんな関わり方をしてますか？",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "こんぶ",
            "text": "女の子",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "こんぶ",
            "text": "パパにそっくりで",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "こんぶ",
            "text": "とっても可愛いです",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "ばなな",
            "text": "女の子はまたちがいそうですね",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "ちくわ",
            "text": "女の子はお父さんに",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "こんぶ",
            "text": "そうですよね、",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "ちくわ",
            "text": "似ると幸せになるっていいますよね",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "ばなな",
            "text": "そう聞きますね",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "こんぶ",
            "text": "それよく色んな人に",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "こんぶ",
            "text": "いわれます！",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "こんぶ",
            "text": "やっぱりそうなんですね！",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "ちくわ",
            "text": "似るといいなあ",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "ちくわ",
            "text": "楽しみです",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "ばなな",
            "text": "仲良しな感じですね",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "ばなな",
            "text": "男の子はなかなか難しいです",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "ちくわ",
            "text": "でも男の子は",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "ちくわ",
            "text": "優しいから",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "ちくわ",
            "text": "いつでも寄り添ってくれますよ",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "こんぶ",
            "text": "きっとお母さん思いですよ",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "ばなな",
            "text": "そうだと良いです",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "こんぶ",
            "text": "男の子って、言葉でうまく",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "こんぶ",
            "text": "表現できないイメージが",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "こんぶ",
            "text": "あります！",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "ばなな",
            "text": "今はうるさいとかしか言われないので",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "ちくわ",
            "text": "きっと照れ屋な部分もありますよね！",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "ばなな",
            "text": "確かに言葉が",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "ばなな",
            "text": "なるほど",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "ちくわ",
            "text": "大人になったらサプライズプレゼント",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "ちくわ",
            "text": "とかありそうですよね！",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "ばなな",
            "text": "欲しいですね",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "こんぶ",
            "text": "期待しておきましょう",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "こんぶ",
            "text": "でも",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "こんぶ",
            "text": "こどもにうるさいと言われたら",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "こんぶ",
            "text": "ショックです",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "ちくわ",
            "text": "将来が楽しみですね！",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "ばなな",
            "text": "そうなんですよ",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "ちくわ",
            "text": "私も早く",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "ちくわ",
            "text": "こどもに大きくなってほしいなあ",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "こんぶ",
            "text": "でも、私も女子ですが",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "こんぶ",
            "text": "母親のことうるさいと思ってたこと",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "こんぶ",
            "text": "あります",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "こんぶ",
            "text": "ほんとのこというと",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "ばなな",
            "text": "そうなんですね",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "こんぶ",
            "text": "自分が母親になって",
            "mention_to": []
        },
        {
            "utterance_id": 103,
            "interlocutor_id": "こんぶ",
            "text": "ようやく、母の偉大さに",
            "mention_to": []
        },
        {
            "utterance_id": 104,
            "interlocutor_id": "こんぶ",
            "text": "気が付きました。",
            "mention_to": []
        },
        {
            "utterance_id": 105,
            "interlocutor_id": "ちくわ",
            "text": "苦労をした分",
            "mention_to": []
        },
        {
            "utterance_id": 106,
            "interlocutor_id": "ちくわ",
            "text": "報われるよね",
            "mention_to": []
        }
