{
    "dialogue_id": "B15401",
    "dialogue_type": "Family",
    "interlocutors": [
        "おおかみ",
        "こんぶ",
        "ちくわ"
    ],
    "relationship": [
        "こんぶ",
        "ちくわ"
    ],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "おおかみ",
            "text": "こんばんは！",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "こんぶ",
            "text": "こんばんは！",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "ちくわ",
            "text": "こんばんは。",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "おおかみ",
            "text": "今日はこちらは雨でした。",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "おおかみ",
            "text": "そちらはどうでしたか？",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "こんぶ",
            "text": "こちらは〜曇りでしたが、、",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "こんぶ",
            "text": "今は雪が、、、",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "ちくわ",
            "text": "雪が降ってます！",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "おおかみ",
            "text": "あ、曇りだったんですね",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "おおかみ",
            "text": "雪が降ってる...",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "おおかみ",
            "text": "寒そうですね",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "ちくわ",
            "text": "昼前からずっと雪ですねー",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "こんぶ",
            "text": "日中は曇ってて、昼頃から雲行きがあやしく、、。",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "おおかみ",
            "text": "それでも普通にお仕事でしたか？",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "ちくわ",
            "text": "@おおかみ 普通に仕事ですよ！",
            "mention_to": [
                "おおかみ"
            ]
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "おおかみ",
            "text": "交通機関が大変そうですよね...",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "こんぶ",
            "text": "私は自宅警備を！笑",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "おおかみ",
            "text": "@ちくわ それはそれは大変ですね...",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "おおかみ",
            "text": "自宅警備いいですね",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "ちくわ",
            "text": "めちゃくちゃ降ってるわけではないので！",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "こんぶ",
            "text": "こちらは車社会なので、、何とか大丈夫そうですよ〜",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "こんぶ",
            "text": "電車の方は大変ですよね",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "ちくわ",
            "text": "明日の午前まで降るみたいです",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "おおかみ",
            "text": "@ちくわ 流石にすごい降ったら、しごとはすとっぷですか？",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "おおかみ",
            "text": "ひらがなになってしまいました",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "こんぶ",
            "text": "@おおかみ ありがたいです自宅警備",
            "mention_to": [
                "おおかみ"
            ]
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "おおかみ",
            "text": "夜に積もりそうですね",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "ちくわ",
            "text": "@おおかみ いや、普通に仕事はありますよ！",
            "mention_to": [
                "おおかみ"
            ]
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "おおかみ",
            "text": "@こんぶ 自宅警備してなんぼですね笑",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "ちくわ",
            "text": "ストップはあんまないと思います。笑",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "こんぶ",
            "text": "これからも、つもりそうです〜",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "おおかみ",
            "text": "出社しないとできないお仕事なんですね",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "ちくわ",
            "text": "ですです！",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "こんぶ",
            "text": "@おおかみはい笑笑",
            "mention_to": [
                "おおかみ"
            ]
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "おおかみ",
            "text": "我が社も必須で出社なのでなかなかに大変です...",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "ちくわ",
            "text": "テレワークができたら最高なんですけど、、",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "おおかみ",
            "text": "家でできる仕事なのになぁと思いつつ頑張っています",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "こんぶ",
            "text": "テレワーク！！！",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "おおかみ",
            "text": "@ちくわ 最高ですよね...",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "おおかみ",
            "text": "コロナ期間中は少しテレワークになりましたか？",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "ちくわ",
            "text": "@おおかみ ですよね。笑",
            "mention_to": [
                "おおかみ"
            ]
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "ちくわ",
            "text": "いや、仕事にいってましたよ！",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "こんぶ",
            "text": "@ちくわ 普通に言ってたね〜",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "おおかみ",
            "text": "それでもならないとなかなかに厳しいですね...",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "ちくわ",
            "text": "@こんぶ うんー！",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "おおかみ",
            "text": "@こんぶ コロナ期間中くらいは家にいて欲しいですよね",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "おおかみ",
            "text": "怖いですし",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "こんぶ",
            "text": "そうですよね。。",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "こんぶ",
            "text": "@おおかみ そうなんですよ〜リスクも伴って",
            "mention_to": [
                "おおかみ"
            ]
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "ちくわ",
            "text": "気持ち的にやる気も起きないですしね！",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "おおかみ",
            "text": "お子さんいたらヒヤヒヤですね...",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "こんぶ",
            "text": "あの頃は毎日がドキドキでした",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "おおかみ",
            "text": "そうなりますね。",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "こんぶ",
            "text": "そう、娘が1人いますが",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "おおかみ",
            "text": "お子さんいらっしゃるんですね",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "ちくわ",
            "text": "はい！",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "こんぶ",
            "text": "私がなって家族でコロナに、、、",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "おおかみ",
            "text": "大変だ...",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "ちくわ",
            "text": "それも年末年始に",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "ちくわ",
            "text": "笑",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "こんぶ",
            "text": "1ヶ月の頃の娘でした、、",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "おおかみ",
            "text": "でもお子さんは重症化しないイメージありますね",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "おおかみ",
            "text": "一番自由にできる時期に...",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "こんぶ",
            "text": "@おおかみ そうなんですよ！！！1番軽かったと思います",
            "mention_to": [
                "おおかみ"
            ]
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "ちくわ",
            "text": "一応重症化リストには入ったけど",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "こんぶ",
            "text": "あの時は、おわったーー。と思いました",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "ちくわ",
            "text": "大丈夫でした！",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "おおかみ",
            "text": "あ、よかったですね",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "おおかみ",
            "text": "重症化リストとはなんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "こんぶ",
            "text": "まだ1ヶ月だったからね、、",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "ちくわ",
            "text": "@こんぶ 最悪な年明け。",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "おおかみ",
            "text": "後遺症とか怖いですもんね、ない可能性が高いとはいえ...",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "ちくわ",
            "text": "@おおかみ なんか、いつでも対応できるように",
            "mention_to": [
                "おおかみ"
            ]
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "こんぶ",
            "text": "たぶん、歳が歳なので重症化リストにのっけられて",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "ちくわ",
            "text": "みたいな？",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "おおかみ",
            "text": "なるほど...",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "こんぶ",
            "text": "保健所から連絡がきて",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "こんぶ",
            "text": "様子を聞いてもらってました！",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "おおかみ",
            "text": "それはそれは怖い",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "おおかみ",
            "text": "でも何事もなくよかったですね。",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "こんぶ",
            "text": "安否確認ですね！",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "ちくわ",
            "text": "ほんまよかったですよ！",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "こんぶ",
            "text": "ほんとに、、終わってみてそう思います。",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "こんぶ",
            "text": "亡くなられてる方だっているのに、、",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "おおかみ",
            "text": "よかったです何はともあれ...",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "こんぶ",
            "text": "本当に何にもなくてよかった。。",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "おおかみ",
            "text": "本当にそうですよね",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "おおかみ",
            "text": "亡くなってる方多いですからね",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "ちくわ",
            "text": "もうかかりたくないですねー",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "こんぶ",
            "text": "芸能人でも多いですよね",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "こんぶ",
            "text": "志村けんさんはびっくりでした",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "おおかみ",
            "text": "志村けんさんとかしょうげきでした",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "ちくわ",
            "text": "@おおかみ まわりの方とか大丈夫でした？",
            "mention_to": [
                "おおかみ"
            ]
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "おおかみ",
            "text": "同じく！",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "こんぶ",
            "text": "@ちくわ ね！もうかかりたくない、。",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "ちくわ",
            "text": "あれはびっくりすねー",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "おおかみ",
            "text": "周りは旦那以外はならなかったですね",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "おおかみ",
            "text": "しかもその時ちょうど結婚式やってまして、私",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "ちくわ",
            "text": "へー！",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "こんぶ",
            "text": "旦那さんはなったんですねー！！",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "こんぶ",
            "text": "自分たちの結婚式ですか？",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "おおかみ",
            "text": "3月末になんとか挙式したので、ギリギリでした",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "おおかみ",
            "text": "はい！",
            "mention_to": []
        },
        {
            "utterance_id": 103,
            "interlocutor_id": "ちくわ",
            "text": "そうだったんすね",
            "mention_to": []
        },
        {
            "utterance_id": 104,
            "interlocutor_id": "こんぶ",
            "text": "そうだったんですね〜〜！！！",
            "mention_to": []
        },
        {
            "utterance_id": 105,
            "interlocutor_id": "おおかみ",
            "text": "なんか志村けんが死んだことが衝撃的すぎて",
            "mention_to": []
        },
        {
            "utterance_id": 106,
            "interlocutor_id": "こんぶ",
            "text": "滑り込みセーフですけ",
            "mention_to": []
        }
    ],
