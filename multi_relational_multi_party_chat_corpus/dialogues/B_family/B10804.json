        },
        {
            "utterance_id": 2,
            "interlocutor_id": "りんご",
            "text": "突然なんですが、焼き芋って好きですか？",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "りんご",
            "text": "お昼に作ってたんです笑",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "つみれ",
            "text": "好きです!",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "つみれ",
            "text": "いいですね！",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "りんご",
            "text": "@つみれ 最近、お芋すごくないですか？",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "たまご",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "たまご",
            "text": "好きです！",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "つみれ",
            "text": "@りんご すごいです",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "りんご",
            "text": "@たまご ねっとり感が！",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "りんご",
            "text": "@つみれ 甘いんですよね",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "つみれ",
            "text": "スーパーで見かけるとつい買ってしまいます",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "たまご",
            "text": "良いですよね！",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "りんご",
            "text": "スーパーの買ってからハマってます！",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "つみれ",
            "text": "美味しいですよね！",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "りんご",
            "text": "あれだけでスイーツ！",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "たまご",
            "text": "石井焼き芋大好きです",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "たまご",
            "text": "石焼き芋",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "つみれ",
            "text": "ほんとスイーツです",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "りんご",
            "text": "@たまご 石焼だと違うよね",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "たまご",
            "text": "甘くてあったかくて大好きです",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "たまご",
            "text": "また変わってきますね",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "たまご",
            "text": "少し",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "たまご",
            "text": "トラックでくるタイプのも美味しいです",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "りんご",
            "text": "@たまご ですよ、やっぱり流行りとかたるのかな",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "りんご",
            "text": "@たまご え？トラック？",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "つみれ",
            "text": "最近トラックの見かけないですね",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "たまご",
            "text": "トラックで街を巡ります",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "りんご",
            "text": "@つみれ 軽トラでくるやつですか？",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "つみれ",
            "text": "石焼き芋を売っている車があって",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "たまご",
            "text": "そうです",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "りんご",
            "text": "あー！確かに！いないですね",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "たまご",
            "text": "声が聞こえたら止めて買うやつです",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "りんご",
            "text": "あれがよかったのにね",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "たまご",
            "text": "懐かしい感じが好きです",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "りんご",
            "text": "@たまご たまにおまけとかしてくれる",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "つみれ",
            "text": "最近いなくて見かけるとつい追いかけてしまいます笑",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "たまご",
            "text": "ありますね！",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "つみれ",
            "text": "子供の頃よくおねだりしました",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "りんご",
            "text": "最近こないですね",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "たまご",
            "text": "優しいおっちゃんが多いです",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "りんご",
            "text": "本当に昔はよくきてたけどね",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "りんご",
            "text": "@たまご また買いたいですよね",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "たまご",
            "text": "今はあんま売れないんですかねー",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "たまご",
            "text": "買いたいです！",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "りんご",
            "text": "なんか悲しいですね",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "つみれ",
            "text": "ほんとですよね",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "たまご",
            "text": "悲しいです",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "りんご",
            "text": "うちのほうは豆腐屋さんはきますよ",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "たまご",
            "text": "復活してほしい",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "りんご",
            "text": "何かきます？",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "つみれ",
            "text": "豆腐屋さんいいですね！",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "たまご",
            "text": "全然来ないです",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "たまご",
            "text": "豆腐屋くるんですか！",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "つみれ",
            "text": "ラーメン屋さん来ます!",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "りんご",
            "text": "そうなんです！あの昔の音ならしながら",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "りんご",
            "text": "@つみれ ラーメン屋さんが！",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "たまご",
            "text": "ラーメン屋いいな",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "りんご",
            "text": "@たまご ラーメン屋さんほしいですね",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "たまご",
            "text": "はい！",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "りんご",
            "text": "毎日来て欲しいよね笑",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "りんご",
            "text": "日替わりで",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "つみれ",
            "text": "音聞こえるとつい外見ちゃいますね",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "りんご",
            "text": "で",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "りんご",
            "text": "ですね",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "たまご",
            "text": "風鈴の音で来るやつですか？",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "たまご",
            "text": "@りんご 来てほしいです",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "りんご",
            "text": "ラーメン屋さん、風鈴？豆腐屋さんはなんていうのラッパ的な感じのやつ",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "たまご",
            "text": "あー",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "りんご",
            "text": "わかります？",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "たまご",
            "text": "思い出しました",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "りんご",
            "text": "@たまご よかった",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "つみれ",
            "text": "ラッパですよね",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "たまご",
            "text": "トゥー↓トゥー↑みたいな",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "りんご",
            "text": "ですよね、ラッパ",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "りんご",
            "text": "そうそう！",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "たまご",
            "text": "感じですよね",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "つみれ",
            "text": "@たまご ちょっとと伝わらない笑",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "りんご",
            "text": "@たまご 伝わってよかったです",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "りんご",
            "text": "昔は色々売りにきたけどね",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "つみれ",
            "text": "アイスとかもありましたよね",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "りんご",
            "text": "@つみれ あったー",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "たまご",
            "text": "アイス良いですね",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "りんご",
            "text": "息子と買いにいきました",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "たまご",
            "text": "夏さいこう",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "つみれ",
            "text": "今もまだたまに来ます",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "りんご",
            "text": "いいよね、季節感あって",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "つみれ",
            "text": "いいですね",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "たまご",
            "text": "めちゃ美味しいでしたか？",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "りんご",
            "text": "いいですね",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "りんご",
            "text": "息子しかたべてないんです涙",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "つみれ",
            "text": "そうだったんですね",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "りんご",
            "text": "たぶん美味しかったと思う、よくねだられたから笑",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "りんご",
            "text": "私小さい頃は雑貨とかも売りにきてましたよ",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "りんご",
            "text": "どんだけ昔？って感じですが",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "たまご",
            "text": "そうなんですね",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "りんご",
            "text": "@たまご ざるとか買ったような",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "たまご",
            "text": "珍しい",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "りんご",
            "text": "@たまご 昭和なんで笑",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "つみれ",
            "text": "@りんご 雑貨!それははじめて聞きましたすごいですね",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "りんご",
            "text": "@つみれ なんかおじさんが色々積んでました",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "つみれ",
            "text": "@りんご 私もバリバリ昭和です笑",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 103,
            "interlocutor_id": "つみれ",
            "text": "そーなんですね！",
            "mention_to": []
