    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "にわとり",
            "text": "こんばんはー！",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "じゃがいも",
            "text": "こんばんは！",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "くま",
            "text": "こんばんは",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "にわとり",
            "text": "今日、雨ですねー",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "にわとり",
            "text": "そちらは？",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "じゃがいも",
            "text": "マジでパラパラの雨で",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "くま",
            "text": "@にわとり こっちもです",
            "mention_to": [
                "にわとり"
            ]
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "じゃがいも",
            "text": "傘刺そうか悩む感じの雨でしたね",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "くま",
            "text": "寒いですね",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "にわとり",
            "text": "おーそちらも！",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "にわとり",
            "text": "そう、気温も下がりましたねー",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "じゃがいも",
            "text": "@にわとり なんかほんとうに寒すぎてびっくりしました",
            "mention_to": [
                "にわとり"
            ]
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "くま",
            "text": "最近ずっと雨ですね",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "にわとり",
            "text": "なんか気温がさがりすぎて、",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "にわとり",
            "text": "トレンドに寒暖差ぎっくりごしって出てましたよ",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "じゃがいも",
            "text": "@くま 明日は晴れるらしいで",
            "mention_to": [
                "くま"
            ]
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "くま",
            "text": "暑いよりはいいです",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "じゃがいも",
            "text": "面白いwww",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "にわとり",
            "text": "寒暖差アレルギーみたいな？",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "じゃがいも",
            "text": "いや今日はなんか寒すぎて死を覚悟しました",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "くま",
            "text": "@にわとり そんなのあるんですね",
            "mention_to": [
                "にわとり"
            ]
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "くま",
            "text": "くしゃみとかは出ますよね",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "じゃがいも",
            "text": "訪問で外出てたんですけど、マジで寒すぎてちょっと笑えませんでしたね",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "にわとり",
            "text": "わかる！死にそうな寒さ",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "くま",
            "text": "鼻むずむずしたり",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "じゃがいも",
            "text": "@くま 確かにあなたなりやすいかもね",
            "mention_to": [
                "くま"
            ]
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "にわとり",
            "text": "そうですね、花粉症もあるかもだけど、",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "じゃがいも",
            "text": "うわ花粉症！",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "にわとり",
            "text": "寒暖差アレルギーかも。くまさんのそれは。",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "くま",
            "text": "@じゃがいも  在宅でぬくぬく",
            "mention_to": [
                "じゃがいも"
            ]
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "じゃがいも",
            "text": "@にわとり 聞きました？",
            "mention_to": [
                "にわとり"
            ]
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "にわとり",
            "text": "いいな！",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "じゃがいも",
            "text": "在宅だったんです、くま",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "にわとり",
            "text": "ぬくぬく！",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "くま",
            "text": "家も寒かったですけどね",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "くま",
            "text": "エアコンつけるか悩むくらい",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "じゃがいも",
            "text": "あり得ない！",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "にわとり",
            "text": "えーーーー　でもぬくぬくいいな",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "にわとり",
            "text": "仕事しながらテレビ見れちゃったりしますか？",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "にわとり",
            "text": "さすがにみない？",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "くま",
            "text": "@にわとり 出社は移動が寒いですからね",
            "mention_to": [
                "にわとり"
            ]
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "じゃがいも",
            "text": "@くま 我が家は南向きとはいえ、今日みたいな日は全く意味ないから普通につけろ",
            "mention_to": [
                "くま"
            ]
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "じゃがいも",
            "text": "見てないと思いますね",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "じゃがいも",
            "text": "背面にテレビなんで",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "くま",
            "text": "@にわとり テレビはつけないですね",
            "mention_to": [
                "にわとり"
            ]
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "じゃがいも",
            "text": "私が知っている限りでは",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "にわとり",
            "text": "おーーー背面・・・！",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "じゃがいも",
            "text": "ただよく携帯を見てる姿は留守番カメラで目にしますね",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "くま",
            "text": "たまにスマホをいじったりはしちゃいますけど",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "にわとり",
            "text": "じゃあぬくぬくで、珈琲飲みながら、お仕事・・・",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "じゃがいも",
            "text": "@くま たまに？",
            "mention_to": [
                "くま"
            ]
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "にわとり",
            "text": "@くま なるほど・・・！",
            "mention_to": [
                "くま"
            ]
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "じゃがいも",
            "text": "怪しいです",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "くま",
            "text": "@にわとり 水も飲み放題です",
            "mention_to": [
                "にわとり"
            ]
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "にわとり",
            "text": "わはは",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "くま",
            "text": "@じゃがいも たまによ",
            "mention_to": [
                "じゃがいも"
            ]
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "じゃがいも",
            "text": "ほんとうに在宅羨ましい〜",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "くま",
            "text": "@じゃがいも ごくたまに",
            "mention_to": [
                "じゃがいも"
            ]
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "にわとり",
            "text": "そうですよね、やっぱりおうちだと気が楽ですよねー！",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "にわとり",
            "text": "洋服も部屋着でらくちん",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "じゃがいも",
            "text": "@くま 逮捕です、にわとりさん、一緒に逮捕しましょう",
            "mention_to": [
                "くま"
            ]
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "くま",
            "text": "インターネットが整ってればいいですね",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "にわとり",
            "text": "わはは！！！",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "にわとり",
            "text": "罪状は、在宅？！？",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "くま",
            "text": "最近は整ってない方が珍しいかもですけど",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "じゃがいも",
            "text": "毎朝メイクして着替えて混んでる電車乗って",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "じゃがいも",
            "text": "地獄です",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "にわとり",
            "text": "うーん　確かに。出勤は大変ですよねー",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "じゃがいも",
            "text": "@にわとり 在宅により逮捕ですね",
            "mention_to": [
                "にわとり"
            ]
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "くま",
            "text": "@じゃがいも 悪いことはしてないはず、、、",
            "mention_to": [
                "じゃがいも"
            ]
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "じゃがいも",
            "text": "わるいですよ",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "くま",
            "text": "@じゃがいも 会社を恨むのです",
            "mention_to": [
                "じゃがいも"
