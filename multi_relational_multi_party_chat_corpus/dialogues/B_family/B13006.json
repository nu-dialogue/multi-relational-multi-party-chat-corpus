        },
        {
            "utterance_id": 72,
            "interlocutor_id": "にわとり",
            "text": "わはは・・・！！！",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "にわとり",
            "text": "在宅勤務も、",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "じゃがいも",
            "text": "会社でもある程度サボりつつ寝る",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "じゃがいも",
            "text": "やるしかないです",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "にわとり",
            "text": "コロナ以降一般的になりましたね＾",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "じゃがいも",
            "text": "本当ですね",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "くま",
            "text": "@じゃがいも 会社でさぼる方がまずいでしょ",
            "mention_to": [
                "じゃがいも"
            ]
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "にわとり",
            "text": "会社でさぼるのって難しいんじゃないですか？",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "くま",
            "text": "@にわとり このまま続いてほしいですね",
            "mention_to": [
                "にわとり"
            ]
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "じゃがいも",
            "text": "画面を暗くして",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "にわとり",
            "text": "パソコンを打ってるふりをして・・・",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "じゃがいも",
            "text": "バレてるのは覚悟してます",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "くま",
            "text": "悪いなあ",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "じゃがいも",
            "text": "まぁ最近は忙しいんで余裕はないですね",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "にわとり",
            "text": "目は半開きかなあ",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "くま",
            "text": "@にわとり 寝るのはまずいですね",
            "mention_to": [
                "にわとり"
            ]
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "じゃがいも",
            "text": "@にわとり お昼明けは半開き",
            "mention_to": [
                "にわとり"
            ]
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "くま",
            "text": "確実にばれちゃう",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "くま",
            "text": "@じゃがいも 食後は大変ね",
            "mention_to": [
