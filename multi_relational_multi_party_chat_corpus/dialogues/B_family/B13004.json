{
    "dialogue_id": "B13004",
    "dialogue_type": "Family",
    "interlocutors": [
        "にわとり",
        "くま",
        "じゃがいも"
    ],
    "relationship": [
        "くま",
        "じゃがいも"
    ],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "にわとり",
            "text": "こんばんはー！",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "くま",
            "text": "こんばんは！",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "じゃがいも",
            "text": "どうもこんばんは！",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "にわとり",
            "text": "よろしくお願いしますー！",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "じゃがいも",
            "text": "こちらこそ！",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "くま",
            "text": "お願いします。",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "じゃがいも",
            "text": "今日かき氷食べました！旦那と",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "にわとり",
            "text": "えーーー！！いいな！",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "じゃがいも",
            "text": "この寒い中！",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "くま",
            "text": "おいしかったねえ",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "にわとり",
            "text": "あったかい部屋で食べるのおいしそう！",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "くま",
            "text": "あったかいところで食べるからいいですね",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "じゃがいも",
            "text": "私の趣味なんです、かき氷食べ歩き",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "くま",
            "text": "こたつみかんと一緒です",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "にわとり",
            "text": "趣味・・・！！すてき・・・！！",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "じゃがいも",
            "text": "@にわとり その通りで暖かいからいいんですね",
            "mention_to": [
                "にわとり"
            ]
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "くま",
            "text": "もしくはこたつアイス",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "にわとり",
            "text": "あーーーこたつアイスもいい！",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "じゃがいも",
            "text": "こたつアイスと同じ論理？だね",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "にわとり",
            "text": "今日のかき氷は何味ですか？",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "くま",
            "text": "@じゃがいも さすがに寒いとこではきついよね",
            "mention_to": [
                "じゃがいも"
            ]
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "じゃがいも",
            "text": "@にわとり 私はいちごミルクでした、こんなオーソドックスなの久しぶりに食べました",
            "mention_to": [
                "にわとり"
            ]
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "にわとり",
            "text": "おーーー定番ですね！でもおいしそう！",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "じゃがいも",
            "text": "@くま あなたなんだっけ",
            "mention_to": [
                "くま"
            ]
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "くま",
            "text": "@にわとり 私はなんかチョコとベリーです",
            "mention_to": [
                "にわとり"
            ]
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "くま",
            "text": "ホワイトデーのやつ",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "じゃがいも",
            "text": "ホワイトデースペシャルってやつですね、名前が偉大",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "にわとり",
            "text": "おおっちょっと変化球。おしゃれなやつ！",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "じゃがいも",
            "text": "結構どれも高いんですけどね",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "くま",
            "text": "最近のかき氷は高くておしゃれなんですよね",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "にわとり",
            "text": "へえ～そんなのもあるんですね！",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "にわとり",
            "text": "わかります。ごはん食べるくらいの値段しますよね。",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "くま",
            "text": "ブルーハワイとか久しく食べてない",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "じゃがいも",
            "text": "いまかき氷界隈はアツいです",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "じゃがいも",
            "text": "@くま マジで食べてないね",
            "mention_to": [
                "くま"
            ]
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "にわとり",
            "text": "@くま なつかしい味ですね！",
            "mention_to": [
                "くま"
            ]
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "にわとり",
            "text": "よくお祭りである・・・",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "くま",
            "text": "@じゃがいも だいぶ金銭感覚狂ってきてますね",
            "mention_to": [
                "じゃがいも"
            ]
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "にわとり",
            "text": "食べると舌が青くなるやつ。",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "じゃがいも",
            "text": "かき氷の伝説で目を隠すと全部同じ味説っての嘘だと思ったら本当でしたよね",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "じゃがいも",
            "text": "また話遮りましたけど",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "くま",
            "text": "香りで何とかしてるみたいね",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "にわとり",
            "text": "あーーー、それ聞いたことありますよ！味いっしょだっての！",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "じゃがいも",
            "text": "@くま ほんまに金銭感覚は狂う",
            "mention_to": [
                "くま"
            ]
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "くま",
            "text": "祭りも久しく行ってないなあ",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "じゃがいも",
            "text": "ガチなんですよ、マジでガチ",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "くま",
            "text": "たこ焼き食べたい",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "じゃがいも",
            "text": "祭行ってないね！",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "くま",
            "text": "お好み焼きもいいなあ",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "にわとり",
            "text": "たこ焼きもいいなあー。お祭り楽しいですよね！",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "じゃがいも",
            "text": "@にわとり 祭りで毎回と言っていいくらい喧嘩するんですよ我々",
            "mention_to": [
                "にわとり"
            ]
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "くま",
            "text": "でも祭り値段が、、、",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "じゃがいも",
            "text": "私はもう混んでるからその場にあるやつを頼もうっていうんですよ",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "じゃがいも",
            "text": "はいそれ！",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "にわとり",
            "text": "数年コロナでお祭りもなくなっちゃって、復活してよかった。",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "にわとり",
            "text": "けんか？",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "くま",
            "text": "@じゃがいも 祭りだと高いからね",
            "mention_to": [
                "じゃがいも"
            ]
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "にわとり",
            "text": "あーーー確かに！！",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "じゃがいも",
            "text": "それです、悪いです。旦那は全部見てから買いたいっていうんですよ",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "にわとり",
            "text": "あーぐるっと回ってね。",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "くま",
            "text": "安いとこ見つけたら損ですからね",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "じゃがいも",
            "text": "私は浴衣だったりするからもうええやん...ってなっちゃう",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "にわとり",
            "text": "@くま なるほど・・・！！",
            "mention_to": [
                "くま"
            ]
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "じゃがいも",
            "text": "付き合ってた頃は割と本気の喧嘩してました",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "くま",
            "text": "節約は大事ですよね！！",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "にわとり",
            "text": "@じゃがいも 本気のけんか・・・",
            "mention_to": [
                "じゃがいも"
            ]
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "じゃがいも",
            "text": "その場の雰囲気も大事です",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "にわとり",
            "text": "でもちょっと、安いとこ見つけたらがっかりする気持ちもわかるw",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "くま",
            "text": "@じゃがいも 悔しいが一理ある",
            "mention_to": [
                "じゃがいも"
            ]
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "くま",
            "text": "@にわとり ですよね！難しい問題です。",
            "mention_to": [
                "にわとり"
            ]
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "じゃがいも",
            "text": "@にわとり わかります、だから冷静になると旦那を叩けない",
            "mention_to": [
                "にわとり"
            ]
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "にわとり",
            "text": "@じゃがいも さんのきもちもわかる・・",
            "mention_to": [
                "じゃがいも"
            ]
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "じゃがいも",
            "text": "だから私は冷静にならない！",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "くま",
            "text": "永遠の課題ですね、、、",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "くま",
            "text": "埋まらない溝です",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "じゃがいも",
            "text": "課題ですね",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "にわとり",
            "text": "わはは、確かにね、永遠の課題・・・！",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "くま",
            "text": "みんな違ってみんな良い",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "じゃがいも",
            "text": "そういうことにしましょうか",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "くま",
            "text": "与謝野晶子",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "にわとり",
            "text": "そうそう！価値観の違いも大事。",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "じゃがいも",
            "text": "まぁでも本当に祭りも復活しましたよね",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "じゃがいも",
            "text": "だいぶコロナ終わって日常に戻ってる感じします",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "じゃがいも",
            "text": "マスクしてない人多いですし",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "にわとり",
            "text": "ずいぶん長いことやってなかったしねえ・・・",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "くま",
            "text": "去年の隅田川とかすごかった見たいですね",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "にわとり",
            "text": "そうそう、マスクは減りましたね。",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "じゃがいも",
            "text": "@くま 帰宅難民だっけ？",
            "mention_to": [
                "くま"
            ]
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "じゃがいも",
            "text": "なんか話題にはなったけど覚えてない...",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "にわとり",
            "text": "帰宅難民？？？",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "くま",
            "text": "@じゃがいも そこまでかはわからない",
            "mention_to": [
                "じゃがいも"
            ]
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "にわとり",
            "text": "あーーー　！　花火？？",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "じゃがいも",
            "text": "思い出した気がする、多分混みすぎてすごいことになったやつじゃない？",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "くま",
            "text": "開放的なとこではマスク要らないと思いますけどね",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "じゃがいも",
            "text": "そうそう",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "じゃがいも",
            "text": "マスクは前髪が乱れるので",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "くま",
            "text": "花火です！祭りとは違うか",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "じゃがいも",
            "text": "お出かけする日はマスクしません",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "じゃがいも",
            "text": "仕事の日は気にせずマスクします",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "くま",
            "text": "電車とかはした方がいいね",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "くま",
            "text": "個人的には",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "じゃがいも",
            "text": "おっしゃる通りですわ",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "にわとり",
            "text": "花火も見たいなー。アッマスクね・・・",
            "mention_to": []
        },
        {
            "utterance_id": 103,
            "interlocutor_id": "にわとり",
            "text": "メガネが曇るんですよね。",
            "mention_to": []
        },
        {
            "utterance_id": 104,
            "interlocutor_id": "じゃがいも",
            "text": "めちゃ曇る",
            "mention_to": []
        }
    ],
