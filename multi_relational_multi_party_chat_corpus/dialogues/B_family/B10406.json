        },
        {
            "utterance_id": 85,
            "interlocutor_id": "つくね",
            "text": "行くみたいです！",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "うさぎ",
            "text": "@つくね それは楽しそう",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "しらたき",
            "text": "@うさぎ 汗だく！",
            "mention_to": [
                "うさぎ"
            ]
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "うさぎ",
            "text": "最近猛暑ですしね",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "しらたき",
            "text": "@うさぎ 猛暑ですね…",
            "mention_to": [
                "うさぎ"
            ]
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "しらたき",
            "text": "嫌になる",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "うさぎ",
            "text": "今年もやばそうですよね",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "つくね",
            "text": "@うさぎ 楽しそうですが親も大変です笑",
            "mention_to": [
                "うさぎ"
            ]
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "うさぎ",
            "text": "今から昨日みたいな暑さじゃ",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "しらたき",
            "text": "@うさぎ 確かに",
            "mention_to": [
                "うさぎ"
            ]
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "うさぎ",
            "text": "@つくね 中学まではそんな感じなんですね",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "しらたき",
            "text": "夏どーなることやら",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "うさぎ",
            "text": "高校生は電車とかになるのかしら？",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "つくね",
            "text": "@うさぎ そうですね！上の子が後2年で",
            "mention_to": [
