            "text": "おはようございます",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "うさぎ",
            "text": "昨日はあたたかかったですね",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "つくね",
            "text": "そうですね！",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "しらたき",
            "text": "息子は半袖でした！",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "うさぎ",
            "text": "こちら関東ですが暑かったです",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "うさぎ",
            "text": "@しらたき 半袖!",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "つくね",
            "text": "こちらもだいぶ暑かったです！",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "しらたき",
            "text": "@うさぎ 汗めっちゃかいてました！",
            "mention_to": [
                "うさぎ"
            ]
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "うさぎ",
            "text": "@つくね 今日は寒いですか？",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "つくね",
            "text": "今日は雨が降ってて",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "うさぎ",
            "text": "@しらたき お子さんおいくつですか？",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "つくね",
            "text": "少し肌寒いです！",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "しらたき",
            "text": "@うさぎ 7歳と10歳です",
            "mention_to": [
                "うさぎ"
            ]
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "しらたき",
            "text": "7歳の方が半袖でした！",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "うさぎ",
            "text": "@つくね こちらも雨です",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "うさぎ",
            "text": "@しらたき 小学生ですか？",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "つくね",
            "text": "@うさぎ 昨日と違って嫌な天気ですね",
            "mention_to": [
                "うさぎ"
            ]
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "しらたき",
            "text": "@うさぎ そうです！",
            "mention_to": [
                "うさぎ"
            ]
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "しらたき",
            "text": "1年生です",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "つくね",
            "text": "@うさぎ 今日こんな中",
            "mention_to": [
                "うさぎ"
            ]
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "つくね",
            "text": "参観日なんです！笑",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "うさぎ",
            "text": "@しらたき うわー代謝いいですものね！",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "うさぎ",
            "text": "えっ？！参観日！",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "うさぎ",
            "text": "お忙しいですね",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "しらたき",
            "text": "@うさぎ 本当羨ましい限りです笑",
            "mention_to": [
                "うさぎ"
            ]
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "うさぎ",
            "text": "午後からですか？",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "つくね",
            "text": "@うさぎ そうなんです！笑",
            "mention_to": [
                "うさぎ"
            ]
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "うさぎ",
            "text": "参観",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "つくね",
            "text": "午後から参観日です！",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "うさぎ",
            "text": "@つくね 学校近いんですか？",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "しらたき",
            "text": "@つくね しっかり見てきてね！",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "つくね",
            "text": "@うさぎ 車で5分くらいです",
            "mention_to": [
                "うさぎ"
            ]
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "つくね",
            "text": "歩くと30分くらいかな",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "うさぎ",
            "text": "@つくね お車でいける！いいですね",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "うさぎ",
            "text": "30分はきつい",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "うさぎ",
            "text": "@しらたき お留守番ですか？",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "つくね",
            "text": "ただ、駐車場が少ないです笑",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "しらたき",
            "text": "@うさぎ 子供の学校",
            "mention_to": [
                "うさぎ"
            ]
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "しらたき",
            "text": "全校生徒数が少ないんです",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "うさぎ",
            "text": "@しらたき 何人くらいですか？",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "しらたき",
            "text": "@うさぎ 全校生徒合わせて",
            "mention_to": [
                "うさぎ"
            ]
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "しらたき",
            "text": "100人です",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "うさぎ",
            "text": "@しらたき ええっ？！全員知り合いですか？",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "つくね",
            "text": "@うさぎ 1学年1クラスなんです！笑",
            "mention_to": [
                "うさぎ"
            ]
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "しらたき",
            "text": "@うさぎ ほぼ知り合いですねー笑",
            "mention_to": [
                "うさぎ"
            ]
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "うさぎ",
            "text": "@つくね クラス替えのドキドキがない？！",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "つくね",
            "text": "@うさぎ そうなんですよ！",
            "mention_to": [
                "うさぎ"
            ]
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "つくね",
            "text": "その代わり",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "つくね",
            "text": "中学は7クラスくらいになります！",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "うさぎ",
            "text": "@つくね いきなりすぎません？",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "しらたき",
            "text": "@つくね マンモス中学だよね笑",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "うさぎ",
            "text": "@しらたき 統合されてるとか？",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "つくね",
            "text": "@うさぎ 中学1つしかないので",
            "mention_to": [
                "うさぎ"
            ]
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "つくね",
            "text": "5つの小学校が",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "うさぎ",
            "text": "@つくね そうすると遠くなりますか？",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "つくね",
            "text": "中学で1つになります！",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "うさぎ",
            "text": "5つ！",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "うさぎ",
            "text": "すごい",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "つくね",
            "text": "@うさぎ 遠くなりますね笑",
            "mention_to": [
                "うさぎ"
            ]
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "つくね",
            "text": "1時間かかります",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "うさぎ",
            "text": "@つくね バスとか？",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "うさぎ",
            "text": "１時間!",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "つくね",
            "text": "@うさぎ バスと電車とチャリです！",
            "mention_to": [
                "うさぎ"
            ]
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "うさぎ",
            "text": "@つくね えっと全部使うということですか？",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "しらたき",
            "text": "@つくね うちはチャリだねー！",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "うさぎ",
            "text": "@しらたき あっ、選べる？",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "つくね",
            "text": "@うさぎ どれ使ってもいいって感じです！",
            "mention_to": [
                "うさぎ"
            ]
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "しらたき",
            "text": "@うさぎ そうそう！",
            "mention_to": [
                "うさぎ"
            ]
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "うさぎ",
            "text": "@つくね なるほど！",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "しらたき",
            "text": "選べます！",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "うさぎ",
            "text": "@しらたき 全部じゃ疲れますしね",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "つくね",
            "text": "@うさぎ 場所的にどれも微妙で笑",
            "mention_to": [
                "うさぎ"
            ]
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "うさぎ",
            "text": "あーわかります",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "しらたき",
            "text": "@うさぎ 疲れますねー迎えとか行った時",
            "mention_to": [
                "うさぎ"
            ]
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "うさぎ",
            "text": "天気によって変えるとか？",
