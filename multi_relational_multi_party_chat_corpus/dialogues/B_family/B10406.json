        },
        {
            "utterance_id": 77,
            "interlocutor_id": "しらたき",
            "text": "チャリあるのはキツイ笑",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "うさぎ",
            "text": "@しらたき 電動ではないですよね？",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "つくね",
            "text": "@うさぎ 結構送り迎えのうちが多いみたいで",
            "mention_to": [
                "うさぎ"
            ]
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "うさぎ",
            "text": "@つくね お車で？",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "しらたき",
            "text": "@うさぎ もちろん！ママチャリ！笑",
            "mention_to": [
                "うさぎ"
            ]
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "つくね",
            "text": "@うさぎ そうみたいです！学校近くまでおくって",
            "mention_to": [
                "うさぎ"
            ]
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "うさぎ",
            "text": "@しらたき 夏とか大変ですね",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "つくね",
            "text": "お友達と一緒に",
