            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "りんご",
            "text": "@しらたき そうなんですね、でもお花見はいいですよね",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "しらたき",
            "text": "@りんご 花より団子ですけどね笑",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "りんご",
            "text": "@つくね 都内とか？",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "しらたき",
            "text": "大体屋台目当てです",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "りんご",
            "text": "@しらたき いっぱいでるんですか？",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "つくね",
            "text": "@りんご 都内行きたいです！",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "しらたき",
            "text": "@りんご そうですね、出てますね！",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "りんご",
            "text": "@つくね お店いっぱいあるもんね",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "しらたき",
            "text": "桜の花びらの塩漬けが乗った",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "りんご",
            "text": "@しらたき どんな屋台すきですか？",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "しらたき",
            "text": "天ぷら饅頭をいつも",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "しらたき",
            "text": "嫁が食べてます！",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "しらたき",
            "text": "@りんご イカ焼きすきでふ",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "りんご",
            "text": "@しらたき 美味しそう",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "つくね",
            "text": "美味しいです！",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "つくね",
            "text": "天ぷら饅頭が好きで笑",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "しらたき",
            "text": "@つくね 太るぞ！",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "りんご",
            "text": "@つくね なんか桜の花びらののったやつなんて食べたい！",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "つくね",
            "text": "@しらたき 大丈夫！笑",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "りんご",
            "text": "@しらたき それは言っちゃダメ",
