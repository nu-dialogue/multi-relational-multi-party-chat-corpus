            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "つくね",
            "text": "だいぶ田舎ですが笑",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "りんご",
            "text": "@しらたき 帰省も兼ねて？",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "しらたき",
