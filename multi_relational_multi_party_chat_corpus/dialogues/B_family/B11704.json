        },
        {
            "utterance_id": 52,
            "interlocutor_id": "ぱんだ",
            "text": "血の繋がり関係ないですね！",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "たまねぎ",
            "text": "関係ないです！",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "ぱんだ",
            "text": "照明してくれました！",
