        },
        {
            "utterance_id": 44,
            "interlocutor_id": "たまねぎ",
            "text": "たしかにー",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "にんじん",
            "text": "あれ逃してたら後悔してたかもー",
