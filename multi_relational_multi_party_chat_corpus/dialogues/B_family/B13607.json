        },
        {
            "utterance_id": 14,
            "interlocutor_id": "にんじん",
            "text": "ネットじゃなかったけど",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "にんじん",
            "text": "店舗で！",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "いるか",
            "text": "パジャマ！",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "たまねぎ",
            "text": "買ってもらった！笑",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "いるか",
            "text": "それは直接見たいやつですね！",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "たまねぎ",
            "text": "なんかジェラピケ？",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "たまねぎ",
            "text": "てきなやつ！",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "にんじん",
            "text": "可愛くて即決でしたね！",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "いるか",
            "text": "可愛いやつじゃないですか！",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "いるか",
            "text": "え～～～いいですね～！",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "たまねぎ",
            "text": "ツルツルしてて気持ちがいい生地のやつ！",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "いるか",
            "text": "もこもこじゃなくて！？",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "にんじん",
            "text": "初めて買ってもらったみたいで喜んでました！",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "たまねぎ",
            "text": "もこもことはまた違うんですよね！",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "いるか",
            "text": "ジェラピケというともこもこのイメージが・・・！",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "にんじん",
            "text": "たしかに！笑",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "いるか",
            "text": "え～ツルツルもあるなら探してみようかな・・・",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "たまねぎ",
            "text": "俺も着る時が来るとは！笑",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "たまねぎ",
            "text": "俺はそーいうの着るキャラじゃないと",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "にんじん",
            "text": "似合ってるよー笑",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "たまねぎ",
            "text": "勝手に思い込んでて！笑",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "いるか",
            "text": "@たまねぎ 肌触りどうですか？！",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "いるか",
            "text": "じぇらぴけ男子！たまねぎさん！",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "たまねぎ",
            "text": "なんか照れますよね！笑",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "たまねぎ",
            "text": "ツルツルっていう方があってるかも！",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "たまねぎ",
            "text": "なんか難しいすね！",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "にんじん",
            "text": "ジェラピケ男子笑",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "たまねぎ",
            "text": "伝えるのが！笑",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "いるか",
            "text": "え～ほんと、探してみよう・・・",
