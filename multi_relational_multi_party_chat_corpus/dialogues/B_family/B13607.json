        },
        {
            "utterance_id": 46,
            "interlocutor_id": "たまねぎ",
            "text": "！",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "たまねぎ",
            "text": "それくらい気に入った！2人で！",
