{
    "dialogue_id": "B12505",
    "dialogue_type": "Family",
    "interlocutors": [
        "すじこ",
        "ちくわ",
        "こんぶ"
    ],
    "relationship": [
        "ちくわ",
        "こんぶ"
    ],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "すじこ",
            "text": "こんばんは",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "ちくわ",
            "text": "こんばんは。",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "こんぶ",
            "text": "こんばんは",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "すじこ",
            "text": "旅行行くとしたら",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "すじこ",
            "text": "どこか行きたい所ありますか？",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "ちくわ",
            "text": "沖縄！！",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "こんぶ",
            "text": "沖縄行きたいです！",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "こんぶ",
            "text": "あ、同じだ！！！",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "すじこ",
            "text": "@ちくわ いいですね",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "すじこ",
            "text": "おお",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "ちくわ",
            "text": "@すじこ そちらは？",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "すじこ",
            "text": "沖縄でなにがしたいですか？",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "ちくわ",
            "text": "暖かいとこに行きたいです笑",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "ちくわ",
            "text": "んーーー",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "こんぶ",
            "text": "私は海を満喫したいです！",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "すじこ",
            "text": "僕はオーストラリア行ってみたいです",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "ちくわ",
            "text": "海ですね！",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "こんぶ",
            "text": "海外ですね！！！",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "ちくわ",
            "text": "おー海外！",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "ちくわ",
            "text": "オーストラリアもいいですね！",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "こんぶ",
            "text": "私、一度も海外行ったことなくて",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "こんぶ",
            "text": "パスポートないです",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "すじこ",
            "text": "海いいですよね",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "こんぶ",
            "text": "やっぱり海外はいいものですか？",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "ちくわ",
            "text": "オーストラリアで何を？",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "こんぶ",
            "text": "海みて、ぼーっとしたいです",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "すじこ",
            "text": "僕も海外ないです",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "すじこ",
            "text": "だからこそですかね",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "ちくわ",
            "text": "@こんぶ マリンスポーツやんね！",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "こんぶ",
            "text": "ないんですね！！失礼しました",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "こんぶ",
            "text": "@ちくわ マリンスポーツいいね",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "ちくわ",
            "text": "あ！ないんですね！",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "こんぶ",
            "text": "だからこそ！！！やっぱり一度は！",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "すじこ",
            "text": "マリンスポーツもしたいですし",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "ちくわ",
            "text": "でも、最近海外行きたいていう",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "ちくわ",
            "text": "欲はないなーー",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "こんぶ",
            "text": "コアラに会えそうですね",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "すじこ",
            "text": "自然の動物も見たいです",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "ちくわ",
            "text": "飛行機に乗ってるの長いし、、",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "すじこ",
            "text": "自然の動物って",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "こんぶ",
            "text": "ん〜！！！オーストラリアならでは！",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "すじこ",
            "text": "野生動物ですね笑",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "ちくわ",
            "text": "野生すね！",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "こんぶ",
            "text": "野性ですよね笑",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "こんぶ",
            "text": "野生でした",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "ちくわ",
            "text": "オーストラリアなら野生動物いっぱいでそう",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "すじこ",
            "text": "@ちくわ 日本語話してくれる方が便利ですしね",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "こんぶ",
            "text": "@すじこ 旅行は、1人で？それとも、ご家族と？",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "ちくわ",
            "text": "まぁそうですね！",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "すじこ",
            "text": "@こんぶ 家族と行きたいですね",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "ちくわ",
            "text": "日本語のほうが楽ですもん！",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "こんぶ",
            "text": "でも、海外でも観光客多いところは、何かと融通ききそうですよね",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "こんぶ",
            "text": "家族サービス！！！ステキです",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "すじこ",
            "text": "みんなの表情も一緒に楽しみたいです",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "ちくわ",
            "text": "@すじこ 家族とどこか旅行いきました？",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "こんぶ",
            "text": "みんなとは、家族？",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "こんぶ",
            "text": "父親の顔ですね",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "ちくわ",
            "text": "@こんぶ そりゃーー家族ちゃう？笑",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "すじこ",
            "text": "@ちくわ 僕もそう思います笑",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "すじこ",
            "text": "家族ですね",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "こんぶ",
            "text": "そのコメントがすっとでてくるなんて",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "こんぶ",
            "text": "私の旦那にな想像できません",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "こんぶ",
            "text": "口下手なんで",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "ちくわ",
            "text": "なんでやねーーーん！！",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "ちくわ",
            "text": "まぁ家族の表情を見たいとは、、",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "こんぶ",
            "text": "@ちくわ 笑",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "ちくわ",
            "text": "絶対に言わない。",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "すじこ",
            "text": "@ちくわ あまり行けてないです",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "こんぶ",
            "text": "言わないんかーーい",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "ちくわ",
            "text": "直近でどこ行きました？",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "こんぶ",
            "text": "お仕事学校、それぞれ忙しいとなかなか時間見つからなそうですね",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "ちくわ",
            "text": "@こんぶ 言わへん。",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "すじこ",
            "text": "@ちくわ 言わないと分かりませんよ",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "こんぶ",
            "text": "え！！！ほんとそう！！！",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "ちくわ",
            "text": "@すじこ わかりました笑",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "こんぶ",
            "text": "@すじこ ほんとそうなんですよ！！！",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "すじこ",
            "text": "@ちくわ 直近だと遊園地ですかね",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "こんぶ",
            "text": "言わないとわかんないのに\n言わないんです！！！",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "こんぶ",
            "text": "遊園地いいですね〜",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "ちくわ",
            "text": "子供も大きくなったら時間合わせるのも大変すよね",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "ちくわ",
            "text": "遊園地！！いいなーー！",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "こんぶ",
            "text": "お子さんも楽しめる年齢だとなお、楽しそう",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "こんぶ",
            "text": "私たちは、まだ一才の娘が1人なので",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "ちくわ",
            "text": "そう！いろんなとこいけるしね！",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "すじこ",
            "text": "言葉って結構大事ですよね",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "こんぶ",
            "text": "いっても公園くらいです",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "すじこ",
            "text": "伝えること",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "こんぶ",
            "text": "@すじこ それは、どうやって気付いたんですか？",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "こんぶ",
            "text": "私の旦那に伝えてほしい！！！！",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "ちくわ",
            "text": "@すじこ いつも伝えてるんですか？",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "ちくわ",
            "text": "奥さんに！",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "すじこ",
            "text": "いつもは伝えません笑",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "ちくわ",
            "text": "なんやねん！！笑",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "こんぶ",
            "text": "あれ！！！！！",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "すじこ",
            "text": "僕も意地悪なので",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "ちくわ",
            "text": "突っ込んでしまいました！",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "ちくわ",
            "text": "まってまってまってーーー！",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "ちくわ",
            "text": "僕は意地悪ちゃいますよ？笑",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "こんぶ",
            "text": "けど、思ってるってことですか？笑",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "すじこ",
            "text": "毎日だと慣れるので",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "ちくわ",
            "text": "流石に、毎日は言いませんよね！",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "こんぶ",
            "text": "あーなるほど。",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "すじこ",
            "text": "@ちくわ もじゃなかったですね笑",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 103,
            "interlocutor_id": "こんぶ",
            "text": "意地悪仲間",
            "mention_to": []
        },
        {
            "utterance_id": 104,
            "interlocutor_id": "こんぶ",
            "text": "適度に伝えてるんですね！！",
            "mention_to": []
        },
        {
            "utterance_id": 105,
            "interlocutor_id": "ちくわ",
            "text": "@すじこ もじゃないですよ！",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 106,
            "interlocutor_id": "ちくわ",
            "text": "笑",
            "mention_to": []
        },
        {
            "utterance_id": 107,
            "interlocutor_id": "すじこ",
            "text": "たまに伝えて喜ぶ姿を見て喜んでます",
            "mention_to": []
        }
