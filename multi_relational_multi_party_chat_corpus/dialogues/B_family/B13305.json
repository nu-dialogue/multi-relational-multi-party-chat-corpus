        },
        {
            "utterance_id": 30,
            "interlocutor_id": "コアラ",
            "text": "賑やかですねー",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "しらたき",
            "text": "@コアラ そのあと嫁にうるさいと言われ",
            "mention_to": [
                "コアラ"
            ]
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "しらたき",
            "text": "みんなで一緒にお風呂に入ります",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "コアラ",
            "text": "あっ、お気持ちわかります",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "コアラ",
            "text": "仲直りのお風呂？",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "しらたき",
            "text": "それですねー！で、またお風呂で騒ぐ笑",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "コアラ",
            "text": "平和ですね",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "つくね",
            "text": "@しらたき ただの悪循環！",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "つくね",
            "text": "@コアラ 平和が一番です",
            "mention_to": [
                "コアラ"
            ]
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "コアラ",
            "text": "@つくね 呆れちゃいますね",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "コアラ",
            "text": "でもいいことです",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "つくね",
            "text": "@コアラ そうなんですよね笑",
            "mention_to": [
                "コアラ"
            ]
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "コアラ",
            "text": "@つくね 見守りですよね",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "しらたき",
            "text": "@コアラ お子さんと喧嘩しますか？",
            "mention_to": [
                "コアラ"
            ]
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "つくね",
            "text": "@コアラ 冷めた目で見守ってます笑",
            "mention_to": [
                "コアラ"
            ]
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "コアラ",
            "text": "@しらたき うちは女の子1人なので友達みたいです",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "コアラ",
            "text": "一緒に買い物行ったり",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "しらたき",
            "text": "@コアラ いいですね！友達みたいな関係",
            "mention_to": [
                "コアラ"
            ]
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "つくね",
            "text": "@コアラ 一緒に買い物いいですねー",
            "mention_to": [
                "コアラ"
            ]
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "コアラ",
            "text": "@しらたき 最近は身長抜かれて見下ろされてます",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "つくね",
            "text": "私もいつかしたいです！",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "コアラ",
            "text": "@つくね すぐきますよ!そういう日",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "しらたき",
            "text": "@コアラ あらら！大きいんですね！",
            "mention_to": [
                "コアラ"
            ]
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "つくね",
            "text": "@コアラ 楽しみですー！女の子いるといつかはって思います",
            "mention_to": [
                "コアラ"
            ]
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "コアラ",
            "text": "@しらたき 私も158ですが、娘は１６０超えたくらいです",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "しらたき",
            "text": "おお！それはちょっと圧を感じますね",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "コアラ",
            "text": "@つくね スイーツ食べたりしますよ！ぜひ",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "コアラ",
            "text": "ケーキ屋さんとか",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "つくね",
            "text": "@コアラ あーめっちゃいいですね！うちも後数年かな…",
            "mention_to": [
                "コアラ"
            ]
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "コアラ",
            "text": "@しらたき はい、チビ呼ばわりですトホホ",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "つくね",
            "text": "娘と趣味が合わないので",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "コアラ",
            "text": "@つくね 女子会ですね",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "つくね",
            "text": "今買い物行ってもほぼ喧嘩です笑",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "コアラ",
            "text": "いや、スイーツは多分大丈夫です",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "コアラ",
            "text": "洋服は仕方ないですが",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "つくね",
            "text": "@コアラ 今度2人でスイーツ食べに行ってみます！",
            "mention_to": [
                "コアラ"
            ]
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "しらたき",
            "text": "@つくね 行ってらっしゃーい！",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "コアラ",
            "text": "@つくね 甘党なら大丈夫なはず",
