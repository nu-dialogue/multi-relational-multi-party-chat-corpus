        },
        {
            "utterance_id": 23,
            "interlocutor_id": "しらたき",
            "text": "@コアラ ガチで怒ってくるので",
            "mention_to": [
                "コアラ"
            ]
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "しらたき",
            "text": "後からケツキックとか！",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "つくね",
            "text": "@しらたき それは仕方ない！",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "コアラ",
            "text": "@しらたき 痛そう",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "しらたき",
            "text": "@コアラ で、やり返すと",
            "mention_to": [
                "コアラ"
            ]
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "しらたき",
            "text": "泣かれる笑",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "コアラ",
            "text": "やり返しちゃう？",
