            ]
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "コアラ",
            "text": "取られないし？",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "しらたき",
            "text": "@コアラ あーなるほど！",
            "mention_to": [
                "コアラ"
            ]
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "しらたき",
            "text": "うちは競争ですね",
