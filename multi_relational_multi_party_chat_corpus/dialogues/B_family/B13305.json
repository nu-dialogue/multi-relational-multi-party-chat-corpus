        },
        {
            "utterance_id": 13,
            "interlocutor_id": "コアラ",
            "text": "@しらたき そうなりますよね、",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "コアラ",
            "text": "それいらないの？みたいな",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "しらたき",
            "text": "そうです！それで俺が横から取って",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "しらたき",
            "text": "怒られますー",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "コアラ",
            "text": "@しらたき パパさん！",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "つくね",
            "text": "@コアラ 好きな物残しておきたい気持ちわかります！",
            "mention_to": [
                "コアラ"
            ]
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "コアラ",
            "text": "@つくね ゆっくりしたいですよね",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "しらたき",
            "text": "@コアラ ははっ！めっちゃ嫌がられます笑",
            "mention_to": [
                "コアラ"
            ]
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "コアラ",
            "text": "@しらたき お子さんは喜びそう",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "コアラ",
            "text": "楽しいパパですね",
