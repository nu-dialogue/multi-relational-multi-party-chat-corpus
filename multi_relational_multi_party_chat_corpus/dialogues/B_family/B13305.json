        },
        {
            "utterance_id": 69,
            "interlocutor_id": "つくね",
            "text": "近くで買ってます",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "コアラ",
            "text": "@つくね ご家族みんな？",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "しらたき",
            "text": "@コアラ ネットショップはよくわからないんで",
            "mention_to": [
                "コアラ"
            ]
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "しらたき",
            "text": "嫁頼みです",
