        },
        {
            "utterance_id": 81,
            "interlocutor_id": "にわとり",
            "text": "うーん　頭にタオル？",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "じゃがいも",
            "text": "サメが温泉に浸かってる人形かしら",
