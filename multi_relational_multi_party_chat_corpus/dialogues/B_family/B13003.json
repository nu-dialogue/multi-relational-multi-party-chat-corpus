        },
        {
            "utterance_id": 88,
            "interlocutor_id": "じゃがいも",
            "text": "@にわとり 腰の位置判定がちょっと難しい",
            "mention_to": [
                "にわとり"
            ]
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "にわとり",
            "text": "確かに・・・・w",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "じゃがいも",
            "text": "バイオハザードとかあるよね",
