            ]
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "つくね",
            "text": "@りんご うちも学童です！",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "りんご",
            "text": "@つくね お弁当が大変よね",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "しらたき",
            "text": "自分のもお願いします！笑",
