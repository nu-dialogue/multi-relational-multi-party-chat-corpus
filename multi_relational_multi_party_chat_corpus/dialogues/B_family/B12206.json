        },
        {
            "utterance_id": 6,
            "interlocutor_id": "えのき",
            "text": "そんなにですか！",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "てばさき",
            "text": "@ばなな 私は今日外でてないですね",
            "mention_to": [
                "ばなな"
