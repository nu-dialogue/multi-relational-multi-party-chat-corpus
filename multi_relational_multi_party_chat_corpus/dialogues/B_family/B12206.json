        },
        {
            "utterance_id": 32,
            "interlocutor_id": "ばなな",
            "text": "@えのき 痛そうです",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "ばなな",
            "text": "辛かったですね",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "えのき",
            "text": "足の爪ってなかなかのびないから…",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "ばなな",
            "text": "そうなんですね",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "えのき",
            "text": "整形外科にも通って",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "てばさき",
            "text": "私も家でよく小指ぶつけるから気をつけないと",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "えのき",
            "text": "一時期松葉づえまで…",
