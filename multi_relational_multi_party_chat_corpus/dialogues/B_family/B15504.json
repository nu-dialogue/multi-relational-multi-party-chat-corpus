    ],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "にわとり",
            "text": "おはようございます！",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "ちくわ",
            "text": "おはようございます。",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "こんぶ",
            "text": "おはようございます〜！",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "こんぶ",
            "text": "朝ごはんはなにを？",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "にわとり",
            "text": "みなさん、趣味をなにかお持ちですか？",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "こんぶ",
            "text": "パン派です？ごはん派？",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "こんぶ",
            "text": "趣味〜〜",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "こんぶ",
            "text": "なかなか見つからなくて探し中です〜",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "にわとり",
            "text": "あっ！パンですねー！",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "ちくわ",
            "text": "趣味かーー",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "こんぶ",
            "text": "@にわとり パン派ですねー！",
            "mention_to": [
                "にわとり"
            ]
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "ちくわ",
            "text": "ご飯です！",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "こんぶ",
            "text": "オシャレです〜",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "にわとり",
            "text": "なんかね、歳をとってくると、",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "にわとり",
            "text": "わはは！ありがとうございます！＞おしゃれ",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "ちくわ",
            "text": "僕は完全ご飯ですね！",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "にわとり",
            "text": "趣味がないとつらくなってきますね・・・",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "こんぶ",
            "text": "私たちはご飯が多くなって〜",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "ちくわ",
            "text": "おしゃれ、、笑",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "こんぶ",
            "text": "@にわとり いまはどんな趣味が？",
            "mention_to": [
                "にわとり"
            ]
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "にわとり",
            "text": "あ！なるほど！ごはんいいですね！",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "こんぶ",
            "text": "おススメありますかー？",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "ちくわ",
            "text": "趣味が気になる！！",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "こんぶ",
            "text": "ハマってよかったものとかあったら",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "にわとり",
            "text": "あっごめんなさい、話題がよれちゃった・・・",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "ちくわ",
            "text": "僕らは飽き性なんです笑",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "こんぶ",
            "text": "聞いときたいです！",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "にわとり",
            "text": "おお！ありがとうございます！",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "にわとり",
            "text": "趣味がですね、古墳めぐりになりました！",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "ちくわ",
            "text": "えーーー！",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "こんぶ",
            "text": "そうなんです〜つい長続きしなくてーー",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "ちくわ",
            "text": "古墳！",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "こんぶ",
            "text": "古墳！！！",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "こんぶ",
            "text": "世界遺産？",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "にわとり",
            "text": "そうそう、古墳！いま、",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "ちくわ",
            "text": "古墳巡りは初めて聞きました！",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "にわとり",
            "text": "古墳界隈が熱いですよー！",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "ちくわ",
            "text": "僕の高校の横に古墳ありましたよ笑",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "にわとり",
            "text": "当地、岡山なんですが、",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "ちくわ",
            "text": "界隈があるんすね",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "こんぶ",
            "text": "えーー！知らなかったです",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "にわとり",
            "text": "岡山に、実は古墳やまほどあるんですよ・・・笑",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "ちくわ",
            "text": "へーー",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "こんぶ",
            "text": "そんなにたくさん！！！",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "ちくわ",
            "text": "それは巡りますね！笑",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "ちくわ",
            "text": "中に入れるんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "こんぶ",
            "text": "わたしは見たこともないかもです笑",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "こんぶ",
            "text": "上から見下ろすけい？",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "にわとり",
            "text": "そうですそうです、古墳を楽しむことを、墳活っていいます笑",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "ちくわ",
            "text": "@こんぶ 上から見下ろせなくない？笑",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "こんぶ",
            "text": "今は古墳ブームなんですね",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "にわとり",
            "text": "えーとね、中に入れたり、登れたりするのもあります。",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "こんぶ",
            "text": "@ちくわ どっこか高台からいけるのかと笑",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "にわとり",
            "text": "入れないものあるんですけど、入れないのは、天皇陵。",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "にわとり",
            "text": "天皇陵なんで、宮内庁管轄なんですーだから入れない。",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "こんぶ",
            "text": "おお！さすが詳しいですね！！",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "にわとり",
            "text": "えへへ",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "ちくわ",
            "text": "すげー詳しい！！",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "こんぶ",
            "text": "あーー！なるほど！！！",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "こんぶ",
            "text": "もう、いくつも巡られてるんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "にわとり",
            "text": "なんか、上にのぼれたりする古墳は楽しくって、",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "ちくわ",
            "text": "高校の横の古墳の周りを走らされてました",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "こんぶ",
            "text": "のぼってみたいです！！",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "こんぶ",
            "text": "話をきくと！！",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "にわとり",
            "text": "えーーーー！古墳の周りを！",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "ちくわ",
            "text": "へー！登れるんだ！",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "ちくわ",
            "text": "はい！",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "ちくわ",
            "text": "3キロ以上あるんで",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "こんぶ",
            "text": "古墳の周りを走る！！！！",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "にわとり",
            "text": "古墳の周りを走るとなると、",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "ちくわ",
            "text": "それを3周走ってました！",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "にわとり",
            "text": "古墳マラソンですね笑",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "ちくわ",
            "text": "まぁ古墳の中ではなく",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "こんぶ",
            "text": "古墳マラソン笑笑",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "ちくわ",
            "text": "まわりの道路すけどね",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "にわとり",
            "text": "おおーーー！！！うらやましい・・・笑",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "ちくわ",
            "text": "古墳マラソンはやばい笑",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "こんぶ",
            "text": "道路かーーい！笑",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "ちくわ",
            "text": "笑",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "ちくわ",
            "text": "だから中の古墳は見てないですよ笑",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "にわとり",
            "text": "上に登れるのは、お弁当とかもってって、",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "こんぶ",
            "text": "私、本当に古墳見たことないです笑",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "にわとり",
            "text": "墳ピクします。",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "ちくわ",
            "text": "航空写真で見ましたが！笑",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "にわとり",
            "text": "墳ピクとは、墳ピクニックです。",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "ちくわ",
            "text": "笑",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "こんぶ",
            "text": "初めてききました！！！その言葉！！！",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "ちくわ",
            "text": "可愛い名前笑",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "にわとり",
            "text": "人の墓のうえで飯食うなよって話ですが・・・",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "こんぶ",
            "text": "墳ピクニック！！！楽しそう〜",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "ちくわ",
            "text": "それ今思ってました笑",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "ちくわ",
            "text": "古墳て",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "ちくわ",
            "text": "確か墓だよなーって",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "こんぶ",
            "text": "あ、、そうか、、",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "こんぶ",
            "text": "たしかに、、、",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "こんぶ",
            "text": "シュールですねーーー",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "ちくわ",
            "text": "先祖さんたちがいる、わら",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "にわとり",
            "text": "そうなんですよ、だから私は墳女です笑",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "ちくわ",
            "text": "楽しそうでいいですね笑",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "にわとり",
            "text": "そうそう！シュールでしょー！",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "ちくわ",
            "text": "神社巡りとかは聞くけど",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "こんぶ",
            "text": "歴女ならぬ！墳女！！！",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "にわとり",
            "text": "なんか、盛り上がってます！",
            "mention_to": []
        },
        {
            "utterance_id": 103,
            "interlocutor_id": "ちくわ",
            "text": "古墳巡りは、お初でした",
            "mention_to": []
        }
