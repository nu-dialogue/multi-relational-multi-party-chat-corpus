    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "ばなな",
            "text": "おはようございます。",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "にんじん",
            "text": "おはようございます",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "たまねぎ",
            "text": "おはようございます",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "ばなな",
            "text": "朝は何を食べましたか？",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "にんじん",
            "text": "あ、まだ食べてないです！",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "たまねぎ",
            "text": "@ばなな まだ食べてないですね",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "ばなな",
            "text": "@にんじん これからですね",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "にんじん",
            "text": "そうですね！",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "にんじん",
            "text": "残り物食べようかなと",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "ばなな",
            "text": "私はだいだいパンです",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "にんじん",
            "text": "私もパンすきです！",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "ばなな",
            "text": "@にんじん 美味しいですよね",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "ばなな",
            "text": "パン屋に行くのが好きです",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "にんじん",
            "text": "セブンのカレーパンもおいしいです！",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "にんじん",
            "text": "パン屋さんワクワクしますよね",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "ばなな",
            "text": "@にんじん そうなのですね",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "たまねぎ",
            "text": "たまにぱんじゃないやつとかおいしい",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "ばなな",
            "text": "@たまねぎ 食べ物、何が好きですか？",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "たまねぎ",
            "text": "@ばなな なんでも食べますがお肉好きです！",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "ばなな",
            "text": "お肉ですね",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "にんじん",
            "text": "手をかけたくない時はステーキ肉焼きます笑",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "にんじん",
            "text": "豪華にみえる笑",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "ばなな",
            "text": "@にんじん ステーキ良いですね",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "にんじん",
            "text": "焼くだけ！",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "ばなな",
            "text": "シンプルで美味しいですね",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "にんじん",
            "text": "ですよね！",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "ばなな",
            "text": "揚げ物は面倒で焼く料理が多いです",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "にんじん",
            "text": "確かに",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "にんじん",
            "text": "でも唐揚げ率たかいです",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "ばなな",
            "text": "から揚げ美味しいですから",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "たまねぎ",
            "text": "家庭の味でますよね",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "ばなな",
            "text": "どんな味付けですか？",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "にんじん",
            "text": "うちはニンニクと醤油です！",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "ばなな",
            "text": "美味しそう",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "にんじん",
            "text": "大きめで作ります",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "ばなな",
            "text": "そうなのですね",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "にんじん",
            "text": "コロッケは市販のにしてます",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "にんじん",
            "text": "あれ手間かかりますよね",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "ばなな",
            "text": "@にんじん わかります",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "にんじん",
            "text": "だから、揚げるだけのやつ",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "ばなな",
            "text": "なので作りません",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "にんじん",
            "text": "ですよね！",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "たまねぎ",
            "text": "そうだったのか",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "たまねぎ",
            "text": "知らなかった",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "ばなな",
            "text": "でも揚げているのですね",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "にんじん",
            "text": "はい！",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "にんじん",
            "text": "お店にあるコロッケは美味しすぎます！",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "にんじん",
            "text": "お肉屋さんのとか",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "ばなな",
            "text": "わかります",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "にんじん",
            "text": "食べ歩きの時とか",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "にんじん",
            "text": "ついつい食べちゃう",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "ばなな",
            "text": "お肉屋で買います",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "にんじん",
            "text": "近くにあるんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "ばなな",
            "text": "揚げたてが美味しくて",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "たまねぎ",
            "text": "@ばなな わかります！",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "ばなな",
            "text": "近くにあります",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "にんじん",
            "text": "いいですね！",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "にんじん",
            "text": "うちはスーパーしかなくて",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "ばなな",
            "text": "そうでしたか",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "にんじん",
            "text": "メンチカツとか",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "にんじん",
            "text": "いいですね！",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "たまねぎ",
            "text": "朝から食べたくなりました",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "ばなな",
            "text": "@にんじん 食べたくなりました",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "にんじん",
            "text": "ですよね",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "にんじん",
            "text": "あ、後",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "にんじん",
            "text": "ソースカツ丼よく作ります！",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "ばなな",
            "text": "すごいですね",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "にんじん",
            "text": "こっちのソースカツ丼はソースが変わってて",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "にんじん",
            "text": "かけるんじゃなくてひたすというか",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "ばなな",
            "text": "どんな感じですか",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "にんじん",
            "text": "甘いソースです！",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "たまねぎ",
            "text": "名物料理ですよ！",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "ばなな",
            "text": "ソースかつ丼でも色々ですね",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "にんじん",
            "text": "そうですよね！",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "にんじん",
            "text": "何か名物ありますか？",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "ばなな",
            "text": "@にんじん 東京なのですけれど",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "にんじん",
            "text": "はい",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "ばなな",
            "text": "下町なので",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "ばなな",
            "text": "もんじゃとかになるのかなと",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "たまねぎ",
            "text": "@ばなな おーいいですね！",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "にんじん",
            "text": "@ばなな もんじゃは食べ方がわからないです！",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "にんじん",
            "text": "正解なのかわからない笑",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "ばなな",
            "text": "私もわからないです",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "にんじん",
            "text": "@ばなな そうなんですか？！笑",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "ばなな",
            "text": "一緒に行った人にお任せで",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "にんじん",
            "text": "あー笑",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "ばなな",
            "text": "私はお好み焼きが好きです",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "たまねぎ",
            "text": "@ばなな 僕もです！",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "ばなな",
            "text": "食べた感じがします",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "にんじん",
            "text": "@ばなな あ、わかります！",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "にんじん",
            "text": "お好み焼きでご飯食べますか？",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "ばなな",
            "text": "@にんじん ないですね",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "にんじん",
            "text": "ですよね！",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "ばなな",
            "text": "お好み焼きだけ",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "にんじん",
            "text": "私も無理です",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "にんじん",
            "text": "焼きそばとご飯はいけます",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "たまねぎ",
            "text": "同じ炭水化物なのに",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "ばなな",
            "text": "その組み合わせありますね",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "にんじん",
            "text": "何が違うのか笑",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "ばなな",
            "text": "炭水化物どうしだと",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "ばなな",
            "text": "ラーメンとチャーハンが良いです",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "にんじん",
            "text": "間違いない",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "ばなな",
            "text": "そうですね",
            "mention_to": []
        },
        {
            "utterance_id": 103,
            "interlocutor_id": "たまねぎ",
            "text": "餃子がすきです",
            "mention_to": []
