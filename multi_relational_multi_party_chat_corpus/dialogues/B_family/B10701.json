        },
        {
            "utterance_id": 76,
            "interlocutor_id": "しらたき",
            "text": "@りんご 結構神経質で",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "りんご",
            "text": "@つくね わかるー、うちも学童いってました！",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "しらたき",
            "text": "夜勤で寝てる時に",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "しらたき",
            "text": "子供の声で目覚めます笑",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "りんご",
            "text": "@しらたき 辛いですねー",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "つくね",
            "text": "@りんご 学童いいですがお弁当が…",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "つくね",
            "text": "子供たちは",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "つくね",
            "text": "楽しみみたいですがね！笑",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "りんご",
            "text": "@つくね そうなんですよね、毎日だから",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "しらたき",
            "text": "@りんご 辛いです！耳栓必須です",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "つくね",
            "text": "@りんご そう！毎日なんですよ",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "りんご",
            "text": "@しらたき 耳栓も必要なくらい？",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "つくね",
            "text": "数日だったら頑張れる！",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "しらたき",
            "text": "そうなんです、、、耳栓必須です",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "りんご",
            "text": "@つくね でも甥っ子そうかも",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "しらたき",
            "text": "結構外の音も気になります笑",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "りんご",
            "text": "@しらたき 叫ぶよねー、子供って",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "つくね",
            "text": "@りんご そうなんですか！？",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "しらたき",
            "text": "@りんご 叫びます笑",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "しらたき",
            "text": "あと足音",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "つくね",
            "text": "@しらたき あー確かに！",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "りんご",
            "text": "@つくね 甥っ子と姪っ子やばいです！",
            "mention_to": [
