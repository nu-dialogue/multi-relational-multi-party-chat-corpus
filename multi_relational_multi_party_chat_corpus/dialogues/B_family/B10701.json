        },
        {
            "utterance_id": 4,
            "interlocutor_id": "つくね",
            "text": "@りんご そうですね！",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "つくね",
            "text": "雨が降ってます",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "りんご",
            "text": "体調とか大丈夫ですか？",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "しらたき",
            "text": "@りんご 昨日だいぶ暖かかったのに",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "しらたき",
            "text": "今日は寒いです",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "りんご",
            "text": "@しらたき 本当ですね",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "つくね",
            "text": "@りんご 大丈夫ですが町内の",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "つくね",
            "text": "小学校では",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "つくね",
            "text": "インフルエンザが流行ってます！",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "しらたき",
            "text": "@りんご さんのところは雨ですか？",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "りんご",
            "text": "@つくね インフル！私もこないだコロナになりました",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "りんご",
            "text": "@しらたき 雨です！",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "つくね",
            "text": "@りんご コロナもインフルも急に広がりますよね",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "しらたき",
            "text": "@りんご雨は嫌いです",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "りんご",
            "text": "@つくね そうなんですよね、息子からもらいました",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "つくね",
            "text": "@りんご そうなんですね！？息子さんおいくつですか？",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "りんご",
            "text": "@しらたき 私も雨いやでーす！",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "しらたき",
            "text": "@りんご 傘とかめんどくさいです笑",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "りんご",
            "text": "@つくね もう29歳ですよー",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "つくね",
            "text": "え！？",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "つくね",
            "text": "大きいですね！！",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "りんご",
            "text": "@しらたき たしかにー傘最近さしてはい",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "りんご",
            "text": "@つくね そうなんです！おっさんはいってます",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "しらたき",
            "text": "@りんご 小さい頃は傘さしたかったのに笑",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "つくね",
            "text": "@りんご ちょっと羨ましいです！",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "つくね",
            "text": "今手に負えない時期で笑",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "りんご",
            "text": "@しらたき そうかも、傘さすの好きだったかも",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "りんご",
            "text": "@つくね いくつですか？",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "しらたき",
            "text": "@りんご カッパより傘さしたかったけど",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "しらたき",
            "text": "かーちゃんに言われて",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "しらたき",
            "text": "しぶしぶカッパでした",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "つくね",
            "text": "@りんご 7歳と10歳なんですが",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "りんご",
            "text": "@カッパ！かっこ悪いから私も着なかった",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "つくね",
            "text": "下の子がイヤイヤ期か？ってくらい",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "つくね",
            "text": "すぐ拗ねます笑",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "しらたき",
            "text": "@りんご ですよねー！",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "しらたき",
            "text": "動きづらいし",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "りんご",
            "text": "@つくね 大変な時期ですね、でも楽しそう",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "しらたき",
            "text": "@つくね 確かに！大変だよね笑",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "つくね",
            "text": "@りんご 楽しい事もありますが",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "つくね",
            "text": "毎日の機嫌取りが笑",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "りんご",
            "text": "@つくねなかなか楽しめないけど終わると寂しい",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "つくね",
            "text": "@りんご それはありますよね！",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "つくね",
            "text": "ハイハイの時期とか懐かし過ぎます！",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "りんご",
            "text": "@つくね 本当切実にそうですよー",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "つくね",
            "text": "@りんご 今を楽しまないとですが中々笑",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "しらたき",
            "text": "@つくね がんばるしかないね！",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "りんご",
            "text": "@しらたき 育児参加型ですか？",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "しらたき",
            "text": "@りんご 参加してますが",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "しらたき",
            "text": "やっぱりママの方が率が多いです",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "つくね",
            "text": "@りんご ちなみに今日はこの後",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "りんご",
            "text": "@しらたき それはそうなりますけどねー",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "つくね",
            "text": "参観日です！笑",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "りんご",
            "text": "@つくね 寒いのに大変！",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "しらたき",
            "text": "@りんご 今日の参加日は不参加です！",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "りんご",
            "text": "@しらたき そうなんですね",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "つくね",
            "text": "@りんご 今年度最後なんです！",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "りんご",
            "text": "@つくね そっか！そういう時期ですね",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "しらたき",
            "text": "@りんご この後仕事なので",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "つくね",
            "text": "@りんご 早いですよね！",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "つくね",
            "text": "もうすぐ魔の春休み！",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "りんご",
            "text": "@しらたき お仕事も大切です",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "しらたき",
            "text": "@つくね ヤバいね！",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "りんご",
            "text": "@つくね きゃー春休み！",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "りんご",
            "text": "@しらたき やばいっすね",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "つくね",
            "text": "@りんご 家にいるので",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "つくね",
            "text": "大変です！！笑",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "りんご",
            "text": "@つくね ご飯とか？",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "しらたき",
            "text": "@りんご ヤバいっす！できれば学校に",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "しらたき",
            "text": "行ってて欲しい笑",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "りんご",
            "text": "@しらたき 誰もが思ってますよー",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "つくね",
            "text": "@りんご ごはんもですが、学童でお弁当が大変です",
            "mention_to": [
                "りんご"
