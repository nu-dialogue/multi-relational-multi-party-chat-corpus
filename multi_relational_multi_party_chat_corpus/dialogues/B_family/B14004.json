        },
        {
            "utterance_id": 77,
            "interlocutor_id": "たまご",
            "text": "しなかったんですねー",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "カンガルー",
            "text": "ピアノもギターも弾けません！",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "たまご",
            "text": "弾けないんかい！",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "カンガルー",
            "text": "夢叶わなかったんで！",
