            "utterance_id": 0,
            "interlocutor_id": "カンガルー",
            "text": "こんにちは〜！",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "つみれ",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "たまご",
            "text": "こんにちはー",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "カンガルー",
            "text": "今、何か習い事とかされてますか？",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "たまご",
            "text": "なんにもしてないですー",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "つみれ",
            "text": "習い事は特にして無いです",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "つみれ",
            "text": "@カンガルー 何かしてますか？",
            "mention_to": [
                "カンガルー"
            ]
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "カンガルー",
            "text": "何か趣味とかやりたい事とか",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "カンガルー",
            "text": "ありますかー？",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "たまご",
            "text": "楽器とかやってみたいです",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "カンガルー",
            "text": "私も何もしてないです",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "たまご",
            "text": "ギターとかドラムとか",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "つみれ",
            "text": "@カンガルー 刺繍を始めたいなと思ってます",
            "mention_to": [
                "カンガルー"
            ]
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "カンガルー",
            "text": "@たまご 楽器！",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "たまご",
            "text": "そうです！",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "つみれ",
            "text": "@たまご 始めたらいい！",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "カンガルー",
            "text": "@つみれ 刺繍ですか！ネットとかで教えてくれそう",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "たまご",
            "text": "弾けたらカッコいいし",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "カンガルー",
            "text": "@たまご ギターかっこいいです！",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "たまご",
            "text": "楽しそう",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "つみれ",
            "text": "@カンガルー YouTubeとかで親切に教えてくれます",
            "mention_to": [
                "カンガルー"
            ]
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "つみれ",
            "text": "ギター弾けたらかっこいいですね",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "つみれ",
            "text": "私、昔ドラムやってましたけど・・・",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "カンガルー",
            "text": "今ってお金かけなくてもYouTube先生が何でも教えてくれますね！",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "たまご",
            "text": "かっこいいよねー",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "たまご",
            "text": "ネット強いです",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "カンガルー",
            "text": "私はZUMBAがやってみたい！",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "つみれ",
            "text": "@カンガルー YouTube先生最高です！",
            "mention_to": [
                "カンガルー"
            ]
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "つみれ",
            "text": "ズンバ！体験はしたことあります",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "カンガルー",
            "text": "@つみれ さんえー！どうでしたか？",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "つみれ",
            "text": "結構ハードでしたよ",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "カンガルー",
            "text": "おお！",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "たまご",
            "text": "ダンスみたいなやつ？",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "つみれ",
            "text": "いろんなジャンルが混ざってるから",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "カンガルー",
            "text": "ますますやってみたくなる！",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "つみれ",
            "text": "楽しかったです！",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "カンガルー",
            "text": "昔、エアロビにハマっていて",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "カンガルー",
            "text": "結婚したら時間がなくなり",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "つみれ",
            "text": "結婚するとなかなか難しいですね",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "カンガルー",
            "text": "いつか復活したいと思ってたんですが",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "カンガルー",
            "text": "今ってスポーツクラブのメニューにエアロビが無いんですよねー",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "つみれ",
            "text": "ズンバってエアロビとかジャズダンスとかヒップホップとか色々が混ざってて",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "カンガルー",
            "text": "うんうん！",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "カンガルー",
            "text": "楽しそう！",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "カンガルー",
            "text": "やっぱりやりたいな！",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "つみれ",
            "text": "体にめっちゃ効きそうでした",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "カンガルー",
            "text": "探してみようかな",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "つみれ",
            "text": "エアロビって確かに聞かなくなりましたね",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "カンガルー",
            "text": "踊ってストレス発散",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "つみれ",
            "text": "体動かしてストレス発散できるのは最高ですね",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "カンガルー",
            "text": "@たまご さんドラムもストレス発散できそう！",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "つみれ",
            "text": "YOSHIKIみたいに叩いたら体使うし",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "カンガルー",
            "text": "YOSHIKI！",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "つみれ",
            "text": "そうでなくても結構ハードですけど",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "カンガルー",
            "text": "ドラムは体力必要そう！",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "カンガルー",
            "text": "ですよね！",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "つみれ",
            "text": "私、結局ものにできずで",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "つみれ",
            "text": "お遊び程度で終わってしまいました",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "カンガルー",
            "text": "刺繍もいいなぁ",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "たまご",
            "text": "ドラむたのしそう",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "カンガルー",
            "text": "何か穏やかな時間が流れてる感じがする",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "たまご",
            "text": "ジャズ系もちょっと興味ある",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "つみれ",
            "text": "@カンガルー 刺繍をゆっくりのんびりやりたいです",
            "mention_to": [
                "カンガルー"
            ]
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "カンガルー",
            "text": "ジャズ難しそう！",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "カンガルー",
            "text": "だけどカッコいい！",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "つみれ",
            "text": "ジャズはセンスないと難しいですね",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "つみれ",
            "text": "ウッドベースとかできたらかっこいいですね",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "たまご",
            "text": "即興みたいな感じだもんね",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "たまご",
            "text": "ベースもかっこいいー",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "つみれ",
            "text": "@たまご そう、その場で合わせていける技術がないと",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "カンガルー",
            "text": "私、親友の結婚式でピアノとかギター弾くのを夢見てたんですよ",
