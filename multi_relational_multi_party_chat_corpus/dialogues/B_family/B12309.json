    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "すじこ",
            "text": "こんばんは",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "しらたき",
            "text": "こんばんは",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "つくね",
            "text": "こんばんは",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "すじこ",
            "text": "体毛って処理してます？",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "しらたき",
            "text": "してました！",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "つくね",
            "text": "@すじこ してます！",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "しらたき",
            "text": "脱毛行ってました",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "すじこ",
            "text": "@しらたき どこまでしてました？",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "しらたき",
            "text": "髭だけですけど",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "すじこ",
            "text": "@つくね どのくらいのペースですか？",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "つくね",
            "text": "@すじこ 冬はサボってますが笑",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "すじこ",
            "text": "@しらたき 髭濃いんですか？",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "つくね",
            "text": "夏はほぼ毎日です！笑",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "すじこ",
            "text": "@つくね 冬サボりがちって言いますよね笑",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "つくね",
            "text": "@すじこ は処理してますかー？",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "しらたき",
            "text": "@すじこ 青髭が嫌で通ってました",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "つくね",
            "text": "@すじこ 見えないんで！笑",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "すじこ",
            "text": "@つくね 僕は腕と脇ですね",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "つくね",
            "text": "おぉ！そうなんですね！",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "しらたき",
            "text": "@すじこ 脱毛ですか？",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "すじこ",
            "text": "@しらたき 脱毛してからは改善されたんですか？",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "つくね",
            "text": "腕ツルツルなんですね！",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "しらたき",
            "text": "@すじこ 改善しました！",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "すじこ",
            "text": "@しらたき 剃ってるだけですね",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "しらたき",
            "text": "@すじこ 青髭ないですよ",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "すじこ",
            "text": "腕毛が濃いというより長くて",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "つくね",
            "text": "@すじこ 私腕が日光アレルギーで",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "つくね",
            "text": "カミソリが中々使えず…",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "つくね",
            "text": "なのでサボり気味です！笑",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "すじこ",
            "text": "@しらたき 青髭がくなっても生えてはくる感じですか？",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "しらたき",
            "text": "生えます！",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "しらたき",
            "text": "最近脱毛してないので生えてきてます",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "すじこ",
            "text": "@つくね 日光アレルギーって日に当たると蕁麻疹が出るとかですか？",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "つくね",
            "text": "@しらたき 確かに最近行ってないよね",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "しらたき",
            "text": "やっぱり通わなきゃダメですね",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "つくね",
            "text": "@すじこ そうです！紫外線だと思うんですが",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "つくね",
            "text": "痒くなっちゃうんで",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "すじこ",
            "text": "@しらたき 整えれるオシャレ髭くらいな感じですか？",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "つくね",
            "text": "刺激を与えられないです…",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "しらたき",
            "text": "そをやな感じです！",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "しらたき",
            "text": "そんな感じです！",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "しらたき",
            "text": "間違えた、、",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "すじこ",
            "text": "@つくね 夏の海やプールは露出出来ないんですね？",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "つくね",
            "text": "@すじこ そうなんです！",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "すじこ",
            "text": "@しらたき 僕は無精髭になるんで羨ましい",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "しらたき",
            "text": "@つくね お腹周りが、、、",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "つくね",
            "text": "日焼け止め必須です！",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "つくね",
            "text": "@しらたき それいうな！！",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "しらたき",
            "text": "@すじこ あんまり手入れはしてないんですがね",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "すじこ",
            "text": "@つくね 露天風呂も難しいですか？",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "つくね",
            "text": "@すじこ 露天風呂は大丈夫です！",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "つくね",
            "text": "多分お湯が良い！",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "すじこ",
            "text": "日焼け止めがあればなんとか避けれるんですね",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "つくね",
            "text": "温泉の成分で！",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "しらたき",
            "text": "@つくね 運転とかやばいよね",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "すじこ",
            "text": "温泉成分凄いな笑",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "しらたき",
            "text": "太陽の光で",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "つくね",
            "text": "@すじこ 日焼け止めあれば大丈夫ですが、結構塗り直さないとです！",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "すじこ",
            "text": "ああそっか",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "つくね",
            "text": "@しらたき 運転ヤバい…",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "つくね",
            "text": "カーテン欲しい！",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "しらたき",
            "text": "フロントに！？",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "すじこ",
            "text": "1歩外に出ればですね",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "つくね",
            "text": "@しらたき サイドに！笑",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "すじこ",
            "text": "フロントは大事に笑",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "つくね",
            "text": "@すじこ 昔はこんなことなかったですが、",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "つくね",
            "text": "産後だいぶ体質が変わりました！",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "すじこ",
            "text": "妻も産後変わりました！",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "しらたき",
            "text": "@つくね 歳じゃね？",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "つくね",
            "text": "@しらたき いや、産後だからね！",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "つくね",
            "text": "@すじこ やっぱそうですよね…",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "すじこ",
            "text": "@しらたき 歳か",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "しらたき",
            "text": "@つくね へー",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "つくね",
            "text": "@しらたき え！？嫌味？笑",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "つくね",
            "text": "自分が若いからか！？笑",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "すじこ",
            "text": "@つくね 病気に強かったのに弱くなったり",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "しらたき",
            "text": "最近仕事終わり足が臭すぎてやばい",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "すじこ",
            "text": "虫歯になったりとかありました",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "つくね",
            "text": "@すじこ それはありますね！",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "つくね",
            "text": "インフルエンザもコロナもやりました笑",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "つくね",
            "text": "@しらたき 前からじゃん！",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "つくね",
            "text": "@しらたき 靴下自分で洗ってくださいー！",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "すじこ",
            "text": "@しらたき 同じく歳を重ねてるので笑",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "しらたき",
            "text": "加齢臭、、、",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "つくね",
            "text": "@しらたき まだ30代なのにね笑",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "すじこ",
            "text": "@つくね インフルエンザもコロナも同じく経験しました笑",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "つくね",
            "text": "@すじこ 両方ヤバいですよね笑",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "しらたき",
            "text": "最近虫歯で歯の神経抜きました",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "すじこ",
            "text": "@しらたき 枕が臭いと言われます笑",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "つくね",
            "text": "コロナは少し後遺症が残りました…",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "しらたき",
            "text": "@すじこ 切ない、、、",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "すじこ",
            "text": "@つくね 後遺症なにが残ったんですか？",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "つくね",
            "text": "@すじこ 味覚が戻るの遅かったですね…",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "つくね",
            "text": "今は全快です！",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "しらたき",
            "text": "@つくね 全く同じ後遺症だったね",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "すじこ",
            "text": "@しらたき 服とか重なってると怒られます笑",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "つくね",
            "text": "@しらたき そうだよね…しんどかったね！",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "つくね",
            "text": "家族でかかるのはヤバいね",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "しらたき",
            "text": "@すじこ うちはそこまでではない！と思う",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "すじこ",
            "text": "味がしない感じですか？",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "つくね",
            "text": "@すじこ 味がしなかったです！",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "つくね",
            "text": "無味でした！",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "しらたき",
            "text": "@すじこ タバコですら味なかったです",
            "mention_to": [
                "すじこ"
            ]
        }
