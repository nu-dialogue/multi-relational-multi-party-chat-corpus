        {
            "utterance_id": 76,
            "interlocutor_id": "にんじん",
            "text": "家での仕事は楽ですね",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "にんじん",
            "text": "家事もできますし",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "ばなな",
            "text": "確かに",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "にんじん",
            "text": "掃除も",
