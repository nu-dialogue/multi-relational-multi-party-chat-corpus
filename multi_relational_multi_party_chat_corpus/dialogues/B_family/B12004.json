        },
        {
            "utterance_id": 94,
            "interlocutor_id": "ばなな",
            "text": "確かに濡れますからね",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "にんじん",
            "text": "子供にやらせる事が多いです",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "ばなな",
            "text": "良いですね",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "たまねぎ",
            "text": "おこづかいあげて",
