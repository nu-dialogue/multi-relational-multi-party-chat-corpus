        },
        {
            "utterance_id": 72,
            "interlocutor_id": "にんじん",
            "text": "店舗併用です！",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "ばなな",
            "text": "@たまねぎ 車なのですね",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "たまねぎ",
            "text": "@ばなな 会社勤めです！",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "ばなな",
            "text": "そうなのですね",
