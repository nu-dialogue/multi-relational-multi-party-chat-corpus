            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "ばなな",
            "text": "@にんじん 作るのですか？",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "にんじん",
            "text": "@ばなな 食べに行っちゃおうかなと",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "にんじん",
            "text": "たまには",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "たまねぎ",
            "text": "@にんじん やったー",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "ばなな",
            "text": "それは良いですね",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "にんじん",
            "text": "何味が好きですか？",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "ばなな",
            "text": "味噌ですかね",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "にんじん",
            "text": "@ばなな 私もです！",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "にんじん",
            "text": "辛いのも好きです",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "ばなな",
            "text": "@にんじん 一緒ですね",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "にんじん",
            "text": "@ばなな 家系と",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "にんじん",
            "text": "二郎系あるじゃないですかー",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "にんじん",
            "text": "どちらか好きですか？",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "ばなな",
            "text": "ありますね",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "ばなな",
            "text": "違いがわかっていなくて",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "にんじん",
            "text": "@ばなな 私もです笑",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "ばなな",
            "text": "どう違うのでしょう？",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "にんじん",
            "text": "もやしたっぷりだと二郎系っていうくらいで",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "にんじん",
            "text": "家系の基準がわかりません",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "ばなな",
            "text": "そうなのですね",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "ばなな",
            "text": "私はあっさりした感じが良いです",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "たまねぎ",
            "text": "@ばなな おしゃれなラーメンも増えましたよね",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "にんじん",
            "text": "カフェっぽい感じ",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "ばなな",
            "text": "そうですね",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "にんじん",
            "text": "行きやすいですね",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "たまねぎ",
            "text": "服も匂わない！",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "ばなな",
            "text": "なるほど",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "にんじん",
            "text": "豚骨は臭くなりますが",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "ばなな",
            "text": "それは良いですね",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "ばなな",
            "text": "臭くなるのは嫌ですね",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "にんじん",
            "text": "昔ラーメン屋でバイトしてまして",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "にんじん",
            "text": "毎日臭かったです笑",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "ばなな",
            "text": "すごいですね",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "にんじん",
            "text": "まかないでたべまくって",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "ばなな",
            "text": "疲れなかったですか？",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "にんじん",
            "text": "太りました！",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "にんじん",
            "text": "ラーメン屋さんは時間が経つのが遅いです",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "ばなな",
            "text": "体力いるイメージがします",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "にんじん",
            "text": "体力減りますね！",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "ばなな",
            "text": "立っているのは疲れますよね",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "にんじん",
            "text": "@ばなな お仕事は立ち仕事ではないですか？",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "ばなな",
            "text": "@にんじん ずっと座っています",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "にんじん",
            "text": "お昼の後は眠くなりますね",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "たまねぎ",
            "text": "ポカポカだと余計",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "ばなな",
            "text": "確かに眠くなりますね",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "ばなな",
            "text": "最近、暖かいので",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "たまねぎ",
            "text": "ですよね！",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "ばなな",
            "text": "@にんじん デスクワークですか？",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "にんじん",
            "text": "@ばなな いえ、今は美容系の仕事してます",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "にんじん",
            "text": "なので座りっぱなしの時もあれば",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "にんじん",
            "text": "立ちっぱなしもあります！",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "ばなな",
            "text": "なるほど",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "たまねぎ",
            "text": "@ばなな OLさんですね！",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "ばなな",
            "text": "@たまねぎ お家で仕事しています",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "ばなな",
            "text": "通勤なしです",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "にんじん",
            "text": "@ばなな そうなんですね！",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "ばなな",
            "text": "もう人混みには出られません",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "にんじん",
            "text": "@ばなな わかりますー！",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "ばなな",
            "text": "通勤は電車ですか？",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "たまねぎ",
            "text": "@ばなな 車です！",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "にんじん",
            "text": "私は家の中です！",
