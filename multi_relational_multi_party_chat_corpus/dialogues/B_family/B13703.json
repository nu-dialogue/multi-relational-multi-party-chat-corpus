        },
        {
            "utterance_id": 2,
            "interlocutor_id": "えのき",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "ぺんぎん",
            "text": "この時期は行事とか忙しいですか？",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "えのき",
            "text": "うちは三女の卒業式がありますね～",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "えのき",
            "text": "中学校卒業です",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "ぺんぎん",
            "text": "@えのき おーそれは大変ですね",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "てばさき",
            "text": "@ぺんぎん 私はスカイツリー行くくらいです",
            "mention_to": [
                "ぺんぎん"
            ]
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "えのき",
            "text": "@てばさき いいな～",
            "mention_to": [
                "てばさき"
            ]
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "てばさき",
            "text": "@ぺんぎん 絶賛春休みです",
            "mention_to": [
                "ぺんぎん"
            ]
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "ぺんぎん",
            "text": "@てばさき スカイツリー！私、行ったことないんです",
            "mention_to": [
                "てばさき"
            ]
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "ぺんぎん",
            "text": "@てばさき 春休み！いいなあ",
            "mention_to": [
                "てばさき"
            ]
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "えのき",
            "text": "@ぺんぎん 私も展望台登ったことないです",
            "mention_to": [
                "ぺんぎん"
            ]
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "ぺんぎん",
            "text": "@えのき 高いと大丈夫ですか？",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "えのき",
            "text": "@てばさき 展望台も行くの？",
            "mention_to": [
                "てばさき"
            ]
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "てばさき",
            "text": "@えのき 展望台がお目当て！",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "えのき",
            "text": "@ぺんぎん まあまあ大丈夫です",
            "mention_to": [
                "ぺんぎん"
            ]
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "ぺんぎん",
            "text": "@てばさき おー！",
            "mention_to": [
                "てばさき"
            ]
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "えのき",
            "text": "@てばさき 展望台目当てなんだね",
            "mention_to": [
                "てばさき"
            ]
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "えのき",
            "text": "なにかイベントが？",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "てばさき",
            "text": "@えのき 原神コラボの展示がやっているのです",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "えのき",
            "text": "原神かー",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "えのき",
            "text": "好きだね",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "ぺんぎん",
            "text": "@てばさき いいですね",
            "mention_to": [
                "てばさき"
            ]
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "てばさき",
            "text": "@えのき 好きだよー",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "えのき",
            "text": "どんな展示やってるの？",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "ぺんぎん",
            "text": "@てばさき 楽しみあるっていいですよね",
            "mention_to": [
                "てばさき"
            ]
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "てばさき",
            "text": "@えのき わかんないけど動画とかもあるらしい",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "えのき",
            "text": "カフェとかもあるの？",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "てばさき",
            "text": "@えのきコラボカフェあるみたい！",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "えのき",
            "text": "食べすぎておなか壊さないでね…！",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "ぺんぎん",
            "text": "@てばさき どんかメニューあるんだろうね！楽しみですね",
            "mention_to": [
                "てばさき"
            ]
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "てばさき",
            "text": "@えのき 楽しみではあるが次の日が試験だと思うと…",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "えのき",
            "text": "そういえばてばさん、その次の日試験なんですよね…",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "えのき",
            "text": "余裕ですね！！",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "ぺんぎん",
            "text": "@てばさき あれ、そ、それは行ってから考える？",
            "mention_to": [
                "てばさき"
            ]
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "えのき",
            "text": "合格間違いなし！！",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "てばさき",
            "text": "@えのき 母上やっぱり延長",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "えのき",
            "text": "@てばさき だが断る",
            "mention_to": [
                "てばさき"
            ]
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "てばさき",
            "text": "@えのき 父に直接交渉すればワンチャン",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "ぺんぎん",
            "text": "@てばさき 仲良いですね笑",
            "mention_to": [
                "てばさき"
            ]
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "えのき",
            "text": "@ぺんぎん さんのとこは、お子さんいらっしゃいますか？",
            "mention_to": [
                "ぺんぎん"
            ]
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "ぺんぎん",
            "text": "@えのき うちはもう成人してるのでそんな感じじゃないんです",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "えのき",
            "text": "そうなんですね",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "ぺんぎん",
            "text": "@えのき しかも男の子",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "えのき",
            "text": "お子さんといっしょの趣味とか…共通の話題とかありますか？",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "てばさき",
            "text": "@ぺんぎん 成人済みでしたか！",
            "mention_to": [
                "ぺんぎん"
            ]
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "ぺんぎん",
            "text": "@てばさき えのきさんと出かけます？",
            "mention_to": [
                "てばさき"
            ]
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "てばさき",
            "text": "@ぺんぎん この間東京一緒に行きました！",
            "mention_to": [
                "ぺんぎん"
            ]
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "ぺんぎん",
            "text": "@えのき カフェとかたまに行きますね、お店とか共有したりとか",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "ぺんぎん",
            "text": "@てばさき お買い物？",
            "mention_to": [
                "てばさき"
            ]
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "ぺんぎん",
            "text": "@えのき 一緒にお出かけいいですね、",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "てばさき",
            "text": "@ぺんぎん 図書館と展示会に行きました",
            "mention_to": [
                "ぺんぎん"
            ]
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "えのき",
            "text": "@ぺんぎん いっしょにカフェ、いいですね！",
            "mention_to": [
                "ぺんぎん"
            ]
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "えのき",
            "text": "@ぺんぎん てばさんと出かけたいんですが、",
            "mention_to": [
                "ぺんぎん"
            ]
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "えのき",
            "text": "勉強が終わらないので行けないんですよ",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "ぺんぎん",
            "text": "@てばさき 図書館すごい！",
            "mention_to": [
                "てばさき"
            ]
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "てばさき",
            "text": "@えのき 勉強とは果てしない",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "ぺんぎん",
            "text": "@てばさき 勉強おわしてえのきさんとでかけましよ",
            "mention_to": [
                "てばさき"
            ]
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "えのき",
            "text": "@ぺんぎん 前々から行ってみたかったので、国会図書館行ってきました",
            "mention_to": [
                "ぺんぎん"
            ]
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "えのき",
            "text": "すごく広々としていて人もいっぱいいました",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "てばさき",
            "text": "@ぺんぎん 本登録してきました！",
            "mention_to": [
                "ぺんぎん"
            ]
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "ぺんぎん",
            "text": "@えのき 国立図書館、すごい本ありそう",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "ぺんぎん",
            "text": "どんな本みたんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "てばさき",
            "text": "@ぺんぎん 新聞がたくさん保存されてました！",
            "mention_to": [
                "ぺんぎん"
            ]
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "ぺんぎん",
            "text": "@てばさき 昔のからずっと？",
            "mention_to": [
                "てばさき"
            ]
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "えのき",
            "text": "かなり古いものや、地方紙も揃ってましたね",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "てばさき",
            "text": "@ぺんぎん 古いのはなんか縮小印刷されて冊子になってました",
            "mention_to": [
                "ぺんぎん"
            ]
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "ぺんぎん",
            "text": "@てばさき なるほど、めっちゃ多いもんね",
            "mention_to": [
                "てばさき"
            ]
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "えのき",
            "text": "@ぺんぎん 図書館とか行かれますか？",
            "mention_to": [
                "ぺんぎん"
            ]
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "ぺんぎん",
            "text": "@えのき 昔はよく行ってました",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "えのき",
            "text": "@ぺんぎん そうなんですね",
            "mention_to": [
                "ぺんぎん"
            ]
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "ぺんぎん",
            "text": "今はわんちゃんいるからあまり行けなくなっちゃって",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "えのき",
            "text": "あー",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "ぺんぎん",
            "text": "散歩ばっかりしてます笑",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "てばさき",
            "text": "@ぺんぎん 本とかは今でも読みますか？",
            "mention_to": [
                "ぺんぎん"
            ]
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "ぺんぎん",
            "text": "@てばさき 少し読んだりしますよー",
            "mention_to": [
                "てばさき"
            ]
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "ぺんぎん",
            "text": "@てばさき どんな本読みますか？",
            "mention_to": [
                "てばさき"
            ]
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "てばさき",
            "text": "@ぺんぎん 最近はライトノベルとか読みますね",
            "mention_to": [
                "ぺんぎん"
            ]
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "えのき",
            "text": "先日、家事のしないことリスト、っていう本を借りてきました",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "えのき",
            "text": "家事したくなさのあまり…",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "てばさき",
            "text": "@えのき 家事したくない",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "ぺんぎん",
            "text": "@えのき できればしたくないよね",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "えのき",
            "text": "満場一致でしたくない",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "てばさき",
            "text": "@ぺんぎん したくないです",
            "mention_to": [
                "ぺんぎん"
            ]
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "ぺんぎん",
            "text": "@えのき そんな本が！よんでみようかな",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "ぺんぎん",
            "text": "@てばさき ですよねー",
            "mention_to": [
                "てばさき"
            ]
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "えのき",
            "text": "しないことリストって、大事ですよね",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "えのき",
            "text": "人生は有限だから…",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "てばさき",
            "text": "ちなみに、せざるをえないこと、リストもある感じですか？",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "ぺんぎん",
            "text": "@えのき そうそう！",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "えのき",
            "text": "@てばさき あるかもね…",
            "mention_to": [
                "てばさき"
            ]
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "ぺんぎん",
            "text": "@えのき えーそんな本も？",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "てばさき",
            "text": "@えのき なきにしもあらず",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "ぺんぎん",
            "text": "でも両方必要かも！",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "えのき",
            "text": "そうですね",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "ぺんぎん",
            "text": "家事っていっぱいあるからねー",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "えのき",
            "text": "できればしたいことだけして生きていきたい…",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "てばさき",
            "text": "@ぺんぎん 家事大変です",
            "mention_to": [
                "ぺんぎん"
            ]
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "ぺんぎん",
            "text": "@えのき 同感！！！",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "ぺんぎん",
            "text": "@てばさき ですよねー、でばさきさんどんな家事がやだ？",
            "mention_to": [
                "てばさき"
            ]
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "てばさき",
            "text": "@ぺんぎん 排水溝のお掃除嫌いです",
            "mention_to": [
                "ぺんぎん"
            ]
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "えのき",
            "text": "うちではてばさんの係です",
            "mention_to": []
        },
        {
            "utterance_id": 103,
            "interlocutor_id": "えのき",
            "text": "ゴミの日の前日に",
            "mention_to": []
        }
    ],
