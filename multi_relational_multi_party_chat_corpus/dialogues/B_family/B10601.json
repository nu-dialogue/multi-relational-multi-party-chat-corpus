        },
        {
            "utterance_id": 2,
            "interlocutor_id": "たまねぎ",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "りんご",
            "text": "宜しくお願いします",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "にんじん",
            "text": "よろしくお願いします",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "りんご",
            "text": "今日は暖かいですね",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "たまねぎ",
            "text": "よろしくお願いします",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "たまねぎ",
            "text": "そうですねー",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "りんご",
            "text": "春にいきなりなった感じですよね",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "にんじん",
            "text": "そうですね！",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "にんじん",
            "text": "花粉が心配ですが",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "りんご",
            "text": "あーわかります！",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "りんご",
            "text": "私も今年は鼻がでます",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "たまねぎ",
            "text": "もう出ますか？",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "たまねぎ",
            "text": "私はまだ大丈夫です！",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "りんご",
            "text": "犬の散歩の時はやばいです",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "りんご",
            "text": "にんじんさんは大丈夫ですか？",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "にんじん",
            "text": "散歩はやばいですね",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "にんじん",
            "text": "私は大丈夫です！",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "りんご",
            "text": "羨ましいです",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "たまねぎ",
            "text": "ね！",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "たまねぎ",
            "text": "花粉症辛いですよね",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "たまねぎ",
            "text": "味もしないし",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "りんご",
            "text": "涙目にもなります",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "たまねぎ",
            "text": "苦しいですよね",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "にんじん",
            "text": "見てる方も辛いです",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "りんご",
            "text": "はい、本当に大変な時期です",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "にんじん",
            "text": "薬飲んでますか？",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "りんご",
            "text": "まだ今年くらいからなのでのんでないですね",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "たまねぎ",
            "text": "花粉症デビュー？？",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "りんご",
            "text": "まだ花粉症認めたくない！",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "たまねぎ",
            "text": "あ、それ花粉症あるあるです",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "たまねぎ",
            "text": "私もそうでした笑",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "りんご",
            "text": "ですよね、なったと思った瞬間からひどくなりそうで",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "たまねぎ",
            "text": "早く認めて病院に行った方が楽になります笑",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "たまねぎ",
            "text": "病院の薬効きますよー",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "りんご",
            "text": "そうですよねー",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "たまねぎ",
            "text": "安いし",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "にんじん",
            "text": "市販薬高いですよね",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "りんご",
            "text": "本当ですか？主人もなんで一緒にいこうかな",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "りんご",
            "text": "高いですよね",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "たまねぎ",
            "text": "1月くらいから予防接種する人もいますよね",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "りんご",
            "text": "予防接種もあるんですね！",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "たまねぎ",
            "text": "そうみたいでふ",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "りんご",
            "text": "したことありますか？",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "たまねぎ",
            "text": "まだないんですよー",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "たまねぎ",
            "text": "ちょっと怖くて",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "たまねぎ",
            "text": "副反応が",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "りんご",
            "text": "何となくね",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "たまねぎ",
            "text": "薬で効くうちはいいかなと",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "たまねぎ",
            "text": "だから、毎年いちご狩りが大変です",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "たまねぎ",
            "text": "行く前に薬飲んで時間調節してます！",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "りんご",
            "text": "そうですね！うちも試してみます",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "りんご",
            "text": "今日はバレンタインですね",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "にんじん",
            "text": "そうですね",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "たまねぎ",
            "text": "作りますか？",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "りんご",
            "text": "いやーつくらないですね",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "たまねぎ",
            "text": "買う？",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "りんご",
            "text": "にんじんさんとタマネギさんは？どうですか？",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "たまねぎ",
            "text": "私は娘がいるので一緒に作ります！",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "りんご",
            "text": "買う派というか、主人は甘いものだめで",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "にんじん",
            "text": "僕は食べる専門です",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "りんご",
            "text": "いいなぁ",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "りんご",
            "text": "私も食べたです笑",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "たまねぎ",
            "text": "最近は甘くないバレンタインもありますね",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "りんご",
            "text": "どんなの作ったんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "たまねぎ",
            "text": "去年はクッキーです",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "たまねぎ",
            "text": "量産型笑",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "りんご",
            "text": "なるほど！",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "にんじん",
            "text": "甘さ控えめ",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "りんご",
            "text": "いいですね！",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "たまねぎ",
            "text": "好きな絵を描いて",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "たまねぎ",
            "text": "後愚痴とか笑",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "にんじん",
            "text": "食べるの怖いやつ",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "りんご",
            "text": "絵もかいて？ぐち？笑",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "たまねぎ",
            "text": "メッセージ込めるやつですね",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "りんご",
            "text": "にんじんさん大変ですね笑",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "にんじん",
            "text": "お腹壊しそう笑",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "りんご",
            "text": "でもいいかもしれなやいです笑",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "りんご",
            "text": "うちもやろーかな",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "たまねぎ",
            "text": "伝わりますよ笑",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "たまねぎ",
            "text": "いつも以上に笑",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "りんご",
            "text": "それいただきます笑",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "たまねぎ",
            "text": "ぜひ笑",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "にんじん",
            "text": "こわいーーー",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "りんご",
            "text": "うち息子もいるのでやります！",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "たまねぎ",
            "text": "お！喜びますね！",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "たまねぎ",
            "text": "おいくつですか？",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "りんご",
            "text": "是非味わって食べてほしい",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "りんご",
            "text": "もう29歳です",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "たまねぎ",
            "text": "え！息子さんそんな大きいんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "りんご",
            "text": "大谷さんと同じです",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "にんじん",
            "text": "おーー",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "りんご",
            "text": "他でいっぱいもらえればねー",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "にんじん",
            "text": "喜びますね！",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "りんご",
            "text": "もう子供ではないですね笑",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "たまねぎ",
            "text": "確かに！",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "たまねぎ",
            "text": "でも息子はいつまでもかわいいですよね",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "たまねぎ",
            "text": "うちも息子いますけど、かわいいです！",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "りんご",
            "text": "タマネギさんちはおいくつのお子さんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "にんじん",
            "text": "1番上が18です",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "たまねぎ",
            "text": "で、15歳",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "りんご",
            "text": "もう大きいですね！",
            "mention_to": []
        },
        {
            "utterance_id": 103,
            "interlocutor_id": "たまねぎ",
            "text": "10.4.1と5人います！",
