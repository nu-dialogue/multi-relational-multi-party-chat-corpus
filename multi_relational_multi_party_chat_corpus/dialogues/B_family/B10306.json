        },
        {
            "utterance_id": 1,
            "interlocutor_id": "こんぶ",
            "text": "おはようございます",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "ちくわ",
            "text": "おはようございます！",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "うさぎ",
            "text": "よろしくお願いします",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "こんぶ",
            "text": "こんぶです〜",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "こんぶ",
            "text": "よろしくお願いします！",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "うさぎ",
            "text": "今日とっても暖かいです！",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "ちくわ",
            "text": "ちくわです！",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "ちくわ",
            "text": "よろしくお願いします！",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "こんぶ",
            "text": "あったかいです！",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "ちくわ",
            "text": "暑いぐらいです！",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "こんぶ",
            "text": "雨は上がりましたね",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "うさぎ",
            "text": "暖房久しぶりに入れてません",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "こんぶ",
            "text": "たしかに！！",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "ちくわ",
            "text": "一緒でした！",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "こんぶ",
            "text": "我が家も暖房いれてないです",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "うさぎ",
            "text": "高校生の娘がいますが、今日体育でマラソンです",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "こんぶ",
            "text": "マラソン！！！",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "うさぎ",
            "text": "ブーブー言ってました",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "こんぶ",
            "text": "高校生でのマラソン",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "ちくわ",
            "text": "何キロ走るんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "こんぶ",
            "text": "嫌だな〜",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "ちくわ",
            "text": "1番嫌な時期！",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "うさぎ",
            "text": "雨こいしてました",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "うさぎ",
            "text": "１キロです",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "こんぶ",
            "text": "@うさぎ 部活はなにか？",
            "mention_to": [
                "うさぎ"
            ]
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "ちくわ",
            "text": "そうなんですね！",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "こんぶ",
            "text": "たしかに、そこは雨降れですね",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "うさぎ",
            "text": "@こんぶ 放送部です",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "うさぎ",
            "text": "文化部です",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "こんぶ",
            "text": "それじゃあ、マラソンなんて",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "こんぶ",
            "text": "嫌の嫌ですね",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "ちくわ",
            "text": "@うさぎ 運動部ではないとやる気起きない！",
            "mention_to": [
                "うさぎ"
            ]
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "うさぎ",
            "text": "@こんぶ はい、地獄です",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "ちくわ",
            "text": "自分は運動部やったから",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "こんぶ",
            "text": "@うさぎ 娘さんファイト！！！",
            "mention_to": [
                "うさぎ"
            ]
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "ちくわ",
            "text": "めちゃ張り切ってました！",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "うさぎ",
            "text": "@ちくわ 運動部の友達もみんな嫌がってるらしいです",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "ちくわ",
            "text": "ふぁいとーー！",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "こんぶ",
            "text": "私も運動部でしたが、嫌でした",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "ちくわ",
            "text": "高校生になるとそんなもんすよね！",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "こんぶ",
            "text": "わかります",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "うさぎ",
            "text": "お二人もしかしてスポーツされますか？",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "こんぶ",
            "text": "その娘さんのお友達の気持ち",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "ちくわ",
            "text": "してましたよ！",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "こんぶ",
            "text": "わたしもしてました！",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "うさぎ",
            "text": "おー何ですか？",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "ちくわ",
            "text": "女の子は喋りながら走りますよね！",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "こんぶ",
            "text": "バドミントンと卓球を",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "ちくわ",
            "text": "僕はバレーボールと野球です",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "うさぎ",
            "text": "いいですね！素晴らしい",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "うさぎ",
            "text": "足早かったですか？",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "ちくわ",
            "text": "@うさぎ 何かスポーツしてました？",
            "mention_to": [
                "うさぎ"
            ]
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "こんぶ",
            "text": "スポーツは楽しいですよ",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "ちくわ",
            "text": "僕は、長距離が得意でした",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "うさぎ",
            "text": "@ちくわ 私はダンス部でしたが、、、恥ずかしながら",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "こんぶ",
            "text": "足は〜人数が少ない学校だったので",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "ちくわ",
            "text": "後ろから追い抜くのがたのしくて！",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "こんぶ",
            "text": "早い方でしたが、、",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "ちくわ",
            "text": "ダンス部！！",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "こんぶ",
            "text": "@うさぎ ダンス！すごいじゃないですか！",
            "mention_to": [
                "うさぎ"
            ]
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "うさぎ",
            "text": "@ちくわ いやー絶対早い人!",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "ちくわ",
            "text": "今では小学校からダンスしないと",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "ちくわ",
            "text": "いけないからいいですね！",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "こんぶ",
            "text": "ダンスてたしかに必須科目なったんだっけ？",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "うさぎ",
            "text": "@ちくわ ですよね？創作ダンス娘、苦しんでました",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "ちくわ",
            "text": "@うさぎ 早くはないけれど、楽しかったです！",
            "mention_to": [
                "うさぎ"
            ]
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "こんぶ",
            "text": "ダンスもリズム感ないと大変ですよね",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "うさぎ",
            "text": "YouTubeでパクってましたよ",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "ちくわ",
            "text": "@こんぶ 必須になった！",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "うさぎ",
            "text": "@こんぶ そうそう成績になります",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "こんぶ",
            "text": "子どもたちが作るんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "ちくわ",
            "text": "@うさぎ 創作ダンス！高校生は",
            "mention_to": [
                "うさぎ"
            ]
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "ちくわ",
            "text": "絶対嫌なやつ！",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "ちくわ",
            "text": "子供達が振り付けとか",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "うさぎ",
            "text": "@こんぶ はい、公開処刑って言ってました",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "こんぶ",
            "text": "私リズム感はないので",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "ちくわ",
            "text": "体形を考えるんすよね！",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "こんぶ",
            "text": "本当そうです",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "ちくわ",
            "text": "みんなの前は特にねー",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "こんぶ",
            "text": "公開処刑です",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "うさぎ",
            "text": "@ちくわ ダンスとかはどうしてもセンス？の問題ありますよね",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "こんぶ",
            "text": "運動神経悪い芸人とかっていう",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "ちくわ",
            "text": "僕はアホみたいに踊りますよ",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "こんぶ",
            "text": "例の番組みたいになります",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "ちくわ",
            "text": "センスもありますが、",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "うさぎ",
            "text": "@こんぶ うわ、見たい!",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "ちくわ",
            "text": "リズム感がないとキツいすよね！",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "こんぶ",
            "text": "見せらないです〜旦那にも",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "うさぎ",
            "text": "私はリズム感なかったですが、好きだったので",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "こんぶ",
            "text": "でも、やっぱり好きだと",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "ちくわ",
            "text": "@こんぶ いつか見せてもらいます！",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "うさぎ",
            "text": "成績もなかったから楽しくだったので",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "こんぶ",
            "text": "何をするにも一生懸命なれるし",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "ちくわ",
            "text": "でも友達と踊ると楽しいんやないすか？",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "こんぶ",
            "text": "@ちくわ やだやだ",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "うさぎ",
            "text": "@こんぶ みたいですよね！",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "こんぶ",
            "text": "たしかに！",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "ちくわ",
            "text": "結局成績ですよねー！",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "こんぶ",
            "text": "友だちと一緒なら！",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "うさぎ",
            "text": "@ちくわ それそれ！",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "ちくわ",
            "text": "文化祭とかも",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "ちくわ",
            "text": "高校生なら",
            "mention_to": []
        },
        {
            "utterance_id": 103,
            "interlocutor_id": "ちくわ",
            "text": "盛り上がりますよね！",
            "mention_to": []
        }
