    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "ポテト",
            "text": "おはようございます！",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "ちくわ",
            "text": "おはようございます",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "こんぶ",
            "text": "おはようございます！！",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "ポテト",
            "text": "お二人でご一緒に楽しんでいる事って何かありますか？",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "ちくわ",
            "text": "住んでいる地域の有名な食べ物あります？",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "ポテト",
            "text": "ピーナツ・・・ですかね・・・千葉は・・・",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "ちくわ",
            "text": "@ポテト 子供が寝たらYouTube見る時です！",
            "mention_to": [
                "ポテト"
            ]
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "こんぶ",
            "text": "@ポテト 最近は娘が寝た後にYouTubeをみるのにハマってます",
            "mention_to": [
                "ポテト"
            ]
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "こんぶ",
            "text": "あ、一緒だ",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "ポテト",
            "text": "あー・・・至福の時！",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "ちくわ",
            "text": "一緒だ。",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "こんぶ",
            "text": "ピーナッツ！",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "ちくわ",
            "text": "@ポテト 聞いたことあります！",
            "mention_to": [
                "ポテト"
            ]
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "こんぶ",
            "text": "わたしも！",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "ポテト",
            "text": "千葉・・・ピーナツくらいしか・・・",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "こんぶ",
            "text": "聞いたことあります！",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "ちくわ",
            "text": "@ポテト チーバくんですね！",
            "mention_to": [
                "ポテト"
            ]
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "こんぶ",
            "text": "赤色の犬？",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "ポテト",
            "text": "@ちくわ ですです！",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "ちくわ",
            "text": "千葉県の形した横顔ですね！",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "こんぶ",
            "text": "犬じゃなかった",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "ポテト",
            "text": "@こんぶ 赤色の犬！",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "こんぶ",
            "text": "失礼しました。",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "ポテト",
            "text": "犬・・・です！たぶん！",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "こんぶ",
            "text": "あ、犬？",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "ちくわ",
            "text": "一回だけ千葉県行ったことあります！",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "こんぶ",
            "text": "あってましたか！",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "ポテト",
            "text": "@こんぶ あってます！",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "こんぶ",
            "text": "私はディズニーランドで",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "こんぶ",
            "text": "お世話になってます！",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "ポテト",
            "text": "@ちくわ なんのご用事で行かれたんですか？",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "ポテト",
            "text": "@こんぶ 間違いないやつですね！",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "ちくわ",
            "text": "@ポテト スポーツをしていて、大会で行きました！",
            "mention_to": [
                "ポテト"
            ]
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "ちくわ",
            "text": "千葉国体のときです！",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "ちくわ",
            "text": "高校三年生の時に！",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "こんぶ",
            "text": "@ポテト 間違いないです！",
            "mention_to": [
                "ポテト"
            ]
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "ポテト",
            "text": "@ちくわ なんのスポーツだったんですか？",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "ポテト",
            "text": "@こんぶ 楽しい夢の国・・・",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "ちくわ",
            "text": "@ポテト バレーボールです！",
            "mention_to": [
                "ポテト"
            ]
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "ポテト",
            "text": "バレーボール！",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "こんぶ",
            "text": "@ポテト 夢の国最高！！！",
            "mention_to": [
                "ポテト"
            ]
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "ちくわ",
            "text": "久しくディズニーランドいってないなー",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "こんぶ",
            "text": "行ってないねー",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "ポテト",
            "text": "私も久しくいってないです・・・",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "こんぶ",
            "text": "千葉在住でも",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "こんぶ",
            "text": "よくは行かないもんですかね〜？",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "ちくわ",
            "text": "@こんぶ 家族で行こうて言ってるよねー",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "ポテト",
            "text": "近いと逆に行かなくなるやつです！",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "ちくわ",
            "text": "@ポテト そうなんですね！",
            "mention_to": [
                "ポテト"
            ]
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "ポテト",
            "text": "@ちくわ 良いですね！",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "こんぶ",
            "text": "@ちくわ そうそう！今年あたり！",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "ポテト",
            "text": "えーたのしみなやつだー！",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "こんぶ",
            "text": "近いと逆に行かない気持ち",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "こんぶ",
            "text": "わかります！！！",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "ちくわ",
            "text": "@こんぶ お金貯めましょう！",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "こんぶ",
            "text": "@ちくわ ためましょう！！！",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "ポテト",
            "text": "@こんぶ 分かります？いつでもいけるし・・・ってなります",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "ちくわ",
            "text": "@ポテト 他に千葉に行ったらどこ行けばいいですか？",
            "mention_to": [
                "ポテト"
            ]
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "ポテト",
            "text": "アンデルセン公園・・・？",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "こんぶ",
            "text": "@ポテト わかります！ほんと！そう！",
            "mention_to": [
                "ポテト"
            ]
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "ちくわ",
            "text": "いつでも行ける！って羨ましい！！",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "こんぶ",
            "text": "@ポテト アンデルセン公園！",
            "mention_to": [
                "ポテト"
            ]
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "ちくわ",
            "text": "@ポテト 今度調べてみますね！",
            "mention_to": [
                "ポテト"
            ]
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "ポテト",
            "text": "子連れなら楽しめるかと！小さい子限定ですが・・・",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "こんぶ",
            "text": "ね！調べてみよう！",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "ちくわ",
            "text": "子供が小さいので良いかもです！",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "こんぶ",
            "text": "@ポテト お子さんいらっしゃるんですか？",
            "mention_to": [
                "ポテト"
            ]
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "ポテト",
            "text": "@ちくわ お子さん、まだ小さいんですね",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "こんぶ",
            "text": "私たちの子どもは一歳です！",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "ポテト",
            "text": "います！小学三年生です",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "ポテト",
            "text": "一歳！尊い！",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "こんぶ",
            "text": "三年生なんですね！",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "ちくわ",
            "text": "@ポテト 活発な時期に入ってきますね！",
            "mention_to": [
                "ポテト"
            ]
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "ポテト",
            "text": "元気に学級閉鎖で家にいます",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "こんぶ",
            "text": "今は、自我の成長がすごくて",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "こんぶ",
            "text": "朝もてんやわんやでした",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "ちくわ",
            "text": "@ポテト インフルエンザですか？",
            "mention_to": [
                "ポテト"
            ]
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "ポテト",
            "text": "@こんぶ 大変な時期・・・",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "こんぶ",
            "text": "あれま！",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "ポテト",
            "text": "@ちくわ インフルエンザ流行りまくりで・・・",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "こんぶ",
            "text": "元気で休めるって1番嬉しいやつ",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "ちくわ",
            "text": "@ポテト 学級閉鎖は親も大変ですよね！",
            "mention_to": [
                "ポテト"
            ]
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "こんぶ",
            "text": "インフルエンザ今どこでもですね",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "ポテト",
            "text": "@こんぶ こどもはいいんですが、おやが・・・",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "こんぶ",
            "text": "たしかに！",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "こんぶ",
            "text": "親は色々融通きかせるの大変ですよね",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "ちくわ",
            "text": "給食のありがたさとか、、、",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "ポテト",
            "text": "本当にそうですね～一歳だと特に心配じゃないですか",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "ポテト",
            "text": "@ちくわ そうです・・・",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "ちくわ",
            "text": "生後1ヶ月でコロナになった時は大変でしたー",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "こんぶ",
            "text": "心配です〜",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "こんぶ",
            "text": "そう！コロナになっちゃって！",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "ポテト",
            "text": "ひゃ・・・",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "ちくわ",
            "text": "あの時は、焦りました！",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "こんぶ",
            "text": "でも、1番症状が軽かったのが",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "こんぶ",
            "text": "娘でした！",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "ポテト",
            "text": "家族パンデミックですか？",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "ちくわ",
            "text": "@こんぶ 薬もちゃんと飲めたもんね！",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "ちくわ",
            "text": "そうです！",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "こんぶ",
            "text": "はい！そうです！",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "こんぶ",
            "text": "@ちくわ ね！上手にのんでくれた",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "ポテト",
            "text": "@ちくわ 薬飲めたのえらい！",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "こんぶ",
            "text": "泣かずにのんでました",
            "mention_to": []
        },
        {
            "utterance_id": 103,
            "interlocutor_id": "ちくわ",
            "text": "@ポテト 外に出れるようになってイオンに行ったのが原因かもです！",
            "mention_to": [
                "ポテト"
            ]
        },
        {
            "utterance_id": 104,
            "interlocutor_id": "ポテト",
            "text": "@こんぶ パンデミック本当に怖い・・・",
            "mention_to": [
                "こんぶ"
            ]
        }
