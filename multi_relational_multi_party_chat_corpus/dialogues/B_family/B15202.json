            ]
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "たまねぎ",
            "text": "わざと俺に見せつけてママにチューする！",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "たまねぎ",
            "text": "ドヤ顔で！",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "ぺんぎん",
            "text": "@たまねぎ 将来天才か！",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "にんじん",
            "text": "笑",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "にんじん",
            "text": "可愛いよねー",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "ぺんぎん",
            "text": "@たまねぎ やられてるね笑",
