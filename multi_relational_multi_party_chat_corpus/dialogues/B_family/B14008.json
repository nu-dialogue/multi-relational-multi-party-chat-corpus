{
    "dialogue_id": "B14008",
    "dialogue_type": "Family",
    "interlocutors": [
        "カンガルー",
        "つみれ",
        "たまご"
    ],
    "relationship": [
        "つみれ",
        "たまご"
    ],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "カンガルー",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "つみれ",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "たまご",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "カンガルー",
            "text": "私には小学生と高校生の子供がいるんですが",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "たまご",
            "text": "はいー",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "カンガルー",
            "text": "昨日、高校生の息子から連絡があり",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "カンガルー",
            "text": "あ、まだ一年生です",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "カンガルー",
            "text": "来年の今頃には大学が決まるんですが",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "つみれ",
            "text": "そうですね",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "たまご",
            "text": "そうなんですね",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "カンガルー",
            "text": "希望していた大学を諦めると",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "たまご",
            "text": "そうでした",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "カンガルー",
            "text": "何で？って聞いたら",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "たまご",
            "text": "そうなんですか",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "カンガルー",
            "text": "嫌いな先輩がいるそうで",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "たまご",
            "text": "えー",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "つみれ",
            "text": "@カンガルー あぁ・・・",
            "mention_to": [
                "カンガルー"
            ]
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "カンガルー",
            "text": "いや、嫌いな先輩が行くそうで",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "たまご",
            "text": "その大学にですか",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "カンガルー",
            "text": "それだけの為に諦めるのか！",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "カンガルー",
            "text": "と、思いました",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "カンガルー",
            "text": "大学ってどんな基準で決めるんですかね？",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "カンガルー",
            "text": "学部？偏差値？",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "たまご",
            "text": "その理由だとちょっともったいない気がしますー",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "つみれ",
            "text": "お母さんの気持ちも息子さんの気持ちも",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "カンガルー",
            "text": "@たまご そうなんですよー",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "つみれ",
            "text": "わかるけど・・・",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "カンガルー",
            "text": "@たまご やっぱり？",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "つみれ",
            "text": "親としてはそんな理由で？ってなりますね",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "カンガルー",
            "text": "私もそう思います",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "つみれ",
            "text": "学部が違ったら関わりないんじゃないですか？",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "カンガルー",
            "text": "@つみれ ですよね",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "たまご",
            "text": "大学だとあんま関わらない気がします",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "つみれ",
            "text": "同じ学部？",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "カンガルー",
            "text": "息子はスポーツやってまして",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "カンガルー",
            "text": "スポーツで進学予定なんで",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "たまご",
            "text": "そういうことなんですね",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "カンガルー",
            "text": "当然同じ部活になるんですよ",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "たまご",
            "text": "部活の先輩だと絶対会いますね",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "つみれ",
            "text": "あぁ・・・それはしんどいかも",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "カンガルー",
            "text": "今も同じ寮で暮らしておりまして",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "たまご",
            "text": "それは嫌ですね",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "つみれ",
            "text": "寮生活なんですね",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "カンガルー",
            "text": "あぁやっぱり？",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "カンガルー",
            "text": "そうなんです",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "カンガルー",
            "text": "大学行っても寮になるかと",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "つみれ",
            "text": "寮で嫌な思いしてたら",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "たまご",
            "text": "他の大学でもやりたいことができるなら別でも良い気がします",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "つみれ",
            "text": "あと何年か続くって",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "カンガルー",
            "text": "東京に実家があっても寮生活",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "つみれ",
            "text": "やですね",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "カンガルー",
            "text": "やっぱりそうですよねー",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "つみれ",
            "text": "部活に集中するためですか？",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "カンガルー",
            "text": "@つみれ チームビルディングですね",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "たまご",
            "text": "そうなんですね",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "つみれ",
            "text": "かっこいいですね",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "たまご",
            "text": "毎日のように顔合わせるので大変そう",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "カンガルー",
            "text": "@たまご やっぱり？",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "カンガルー",
            "text": "運動部は上下関係も昭和の世界なんです",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "カンガルー",
            "text": "今一年生だから奴隷ですよ！",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "つみれ",
            "text": "大学の上下関係ってより強くなりそうなイメージです",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "たまご",
            "text": "いやになるのも分かります",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "カンガルー",
            "text": "あああやっぱりそうおもいますか？",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "カンガルー",
            "text": "私は是非、行きたい大学に行ってもらいたかったんですが",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "つみれ",
            "text": "スポーツの世界って厳しいそうですもんね",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "カンガルー",
            "text": "あまり言ってもダメですよね…",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "たまご",
            "text": "一年だから1番後輩でより感じてしまうのかも",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "カンガルー",
            "text": "本人が決める事…ですものね",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "カンガルー",
            "text": "@たまご まぁそれもありますね！",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "つみれ",
            "text": "その先輩が大学に行って大人になってくれればいいですけど",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "カンガルー",
            "text": "またこの1年で変わるかもしれないし",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "たまご",
            "text": "後輩ができたら考え方も少し変わる可能性が",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "つみれ",
            "text": "変わらないと厳しいでうよね",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "つみれ",
            "text": "息子さん的には",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "カンガルー",
            "text": "で、他の大学も考えたんです",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "カンガルー",
            "text": "他の体育大学",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "つみれ",
            "text": "どうなんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "カンガルー",
            "text": "ちょうど違法薬物の話がニュースになっていて",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "つみれ",
            "text": "あ～そうですね",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "たまご",
            "text": "ニュースなってましたね",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "カンガルー",
            "text": "薬物の印象が払拭しきれません",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "たまご",
            "text": "怖いですね",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "つみれ",
            "text": "そのイメージが強いですよね",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "つみれ",
            "text": "親としては。。。",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "カンガルー",
            "text": "ま、どちらにしても学費が高い",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "つみれ",
            "text": "やめてーってなりますね",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "カンガルー",
            "text": "そういう大学はいやーっってなってます",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "つみれ",
            "text": "大学の学費、誰か払ってくれないですかねー",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "カンガルー",
            "text": "ホントですよ",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "カンガルー",
            "text": "希望大学ですが",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "カンガルー",
            "text": "学費と寮費で、年間250万",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "つみれ",
            "text": "ヒョエー！！",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "カンガルー",
            "text": "4年間で1000万！",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "たまご",
            "text": "高い！！",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "カンガルー",
            "text": "信じランない！",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "カンガルー",
            "text": "ですよねー",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "カンガルー",
            "text": "寮費が学費と同じくらいの割合",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "つみれ",
            "text": "1人１０００万てよく言いますけど",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "カンガルー",
            "text": "確かバイトもできない",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "つみれ",
            "text": "ほんとだなって思いました",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "たまご",
            "text": "バイトできないんですね",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "たまご",
            "text": "きびしー",
            "mention_to": []
        }
