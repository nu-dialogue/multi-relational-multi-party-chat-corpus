        "こんぶ"
    ],
    "relationship": [
        "ちくわ",
        "こんぶ"
    ],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "コアラ",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "ちくわ",
            "text": "こんにちは。",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "こんぶ",
            "text": "こんにちは〜！",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "コアラ",
            "text": "もうじきひな祭りですね",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "こんぶ",
            "text": "ひな祭りですね！！！今週か",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "ちくわ",
            "text": "飾りましたよー！",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "ちくわ",
            "text": "お雛さん",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "こんぶ",
            "text": "娘がいるので、パーティーです",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "コアラ",
            "text": "うちは娘がいるので料理作ります",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "コアラ",
            "text": "ちらし寿司",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "ちくわ",
            "text": "お！",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "こんぶ",
            "text": "あら！一緒だ！！",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "ちくわ",
            "text": "一緒だ！",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "コアラ",
            "text": "@こんぶ 同じだ！",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "こんぶ",
            "text": "私たちもちらし寿司を！",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "コアラ",
            "text": "@こんぶ ですよね！定番",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "ちくわ",
            "text": "定番すよね！",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "コアラ",
            "text": "お刺身多めに飼ってきて一緒に",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "こんぶ",
            "text": "@コアラ 定番！！いいですね〜",
            "mention_to": [
                "コアラ"
            ]
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "コアラ",
            "text": "買って",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "コアラ",
            "text": "飼うんじゃ大変笑",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "ちくわ",
            "text": "飼ったら捌かないと。笑",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "こんぶ",
            "text": "笑ってしまいました",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "コアラ",
            "text": "でんぶとかのせます？",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "コアラ",
            "text": "ピンクの",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "コアラ",
            "text": "甘め",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "こんぶ",
            "text": "甘いのですよね！！！",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "コアラ",
            "text": "娘あまり好きじゃなくて",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "こんぶ",
            "text": "正直そんなに得意じゃなくて〜",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "コアラ",
            "text": "彩りいいから載せたいけどー",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "こんぶ",
            "text": "あ、やっぱりそうですよね！",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "ちくわ",
            "text": "甘いの好きな人すくないですよね",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "こんぶ",
            "text": "あれって、好みありますよね",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "コアラ",
            "text": "そうそう",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "ちくわ",
            "text": "彩りは気にしますよね！",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "こんぶ",
            "text": "でもたしかに！彩りはよし！",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "コアラ",
            "text": "あとはまぐりのお吸い物かな？",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "ちくわ",
            "text": "僕はちらし寿司のしいたけが好きです",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "こんぶ",
            "text": "ちらし寿司のほかに何かつくります？",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "コアラ",
            "text": "でも塩抜きされたの買います",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "ちくわ",
            "text": "はまぐりのお吸い物いいすね！",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "こんぶ",
            "text": "お吸い物ー！いいですね！",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "コアラ",
            "text": "@こんぶ なぜかグラタン、揚げワンタンとかリクエストありです",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "こんぶ",
            "text": "うちらはどうしようかと〜",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "ちくわ",
            "text": "はまぐり好きです！",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "コアラ",
            "text": "@こんぶ まだお子さん小さい？",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "こんぶ",
            "text": "@コアラ リクエストしてくれるのは、ありがたいですー！！",
            "mention_to": [
                "コアラ"
            ]
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "こんぶ",
            "text": "作るのは大変だけど",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "こんぶ",
            "text": "そうなんですよーー",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "コアラ",
            "text": "@こんぶ でもへんな組み合わせな感じ？",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "こんぶ",
            "text": "まだ一歳で",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "ちくわ",
            "text": "僕の母も一緒に妻が作るからもう一品考えてたよねー",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "こんぶ",
            "text": "@コアラ きっと、お子さんの好みで固められてますね",
            "mention_to": [
                "コアラ"
            ]
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "コアラ",
            "text": "@こんぶ あっ、じゃあ酢飯のところとか？",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "ちくわ",
            "text": "普通のご飯だと思います",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "コアラ",
            "text": "卵のところとか？",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "ちくわ",
            "text": "大人は酢飯で！",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "こんぶ",
            "text": "そうなんです〜卵は食べられるから錦糸卵と",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "コアラ",
            "text": "酢飯、意外とうちは小さい時から好きでした",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "こんぶ",
            "text": "味付きご飯て好きですよね？",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "コアラ",
            "text": "錦卵！美味しいですよね",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "ちくわ",
            "text": "酢飯食べれてる方がいいですよ！",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "コアラ",
            "text": "@ちくわ 酸っぱいのあまり好きじゃないのにね",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "ちくわ",
            "text": "ちらし寿司たのしみーー",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "コアラ",
            "text": "酢飯だけは別",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "こんぶ",
            "text": "ね！楽しみーーー",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "コアラ",
            "text": "でも娘は期末テスト中",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "ちくわ",
            "text": "@コアラ 特別ですね！",
            "mention_to": [
                "コアラ"
            ]
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "こんぶ",
            "text": "お義母さん頼み笑",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "コアラ",
            "text": "@こんぶ いいな！",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "こんぶ",
            "text": "期末テスト！！！",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "コアラ",
            "text": "うちにも来て欲しい",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "ちくわ",
            "text": "そか！",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "こんぶ",
            "text": "テスト期間か〜懐かしい",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "ちくわ",
            "text": "テスト期間かー",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "コアラ",
            "text": "@こんぶ 今日からです",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "ちくわ",
            "text": "早く帰れるけど",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "コアラ",
            "text": "なので毎年、食べたらすぐ引っ込むので",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "こんぶ",
            "text": "@コアラ お料理上手で尊敬ですよ",
            "mention_to": [
                "コアラ"
            ]
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "ちくわ",
            "text": "勉強はいやでした",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "こんぶ",
            "text": "今日から、、",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "コアラ",
            "text": "@ちくわ ですよねー",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "こんぶ",
            "text": "いつまで？",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "コアラ",
            "text": "@こんぶ ３月６日かな？",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "コアラ",
            "text": "長い",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "ちくわ",
            "text": "大学の時のテスト勉強はたのしかったけど",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "こんぶ",
            "text": "何をどう勉強したはよいかわからないタイプでした",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "こんぶ",
            "text": "え！！！なが！！！",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "ちくわ",
            "text": "高校までのテスト勉強は",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "ちくわ",
            "text": "ほんまに嫌でした",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "コアラ",
            "text": "@ちくわ いい学生さん",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "こんぶ",
            "text": "大学は専門的なことも入ってくるから！",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "コアラ",
            "text": "確かに！専攻なんでしたか？",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "こんぶ",
            "text": "自分の学べることは楽しいか",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "ちくわ",
            "text": "@コアラ @こんぶ それもやし、友達と勉強できるので",
            "mention_to": [
                "コアラ",
                "こんぶ"
            ]
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "ちくわ",
            "text": "夜中に",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "ちくわ",
            "text": "笑",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "コアラ",
            "text": "@ちくわ それか！",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "こんぶ",
            "text": "あーー！たしかに！！！",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "こんぶ",
            "text": "ガストでやってました！",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "ちくわ",
            "text": "ドトールとかファミレス行って",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "コアラ",
            "text": "えっ、勉強になります？",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "コアラ",
            "text": "話しちゃう",
            "mention_to": []
        },
        {
            "utterance_id": 103,
            "interlocutor_id": "ちくわ",
            "text": "朝までやってテスト受けに行ってました",
            "mention_to": []
        },
        {
            "utterance_id": 104,
            "interlocutor_id": "ちくわ",
            "text": "それです！笑",
            "mention_to": []
        },
        {
            "utterance_id": 105,
            "interlocutor_id": "こんぶ",
            "text": "ん？結局楽しいだけのやつ",
            "mention_to": []
        },
        {
            "utterance_id": 106,
            "interlocutor_id": "コアラ",
            "text": "@ちくわ うわー私無理",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 107,
            "interlocutor_id": "コアラ",
            "text": "寝ないと",
            "mention_to": []
        },
        {
            "utterance_id": 108,
            "interlocutor_id": "ちくわ",
            "text": "でも過去問とかあったから",
            "mention_to": []
        },
        {
            "utterance_id": 109,
            "interlocutor_id": "ちくわ",
            "text": "楽でしたよ笑",
            "mention_to": []
        }
