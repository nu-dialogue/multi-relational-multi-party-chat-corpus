{
    "dialogue_id": "B15505",
    "dialogue_type": "Family",
    "interlocutors": [
        "にわとり",
        "ちくわ",
        "こんぶ"
    ],
    "relationship": [
        "ちくわ",
        "こんぶ"
    ],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "にわとり",
            "text": "こんにちはー！！",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "ちくわ",
            "text": "こんにちは！",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "こんぶ",
            "text": "おはようございます！",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "こんぶ",
            "text": "こんにちはーー！",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "にわとり",
            "text": "うちはこどもが大きくなって、",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "こんぶ",
            "text": "大きいお子さんが！",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "にわとり",
            "text": "いま浪人生なんですけど、もう立派なおっさんですよー",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "ちくわ",
            "text": "おーー！",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "ちくわ",
            "text": "ご立派なおっさん笑",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "にわとり",
            "text": "お子さんいらっしゃいますか？",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "こんぶ",
            "text": "立派なおっさん笑",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "ちくわ",
            "text": "言い方が面白い！",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "こんぶ",
            "text": "言い方に笑ってしまいましたが",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "ちくわ",
            "text": "一歳のビックベイビー",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "こんぶ",
            "text": "頑張っておられるんですねーー",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "ちくわ",
            "text": "がいます。笑",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "にわとり",
            "text": "そうです、足もりっぱにくさいです笑",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "こんぶ",
            "text": "はい！一歳の！娘が",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "ちくわ",
            "text": "笑",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "ちくわ",
            "text": "面白すぎる。",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "にわとり",
            "text": "ビッグベイビー！！",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "こんぶ",
            "text": "@にわとり 若い男性の方が匂うとか聞いたことあります！",
            "mention_to": [
                "にわとり"
            ]
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "にわとり",
            "text": "すくすく育てー！！ですね！！",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "ちくわ",
            "text": "別に大きいとかそういう意味ではないんですが",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "にわとり",
            "text": "おお、娘さん！それはかわいいー！！",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "こんぶ",
            "text": "人生2度目？とも思える娘が",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "ちくわ",
            "text": "もう活発です笑",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "にわとり",
            "text": "えっ人生二度目？！！",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "ちくわ",
            "text": "すげー理解するのが早いし",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "ちくわ",
            "text": "人見てなんかするし",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "にわとり",
            "text": "へーーーー！！オモシロイですね！！！",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "こんぶ",
            "text": "ていうくらい、え？本当に一歳？",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "ちくわ",
            "text": "意志強いし",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "こんぶ",
            "text": "て思うことが多いです",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "ちくわ",
            "text": "まじでやば女です笑",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "こんぶ",
            "text": "え！それ、知ってたでしょー？",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "にわとり",
            "text": "おおーー！！！そうなんだ！！！",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "こんぶ",
            "text": "みたいな笑笑",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "にわとり",
            "text": "自我がしっかりあるんですねー！！",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "ちくわ",
            "text": "はい、わら",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "こんぶ",
            "text": "そうなんです、意思強女子です",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "にわとり",
            "text": "えーーー！！すごい！！！",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "ちくわ",
            "text": "最近少し横目でニヤリ顔したりするんすよ！",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "こんぶ",
            "text": "誰に似たんだか〜〜",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "こんぶ",
            "text": "あ、わたしかーーーあはは",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "ちくわ",
            "text": "@こんぶ 大人になったら喧嘩してそう、わら",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "にわとり",
            "text": "わはは！！！横目！！にやり！！！",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "こんぶ",
            "text": "そうなんです！！！横目で見るんですよ笑",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "ちくわ",
            "text": "似たものの性格で！",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "こんぶ",
            "text": "@ちくわ うん！絶対してると思う",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "にわとり",
            "text": "想像していまめちゃくちゃ和んでます",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "ちくわ",
            "text": "女の戦いが、、笑",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "にわとり",
            "text": "横目・・・・笑",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "こんぶ",
            "text": "女の戦いですよーー！笑",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "ちくわ",
            "text": "で、その後に妻の顔をみて笑うんです",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "こんぶ",
            "text": "立派なおっさん、、ということは笑",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "にわとり",
            "text": "女の子いいですねえ。",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "こんぶ",
            "text": "息子さんです？",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "ちくわ",
            "text": "もう演技派です！",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "ちくわ",
            "text": "@こんぶ そりゃそーやろ。笑",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "こんぶ",
            "text": "そうだよね笑",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "にわとり",
            "text": "わはは！もうりっぱなお嬢さんですね！一歳なのに！！",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "こんぶ",
            "text": "そう、一丁前な一歳です！！！笑",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "ちくわ",
            "text": "@にわとり これが喋り出すとどうなるやら",
            "mention_to": [
                "にわとり"
            ]
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "にわとり",
            "text": "@こんぶ そうですー息子です！",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "ちくわ",
            "text": "でも、今でも仲良しなんですね！",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "にわとり",
            "text": "なんかね、自分の中では幼稚園のイメージなのに、",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "ちくわ",
            "text": "息子さんと！",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "こんぶ",
            "text": "今でも、解読不能な言葉でおしゃべりしまくってますが笑",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "にわとり",
            "text": "もう成人しちゃいましたよー。",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "こんぶ",
            "text": "そっかーー！ご成人をーー",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "ちくわ",
            "text": "@こんぶ でも何かを訴えてはいる",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "にわとり",
            "text": "わーーーそれなんか自分の意志で絶対話してますね！お嬢さん！",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "こんぶ",
            "text": "あっという間でしたかー？",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "にわとり",
            "text": "もうほんと、あっという間です・・・・",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "ちくわ",
            "text": "そーなんすね",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "こんぶ",
            "text": "そうなんです！！！きっと絶対なにか言ってるんですが",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "こんぶ",
            "text": "解読不能です笑",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "ちくわ",
            "text": "もう少し時間はゆっくりしてほしいです",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "にわとり",
            "text": "幼稚園だなーと思ってたら、すぐ小学生卒業しちゃって、",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "こんぶ",
            "text": "子ども翻訳機ほしーーー",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "にわとり",
            "text": "中学高校は光速でした・・・",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "ちくわ",
            "text": "ドラえもーーーん！！！笑",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "こんぶ",
            "text": "3年間はあっという間ですよねーー",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "にわとり",
            "text": "そうですよね、ほんと、こそだてはあっという間なので、",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "ちくわ",
            "text": "えーまだ一歳のままでもいいです笑",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "にわとり",
            "text": "特に小さい頃の時間は、一緒にゆっくり過ごしてあげてほしいです！",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "ちくわ",
            "text": "そしたらこちらも歳を取らなくていいので笑",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "こんぶ",
            "text": "先輩ママさんにも言われます",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "こんぶ",
            "text": "今を大事にーって！！！",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "にわとり",
            "text": "ですです、ずっと一歳でもいいよね。",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "にわとり",
            "text": "ちょっと大変だけど笑",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "こんぶ",
            "text": "こちらも歳をとらなくていい笑",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "ちくわ",
            "text": "笑",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "にわとり",
            "text": "あっ！それ重要ですね！",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "ちくわ",
            "text": "ただこちらの願い笑",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "にわとり",
            "text": "こっちも年取らない仕様！！",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "こんぶ",
            "text": "たしかに、一歳はちょっと大変だから",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "こんぶ",
            "text": "３歳くらい！笑",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "ちくわ",
            "text": "笑",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "こんぶ",
            "text": "年とりたくないですーー！笑",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "ちくわ",
            "text": "そして、喋れる時ね笑",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "にわとり",
            "text": "そうですね、いやいや期を終わったくらいで！笑",
            "mention_to": []
        },
        {
            "utterance_id": 103,
            "interlocutor_id": "こんぶ",
            "text": "まあ、いい歳の取り方をしたいですね！",
            "mention_to": []
        },
        {
            "utterance_id": 104,
            "interlocutor_id": "ちくわ",
            "text": "いやいや期かーーー",
            "mention_to": []
        },
        {
            "utterance_id": 105,
            "interlocutor_id": "こんぶ",
            "text": "はい！イヤイヤ期おわったら",
            "mention_to": []
        },
        {
            "utterance_id": 106,
            "interlocutor_id": "にわとり",
            "text": "あ、そうですね！ちょっとしゃべれるくらいがいいかなー！",
            "mention_to": []
        },
        {
            "utterance_id": 107,
            "interlocutor_id": "ちくわ",
            "text": "@こんぶ 美容を意識とした？",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 108,
            "interlocutor_id": "にわとり",
            "text": "そうそう、いやいや期はね、ほんとに魔物になるから・・・笑",
            "mention_to": []
        },
        {
            "utterance_id": 109,
            "interlocutor_id": "にわとり",
            "text": "うちのおっさんも魔物でしたよ！",
            "mention_to": []
        }
