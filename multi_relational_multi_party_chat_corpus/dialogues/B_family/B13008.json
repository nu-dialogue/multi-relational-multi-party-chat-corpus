    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "にわとり",
            "text": "こんばんは！",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "じゃがいも",
            "text": "こんばんはー",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "くま",
            "text": "こんばんは！",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "じゃがいも",
            "text": "今日仕事終わりにさつまいも博に行きました",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "にわとり",
            "text": "今２月ですよねー",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "くま",
            "text": "わお",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "にわとり",
            "text": "このあいだバレンタインでしたが、",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "じゃがいも",
            "text": "@にわとり かぶってしまった！ごめんなさい",
            "mention_to": [
                "にわとり"
            ]
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "にわとり",
            "text": "おおお！！",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "じゃがいも",
            "text": "バレンタインの話します！",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "くま",
            "text": "@じゃがいも よくないねえ",
            "mention_to": [
                "じゃがいも"
            ]
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "にわとり",
            "text": "あっ",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "じゃがいも",
            "text": "バレンタインが？",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "くま",
            "text": "バレンタインのさつまいも？",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "にわとり",
            "text": "さつまいも！！",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "くま",
            "text": "さつまいもおいしいですよね",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "じゃがいも",
            "text": "@くま 違いますね絶対",
            "mention_to": [
                "くま"
            ]
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "にわとり",
            "text": "ごめんなさい、さつまいもはくが気になりすぎて・・・・！",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "にわとり",
            "text": "さつまいも博ってなんですか？！？！",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "くま",
            "text": "さつまいも博でいきましょう",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "じゃがいも",
            "text": "なんかね日本中のさつまいも関連の店が集まってるんです",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "にわとり",
            "text": "はい！",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "くま",
            "text": "@じゃがいも お願いします",
            "mention_to": [
                "じゃがいも"
            ]
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "にわとり",
            "text": "えーーーーーー！",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "じゃがいも",
            "text": "@にわとり ノリで一番長いところに並んだんですけど",
            "mention_to": [
                "にわとり"
            ]
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "にわとり",
            "text": "さつまいも製品をうっている・・・？",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "じゃがいも",
            "text": "いや",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "じゃがいも",
            "text": "石焼き芋的な感じとか芋けんぴとか",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "くま",
            "text": "さつまいもも奥が深い見たいですね",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "にわとり",
            "text": "なるほど・・・・！",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "じゃがいも",
            "text": "まぁ製品ではあるんですけど、結構焼き芋そのものを売ってる",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "じゃがいも",
            "text": "3本入り2,500円！",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "くま",
            "text": "@じゃがいも おととしくらいにも行ったよね",
            "mention_to": [
                "じゃがいも"
            ]
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "じゃがいも",
            "text": "高すぎました",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "にわとり",
            "text": "いろんな種類ありますもんねー。",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "くま",
            "text": "@じゃがいも 高い",
            "mention_to": [
                "じゃがいも"
            ]
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "じゃがいも",
            "text": "@くま 極寒だったねあれも",
            "mention_to": [
                "くま"
            ]
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "にわとり",
            "text": "おおおお＾！お値段！！！",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "にわとり",
            "text": "でもさつまいもって、",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "くま",
            "text": "寒い方が焼き芋はおいしいですね",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "にわとり",
            "text": "トラックで売りに来るのもそのくらの値段するかもですね。",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "じゃがいも",
            "text": "確かに",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "じゃがいも",
            "text": "石を焼くのが大変なのかしら",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "じゃがいも",
            "text": "ただなんとか黄金みたいなのがマジでスイートポテトでした",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "くま",
            "text": "@にわとり トラックたまに見ますね",
            "mention_to": [
                "にわとり"
            ]
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "くま",
            "text": "スーパーの前とか",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "じゃがいも",
            "text": "ほんとうに美味しくて旦那と感動しちゃいました",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "にわとり",
            "text": "@くま そうですね！",
            "mention_to": [
                "くま"
            ]
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "くま",
            "text": "@じゃがいも あれはおいしかった",
            "mention_to": [
                "じゃがいも"
            ]
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "にわとり",
            "text": "やっぱり違いますよね、",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "にわとり",
            "text": "プロがつくる石焼き芋・・！",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "くま",
            "text": "さつまいもも随分と種類ありますよね",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "じゃがいも",
            "text": "@にわとり 行ける機会があれば絶対に食べた方がいいレベルですね",
            "mention_to": [
                "にわとり"
            ]
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "じゃがいも",
            "text": "そう！",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "にわとり",
            "text": "わーーーーいってみたい！",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "じゃがいも",
            "text": "いちごと同じくらいあるんじゃないかなぁ",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "くま",
            "text": "スーパーで売ってるのですらまあまあうまいのに",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "にわとり",
            "text": "べにあずまとか、べにはるかとかよく聞きますね！",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "にわとり",
            "text": "あーーーわかります。スーパーのもおいしいですよね、",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "じゃがいも",
            "text": "@にわとり なんかもう聞いたこともない名前でした",
            "mention_to": [
                "にわとり"
            ]
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "くま",
            "text": "@じゃがいも 全国色々なとこでやってるのかな",
            "mention_to": [
                "じゃがいも"
            ]
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "にわとり",
            "text": "ほかほかにあったかいやつ。",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "じゃがいも",
            "text": "もしかしたら出回らない？",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "にわとり",
            "text": "そうかも！！",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "にわとり",
            "text": "出回らないのかも！",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "くま",
            "text": "@じゃがいも 大きい都市とかでやってそうだけど",
            "mention_to": [
                "じゃがいも"
            ]
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "じゃがいも",
            "text": "@くま 代々木公園とかでも前やってた気がするよ",
            "mention_to": [
                "くま"
            ]
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "にわとり",
            "text": "いいなあ、そちらは大きい都市ですか？",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "くま",
            "text": "プロ専用の芋とかあるんでしょうか",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "じゃがいも",
            "text": "やってたのは大宮近くだったんで",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "にわとり",
            "text": "大宮かあー",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "じゃがいも",
            "text": "でかいところですね、埼玉では",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "にわとり",
            "text": "そうですね、いいなあ！",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "にわとり",
            "text": "こちらは田舎なもので",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "じゃがいも",
            "text": "@くま プロしか手に入らない芋はありそう",
            "mention_to": [
                "くま"
            ]
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "にわとり",
            "text": "大きなイベントはあんまりないですうー",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "じゃがいも",
            "text": "あら",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "くま",
            "text": "@じゃがいも あとは大阪とかでもやってそうね",
            "mention_to": [
                "じゃがいも"
            ]
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "にわとり",
            "text": "そうですね、大阪とかもやってそう！いも博・・・！",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "くま",
            "text": "福岡とか",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "じゃがいも",
            "text": "ぜひ言ってみて欲しい！",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "にわとり",
            "text": "ア―――確かに！！",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "くま",
            "text": "なんか3都市くらいでやってそうなイメージですね",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "じゃがいも",
            "text": "ほんとうに感動してしまうから",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "にわとり",
            "text": "たべたーい！",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "にわとり",
            "text": "すごいですよね、さつまいも、野菜なのに、",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "じゃがいも",
            "text": "@にわとり なんか投票みたいなものもあって",
            "mention_to": [
                "にわとり"
            ]
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "じゃがいも",
            "text": "何処が良かったかみたいな",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "にわとり",
            "text": "投票？",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "くま",
            "text": "意外と出店してる店舗が近くにあったりして",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "じゃがいも",
            "text": "いろんな店が出店してるから",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "にわとり",
            "text": "なるほどー！そうかもしれない・・・",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "にわとり",
            "text": "当地田舎だから、農作物は作ってるとこおおいからな、",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "くま",
            "text": "@じゃがいも 優勝すると箔がつくのかしら",
            "mention_to": [
                "じゃがいも"
            ]
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "じゃがいも",
            "text": "ただ私は寒すぎて投票できずに投票コインを持って帰ってきました",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "にわとり",
            "text": "意外とさつまいものおおもとが当地から出てたりして。",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "じゃがいも",
            "text": "最低な子です",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "くま",
            "text": "野菜おいしいですかね",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "じゃがいも",
            "text": "@にわとり その節はあります",
            "mention_to": [
                "にわとり"
            ]
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "じゃがいも",
            "text": "説か",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "くま",
            "text": "@にわとり ありえますね",
            "mention_to": [
                "にわとり"
            ]
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "にわとり",
            "text": "あっでも記念にコインもらってきてもいいですよね！",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "じゃがいも",
            "text": "確かですけど多分宮城のお店だった気がする",
            "mention_to": []
        },
        {
            "utterance_id": 103,
            "interlocutor_id": "にわとり",
            "text": "へーーー！！！東北かー",
            "mention_to": []
        },
        {
            "utterance_id": 104,
            "interlocutor_id": "じゃがいも",
            "text": "なんか日本一を2回取った的な",
            "mention_to": []
        },
        {
            "utterance_id": 105,
            "interlocutor_id": "くま",
            "text": "さつまいも有名なとこどこだろう",
            "mention_to": []
        },
        {
            "utterance_id": 106,
            "interlocutor_id": "にわとり",
            "text": "すごい！",
            "mention_to": []
        },
        {
            "utterance_id": 107,
            "interlocutor_id": "にわとり",
            "text": "さつまいも・・・",
            "mention_to": []
        },
        {
            "utterance_id": 108,
            "interlocutor_id": "くま",
            "text": "土がいいとこ？",
            "mention_to": []
        }
