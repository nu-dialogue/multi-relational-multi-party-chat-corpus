        },
        {
            "utterance_id": 64,
            "interlocutor_id": "しらたき",
            "text": "地元にはないですが",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "しらたき",
            "text": "この前東京の",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "すじこ",
