            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "しらたき",
            "text": "アメ横にありました",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "すじこ",
            "text": "凄い進化を遂げているんですね",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "つくね",
            "text": "@すじこ シナモンとかきなことかありましたよー！",
