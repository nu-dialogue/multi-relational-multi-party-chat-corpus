    ],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "いるか",
            "text": "こんばんは～",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "たまご",
            "text": "こんばんは",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "つみれ",
            "text": "こんばんは",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "いるか",
            "text": "お二人は寝つきいいほうですか？",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "たまご",
            "text": "いいほうです！",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "つみれ",
            "text": "いつでも即寝れます！",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "たまご",
            "text": "横になるとすぐ寝ます",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "いるか",
            "text": "二人とも！",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "たまご",
            "text": "@いるか どうですか？",
            "mention_to": [
                "いるか"
            ]
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "いるか",
            "text": "@たまご のび太君スタイル・・・",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "つみれ",
            "text": "すぐ寝る親子です",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "たまご",
            "text": "3秒で寝れます",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "いるか",
            "text": "私なかなか寝れなくてゴロゴロしちゃいます",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "いるか",
            "text": "@たまご 完全にのび太君・・・！",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "たまご",
            "text": "たまにそういう日もあります",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "たまご",
            "text": "寝るの好きです",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "つみれ",
            "text": "@いるか お布団入ってから寝るまで長いですか？",
            "mention_to": [
                "いるか"
            ]
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "いるか",
            "text": "@たまご そういう時どうしてますか？",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "いるか",
            "text": "@つみれ 最近は眠くなるまで起きているので・・・",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "たまご",
            "text": "スマホいじって眠くなってきたら目を無理矢理閉じます",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "つみれ",
            "text": "@たまご 無理矢理閉じなくても",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "いるか",
            "text": "なるほど・・・強制就寝・・・",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "たまご",
            "text": "けど寝れないとき目閉じると",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "たまご",
            "text": "いろんなことが頭にでてきて",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "たまご",
            "text": "余計寝れないです",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "いるか",
            "text": "分かります・・・",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "たまご",
            "text": "眠くなるまで何かしないと",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "つみれ",
            "text": "@いるか 寝れない時どうしてるんですか？",
            "mention_to": [
                "いるか"
            ]
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "いるか",
            "text": "そして大概夜の考え事よくない・・・",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "たまご",
            "text": "寝れないです",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "たまご",
            "text": "そうですー",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "いるか",
            "text": "@つみれ 起きています！",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "いるか",
            "text": "がっつり、起きます！",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "つみれ",
            "text": "起きて何してます？",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "いるか",
            "text": "音楽聞いたり、作業をしたり、起きている友人と通話したりしています！",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "たまご",
            "text": "良いですねー",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "たまご",
            "text": "寝れなすぎると僕は映画観ます",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "いるか",
            "text": "夜の映画いいですね！",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "いるか",
            "text": "怖いの以外で！",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "たまご",
            "text": "怖いのは見れないです",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "つみれ",
            "text": "@いるか ですね・・・怖いのは・・・",
            "mention_to": [
                "いるか"
            ]
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "いるか",
            "text": "@つみれ 大人になってもこわいものは怖いですよね・・・",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "つみれ",
            "text": "@いるか はい、夜中見たらトイレ行けなくなります",
            "mention_to": [
                "いるか"
            ]
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "つみれ",
            "text": "お風呂で頭洗うのも怖い",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "たまご",
            "text": "ホラー映画は無理です",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "いるか",
            "text": "すっごいわかります",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "たまご",
            "text": "お化け屋敷とかも無理なんで",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "いるか",
            "text": "こないだ友人と話していたのは、海外のホラーは物理で何とかなりそうだけど、ジャパニーズホラーは物理じゃないから無理って",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "いるか",
            "text": "@たまご 私もです！",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "たまご",
            "text": "たしかに！",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "つみれ",
            "text": "@いるか わかる気がする",
            "mention_to": [
                "いるか"
            ]
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "いるか",
            "text": "殴れば倒せそうな気がするって言ってました",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "たまご",
            "text": "つよい",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "つみれ",
            "text": "@いるか 確かに",
            "mention_to": [
                "いるか"
            ]
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "いるか",
            "text": "でも、確かに物理だからな・・・って思いました",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "つみれ",
            "text": "でも日本のホラーは違いますよね",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "いるか",
            "text": "@つみれ そうそう！",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "つみれ",
            "text": "浸透してくる感じ・・・",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "たまご",
            "text": "物理でも僕は負けそうです",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "つみれ",
            "text": "@たまご 無理だね",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "いるか",
            "text": "概念・・・ジャパニーズホラーは",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "つみれ",
            "text": "概念！",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "いるか",
            "text": "@つみれ そんなに？！",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "いるか",
            "text": "無理です？？",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "つみれ",
            "text": "@いるか 弱い男です",
            "mention_to": [
                "いるか"
            ]
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "いるか",
            "text": "あら～",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "つみれ",
            "text": "肩揉みしてもらっても撫でられてるのかと思います",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "たまご",
            "text": "走って逃げます",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "いるか",
            "text": "@つみれ 笑",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "いるか",
            "text": "@たまご 間違いない・・・！",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "たまご",
            "text": "逃げるは恥だが役に立ちます",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "いるか",
            "text": "星野源さん出てきた！",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "たまご",
            "text": "足だけは早いので",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "いるか",
            "text": "@たまご 逃げ足早いのは利点です！",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "つみれ",
            "text": "@たまご 変だけど走り方",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "いるか",
            "text": "きになるぅ",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "たまご",
            "text": "変じゃないー",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "いるか",
            "text": "どんな走り方・・・",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "つみれ",
            "text": "恥ずかしいです",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "いるか",
            "text": "気になりすぎる・・・",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "つみれ",
            "text": "もう説明できないくらい変",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "たまご",
            "text": "普通です",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "たまご",
            "text": "ちゃんとしたフォームで走ったら",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "いるか",
            "text": "@つみれ めちゃくちゃ気になります笑",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "たまご",
            "text": "凄い早いんじゃない",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "つみれ",
            "text": "逃走中のハンターを誇張したような走り",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "つみれ",
            "text": "違うかな・・・",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "たまご",
            "text": "あれが1番速いです",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "いるか",
            "text": "え、怖い・・・！",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "つみれ",
            "text": "背筋伸ばして走る感じです",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "いるか",
            "text": "腕ブンブンしてる？？？かんじです？？？",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "つみれ",
            "text": "そうです",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "つみれ",
            "text": "グーじゃなくてパーで！",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "いるか",
            "text": "@つみれ え、背後からきたら怖い",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "いるか",
            "text": "パー！！！！！",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "つみれ",
            "text": "@いるか 怖いし、恥ずかしいです",
            "mention_to": [
                "いるか"
            ]
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "たまご",
            "text": "トムクルーズも背筋伸ばして走ります",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "いるか",
            "text": "@つみれ 恥ずかしくないですよ・・・個性です！！！きっと！",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "いるか",
            "text": "@たまご たしかに！",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "つみれ",
            "text": "@いるか 優しい",
            "mention_to": [
                "いるか"
            ]
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "たまご",
            "text": "個性伸ばします",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "いるか",
            "text": "伸ばしましょ・・・！",
            "mention_to": []
        }
