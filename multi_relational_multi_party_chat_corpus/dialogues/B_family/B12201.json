        },
        {
            "utterance_id": 2,
            "interlocutor_id": "てばさき",
            "text": "おはようございます",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "ばなな",
            "text": "土曜日になりましたね",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "えのき",
            "text": "そうですね",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "ばなな",
            "text": "どこかお出かけしますか？",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "てばさき",
            "text": "お出かけですか…",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "えのき",
            "text": "仕事が立て込んでいて…",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "ばなな",
            "text": "それはたいへんですね",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "てばさき",
            "text": "べ、勉強が立て込んでいて…",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "えのき",
            "text": "明日もパートなんです…",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "ばなな",
            "text": "勉強も",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "えのき",
            "text": "@ばなな さんはどこかおでかけしますか？",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "てばさき",
            "text": "何もわからなくて辛いです",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "ばなな",
            "text": "@てばさき テストですか？",
            "mention_to": [
                "てばさき"
            ]
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "てばさき",
            "text": "@ばなな 資格試験です",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "てばさき",
            "text": "簿記二級",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "えのき",
            "text": "勉強大変そうだよね",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "ばなな",
            "text": "すごいですね",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "てばさき",
            "text": "@えのき ソウダヨタイヘンダヨ",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "えのき",
            "text": "月末が試験日で…",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "ばなな",
            "text": "簿記は持ってないです",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "えのき",
            "text": "私も持ってないです",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "えのき",
            "text": "てばさんすごいね",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "ばなな",
            "text": "そうでしたか",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "ばなな",
            "text": "頑張って下さいね",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "ばなな",
            "text": "家の子再来週テストです",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "てばさき",
            "text": "@ばなな がんばります！",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "えのき",
            "text": "@ばなな お子さんはおいくつなんですか？",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "ばなな",
            "text": "@えのき 中1です",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "えのき",
            "text": "そうなんですね",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "ばなな",
            "text": "期末テストがあるんです",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "てばさき",
            "text": "@ばなな では来年から先輩になりますね",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "えのき",
            "text": "そろそろそんな時期ですよね",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "ばなな",
            "text": "そうなんですよ",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "てばさき",
            "text": "期末テスト…ボロボロだった覚えしかない",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "えのき",
            "text": "うちの子は中3なので、もう期末テスト終わりました",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "ばなな",
            "text": "なかなか勉強しません",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "えのき",
            "text": "うちのもなかなか勉強してくれません",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "えのき",
            "text": "悩ましいですよね",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "てばさき",
            "text": "好きなものと関連付けると勉強しやすいですよ",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "ばなな",
            "text": "そうですよね",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "ばなな",
            "text": "@てばさき 何の教科が好きですか？",
            "mention_to": [
                "てばさき"
            ]
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "てばさき",
            "text": "@ばなな 国語が好きです。勉強しなくていいので",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "えのき",
            "text": "ひどい理由",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "てばさき",
            "text": "@えのき だって問題みれば大体答え察せるから",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "えのき",
            "text": "それはそれですごい",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "ばなな",
            "text": "すごいですね",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "ばなな",
            "text": "家の子は国語苦手です",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "てばさき",
            "text": "読書とかよくしますので",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "えのき",
            "text": "@ばなな さんは、読書お好きですか？",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "ばなな",
            "text": "そうですね",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "えのき",
            "text": "どんなジャンルを読みますか？",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "ばなな",
            "text": "最近は出来ていなくて",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "えのき",
            "text": "時間ないですよね～",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "ばなな",
            "text": "陸王よみ始めました",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "てばさき",
            "text": "「かがみの孤城」とか前話題になってましたよね",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "えのき",
            "text": "私は、寝る前にちょっと読もうかと、思った途端に寝落ちしてます",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "ばなな",
            "text": "それは知らなかった",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "ばなな",
            "text": "@えのき わかります",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "てばさき",
            "text": "@ばなな 陸王ってどんな話ですか？",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "えのき",
            "text": "井戸潤さんのですか？",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "えのき",
            "text": "ドラマにもなった",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "ばなな",
            "text": "そうです",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "えのき",
            "text": "池井戸潤さん、でした",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "ばなな",
            "text": "くつを作る話だったような",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "えのき",
            "text": "そうなんですね",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "てばさき",
            "text": "じゃあ心情描写とかすごそうですね",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "えのき",
            "text": "面白いですか？",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "ばなな",
            "text": "そうですね",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "ばなな",
            "text": "これから面白くなるかと",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "えのき",
            "text": "ドラマ版は見ましたか？",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "ばなな",
            "text": "見てないんです",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "えのき",
            "text": "そうなんですね",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "えのき",
            "text": "ドラマ好きなので、面白そうだったら見てみたいです",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "ばなな",
            "text": "見れば良かったです",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "えのき",
            "text": "サブスクには入ってないんですかね",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "ばなな",
            "text": "サブスクですか？",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "ばなな",
            "text": "すみません、わからなくて",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "てばさき",
            "text": "@ばなな アマプラとか、U-NEXTとかのことです",
            "mention_to": [
