        {
            "utterance_id": 87,
            "interlocutor_id": "ばなな",
            "text": "珍しい？",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "てばさき",
            "text": "@ばなな どんなドラマを見ますか？",
            "mention_to": [
                "ばなな"
