    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "ポテト",
            "text": "おはようございます！",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "にんじん",
            "text": "おはようございます！",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "たまねぎ",
            "text": "おはようございます！",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "ポテト",
            "text": "今日寒いですね～",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "ポテト",
            "text": "寒暖差がすごい・・・",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "にんじん",
            "text": "@ポテト そうですね！",
            "mention_to": [
                "ポテト"
            ]
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "たまねぎ",
            "text": "@にんじん そちらは雨ですか？",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "ポテト",
            "text": "@たまねぎ 晴れてます！そちらはどうですか？",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "たまねぎ",
            "text": "雨が降っています！",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "ポテト",
            "text": "雨かな？",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "ポテト",
            "text": "@たまねぎ 雨なんですね！",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "ポテト",
            "text": "あめだと、余計に寒く感じそうです・・",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "たまねぎ",
            "text": "今保育園送って来たんですが",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "たまねぎ",
            "text": "ズボンが濡れて寒いです！",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "ポテト",
            "text": "ひぃ・・・！",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "ポテト",
            "text": "風邪ひいてしまうやつ・・・！",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "ポテト",
            "text": "@にんじん 寒いの平気な方ですか？",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "にんじん",
            "text": "@ポテト 私は嫌いです！",
            "mention_to": [
                "ポテト"
            ]
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "にんじん",
            "text": "暖かくないと肩凝ります",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "ポテト",
            "text": "あー・・・分かります・・・",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "ポテト",
            "text": "ストレッチしないとつらくなってしまう・・・",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "にんじん",
            "text": "ストレッチ？？",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "にんじん",
            "text": "すると暖かくなりますか？",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "ポテト",
            "text": "肩とかまわすと、だいぶ変わりませんか？？？",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "にんじん",
            "text": "あー！",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "にんじん",
            "text": "確かに",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "ポテト",
            "text": "PCいじっていると、すぐに肩が内側に入っちゃって・・・",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "たまねぎ",
            "text": "@ポテト 猫背になりますよね",
            "mention_to": [
                "ポテト"
            ]
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "ポテト",
            "text": "ですです！",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "にんじん",
            "text": "私も仕事が座り仕事なので",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "にんじん",
            "text": "肩こりまくりです！",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "ポテト",
            "text": "わぁ・・・そして今日は雨のダブルパンチと・・・",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "ポテト",
            "text": "@たまねぎ 肩こりとか酷かったりしますか？",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "にんじん",
            "text": "ですね",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "たまねぎ",
            "text": "僕はないですね",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "ポテト",
            "text": "すごい・・・！",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "たまねぎ",
            "text": "だから見てて辛そうです！",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "ポテト",
            "text": "え・・・血行がいいのかな・・・",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "たまねぎ",
            "text": "そうゆうこと？？",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "にんじん",
            "text": "羨ましいー",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "ポテト",
            "text": "@たまねぎ 肩こり、血行が悪くなると痛くなるイメージが！",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "ポテト",
            "text": "@にんじん ほんとに・・・",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "ポテト",
            "text": "うらやましい・・・",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "にんじん",
            "text": "ヘッドスパすると楽になります！",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "ポテト",
            "text": "@にんじん 絶対にきもちいいやつじゃないですか！",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "にんじん",
            "text": "でも、自分がする側です！",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "ポテト",
            "text": "え！？",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "にんじん",
            "text": "美容院やってるので！",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "にんじん",
            "text": "お客さんにやってます！",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "ポテト",
            "text": "すごい！すごい！",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "にんじん",
            "text": "気持ちいいだろーなーと思いながら笑",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "ポテト",
            "text": "あぁ～笑",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "たまねぎ",
            "text": "僕もたまにやってもらいます！",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "たまねぎ",
            "text": "凝らないのに笑",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "ポテト",
            "text": "@たまねぎ いいなぁ～～～～！",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "ポテト",
            "text": "凝らないのに笑",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "たまねぎ",
            "text": "です笑",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "ポテト",
            "text": "え～でもプロだから絶対に気持ちいい・・・",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "にんじん",
            "text": "たまに寝ちゃってます笑",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "にんじん",
            "text": "ずるいなーと思いながら笑",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "ポテト",
            "text": "まって・・・もしかしてやってもらっているから肩凝らない・・・？！",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "たまねぎ",
            "text": "あ！そうかも笑",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "にんじん",
            "text": "それじゃん！",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "にんじん",
            "text": "私のおかげか！",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "ポテト",
            "text": "@にんじん ほら！それだ！",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "たまねぎ",
            "text": "なんかすみません笑",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "ポテト",
            "text": "そしてにんじんさんの肩がこる・・・笑",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "にんじん",
            "text": "そーそー",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "にんじん",
            "text": "たまにはやってもらおー",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "ポテト",
            "text": "ヘッドマッサージとか、意外と全身運動では・・・",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "にんじん",
            "text": "なんか、聞いた話によると",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "ポテト",
            "text": "ほうほう",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "にんじん",
            "text": "その人の気をもらっちゃうみたいです",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "ポテト",
            "text": "気を？！",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "にんじん",
            "text": "疲れとかストレスとか",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "にんじん",
            "text": "信じます？",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "にんじん",
            "text": "目に見えないパワー",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "たまねぎ",
            "text": "わかる気がする！",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "ポテト",
            "text": "@にんじん たまねぎさんに肩こりが・・・ないから・・・信じるしかない・・・",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "にんじん",
            "text": "@ポテト 笑",
            "mention_to": [
                "ポテト"
            ]
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "ポテト",
            "text": "@たまねぎ にんじんさんをいたわってあげないと・・・！",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "たまねぎ",
            "text": "いたわってますよー！笑",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "たまねぎ",
            "text": "家事もやってます！",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "ポテト",
            "text": "いたわってた！笑",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "ポテト",
            "text": "しかもえらい！",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "にんじん",
            "text": "休みの日家が綺麗笑",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "にんじん",
            "text": "私よりも綺麗笑",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "ポテト",
            "text": "しかもすごく頑張ってる！笑",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "たまねぎ",
            "text": "これくらいは余裕です！",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "にんじん",
            "text": "任せっきり！",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "ポテト",
            "text": "え、偉すぎる・・・！",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "にんじん",
            "text": "助かってます！",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "にんじん",
            "text": "例え肩が凝ろうと！",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "ポテト",
            "text": "じゃぁ・・・気をもらってもしかたないかぁ・・・",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "にんじん",
            "text": "そうなりますね…",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "たまねぎ",
            "text": "あざーす！",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "ポテト",
            "text": "@たまねぎ 軽いなぁ笑",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "たまねぎ",
            "text": "え？！笑",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "にんじん",
            "text": "そういうやつです！笑",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "ポテト",
            "text": "でも頑張ってるから仕方ない・・・！むん！",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "にんじん",
            "text": "もちつもたれつですね！",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "ポテト",
            "text": "ですね～！",
            "mention_to": []
        }
