    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "うさぎ",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "ちくわ",
            "text": "おはようございます！",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "こんぶ",
            "text": "こんにちは！",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "うさぎ",
            "text": "今までで1番ひやっとしたことありますか？",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "ちくわ",
            "text": "ひやっとしたことーー？",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "こんぶ",
            "text": "え〜なんでしょう",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "うさぎ",
            "text": "あまりないですか？",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "ちくわ",
            "text": "雪道を歩いてた時に",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "こんぶ",
            "text": "@うさぎ ありますか？",
            "mention_to": [
                "うさぎ"
            ]
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "ちくわ",
            "text": "滑りそうなった時かな？",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "ちくわ",
            "text": "ん？そういうことですよね？",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "ちくわ",
            "text": "ちがうか？",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "うさぎ",
            "text": "@こんぶ 昔、仙台に住んでたのですが",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "こんぶ",
            "text": "子どもが頭をぶつけて",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "こんぶ",
            "text": "ぷっくりコブができました",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "ちくわ",
            "text": "@こんぶ あーーあったね！",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "こんぶ",
            "text": "@うさぎ 仙台に住んでたんですね！",
            "mention_to": [
                "うさぎ"
            ]
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "うさぎ",
            "text": "雪道、スタッドレスタイヤでしたが、山道でくるっと一回転しました",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "ちくわ",
            "text": "@うさぎ あちゃーーー怖い！",
            "mention_to": [
                "うさぎ"
            ]
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "うさぎ",
            "text": "あと少しで崖から落ちて死ぬとこでした",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "こんぶ",
            "text": "スタッドレスタイヤだったのに",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "こんぶ",
            "text": "ましてや、山道ときたら",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "うさぎ",
            "text": "蔵王だったので",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "ちくわ",
            "text": "冬の山道はあぶないですよ！",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "こんぶ",
            "text": "怖すぎます",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "こんぶ",
            "text": "蔵王！！！",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "うさぎ",
            "text": "スキー場です",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "こんぶ",
            "text": "聞いた事あります！",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "ちくわ",
            "text": "蔵王は雪すごいですもんね！",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "ちくわ",
            "text": "蔵王にいったことありますよ！",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "こんぶ",
            "text": "温泉とかも有名じゃないですか？",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "うさぎ",
            "text": "@ちくわ そうなんです、半端ない雪です",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "ちくわ",
            "text": "玉こんにゃく食べた気がする！",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "ちくわ",
            "text": "なんか道の駅？で",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "こんぶ",
            "text": "わたしも出身は岩手なんですが",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "うさぎ",
            "text": "@こんぶ はい、週末は温泉でした!",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "こんぶ",
            "text": "蔵王行った事ないです",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "こんぶ",
            "text": "いいな〜",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "うさぎ",
            "text": "@こんぶ じゃじゃ麺！",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "うさぎ",
            "text": "大好きでした",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "こんぶ",
            "text": "雪景色の中の温泉？",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "こんぶ",
            "text": "じゃじゃ麺",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "ちくわ",
            "text": "雪景色の温泉はおしゃれ！",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "こんぶ",
            "text": "美味しい店あります！",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "うさぎ",
            "text": "@こんぶ そうですね！",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "ちくわ",
            "text": "じゃじゃ麺は、、、",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "こんぶ",
            "text": "@ちくわ ね？あるよね！",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "ちくわ",
            "text": "俺のじゃじゃが",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "うさぎ",
            "text": "@こんぶ えー知りたかった",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "ちくわ",
            "text": "おすすめです！",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "こんぶ",
            "text": "そう！そこだ！",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "うさぎ",
            "text": "@ちくわ じゃじゃが？お店ですか？",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "ちくわ",
            "text": "俺のじゃじゃ",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "ちくわ",
            "text": "です！",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "うさぎ",
            "text": "もう何年も行ってなくて",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "こんぶ",
            "text": "そうなんですよ！",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "うさぎ",
            "text": "あっ、それが店名!",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "ちくわ",
            "text": "じゃじゃ麺食べた後のチータンが",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "こんぶ",
            "text": "そーいう名前のお店があります！",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "ちくわ",
            "text": "うまいんです！",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "こんぶ",
            "text": "そうですそうです",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "うさぎ",
            "text": "@ちくわ ちーたん？？",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "ちくわ",
            "text": "結構地元では有名です！",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "こんぶ",
            "text": "ただ、お店広くないし狭いんですけど",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "こんぶ",
            "text": "混んでます",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "うさぎ",
            "text": "お料理？",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "こんぶ",
            "text": "じゃじゃ麺食べた後に",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "ちくわ",
            "text": "なんか残ったタレと麺に",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "ちくわ",
            "text": "スープをいれて",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "ちくわ",
            "text": "卵をいれて",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "こんぶ",
            "text": "卵をといてスープに",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "うさぎ",
            "text": "美味しそう！！",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "ちくわ",
            "text": "生姜いれるんですよ！",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "こんぶ",
            "text": "美味しいですよ！",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "うさぎ",
            "text": "食べたいです!",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "うさぎ",
            "text": "絶対おいしい！",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "ちくわ",
            "text": "@こんぶ 確かそうだよね？",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "こんぶ",
            "text": "しめでおすすめです！",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "こんぶ",
            "text": "@ちくわ そうそう！",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "ちくわ",
            "text": "麺類が好きな僕からしたら",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "うさぎ",
            "text": "@こんぶ 今度お鍋でやろうかな？",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "ちくわ",
            "text": "最高でした！",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "うさぎ",
            "text": "宮沢賢治のイメージですよね？",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "こんぶ",
            "text": "@うさぎ なるほど！家でもできますね！",
            "mention_to": [
                "うさぎ"
            ]
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "ちくわ",
            "text": "宮沢賢治は",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "ちくわ",
            "text": "花巻空港の近くにありますよ！",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "こんぶ",
            "text": "宮沢賢治！よくご存知で",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "こんぶ",
            "text": "記念館みたいなところもあって",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "うさぎ",
            "text": "@こんぶ 娘が課題でよく読んでました",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "ちくわ",
            "text": "なるほど！",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "ちくわ",
            "text": "勉強しますよね！",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "うさぎ",
            "text": "中尊寺金色堂よく行きました",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "こんぶ",
            "text": "たしかに！そうですよ」",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "ちくわ",
            "text": "僕も勉強したと思っていたけれど",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "ちくわ",
            "text": "岩手に宮沢賢治が",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "うさぎ",
            "text": "あれ？岩手ですよね？",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "ちくわ",
            "text": "絡んでるとは知らなかったです！",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "こんぶ",
            "text": "そう！岩手ですよ！！",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "ちくわ",
            "text": "僕は大阪ですー",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "こんぶ",
            "text": "私岩手出身なのに",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "うさぎ",
            "text": "@ちくわ うわー私も高槻住んでました！",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "こんぶ",
            "text": "中尊寺行ったことないです",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "うさぎ",
            "text": "@こんぶ 近いと行かないですよね",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 103,
            "interlocutor_id": "ちくわ",
            "text": "@こんぶ 俺も行ったことないわ！",
            "mention_to": [
                "こんぶ"
            ]
        }
