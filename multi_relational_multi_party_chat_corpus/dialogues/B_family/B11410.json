    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "ぱんだ",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "つみれ",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "たまご",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "ぱんだ",
            "text": "今季、インフルエンザとかなりましたか？",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "ぱんだ",
            "text": "今ものすごい勢いで流行ってますよね",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "つみれ",
            "text": "インフルにはなってないですが",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "たまご",
            "text": "なってないですー",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "つみれ",
            "text": "胃腸炎になりました",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "つみれ",
            "text": "インフルすごい流行ってますね",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "ぱんだ",
            "text": "@つみれ 胃腸炎！",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "ぱんだ",
            "text": "それはツライ",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "つみれ",
            "text": "@ぱんだ はい、辛かったです",
            "mention_to": [
                "ぱんだ"
            ]
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "たまご",
            "text": "どっちも症状重いですよね",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "ぱんだ",
            "text": "@つみれ 流行ってますよね",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "つみれ",
            "text": "B型のインフルが流行ってるみたいですね",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "ぱんだ",
            "text": "そうなんですよ",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "たまご",
            "text": "そうなんですね",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "つみれ",
            "text": "胃腸炎に症状が似てるみたいで",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "ぱんだ",
            "text": "実は昨日娘が熱を出して",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "つみれ",
            "text": "えーー？それは大変ですね",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "ぱんだ",
            "text": "午前中に熱出したから夕方病院行ったら",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "たまご",
            "text": "大変ですめ",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "たまご",
            "text": "ですね",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "ぱんだ",
            "text": "まだ熱が出てまもないから検査しても出ないかもって言われて",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "ぱんだ",
            "text": "でも検査してもらったら、やっぱり陰性で",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "たまご",
            "text": "熱出ないとだめなんですね",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "つみれ",
            "text": "少し時間経たないと結果出ないんですよね",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "ぱんだ",
            "text": "二度手間ですが後ほどもう一回検査に行きます",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "たまご",
            "text": "大変ですね",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "ぱんだ",
            "text": "多分、インフルエンザBなんじゃないかと",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "つみれ",
            "text": "吐き気とかあります？",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "ぱんだ",
            "text": "先日学級閉鎖になったばかり",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "ぱんだ",
            "text": "吐き気はないようです",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "つみれ",
            "text": "おお！それはきっとインフルですね",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "つみれ",
            "text": "吐き気はないんですね",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "ぱんだ",
            "text": "流行りに若干乗り遅れた感じが否めない",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "たまご",
            "text": "早く治るといいですね！",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "つみれ",
            "text": "頑張って耐えたけど力尽きてしまったんですね",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "ぱんだ",
            "text": "どうせなら学級閉鎖中にインフルになってもらいたかった",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "たまご",
            "text": "タイミングがずれましたね",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "ぱんだ",
            "text": "因みに、10月にもインフルになってます",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "ぱんだ",
            "text": "A型",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "たまご",
            "text": "えー",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "つみれ",
            "text": "あらら",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "ぱんだ",
            "text": "移る時は移るんで、家の中では全く感染対策してません！",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "つみれ",
            "text": "これでBだったら一年以内にどちらもですね",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "つみれ",
            "text": "移ったら移ったで仕方ないですね",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "たまご",
            "text": "うつらない人は何しても逆にうつらないですよね",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "ぱんだ",
            "text": "結局、A型になった時も移らなかったんですよねー",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "ぱんだ",
            "text": "@たまご そうそう！不思議ですね！",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "ぱんだ",
            "text": "免疫力の問題ですかね",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "たまご",
            "text": "不思議です",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "ぱんだ",
            "text": "免疫力高めた方が予防接種より良いかと！",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "つみれ",
            "text": "家族みんな感染しても1人だけ移らなかった知り合いがいます",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "ぱんだ",
            "text": "って免疫力ってどうしたら高まるんだろ？",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "たまご",
            "text": "@つみれ つよい",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "つみれ",
            "text": "予防接種しても移る人もいますからね",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "ぱんだ",
            "text": "@つみれ それ！ウチの娘がそんな感じです。",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "たまご",
            "text": "その人次第ですね",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "ぱんだ",
            "text": "コロナの時、家族中がコロナなのに娘だけケロッと",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "たまご",
            "text": "すごいですね",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "ぱんだ",
            "text": "びっくりしました！",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "たまご",
            "text": "コロナは勝てたんですね！",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "つみれ",
            "text": "すごい！",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "ぱんだ",
            "text": "コロナには強かった！",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "ぱんだ",
            "text": "インフルには負けてます！",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "つみれ",
            "text": "でもインフルには負けちゃいましたか",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "たまご",
            "text": "一勝一敗",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "たまご",
            "text": "ですね",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "ぱんだ",
            "text": "コロナ以降私はインフルとかにもなってないですが、",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "ぱんだ",
            "text": "息子はコロナもインフルAB共に感染してます",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "つみれ",
            "text": "息子さん、モーラしてますね",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "ぱんだ",
            "text": "まぁ若い時って免疫力つけとけ！って思いますが！",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "つみれ",
            "text": "若いと回復も早いですしね",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "ぱんだ",
            "text": "自分がウィルスに感染すると死ぬかと思っちゃいますよー",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "たまご",
            "text": "大人でかからないように免疫つければ",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "つみれ",
            "text": "コロナ、私も感染しましたが辛かったー",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "ぱんだ",
            "text": "@つみれ コロナ辛いですよね！",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "たまご",
            "text": "僕もなりましたー",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "ぱんだ",
            "text": "私、ホテル隔離したんですが",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "たまご",
            "text": "熱がすごい上がります",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "ぱんだ",
            "text": "10日間ホテル暮らし！",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "たまご",
            "text": "長いですね",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "つみれ",
            "text": "ホテルに10日間はきついですね",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "ぱんだ",
            "text": "三日間くらいで熱下がったんで、ホテル内でグダグダさせてもらいましたが、",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "ぱんだ",
            "text": "飽きました…",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "ぱんだ",
            "text": "出れませんからね。外に。",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "つみれ",
            "text": "ホテル隔離の人はみんな後半辛かったって言いますね",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "ぱんだ",
            "text": "お菓子とかジュースとか沢山持ち込んで、",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "たまご",
            "text": "出れないのきついです",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "つみれ",
            "text": "気持ちが滅入るって人もいました",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "ぱんだ",
            "text": "朝からお風呂にお湯溜めまくってお風呂に浸かってました",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "ぱんだ",
            "text": "そうそう！",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "ぱんだ",
            "text": "ビジネスホテルなんで",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "つみれ",
            "text": "部屋の中でどう快適に過ごすか！ですね",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "ぱんだ",
            "text": "窓開けても隣のビルの壁",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "たまご",
            "text": "たしかに",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "たまご",
            "text": "景色も悪いんですね",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "ぱんだ",
            "text": "晴れてんだか、雨なんだかもわからないくらいです",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "つみれ",
            "text": "外に少しも出れないってしんどいですね",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "ぱんだ",
            "text": "だんだんと、昼何だか夜なんだかもわからなくなってくるんですよ",
            "mention_to": []
        }
