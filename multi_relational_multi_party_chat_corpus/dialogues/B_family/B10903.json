    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "ポテト",
            "text": "こんにちは！",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "えのき",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "てばさき",
            "text": "こんにちは！",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "ポテト",
            "text": "今日関東はあったかくなるみたいですね～",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "えのき",
            "text": "そうですね",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "えのき",
            "text": "こないだの大雪は大変でしたね",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "てばさき",
            "text": "そうなんですか！？",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "ポテト",
            "text": "てばさきさんとえのきさんは、関東の方ですか？",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "えのき",
            "text": "そうです",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "ポテト",
            "text": "雪・・・大変でしたね",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "てばさき",
            "text": "こないだ雪めっちゃ降ってましたね",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "えのき",
            "text": "昨日もまだ雪が残っていました",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "ポテト",
            "text": "ところどころまだ残ってますよね",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "えのき",
            "text": "結構残ってますね",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "ポテト",
            "text": "でも、今日のあたたかさで溶けちゃうかな・・・",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "てばさき",
            "text": "冷たいけど踏み崩すの楽しかったです",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "えのき",
            "text": "自転車だと危ないので早く溶けてほしいです",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "ポテト",
            "text": "たしかに！",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "てばさき",
            "text": "雪の中自転車を使う勇気",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "ポテト",
            "text": "自転車、雪ほんとに危ないですよね",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "えのき",
            "text": "自転車でよく遠出をするので…",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "ポテト",
            "text": "すごい！",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "ポテト",
            "text": "えのきさんだけで？",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "えのき",
            "text": "自転車通勤片道20分なんです",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "てばさき",
            "text": "母は遠出どころの話ではない気が…",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "ポテト",
            "text": "自転車で片道２０分は、結構・・・",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "ポテト",
            "text": "てばさきさんのいう通り・・・",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "えのき",
            "text": "帰りにいろんなスーパー寄れるのでいいですよ",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "ポテト",
            "text": "それは確かに！",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "えのき",
            "text": "気分で選べます",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "ポテト",
            "text": "色々寄り道しちゃいそう",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "えのき",
            "text": "スーパーによって特徴があるので…",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "てばさき",
            "text": "その恩恵には預かってます",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "てばさき",
            "text": "お魚が美味しいスーパーとか、お肉がお得用のスーパーとか",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "えのき",
            "text": "肉が安いとかサラダがいいとかすぐ割引するとか…",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "ポテト",
            "text": "てばさきさんが恩恵に預かっている・・・",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "えのき",
            "text": "てばさんは娘です",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "ポテト",
            "text": "いいですね～",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "てばさき",
            "text": "娘なので",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "ポテト",
            "text": "そしたら、ご家族みんなですね！",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "えのき",
            "text": "牛乳の消費が激しいんですが",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "ポテト",
            "text": "我が家もです・・・",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "えのき",
            "text": "重いので買ってくるのが大変です",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "てばさき",
            "text": "それは妹に相談してください、私はホットミルクにしか使ってません",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "ポテト",
            "text": "一週間で５本くらいなくなります・・・",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "えのき",
            "text": "1日2本消費します…",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "えのき",
            "text": "助けてください…",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "ポテト",
            "text": "やばい",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "えのき",
            "text": "やばいです",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "ポテト",
            "text": "それはやばい笑",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "てばさき",
            "text": "その日はコーンフレークに消えてたりしますね、妹によって",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "ポテト",
            "text": "妹さんが飲んでるのかな・・・",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "えのき",
            "text": "中学生で成長期だからか…",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "ポテト",
            "text": "身長が高くなったり・・・？",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "えのき",
            "text": "飲むなら買ってきてほしい",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "えのき",
            "text": "妹がいちばん身長が高いです",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "てばさき",
            "text": "160あるのにあれ以上でかくなるつもりか",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "ポテト",
            "text": "確かに、重い・・・",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "えのき",
            "text": "うちは、下にいけばいくほど大きくなります",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "ポテト",
            "text": "１６０！中学生で！",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "てばさき",
            "text": "ほら、ピラミッドも下が一番大きいから",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "ポテト",
            "text": "下に行くほど大きく笑",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "えのき",
            "text": "そんな…",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "えのき",
            "text": "ピラミッド理論だったなんて…",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "ポテト",
            "text": "たとえがピラミッド笑",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "てばさき",
            "text": "栄養バランスの駒のようでありたかった",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "えのき",
            "text": "今初めて知った…",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "ポテト",
            "text": "逆ピラミッドじゃなかった・・・",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "えのき",
            "text": "おさがりができなくて大変です",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "ポテト",
            "text": "たしかに！",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "えのき",
            "text": "ポテトさんはお子さんいらっしゃるんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "ポテト",
            "text": "下の子にはいいかもしれないけど・・・",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "ポテト",
            "text": "うちは、小学生の娘が一人います",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "ポテト",
            "text": "三年生です",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "えのき",
            "text": "可愛い時期ですね！",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "てばさき",
            "text": "おお、可愛い盛りですね",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "ポテト",
            "text": "まだまだあかちゃんに近いですね・・・",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "てばさき",
            "text": "小学3年生…記憶が曖昧です",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "ポテト",
            "text": "急に大人になるのかな・・・",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "えのき",
            "text": "その頃はまだ可愛かったです・・・",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "てばさき",
            "text": "今も可愛いの間違いでは？",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "ポテト",
            "text": "えのきさんが覚えてますよ・・・てばさきさん！",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "えのき",
            "text": "あー",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "ポテト",
            "text": "笑",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "えのき",
            "text": "可愛い可愛い",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "ポテト",
            "text": "棒読みのやつだ！",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "えのき",
            "text": "棒",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "てばさき",
            "text": "よく聞きますよ、親にとって子供はいつまでも可愛いものだって",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "えのき",
            "text": "てばさんはもう少し女子力を磨いたほうがいいと思う",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "えのき",
            "text": "もう大学生なんだし",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "ポテト",
            "text": "あら",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "てばさき",
            "text": "おなごぢからなら多少",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "えのき",
            "text": "まさか…",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "えのき",
            "text": "女子力、物理？",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "ポテト",
            "text": "おなごぢから笑",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "ポテト",
            "text": "物理のおなごぢから強い・・・",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "えのき",
            "text": "おなごぢからってなんなんですか",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "てばさき",
            "text": "筋肉もっとつけたいです",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "てばさき",
            "text": "そういう概念です",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "ポテト",
            "text": "腕力のほうかぁ・・・",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "ポテト",
            "text": "えのきさん、がんばってください！",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "えのき",
            "text": "てばさんにそんな話題をふった私が悪かったです",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "えのき",
            "text": "長女はもっとメイクとかがんばっているんですが",
            "mention_to": []
        },
        {
            "utterance_id": 103,
            "interlocutor_id": "てばさき",
            "text": "そんなことありません、多少料理できますから",
            "mention_to": []
        },
        {
            "utterance_id": 104,
            "interlocutor_id": "てばさき",
            "text": "女子力は多分ある",
            "mention_to": []
        }
