{
    "dialogue_id": "B13106",
    "dialogue_type": "Family",
    "interlocutors": [
        "くじら",
        "しらたき",
        "つくね"
    ],
    "relationship": [
        "しらたき",
        "つくね"
    ],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "くじら",
            "text": "おはようございます",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "しらたき",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "しらたき",
            "text": "おはようございます",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "つくね",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "くじら",
            "text": "雨になりましたね",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "くじら",
            "text": "寒いです",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "つくね",
            "text": "寒いですね",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "しらたき",
            "text": "こっちは大雪です",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "つくね",
            "text": "雪降ってます",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "くじら",
            "text": "@しらたき 大雪なのですね",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "しらたき",
            "text": "@くじら 家から出たく無いです",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "くじら",
            "text": "明日降る予報でしたけれど",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "つくね",
            "text": "@くじら そうなんですね！",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "くじら",
            "text": "雪マークなくなっていました",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "つくね",
            "text": "関東ですか？",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "くじら",
            "text": "はい",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "くじら",
            "text": "東京なのであまり降りません",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "つくね",
            "text": "たまに降ると",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "つくね",
            "text": "交通規制がかかりますよね？",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "くじら",
            "text": "@つくね そうなんです",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "くじら",
            "text": "それがたいへんで",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "しらたき",
            "text": "電車止まるの",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "しらたき",
            "text": "最悪ですね",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "つくね",
            "text": "@くじら ですよね",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "くじら",
            "text": "電車が止まると困りますね",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "しらたき",
            "text": "長野は車なので雪降っても何とかなる",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "つくね",
            "text": "交通手段がないの困りますよね",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "くじら",
            "text": "大雪だと雪かきもしないとですね",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "つくね",
            "text": "今日が仕事でなくてよかった",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "つくね",
            "text": "@くじら 雪かき大変です",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "つくね",
            "text": "今日の雪は積もりそうです",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "くじら",
            "text": "休日に降った方が良いですね",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "つくね",
            "text": "でも雪かきが",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "くじら",
            "text": "今日の雪は積もりそうなんですね",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "つくね",
            "text": "しんどいですね",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "つくね",
            "text": "猫が外見て",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "つくね",
            "text": "雪を目で追ってます笑",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "しらたき",
            "text": "午後は雪かきかな",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "つくね",
            "text": "@しらたき だねー",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "つくね",
            "text": "また洗濯物が乾かない",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "しらたき",
            "text": "@つくね 手伝ってねー",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "くじら",
            "text": "雪かきは皆でやる感じですか？",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "しらたき",
            "text": "@くじら 自分1人です！",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "つくね",
            "text": "@しらたき できれば遠慮したい笑",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "しらたき",
            "text": "たまに子供参戦",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "くじら",
            "text": "@しらたき そうなんですか！",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "つくね",
            "text": "@くじら 子供参加型です！",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "しらたき",
            "text": "戦力になりませんが",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "つくね",
            "text": "@しらたき 確かに笑",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "つくね",
            "text": "遊ぶだけだよね",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "しらたき",
            "text": "すぐ鎌倉作り始める",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "くじら",
            "text": "子供はそのうち遊んでしまいそうですね",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "つくね",
            "text": "そうそう！",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "つくね",
            "text": "それで雪が余計に散乱する",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "くじら",
            "text": "鎌倉作れるのは羨ましい",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "くじら",
            "text": "鎌倉作れるほど積もらないので",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "しらたき",
            "text": "子供は大喜び",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "くじら",
            "text": "子供は喜びますね",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "つくね",
            "text": "@くじら 遊んでますね！",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "つくね",
            "text": "でも明日からの",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "くじら",
            "text": "でもそれだけ積もると",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "つくね",
            "text": "学校は嫌だと言っていました",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "くじら",
            "text": "通学たいへんですね",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "しらたき",
            "text": "最近の子は雪だるま作らないね",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "つくね",
            "text": "@しらたき 作らないね！",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "くじら",
            "text": "@しらたき そうなんですか！",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "つくね",
            "text": "なんでだろーね？",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "つくね",
            "text": "色つけたがる",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "しらたき",
            "text": "何でだろー",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "くじら",
            "text": "雪というと雪だるま作る感じが",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "つくね",
            "text": "@くじら 寒いし、歩道に雪があるから",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "つくね",
            "text": "行きたくないみたいです",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "つくね",
            "text": "@くじら 雪だるまですよね！",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "つくね",
            "text": "どちらかと言うと丸めて投げてきます",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "つくね",
            "text": "雪合戦！",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "くじら",
            "text": "@つくね 寒いとそうなりますよね",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "くじら",
            "text": "雪合戦は楽しいです。",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "つくね",
            "text": "@くじら スノーブーツも嫌いな様で",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "しらたき",
            "text": "雪合戦楽しい",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "つくね",
            "text": "楽しいけど、痛いじゃんね！",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "つくね",
            "text": "雪硬くて",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "しらたき",
            "text": "本気でやると子供泣く笑",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "くじら",
            "text": "確かに固いと痛いかも",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "つくね",
            "text": "@しらたき 手加減してあげてー",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "くじら",
            "text": "加減しないとですね",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "つくね",
            "text": "@くじら 今雪玉作るおもちゃもあって",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "しらたき",
            "text": "@つくね 手加減難しい",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "つくね",
            "text": "@しらたき 優しくだよー",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "くじら",
            "text": "@つくね そんなものがあるんですね",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "つくね",
            "text": "すごいですよね！",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "つくね",
            "text": "お値段も割とするので",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "つくね",
            "text": "買いませんが笑",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "くじら",
            "text": "どうやって作るんでしょうね？",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "しらたき",
            "text": "でも普通に手で作った方が早いら",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "くじら",
            "text": "確かに手で作らないと",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "つくね",
            "text": "@くじら トングの先端が丸くなってる様なイメージです！",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "くじら",
            "text": "やられっぱなしになるかも",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "くじら",
            "text": "@つくね なるほど",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "つくね",
            "text": "@しらたき 手で作った方が早いね",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "しらたき",
            "text": "でも手が死んじゃう",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "つくね",
            "text": "固めるのが遅いと",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "つくね",
            "text": "ただの集中攻撃！",
            "mention_to": []
        }
