{
    "dialogue_id": "B12104",
    "dialogue_type": "Family",
    "interlocutors": [
        "ばなな",
        "たまご",
        "つみれ"
    ],
    "relationship": [
        "たまご",
        "つみれ"
    ],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "ばなな",
            "text": "おはようございます",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "たまご",
            "text": "おはようございます",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "つみれ",
            "text": "おはようございます",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "ばなな",
            "text": "今日は何をして過ごしますか？",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "たまご",
            "text": "部屋掃除と",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "ばなな",
            "text": "私は洗濯と掃除をしようかと",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "つみれ",
            "text": "施設に入っている祖母に会いに行きます",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "たまご",
            "text": "家で映画見ようと思ってます",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "たまご",
            "text": "そうなんですね",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "つみれ",
            "text": "洗濯と掃除もしなきゃです",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "たまご",
            "text": "いい加減ダンボール開けようと思います",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "ばなな",
            "text": "@つみれ 会いに行ったら喜びますね",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "つみれ",
            "text": "天気のいいお休みだとお出かけしたくなりませんか？",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "たまご",
            "text": "ダンボールが邪魔で片付けようと",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "ばなな",
            "text": "出かけたくなりますよね。",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "たまご",
            "text": "したくなります",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "つみれ",
            "text": "@ばなな そうですね。喜んでくれると思います",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "たまご",
            "text": "散歩とか",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "たまご",
            "text": "自転車とか",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "ばなな",
            "text": "午後は散歩に行ってこようかなと思います。",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "ばなな",
            "text": "歩くの好きなので",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "つみれ",
            "text": "お散歩、いいですね！",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "たまご",
            "text": "引っ越したばかりなので",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "たまご",
            "text": "町を探索したいです",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "たまご",
            "text": "散歩気持ちいいですね",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "つみれ",
            "text": "歩くの好きなんて羨ましいです",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "たまご",
            "text": "音楽聴きながら歩くと楽しいです",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "ばなな",
            "text": "@たまご 町探索楽しいですよね",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "たまご",
            "text": "歩くの好きじゃない？",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "たまご",
            "text": "楽しいです",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "つみれ",
            "text": "@たまご あまり好きでない",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "たまご",
            "text": "良い店見つけるの面白いです",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "たまご",
            "text": "えー",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "ばなな",
            "text": "こんなお店があるとか発見したり",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "たまご",
            "text": "そうです",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "ばなな",
            "text": "@つみれ あまり歩くの好きではないんですね",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "たまご",
            "text": "飲食店とか探したいです",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "つみれ",
            "text": "でも、散策しながら歩いたら楽しそう",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "たまご",
            "text": "楽しい",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "つみれ",
            "text": "@ばなな グータラなんです",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "たまご",
            "text": "ぼくもです",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "ばなな",
            "text": "いやいやお疲れが溜まっているのかも",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "つみれ",
            "text": "すぐ車で移動しちゃう",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "たまご",
            "text": "散歩好きだけどあんま行かないです",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "ばなな",
            "text": "なるほど車があるとそうなりますね",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "たまご",
            "text": "寝るのが好きで",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "つみれ",
            "text": "歩ける距離なのに車移動しちゃいます",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "たまご",
            "text": "ぐーたら好きです",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "ばなな",
            "text": "私はよほど天気が悪くないかぎりは歩きに行きます",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "つみれ",
            "text": "@たまご あなたは寝すぎです",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "たまご",
            "text": "車ないので歩きになる",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "つみれ",
            "text": "@ばなな 私もそうなりたいです",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "たまご",
            "text": "寝すぎないようにしないと",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "ばなな",
            "text": "@たまご 長い時間眠れるんですね",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "たまご",
            "text": "昼寝は気持ちいい",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "たまご",
            "text": "めちゃ寝れます",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "ばなな",
            "text": "最高何時間寝ました？",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "たまご",
            "text": "キリンと真逆です",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "たまご",
            "text": "最高は14時間とkq",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "たまご",
            "text": "とかです",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "ばなな",
            "text": "それはすごいですね",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "つみれ",
            "text": "@ばなな たまごに電話しても、いっつも寝てて出ません",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "たまご",
            "text": "逆に体が疲れました",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "ばなな",
            "text": "@たまご そうなりそうですよね",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "たまご",
            "text": "毎回寝てるタイミングでくるから",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "たまご",
            "text": "ずっと寝てるやつみたいになる",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "たまご",
            "text": "そんなに寝てなくても",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "ばなな",
            "text": "@つみれ なかなか電話がつながらないんですね",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "つみれ",
            "text": "@たまご ずっと寝てると思ってます",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "たまご",
            "text": "最近は繋がります",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "たまご",
            "text": "ずっと寝てない！",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "つみれ",
            "text": "@ばなな 全然つながりません",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "つみれ",
            "text": "前よりはつながります",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "ばなな",
            "text": "よく眠れるのは良いですよ",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "たまご",
            "text": "寝てるタイミングとか予定がある時にかかってくるから",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "たまご",
            "text": "出れない時がおおいー",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "つみれ",
            "text": "そういう私もずっと寝てられるタイプなので",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "つみれ",
            "text": "遺伝ですね",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "たまご",
            "text": "遺伝だ",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "ばなな",
            "text": "なるほど。遺伝ですね",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "たまご",
            "text": "二度寝がやめられない",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "ばなな",
            "text": "逆に睡眠が短くても大丈夫な人はすごいと思って",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "たまご",
            "text": "ショートスリーパー憧れます",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "つみれ",
            "text": "@ばなな どれくらい寝るのがベストですか？",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "たまご",
            "text": "けど睡眠ってすごい大切らしいです",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "ばなな",
            "text": "@つみれ ７時間ぐらい眠れれば",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "ばなな",
            "text": "たまにそれより短いと昼間に眠くて眠くて",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "たまご",
            "text": "寝るか寝てないかでその日のパフォーマンスが大きく変わるらしいです",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "つみれ",
            "text": "@ばなな 理想的ですね",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "たまご",
            "text": "理想的ー",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "ばなな",
            "text": "眠い日に電車に乗ると寝てしまいますね",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "たまご",
            "text": "見た目とかも変わるらしいです",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "たまご",
            "text": "寝ちゃいます",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "たまご",
            "text": "けど降りる駅になると自然と起きます",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "たまご",
            "text": "特技です",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "ばなな",
            "text": "乗り過ごしたらたいへんですよね",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "つみれ",
            "text": "@ばなな 電車の揺れ気持ちいいですしね",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "たまご",
            "text": "良い揺れになる",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "たまご",
            "text": "乗り過ごしたらやばいです",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "たまご",
            "text": "たまに隣の駅まで行っちゃうことあります",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "たまご",
            "text": "電車だと立ちながらでも寝れます",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "つみれ",
            "text": "それが怖くて、終点で降りる時以外寝れないです",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "つみれ",
            "text": "@たまご 立ちながら？",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 103,
            "interlocutor_id": "ばなな",
            "text": "ぼーっとして手前の駅で降りてしまったことあります",
            "mention_to": []
        },
        {
            "utterance_id": 104,
            "interlocutor_id": "たまご",
            "text": "たしかに",
            "mention_to": []
        },
        {
            "utterance_id": 105,
            "interlocutor_id": "たまご",
            "text": "@つみれ そう",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 106,
            "interlocutor_id": "つみれ",
            "text": "@ばなな 起きてたのに？",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 107,
            "interlocutor_id": "ばなな",
            "text": "立ちながらはすごいですね。",
            "mention_to": []
        }
