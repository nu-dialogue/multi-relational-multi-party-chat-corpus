        },
        {
            "utterance_id": 95,
            "interlocutor_id": "すじこ",
            "text": "僕も英語も日本語も出来ません笑",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "つみれ",
            "text": "そこにも共通点が！笑",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "すじこ",
            "text": "高校時代卒業証書貰うために",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "たまご",
            "text": "日本語難しいです",
            "mention_to": []
        },
        {
            "utterance_id": 99,
