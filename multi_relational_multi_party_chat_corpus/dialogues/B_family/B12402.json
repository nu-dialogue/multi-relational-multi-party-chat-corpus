    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "すじこ",
            "text": "こんばんは",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "つみれ",
            "text": "こんばんは",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "たまご",
            "text": "こんばんは",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "すじこ",
            "text": "小さい頃ってどんな子でした？",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "すじこ",
            "text": "僕はとある先生に",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "つみれ",
            "text": "生意気な子でした",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "たまご",
            "text": "笑わせるのが好きで",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "たまご",
            "text": "ふざけてました",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "すじこ",
            "text": "悪魔と言われ笑",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "たまご",
            "text": "えー",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "つみれ",
            "text": "悪魔笑",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "すじこ",
            "text": "ある先生には天使と言われました笑",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "つみれ",
            "text": "どんなとこがですか？",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "たまご",
            "text": "真逆笑",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "つみれ",
            "text": "二面性が？",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "すじこ",
            "text": "@つみれ 生意気とはどんな感じですか？",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "すじこ",
            "text": "@たまご あー一緒ですね笑",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "つみれ",
            "text": "大人びたこと言っちゃう感じです",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "たまご",
            "text": "そうなんですね笑",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "つみれ",
            "text": "タクシーでお釣りいりませんて小学生の時言いました",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "たまご",
            "text": "かっこいい",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "すじこ",
            "text": "@つみれ 悪魔と言った先生は妊娠中の先生だったので",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "つみれ",
            "text": "はい",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "すじこ",
            "text": "うっとおしかったのかと笑",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "たまご",
            "text": "えーー",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "つみれ",
            "text": "笑わせすぎたんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "つみれ",
            "text": "笑わせに行きすぎた？",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "すじこ",
            "text": "@つみれ イカしてますね笑",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "たまご",
            "text": "ふざけすぎたんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "すじこ",
            "text": "そうですね笑",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "たまご",
            "text": "わかります笑",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "つみれ",
            "text": "たまごもだいぶうっとおしいタイプでしたよ",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "すじこ",
            "text": "ウザかったり疲れる存在だったんでしょう笑",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "つみれ",
            "text": "同じタイプの2人",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "つみれ",
            "text": "笑",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "すじこ",
            "text": "僕も小さい頃カッコつけて",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "たまご",
            "text": "同じ笑",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "すじこ",
            "text": "四字熟語で覚えたての言葉を",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "すじこ",
            "text": "母に向けて言ったんですが",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "つみれ",
            "text": "はい",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "たまご",
            "text": "はい",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "すじこ",
            "text": "夏の晴れた空を眺めて",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "つみれ",
            "text": "期待高まる",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "すじこ",
            "text": "今日は青天白日やね",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "すじこ",
            "text": "と言って母の時を止めました",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "つみれ",
            "text": "意味が・・・違いますね笑",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "たまご",
            "text": "全然違う",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "すじこ",
            "text": "いてもたってもいられず赤面しながら逃げました笑",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "つみれ",
            "text": "でも、晴れ渡った空にも使うのか",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "つみれ",
            "text": "笑",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "つみれ",
            "text": "可愛いですね",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "たまご",
            "text": "笑",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "たまご",
            "text": "子供ならでは",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "つみれ",
            "text": "突然の４文字熟語にお母様困惑",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "すじこ",
            "text": "付け焼き刃でカッコつけるものではないと学んだ夏でした",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "つみれ",
            "text": "私も止まるな",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "つみれ",
            "text": "きっと",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "つみれ",
            "text": "甘酸っぱい思い出ですね",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "すじこ",
            "text": "そうなりますよね笑",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "たまご",
            "text": "かっこつけるの難しいです",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "つみれ",
            "text": "@すじこ おもしろネタいっぱいありそうですね",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "たまご",
            "text": "エピソードが強いです",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "つみれ",
            "text": "@たまご かっこいいとこみたことない",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "すじこ",
            "text": "遊びの天才と母から言われてました笑",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "たまご",
            "text": "@つみれ かっこいいとこない",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "つみれ",
            "text": "最高じゃないですか！",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "つみれ",
            "text": "良いお母様",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "すじこ",
            "text": "@つみれ 息子さんの面白いエピソードないですか？",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "つみれ",
            "text": "たまごは基本面白いことばっかり言ってますが",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "すじこ",
            "text": "@つみれ ほっといてもひとりで勝手盛り上がって遊んでたので笑",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "つみれ",
            "text": "外しも多いので",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "すじこ",
            "text": "いいですね！",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "すじこ",
            "text": "外すんだ笑",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "たまご",
            "text": "100発1中",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "つみれ",
            "text": "外すんです・・・止まります私",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "すじこ",
            "text": "100当たるなんてことはないですからね笑",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "つみれ",
            "text": "でも時々天才！って思います",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "つみれ",
            "text": "忘れちゃうけど",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "たまご",
            "text": "やったー",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "すじこ",
            "text": "@たまご 数打ちゃ当たる戦法ですね笑",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "つみれ",
            "text": "たまごは言い間違いが面白いです",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "すじこ",
            "text": "@つみれ やっぱり止まるんですね笑",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "つみれ",
            "text": "ディズニーで",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "つみれ",
            "text": "インディージョーンズのこと",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "つみれ",
            "text": "スティーブンジョブスに乗ろうって言ったり",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "つみれ",
            "text": "ビックサンダーマウンテンを",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "すじこ",
            "text": "それは強い笑",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "つみれ",
            "text": "ザ・マウンテンて言ったり",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "すじこ",
            "text": "山！笑",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "つみれ",
            "text": "ただの山です笑",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "たまご",
            "text": "ありました笑",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "つみれ",
            "text": "@たまご ね",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "すじこ",
            "text": "横文字が苦手なんですね笑",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "つみれ",
            "text": "日本語も危ういですけど",
