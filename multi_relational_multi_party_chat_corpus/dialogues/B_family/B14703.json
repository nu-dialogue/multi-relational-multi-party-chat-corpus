        },
        {
            "utterance_id": 14,
            "interlocutor_id": "たまご",
            "text": "あとちょっとで全部見れます",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "コアラ",
            "text": "@たまご 時系列ぐちゃぐちゃになりませんか？",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "コアラ",
            "text": "すごい，制覇？",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "たまご",
            "text": "なります！",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "コアラ",
            "text": "あと他のヒーローシリーズともごっちゃになります",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "たまご",
            "text": "どこで何が起きてるかが難しいです",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "コアラ",
            "text": "そうそう、これ世界のどこの話？みたいな",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "コアラ",
            "text": "イギリス？いやアメリカ？",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "コアラ",
            "text": "とか悩んでるうちに移動してたり",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "たまご",
            "text": "そうなんです",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "つみれ",
            "text": "私きっとついていけないです",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "たまご",
            "text": "いつのまにか倒してたり",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "コアラ",
            "text": "@つみれ ただショートとしても楽しめるには楽しめます",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "コアラ",
            "text": "@たまご そうそうたまにはしょってません？",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "コアラ",
            "text": "いつのまにか死んでたり",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "つみれ",
            "text": "@コアラ 今度見てみようかな！",
            "mention_to": [
                "コアラ"
            ]
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "たまご",
            "text": "端折ってます",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "コアラ",
            "text": "@つみれ たまごさんのおすすめからぜひ",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "たまご",
            "text": "単体でも楽しめる",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "コアラ",
            "text": "@たまご エージェントカーターってマーベルでしたっけ？",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "コアラ",
            "text": "あれ好きでした",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "たまご",
            "text": "マーベルです！",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "コアラ",
            "text": "女性が主役わりと好きです",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "たまご",
            "text": "分かります",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "コアラ",
            "text": "かなりグロいシーンもありますよね",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "たまご",
            "text": "結構ありますねー",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "コアラ",
            "text": "@つみれ それさえ大丈夫なら是非",
