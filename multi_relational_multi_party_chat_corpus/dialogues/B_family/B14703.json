        },
        {
            "utterance_id": 10,
            "interlocutor_id": "たまご",
            "text": "あとらららんどとか",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "コアラ",
            "text": "全部見てる感じですか？たくさんありますよねー",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "コアラ",
            "text": "@たまご ひらがなで書くとかわいいですね",
            "mention_to": [
