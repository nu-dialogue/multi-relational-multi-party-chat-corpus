            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "つみれ",
            "text": "私はあまり見ないですがたまごは好きです",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "コアラ",
            "text": "@つみれ お忙しいんでしょうね",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "たまご",
            "text": "マーベル系みました",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "コアラ",
            "text": "@たまご うわー私はディズニーチャンネルとかで見ます",
