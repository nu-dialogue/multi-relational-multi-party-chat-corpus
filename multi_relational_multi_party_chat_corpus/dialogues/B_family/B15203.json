        },
        {
            "utterance_id": 2,
            "interlocutor_id": "たまねぎ",
            "text": "こんにちは！",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "ぺんぎん",
            "text": "好きなことはなんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "たまねぎ",
            "text": "@ぺんぎん ざっくりですなー",
            "mention_to": [
                "ぺんぎん"
            ]
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "にんじん",
            "text": "寝ること",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "にんじん",
            "text": "笑",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "ぺんぎん",
            "text": "@たまねぎ うちの旦那と同じだ",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "たまねぎ",
            "text": "寝ること？",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "ぺんぎん",
            "text": "@にんじん 寝る事うちも好きでー",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "にんじん",
            "text": "すきあらば寝る！",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "ぺんぎん",
            "text": "@にんじん 週末はお昼寝かかさない！",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "にんじん",
            "text": "寝る前が幸せー",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "ぺんぎん",
            "text": "たまに寝てそのまま夜！",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "たまねぎ",
            "text": "寝られるとどこもでかけられないー",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "たまねぎ",
            "text": "@ぺんぎん それはやばい",
            "mention_to": [
                "ぺんぎん"
            ]
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "たまねぎ",
            "text": "イライラしないっすか？",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "にんじん",
            "text": "さすがにないなー",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "にんじん",
            "text": "休日は",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "ぺんぎん",
            "text": "@たまねぎ イラつくけど慣れました",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "にんじん",
            "text": "車で寝る",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "たまねぎ",
            "text": "@ぺんぎん 慣れるのかー",
            "mention_to": [
                "ぺんぎん"
            ]
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "ぺんぎん",
            "text": "@にんじん 同じー",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "ぺんぎん",
            "text": "@たまねぎ 車でもねるんですよー",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "にんじん",
            "text": "でも、ちゃんと手ェ繋いで寝ますよ？",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "たまねぎ",
            "text": "そーね",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "ぺんぎん",
            "text": "@にんじん ラブラブじゃないですかー",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "にんじん",
            "text": "@ぺんぎん 一応笑",
            "mention_to": [
                "ぺんぎん"
            ]
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "たまねぎ",
            "text": "初心忘るべからずです！",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "ぺんぎん",
            "text": "@たまねぎ 優しいんですね！",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "ぺんぎん",
            "text": "@たまねぎ なるほど反省",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "たまねぎ",
            "text": "会話なくてもOK",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "にんじん",
            "text": "ありがたやー",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "ぺんぎん",
            "text": "@たまねぎ勉強になります笑",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "たまねぎ",
            "text": "だから、今の生活が好きなことかなー",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "ぺんぎん",
            "text": "@にんじん いいですね",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "たまねぎ",
            "text": "子供たちいて生活できて",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "ぺんぎん",
            "text": "@たまねぎ それが一番ですよ",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "たまねぎ",
            "text": "普通が1番！",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "にんじん",
            "text": "わかるー",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "にんじん",
            "text": "確かにね",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "ぺんぎん",
            "text": "@たまねぎ うちもそういつも言ってますよー",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "にんじん",
            "text": "穏やかに生きたいよねー",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "ぺんぎん",
            "text": "@たまねぎ 普通って大変なことだからね",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "たまねぎ",
            "text": "旦那さんも穏やかなんですねー",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "ぺんぎん",
            "text": "@にんじん 私もそれが一番好き",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "ぺんぎん",
            "text": "@たまねぎ いやー今まではそうでもなかった",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "たまねぎ",
            "text": "@ぺんぎん あら",
            "mention_to": [
                "ぺんぎん"
            ]
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "ぺんぎん",
            "text": "@たまねぎ だから今が穏やかでいいみたい",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "にんじん",
            "text": "そーそー",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "にんじん",
            "text": "それが一番",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "ぺんぎん",
            "text": "@にんじん そうだよね",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "たまねぎ",
            "text": "たまーに旅行行ったり",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "たまねぎ",
            "text": "旅行って喧嘩になりません？",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "にんじん",
            "text": "なるなるー",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "ぺんぎん",
            "text": "@たまねぎ おー旅行すきです、喧嘩はあんまりないですね",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "にんじん",
            "text": "疲れてくるし眠くなるしで",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "たまねぎ",
            "text": "@ぺんぎん 相性いいんですよー",
            "mention_to": [
                "ぺんぎん"
            ]
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "ぺんぎん",
            "text": "@にんじん あー、眠くなると機嫌がわるくなるー",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "にんじん",
            "text": "そーそー",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "にんじん",
            "text": "そこをいかに我慢できるか！",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "ぺんぎん",
            "text": "@にんじん あとお腹すくと",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "にんじん",
            "text": "子供か！笑",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "たまねぎ",
            "text": "なるなる",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "ぺんぎん",
            "text": "@にんじん 本当子供見たいなんです笑",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "たまねぎ",
            "text": "うちらバツイチ同士なんですが",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "ぺんぎん",
            "text": "@にんじん 眠くなると手が温かくなるし",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "たまねぎ",
            "text": "奥さんとは喧嘩にならないんですよー",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "にんじん",
            "text": "@ぺんぎん なる！",
            "mention_to": [
                "ぺんぎん"
            ]
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "にんじん",
            "text": "いや、常にあったかい",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "にんじん",
            "text": "@たまねぎ 喧嘩にならないねー",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "ぺんぎん",
            "text": "@たまねぎ うちもバツイチ同士",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "たまねぎ",
            "text": "@ぺんぎん そうなんだ！",
            "mention_to": [
                "ぺんぎん"
            ]
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "たまねぎ",
            "text": "同じだ！",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "ぺんぎん",
            "text": "@たまねぎ そうなんですー",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "にんじん",
            "text": "再婚して幸せです",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "たまねぎ",
            "text": "ね！",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "ぺんぎん",
            "text": "@たまねぎ だから普通がいいのがわかる",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "たまねぎ",
            "text": "@ぺんぎん そーそー",
            "mention_to": [
                "ぺんぎん"
            ]
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "たまねぎ",
            "text": "ありがたみが違う",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "ぺんぎん",
            "text": "@にんじん ほんとに昔からしたら幸せよ",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "にんじん",
            "text": "前の旦那の時は私だけ我慢してたからなー",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "にんじん",
            "text": "子供達にせめていい思い出を！と",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "ぺんぎん",
            "text": "@にんじん 私もですよー",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "たまねぎ",
            "text": "俺も喧嘩ばっかだったから",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "ぺんぎん",
            "text": "@にんじん 子供だけは守りたいからね",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "にんじん",
            "text": "穏やかに旅行できるのが奇跡笑",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "ぺんぎん",
            "text": "本当、旅行はいいよねどこにいくんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "ぺんぎん",
            "text": "@にんじん 長野はいいとこだよね",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "たまねぎ",
            "text": "この前に時間しか寝てなくて、日帰り旅行行ってきたんだけど",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "ぺんぎん",
            "text": "@たまねぎどこいったんですか？",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "たまねぎ",
            "text": "静岡行きました！",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "にんじん",
            "text": "アウトレットに！",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "ぺんぎん",
            "text": "@たまねぎ 富士山みえました？",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "にんじん",
            "text": "あ！見てない！",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "たまねぎ",
            "text": "見てないね！",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "ぺんぎん",
            "text": "@にんじん お買い物できました？",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "たまねぎ",
            "text": "忘れてた笑",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "にんじん",
            "text": "娘の服を買いに！",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "ぺんぎん",
            "text": "@たまねぎ あれー富士山みえないんだ",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "たまねぎ",
            "text": "たぶん見える",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "たまねぎ",
            "text": "富士急に興奮してたね！",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "にんじん",
            "text": "してたしてた！",
            "mention_to": []
        },
        {
            "utterance_id": 103,
            "interlocutor_id": "ぺんぎん",
            "text": "@にんじん やっぱりお母さんですね、娘の服が優先なんですね",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 104,
            "interlocutor_id": "にんじん",
            "text": "@ぺんぎん 普段我慢させてるなーと",
            "mention_to": [
                "ぺんぎん"
            ]
        },
        {
