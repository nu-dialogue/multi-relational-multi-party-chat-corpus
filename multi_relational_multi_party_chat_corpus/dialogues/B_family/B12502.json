        "こんぶ"
    ],
    "relationship": [
        "ちくわ",
        "こんぶ"
    ],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "すじこ",
            "text": "こんばんは",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "ちくわ",
            "text": "こんばんは！",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "こんぶ",
            "text": "こんばんは",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "こんぶ",
            "text": "夕飯はなにたべました？",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "こんぶ",
            "text": "私たちは蓮根ハンバーグでさ",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "ちくわ",
            "text": "僕らは、ハンバーグでしたー！",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "こんぶ",
            "text": "でしたよ！",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "ちくわ",
            "text": "食いすぎてもう眠いです。",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "すじこ",
            "text": "うちは生姜焼きでした",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "こんぶ",
            "text": "@ちくわ ね、眠い",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "ちくわ",
            "text": "おー！スタミナ！",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "こんぶ",
            "text": "生姜焼き美味しそう！！！",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "ちくわ",
            "text": "生姜焼きいいですね！！",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "すじこ",
            "text": "ハンバーグいいですね！",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "ちくわ",
            "text": "僕も好きです！",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "こんぶ",
            "text": "@すじこ 沢山たべられます？",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "こんぶ",
            "text": "大食いの方ですか？",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "ちくわ",
            "text": "ハンバーグ小さいのを4個食べました！",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "すじこ",
            "text": "妻の1番好きな料理がハンバーグです",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "ちくわ",
            "text": "@すじこ おお！いいですね！",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "こんぶ",
            "text": "奥様、お料理上手なんですね！！！",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "すじこ",
            "text": "@こんぶ あんまり食べない方ですかね",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "ちくわ",
            "text": "ぜひ作ってあげてくださいよ！笑",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "こんぶ",
            "text": "そうなんですね！",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "すじこ",
            "text": "一般男性の中でわ",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "こんぶ",
            "text": "私たちは大食いだと思います",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "ちくわ",
            "text": "僕ら2人ともめちゃ食います",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "すじこ",
            "text": "@こんぶ 目分量でやるので",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "すじこ",
            "text": "毎回味が違って",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "すじこ",
            "text": "面白いです",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "ちくわ",
            "text": "目分量は料理の得意な証拠です！",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "こんぶ",
            "text": "@すじこ 目分量めちゃくちゃわかります",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "すじこ",
            "text": "どのくらい食べるんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "こんぶ",
            "text": "でも、味が違っておもしろい",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "ちくわ",
            "text": "@すじこ でも美味しく食べれるのは幸せですね！",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "こんぶ",
            "text": "って表現されるのは",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "ちくわ",
            "text": "どのくらい？",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "こんぶ",
            "text": "素敵ですよ！！！",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "ちくわ",
            "text": "んー",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "ちくわ",
            "text": "回転寿司でーー",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "ちくわ",
            "text": "25枚とか？",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "すじこ",
            "text": "@こんぶ ありがとうございます",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "こんぶ",
            "text": "私は、１０枚！",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "ちくわ",
            "text": "そこにうどんと茶碗蒸しとデザートですね！",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "こんぶ",
            "text": "デザートは別腹です",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "すじこ",
            "text": "@ちくわ 1人で！？",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "ちくわ",
            "text": "@すじこ もちろん！！！",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "すじこ",
            "text": "@こんぶ 僕もそのくらいです",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "すじこ",
            "text": "@ちくわ 凄い！",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "こんぶ",
            "text": "あら、じゃあでも、男性だと",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "こんぶ",
            "text": "少なめなんですかね！",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "ちくわ",
            "text": "でも、３０歳になってから",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "ちくわ",
            "text": "食べれなくなりましたよ！",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "すじこ",
            "text": "@こんぶ デザートは別腹で2つくらいいきます笑",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "こんぶ",
            "text": "年にも影響が、、、",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "こんぶ",
            "text": "@すじこ それは、わかります",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "ちくわ",
            "text": "@すじこ 普通に食べますやん！笑",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "すじこ",
            "text": "歳の影響ありますよね",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "こんぶ",
            "text": "甘党なんですね！",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "ちくわ",
            "text": "娘も一歳なんですが、、、",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "ちくわ",
            "text": "めちゃ食べます！笑",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "すじこ",
            "text": "クレープ屋見つけるととりあえず手を出すくらいにわ",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "こんぶ",
            "text": "毎日お腹ぽんぽこりんです",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "ちくわ",
            "text": "僕らが食べてるのも食べたがります！",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "すじこ",
            "text": "可愛い",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "こんぶ",
            "text": "クレープ！好きなんですね！",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "ちくわ",
            "text": "@すじこ クレープいいすよね！",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "ちくわ",
            "text": "僕もクレープ屋行ったら",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "ちくわ",
            "text": "ホイップ増量です！笑",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "こんぶ",
            "text": "絶対ホイップ増量だよね！！！",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "すじこ",
            "text": "増量！",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "ちくわ",
            "text": "@こんぶ そこは欠かせない！",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "こんぶ",
            "text": "@ちくわ ほんと好きだよね〜",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "ちくわ",
            "text": "後から気持ち悪くなるパターン！",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "こんぶ",
            "text": "私は、アイス入りのクレープが好みです",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "ちくわ",
            "text": "@すじこ なんのクレープが好きですか？",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "すじこ",
            "text": "口周りがえらいことになりそう",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "ちくわ",
            "text": "アイス入りは食べづらそう。",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "こんぶ",
            "text": "でも、アイスは欲しいんだよな〜",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "すじこ",
            "text": "@ちくわ 僕はチョコバナナ系か",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "すじこ",
            "text": "マンゴー系ですね",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "ちくわ",
            "text": "@すじこ 途中ホイップが漏れないか心配です！",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "こんぶ",
            "text": "マンゴー！！！",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "ちくわ",
            "text": "マンゴーー！",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "こんぶ",
            "text": "マンゴー好きなんですね！",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "こんぶ",
            "text": "マンゴークレープは食べたことないです！",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "ちくわ",
            "text": "クレープでマンゴーを食べたことないですね！",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "こんぶ",
            "text": "マンゴーの中身はなにが？",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "すじこ",
            "text": "マンゴー育てようとしたことがあるくらい",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "こんぶ",
            "text": "ホイップ？",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "すじこ",
            "text": "好きです",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "ちくわ",
            "text": "やばい。",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "こんぶ",
            "text": "あら、それは、やってみたら！！！",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "ちくわ",
            "text": "発想が斬新",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "こんぶ",
            "text": "実践あるのみです",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "ちくわ",
            "text": "マンゴーを育てるって",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "すじこ",
            "text": "ホイップとカスタードですね",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "こんぶ",
            "text": "マンゴーも種から？できるのかな",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "ちくわ",
            "text": "南国の知恵がないと！",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "ちくわ",
            "text": "そんなにマンゴー好きなんですね！",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "こんぶ",
            "text": "マンゴーにも、ホイップカスタードあいそうですね",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "すじこ",
            "text": "マンゴーを食べたあと",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "ちくわ",
            "text": "宮崎のマンゴーとか最高に美味いすよ！",
            "mention_to": []
        },
        {
            "utterance_id": 103,
            "interlocutor_id": "すじこ",
            "text": "種から育てたんですが",
            "mention_to": []
        },
        {
            "utterance_id": 104,
            "interlocutor_id": "こんぶ",
            "text": "@ちくわ 宮崎のマンゴー美味しいよね",
            "mention_to": [
                "ちくわ"
            ]
        }
