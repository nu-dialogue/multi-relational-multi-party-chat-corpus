    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "ばなな",
            "text": "こんばんは",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "ちくわ",
            "text": "こんばんは！",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "こんぶ",
            "text": "こんばんは！",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "ちくわ",
            "text": "@ばなな 晩御飯は食べられましたか？",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "こんぶ",
            "text": "私たちはたべましよ！",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "ばなな",
            "text": "私も聞こうと思っていました",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "ちくわ",
            "text": "僕らは、ホッケ食べました！",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "こんぶ",
            "text": "以心伝心ですね！",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "ばなな",
            "text": "豚肉を炒めました",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "こんぶ",
            "text": "いいですね！",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "ばなな",
            "text": "ホッケ、美味しそう",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "ちくわ",
            "text": "美味しかったですよ！",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "こんぶ",
            "text": "毎日献立考えるのに悩みます",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "ばなな",
            "text": "明日はカレーです",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "こんぶ",
            "text": "いいですね！",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "ちくわ",
            "text": "@ばなな カレーライス好きです！",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "こんぶ",
            "text": "すでに明日のメニューが決まってるんですね！",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "ばなな",
            "text": "金曜日はカレーなんです",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "こんぶ",
            "text": "私たちは、何にしようかな。",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "ちくわ",
            "text": "@こんぶ 毎日の献立ありがとうございます！",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "こんぶ",
            "text": "@ばなな 決まってるんですね！",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "ちくわ",
            "text": "お子さんたちは嬉しいですね！",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "こんぶ",
            "text": "それは、いい！！！",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "こんぶ",
            "text": "@ちくわ いえいえ",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "ばなな",
            "text": "@こんぶ 献立考えるのたいへんですよね",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "ちくわ",
            "text": "金曜日にカレーライスということは、",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "ちくわ",
            "text": "次の日はカレーうどんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "こんぶ",
            "text": "@ばなな ほんと大変です。。",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "ばなな",
            "text": "@ちくわ カレーうどん作ります",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "こんぶ",
            "text": "やっぱりそうですか！",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "ちくわ",
            "text": "@ばなな やっぱり！！！",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "ちくわ",
            "text": "僕らもそうですよ！",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "ばなな",
            "text": "それで終わらせるというか",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "こんぶ",
            "text": "私たちも次の日は、カレーうどんになります！",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "ばなな",
            "text": "美味しいですしね",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "こんぶ",
            "text": "@ちくわ がうどん好きで！",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "ばなな",
            "text": "子供はうどんも好きなので",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "こんぶ",
            "text": "お子さんと一緒だ",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "ちくわ",
            "text": "子供と僕は一緒だ！！",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "こんぶ",
            "text": "うんうん",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "ばなな",
            "text": "一緒ですね",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "ちくわ",
            "text": "トマト煮込みハンバーグの次の日は",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "ちくわ",
            "text": "いつもパスタです！",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "ばなな",
            "text": "それも美味しそうです",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "こんぶ",
            "text": "義母に教えてもらいました！",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "ばなな",
            "text": "煮込み、手が込んでいますね",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "ちくわ",
            "text": "前の日の残りをまた違うメニューにしてます！",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "こんぶ",
            "text": "でも、煮込む方が火が通りやすくて",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "こんぶ",
            "text": "簡単ですよ！",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "ばなな",
            "text": "なるほど",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "ばなな",
            "text": "簡単で美味しいのが良いですね",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "こんぶ",
            "text": "ほんとそうです！",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "ちくわ",
            "text": "お子さんならハンバーグも好きそうですね！",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "こんぶ",
            "text": "時短がなによりです！",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "こんぶ",
            "text": "ハンバーグも人気そう！",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "ばなな",
            "text": "ハンバーグは夫の好物です",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "ちくわ",
            "text": "お！！！そうなんですね！",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "こんぶ",
            "text": "@ばなな 旦那さんが！！",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "ちくわ",
            "text": "@ばなな ぜひ煮込みハンバーグ作ってみてください！",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "こんぶ",
            "text": "煮込みですか？和風とかですか？",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "ばなな",
            "text": "子供は唐揚げ",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "こんぶ",
            "text": "あー！",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "ちくわ",
            "text": "唐揚げもいいですね！",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "こんぶ",
            "text": "唐揚げですね！",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "こんぶ",
            "text": "不動の人気ですね！！",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "ばなな",
            "text": "ハンバーグは煮込みはなかなか",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "ばなな",
            "text": "難しそうで",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "ばなな",
            "text": "普通のハンバーグです",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "こんぶ",
            "text": "んじゃ、今度ぜひ！！！",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "ちくわ",
            "text": "唐揚げも油物だから大変ですよね！",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "こんぶ",
            "text": "手作りなだけ、素晴らしいです！",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "こんぶ",
            "text": "揚げ物は、油の処理が大変だね",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "ちくわ",
            "text": "@ばなな 好きな食べ物はなんですか？",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "こんぶ",
            "text": "周りがよごれるし",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "ばなな",
            "text": "唐揚げは作ってないです",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "ちくわ",
            "text": "@こんぶ いつもありがとうございます。",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "こんぶ",
            "text": "美味しいお店がありますか？",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "ばなな",
            "text": "揚げ物が面倒で",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "こんぶ",
            "text": "わかります！",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "ちくわ",
            "text": "@ばなな お惣菜屋さんのとかですか？",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "こんぶ",
            "text": "とても！！！",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "ばなな",
            "text": "夫がいつもお惣菜屋で買ってきます",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "こんぶ",
            "text": "美味しそう！",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "ちくわ",
            "text": "最近は、揚げなくても作れたりしますよね！",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "こんぶ",
            "text": "旦那さんのおすすめがあるんですね！",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "ちくわ",
            "text": "なんでお店の唐揚げって美味しいんですかね？",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "こんぶ",
            "text": "たっぷりの油であげてるから",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "こんぶ",
            "text": "とかかな？",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "ちくわ",
            "text": "@こんぶ なるほど！",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "ばなな",
            "text": "そうかもしれないです",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "ばなな",
            "text": "後は味付けとか",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "ちくわ",
            "text": "久々に唐揚げ食べたいです！",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "こんぶ",
            "text": "私たちの近くのスーパーの唐揚げも",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "こんぶ",
            "text": "美味しいね！！",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "ちくわ",
            "text": "うん！美味しい！",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "こんぶ",
            "text": "お、それでは、、今度",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "こんぶ",
            "text": "唐揚げにしよう！",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "こんぶ",
            "text": "今度！！！",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "こんぶ",
            "text": "明日は無理です！",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "ばなな",
            "text": "食べたくなりますね",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "ちくわ",
            "text": "唐揚げの味付けを",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "こんぶ",
            "text": "食べたくなります",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "ちくわ",
            "text": "焼きそばの粉入れると美味しいですよ！",
            "mention_to": []
        },
        {
            "utterance_id": 103,
            "interlocutor_id": "こんぶ",
            "text": "そういえば前にそんなこと言ってたね！",
            "mention_to": []
        },
        {
            "utterance_id": 104,
            "interlocutor_id": "ちくわ",
            "text": "お子さんも好きな味かと！",
            "mention_to": []
        },
        {
            "utterance_id": 105,
            "interlocutor_id": "ばなな",
            "text": "@ちくわ 初めて聞きました",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 106,
            "interlocutor_id": "こんぶ",
            "text": "やったことないですが",
            "mention_to": []
        },
        {
            "utterance_id": 107,
            "interlocutor_id": "ちくわ",
            "text": "小さい時なんか食べた記憶が、、、",
            "mention_to": []
        }
