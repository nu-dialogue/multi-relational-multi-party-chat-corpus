        {
            "utterance_id": 1,
            "interlocutor_id": "つくね",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "しらたき",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "つくね",
            "text": "コロナが流行ってから",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "ぱんだ",
            "text": "流行ってますね！",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "ぱんだ",
            "text": "インフルエンザも流行ってます！",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "つくね",
            "text": "ウエットティッシュが手放せなくて",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "つくね",
            "text": "インフルこちらも流行ってます！",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "つくね",
            "text": "学級閉鎖でした",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "ぱんだ",
            "text": "ウェットティッシュ？",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "ぱんだ",
            "text": "ウチも学級閉鎖でした！",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "つくね",
            "text": "@ぱんだ そうです！なんか持ち歩かないと不安で笑",
            "mention_to": [
                "ぱんだ"
            ]
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "ぱんだ",
            "text": "除去シートでなくて？",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "ぱんだ",
            "text": "同じか…！",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "しらたき",
            "text": "今年はコロナでも無くインフルでもない",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "つくね",
            "text": "@ぱんだ 除去シートもですが、アルコールで手が荒れてしまって…",
            "mention_to": [
                "ぱんだ"
            ]
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "しらたき",
            "text": "風邪ひいた",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "つくね",
            "text": "@しらたき 確かに！",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "しらたき",
            "text": "何日も",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "ぱんだ",
            "text": "隣の小学校では水疱瘡が流行ってて",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "つくね",
            "text": "1人具合悪かったよね…笑",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "しらたき",
            "text": "仕事やすんだ",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "つくね",
            "text": "@ぱんだ 水疱瘡流行ってます！",
            "mention_to": [
                "ぱんだ"
            ]
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "ぱんだ",
            "text": "@つくね \nそうか！確かに荒れますね！",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "つくね",
            "text": "予防接種したのに…",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "ぱんだ",
            "text": "やっぱり！",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "つくね",
            "text": "水疱瘡ってかかるんですね",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "つくね",
            "text": "@ぱんだ うちの子はセーフでした！",
            "mention_to": [
                "ぱんだ"
            ]
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "ぱんだ",
            "text": "ウチは水疱瘡まだなんで今のうちに移して欲しかった",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "しらたき",
            "text": "おとながなると大変",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "つくね",
            "text": "@しらたき たしかに！",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "ぱんだ",
            "text": "長女は今期、インフルエンザa　長男はab両方かかりました",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "つくね",
            "text": "今朝！？",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "つくね",
            "text": "間違えました！",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "しらたき",
            "text": "今期だよ",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "ぱんだ",
            "text": "今期です！",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "ぱんだ",
            "text": "10月！",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "ぱんだ",
            "text": "大人が水疱瘡はヤバイですね",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "つくね",
            "text": "インフルもしんどいですよね",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "つくね",
            "text": "息子はタミフルが飲めなくて",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "しらたき",
            "text": "おたふくとかも",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "ぱんだ",
            "text": "私はりんご病に大人になってかかりました",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "つくね",
            "text": "アイスに混ぜて食べてました",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "ぱんだ",
            "text": "足が痒くなるんですよ！りんご病！",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "つくね",
            "text": "リンゴ病！",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "しらたき",
            "text": "ほっぺが",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "しらたき",
            "text": "赤いやつ",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "ぱんだ",
            "text": "タミフルが1番効くらしいですね",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "つくね",
            "text": "ほっぺ赤くなるやつ！",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "ぱんだ",
            "text": "インフルは！",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "ぱんだ",
            "text": "そうそう！ほっぺ",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "ぱんだ",
            "text": "赤くなる。",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "つくね",
            "text": "@ぱんだ 息子は粉薬が苦手で",
            "mention_to": [
                "ぱんだ"
            ]
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "ぱんだ",
            "text": "子供は",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "つくね",
            "text": "中々飲めなかったです",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "つくね",
            "text": "大人も赤くなるんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "ぱんだ",
            "text": "ウチの娘は錠剤が苦手でしたよ",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "つくね",
            "text": "@ぱんだ うちの娘もです！",
            "mention_to": [
                "ぱんだ"
            ]
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "ぱんだ",
            "text": "大人は赤くなりません！",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "つくね",
            "text": "いまだに飲めない",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "ぱんだ",
            "text": "足が痒くなるんですよ！",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "ぱんだ",
            "text": "蕁麻疹みたいなのが出ます",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "つくね",
            "text": "足が痒いのもしんどい…",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "しらたき",
            "text": "水虫みたい？",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "つくね",
            "text": "@しらたき なった事ないからわからない！",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "ぱんだ",
            "text": "いや、水虫っぽくはないです！",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "つくね",
            "text": "@しらたき 水虫なの？",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "しらたき",
            "text": "違うよ！",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "ぱんだ",
            "text": "@しらたき さん　水虫？",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "つくね",
            "text": "@しらたき 知らなかった！",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "しらたき",
            "text": "違います！",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "ぱんだ",
            "text": "何か水虫疑惑！",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "しらたき",
            "text": "足は臭いけど",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "つくね",
            "text": "@しらたき 移さないようにね！",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "しらたき",
            "text": "水虫では無い！",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "つくね",
            "text": "確かに！",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "ぱんだ",
            "text": "私は水虫になった事あります",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "つくね",
            "text": "@ぱんだ え！？そうなんですか？",
            "mention_to": [
                "ぱんだ"
            ]
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "しらたき",
            "text": "痒いですか？",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "ぱんだ",
            "text": "高校生の時",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "ぱんだ",
            "text": "ブーツ流行りましたから！",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "ぱんだ",
            "text": "かゆいですよー",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "つくね",
            "text": "あー！ブーツ！",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "しらたき",
            "text": "アムラー？",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "しらたき",
            "text": "ですか？",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "ぱんだ",
            "text": "母親がでっかい声で",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "ぱんだ",
            "text": "この子水虫なんです！薬ください！って",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "つくね",
            "text": "高校生からしたら恥ずかいですよね",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "ぱんだ",
            "text": "薬局で…",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "つくね",
            "text": "薬局ではキツイですね",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "しらたき",
            "text": "母最強！",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "ぱんだ",
            "text": "恥ずかしい通り越してびっくりしました",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "つくね",
            "text": "でもお母様素敵！",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "ぱんだ",
            "text": "母恐るべし！です。",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "つくね",
            "text": "確かに！母は強いですよね！",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "ぱんだ",
            "text": "自分の娘が水虫になったら気をつけます",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "つくね",
            "text": "私も気を付けます！",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "しらたき",
            "text": "小声で",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "つくね",
            "text": "しらたきの水虫に",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "ぱんだ",
            "text": "水虫は皮膚科ですよね？",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "しらたき",
            "text": "薬もらってください",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "つくね",
            "text": "確かそうですよね！",
            "mention_to": []
        }
