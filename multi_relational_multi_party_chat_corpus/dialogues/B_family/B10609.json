                "たまねぎ"
            ]
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "たまねぎ",
            "text": "珍しい！",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "たまねぎ",
            "text": "由来は？",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "にんじん",
            "text": "かわいいー",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "りんご",
            "text": "@たまねぎ 主人がつけました、なんか夢で見たみたい",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "たまねぎ",
            "text": "@りんご え！すご",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "にんじん",
            "text": "運命だー",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "りんご",
            "text": "@にんじん なんかそういうのが多くて主人",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "たまねぎ",
            "text": "他にもあるんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "にんじん",
            "text": "気になる",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "にんじん",
            "text": "予知夢？",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "にんじん",
            "text": "わたし夢占いすきー",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "りんご",
            "text": "@にんじん 車椅子にクービン乗ってるんです",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "たまねぎ",
            "text": "ほう",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "りんご",
            "text": "@たまねぎ その車椅子の名前はカップンカー、これも主人が夢でみいらしくて",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "たまねぎ",
            "text": "韓国？？",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "たまねぎ",
            "text": "笑",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "にんじん",
            "text": "それでそれで？",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "りんご",
            "text": "なんかタイっぽいですよね",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "たまねぎ",
            "text": "あ、タイか笑",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "りんご",
            "text": "とにかくクービンの出会いもすごくて",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "にんじん",
            "text": "タイだな",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "たまねぎ",
            "text": "ほうほう",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "にんじん",
            "text": "買ったわけじゃないんだ",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "りんご",
            "text": "知らないおばあちゃんからもらったんです",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "りんご",
            "text": "主人が",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "たまねぎ",
            "text": "知らない？",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "たまねぎ",
            "text": "そんなことあります？",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "りんご",
            "text": "通りかかりの",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "にんじん",
            "text": "えーー",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "にんじん",
            "text": "笑",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "りんご",
            "text": "そうなんですよ！",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "たまねぎ",
            "text": "この子もらってください！的な？",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "りんご",
            "text": "なんか歳だし病気だからかってって言われて",
