        },
        {
            "utterance_id": 58,
            "interlocutor_id": "にんじん",
            "text": "えー！",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "にんじん",
            "text": "それでもらってくるご主人がすごい",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "りんご",
            "text": "じゃないと保健所いくって",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "にんじん",
            "text": "病気だと貰っても困るだろうに",
