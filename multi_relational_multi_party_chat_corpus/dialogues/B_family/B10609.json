        },
        {
            "utterance_id": 64,
            "interlocutor_id": "たまねぎ",
            "text": "確かに可哀想だけど",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "りんご",
            "text": "すごい病気でした、クービン",
