            "utterance_id": 69,
            "interlocutor_id": "たまねぎ",
            "text": "？？？？",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "りんご",
            "text": "フィラリアも",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "にんじん",
            "text": "まじか",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "りんご",
            "text": "もう買う以上にかかりました笑",
