        },
        {
            "utterance_id": 3,
            "interlocutor_id": "りんご",
            "text": "趣味とかなんでか？",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "しらたき",
            "text": "ゲームですね！",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "つくね",
            "text": "私は漫画読んだりアニメ見たりしてます！",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "つくね",
            "text": "あと、編み物！",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "りんご",
            "text": "@しらたき どんな系の？",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "りんご",
            "text": "@つくね 編み物いいですね",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "しらたき",
            "text": "@りんご グロい系がおおいですね",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "りんご",
            "text": "@つくね 私も手芸をしてます",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "つくね",
            "text": "@りんご 一緒ですね！",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "つくね",
            "text": "母に習って趣味程度ですが！",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "りんご",
            "text": "@しらたき グロい系！",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "しらたき",
            "text": "@りんご バイオハザードとか良くやります",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "りんご",
            "text": "@つくね 私もですよ最近、手芸講座いってます",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "りんご",
            "text": "@しらたき なるほどー！息子はやってます",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "つくね",
            "text": "@りんご いいですね！！わたしも行ってみたいです！",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "しらたき",
            "text": "@りんご そうなんですか！？息子さんゲーム好きです？",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "りんご",
            "text": "@つくね 自分で着る服作ってるんですよ",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "りんご",
            "text": "@しらたき ゲームしかしてません",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "つくね",
            "text": "@りんご 凄い！！私はそこまではできないです",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "つくね",
            "text": "子供のちょっとしたものとかなら",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "つくね",
            "text": "作ってますが…",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "りんご",
            "text": "@つくね ほとんど先生の言う通りにやってるだけなんです、はい",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "しらたき",
            "text": "@りんご 同じだー！家帰ってすぐゲームです笑",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "しらたき",
            "text": "息子も娘もゲーム好きで",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "しらたき",
            "text": "今はポケモンとか一緒にやってます",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "りんご",
            "text": "@しらたき 一緒にできるんですね！いいですね！",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "つくね",
            "text": "@りんご それでも教えてもらった方が上達しますよね！",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "しらたき",
            "text": "@りんご でもすぐ喧嘩する…",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "りんご",
            "text": "@つくね そうなんですよ",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "りんご",
            "text": "@しらたき うちは一人っ子なのでそれはないけどずっとやっちゃいます",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "つくね",
            "text": "@りんご ミシンをコロナ禍の時に買ったんですが",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "つくね",
            "text": "縫い物は難しいです！",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "りんご",
            "text": "@つくね 私もです！買いました",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "しらたき",
            "text": "@つくね 頑張ってるよね！",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "しらたき",
            "text": "うるさいけど笑",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "りんご",
            "text": "@つくね youtubeとかみてやります？",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "しらたき",
            "text": "@りんご ずっとやってますよね！",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "しらたき",
            "text": "今オンラインもあるからな…",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "つくね",
            "text": "@りんご 見てやってます！でも中々思う様にいかないです笑",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "りんご",
            "text": "@しらたき やりたくなるんですよ、しかもやり始めると終わらない",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "しらたき",
            "text": "@りんご ですです！俺も終わらないです",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "りんご",
            "text": "@つくね わかるー、細かいとこわからないです",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "しらたき",
            "text": "よくそれで嫁に怒られます",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "つくね",
            "text": "@りんご そうなんですよ！同じようにやってるんですけどね…",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "つくね",
            "text": "@しらたき そりゃ怒るでしょ笑",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "しらたき",
            "text": "@つくね すみません！",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "りんご",
            "text": "@しらたき ですね！",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "しらたき",
            "text": "@りんご ハマると何時間でもやってられるので",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "しらたき",
            "text": "中毒性高いです！",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "つくね",
            "text": "@しらたき もう少し自重してください！笑",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "りんご",
            "text": "@しらたき うちの子もそんな感じです！",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "しらたき",
            "text": "@つくね 了解です！笑",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "りんご",
            "text": "@しらたき ご飯って呼んでもやってるから",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "りんご",
            "text": "@つくね 子供たちは何か運動とかやってるんですか？",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "しらたき",
            "text": "@りんご 耳が痛い…",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "つくね",
            "text": "@りんご やってます！",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "つくね",
            "text": "息子は野球で",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "つくね",
            "text": "娘はバドミントンです！",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "りんご",
            "text": "@つくね 野球とバトミントンですね、いいですね！",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "つくね",
            "text": "@りんご 野球は朝が早いので",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "りんご",
            "text": "@しらたき 試合とか応援行きますか？",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "つくね",
            "text": "結構しんどいです笑",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "りんご",
            "text": "@つくね 野球の当番とかあるんですか？",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "しらたき",
            "text": "@りんご 行きますよー！まだ試合に出ても",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "しらたき",
            "text": "バットに当たらないですが",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "しらたき",
            "text": "マグレもあって",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "しらたき",
            "text": "この前はホームベースまで戻ってきました！",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "りんご",
            "text": "@しらたき でも応援するのも楽しいですよね",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "りんご",
            "text": "@しらたき おー、子供より喜びますね",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "つくね",
            "text": "@りんご ありますー！今の時期は体育館でやるので",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "つくね",
            "text": "鍵開けが…",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "しらたき",
            "text": "@りんご 応援楽しいですが",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "しらたき",
            "text": "自分の子供の時は",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "りんご",
            "text": "@つくね 体育館、寒いし大変ですね",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "しらたき",
            "text": "ハラハラします！笑",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "つくね",
            "text": "@りんご そうなんですよね…",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "つくね",
            "text": "でも頑張ってるんで",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "つくね",
            "text": "親も頑張ります！笑",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "りんご",
            "text": "@しらたき 確かに子供よりハラハラになりますね",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "しらたき",
            "text": "@りんご 打てーって心で思ってても",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "しらたき",
            "text": "声に出てます！笑",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "りんご",
            "text": "@つくね いいですね！なんかまだ子育てしてるーって感じでー！",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "つくね",
            "text": "@しらたき 心の叫びが",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "つくね",
            "text": "出ちゃうよね！笑",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "りんご",
            "text": "@しらたき 思いっきりやってください！！！",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "つくね",
            "text": "@りんご 子育ては毎日が戦争ですね！笑",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "りんご",
            "text": "@つくね 大変な分、いい思い出になりますよー",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "しらたき",
            "text": "@りんご やっちゃいます！",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "しらたき",
            "text": "そろそろシーズンなので",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "しらたき",
            "text": "楽しみです",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "りんご",
            "text": "@つくね そうそう、毎日めっちゃ忙しかった",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "つくね",
            "text": "@りんご そうですよね！",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "つくね",
            "text": "あの時はっていう話をするのが",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "りんご",
            "text": "@しらたき 応援してまーす！",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "つくね",
            "text": "楽しみではあります！",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "しらたき",
            "text": "@りんご あざっす！",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "しらたき",
            "text": "@りんご さんの息子さんは何かスポーツしてましたか？",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "りんご",
            "text": "@つくね 終わるとホッとしますが寂しいのがほとんどかな",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "つくね",
            "text": "@りんご 寂しいのありますよね…",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "りんご",
            "text": "@しらたき 小さい頃は空手やってました",
            "mention_to": [
