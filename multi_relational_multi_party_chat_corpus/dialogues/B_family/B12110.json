{
    "dialogue_id": "B12110",
    "dialogue_type": "Family",
    "interlocutors": [
        "ばなな",
        "つみれ",
        "たまご"
    ],
    "relationship": [
        "つみれ",
        "たまご"
    ],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "ばなな",
            "text": "こんばんは",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "つみれ",
            "text": "こんばんは",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "たまご",
            "text": "こんばんは",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "ばなな",
            "text": "@つみれ 長野は雪たくさん降りますか",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "つみれ",
            "text": "最近はだいぶ減りました",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "つみれ",
            "text": "でも、先週の月曜日は大雪でした",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "ばなな",
            "text": "東京はこの間降りました",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "つみれ",
            "text": "同じ時ですかね",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "たまご",
            "text": "神奈川も最近凄い大雪ありました",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "ばなな",
            "text": "雪かきたいへんですよね",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "たまご",
            "text": "外出してて凍えるとこでした",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "つみれ",
            "text": "雪かき大変です",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "ばなな",
            "text": "この間は結構降りましたね",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "つみれ",
            "text": "私、頑張り過ぎて寝込みました",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "たまご",
            "text": "寒いし手が震えます",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "ばなな",
            "text": "それはたいへん",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "たまご",
            "text": "えー",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "たまご",
            "text": "大変",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "つみれ",
            "text": "4日くらい不調でした",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "ばなな",
            "text": "そんなに",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "ばなな",
            "text": "それだけたいへんだったのですね",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "つみれ",
            "text": "胃腸炎だったので、長引きました",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "つみれ",
            "text": "痩せましたけど",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "ばなな",
            "text": "胃腸炎、辛いですね",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "つみれ",
            "text": "でもすぐ戻りました",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "つみれ",
            "text": "胃腸炎辛いです",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "たまご",
            "text": "回復してよかったー",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "つみれ",
            "text": "ありがとう",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "ばなな",
            "text": "すぐ治って良かったです",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "つみれ",
            "text": "ありがとうございます",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "ばなな",
            "text": "今はインフルエンザが流行っていますね",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "たまご",
            "text": "ずっと何か流行ってますね",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "つみれ",
            "text": "すごい流行ってますね",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "ばなな",
            "text": "学校の部活中止になりました",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "たまご",
            "text": "ドラックストアでバイトしてますが",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "たまご",
            "text": "みんな薬買ってきます",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "ばなな",
            "text": "@つみれ そうなんですね",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "たまご",
            "text": "そうなんですね",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "つみれ",
            "text": "学級閉鎖とかすごいみたいですね",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "ばなな",
            "text": "ドラッグストアもたいへんそう",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "たまご",
            "text": "大変だー",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "たまご",
            "text": "結構混んでます",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "ばなな",
            "text": "結構力仕事ありますよね",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "たまご",
            "text": "ドリンクとか",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "たまご",
            "text": "重いものが多いです",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "つみれ",
            "text": "薬だけじゃないから",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "たまご",
            "text": "案外力作業です",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "ばなな",
            "text": "@たまご 友達が腰痛で辛そうでした",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "たまご",
            "text": "自分も腰痛いです",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "つみれ",
            "text": "力仕事は足腰にきますね",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "たまご",
            "text": "腰痛いのきついです",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "ばなな",
            "text": "腰痛気を付けて下さいね",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "たまご",
            "text": "ありがとうございます",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "ばなな",
            "text": "痛すぎて動けなくなりますよね",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "つみれ",
            "text": "私は膝が痛くて",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "ばなな",
            "text": "そうでしたか",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "つみれ",
            "text": "注射に通ってます",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "たまご",
            "text": "今からストレッチとかしてかないとですね",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "ばなな",
            "text": "階段とか辛いですね",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "たまご",
            "text": "注射いたそう",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "つみれ",
            "text": "ヒアルロン酸注入",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "ばなな",
            "text": "注射は効きますか？",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "つみれ",
            "text": "その時は聞きます",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "ばなな",
            "text": "なるほど",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "たまご",
            "text": "そうなんだ",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "つみれ",
            "text": "階段や正座が辛いです",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "ばなな",
            "text": "でも、また痛みが出るんですね",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "たまご",
            "text": "きつい",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "つみれ",
            "text": "完全に治るのは時間がかかりそうです",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "たまご",
            "text": "頑張って治してくしかないですね",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "ばなな",
            "text": "気長に行かないとですね",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "ばなな",
            "text": "お二人は運動はしますか？",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "つみれ",
            "text": "はい、気長に治しますます",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "つみれ",
            "text": "たまにヨガをします",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "たまご",
            "text": "最近はできてないんですけど",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "ばなな",
            "text": "何かしていましたか？",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "たまご",
            "text": "大学でテニスとかサッカーとバスケしてました",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "ばなな",
            "text": "スポーツ得意なんですね",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "たまご",
            "text": "たまにやるとすぐ筋肉痛になっちゃいます",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "たまご",
            "text": "普通くらいしかできないです！",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "ばなな",
            "text": "私もテニスしてました",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "たまご",
            "text": "けど楽しくてやりますー",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "たまご",
            "text": "テニス良いですね！",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "ばなな",
            "text": "楽しいのが良いですね",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "つみれ",
            "text": "@ばなな テニスは今もしますか？",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "たまご",
            "text": "中学の時軟式を部活でしてました",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "たまご",
            "text": "@つみれ 硬式ですか？",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "ばなな",
            "text": "息子がソフトテニスしてます",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "たまご",
            "text": "@ばなな 硬式ですか？",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "たまご",
            "text": "間違えました",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "ばなな",
            "text": "私は硬式です",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "たまご",
            "text": "そうなんですね！",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "つみれ",
            "text": "息子さんもテニス部なんですね",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "つみれ",
            "text": "親子でできますね",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "たまご",
            "text": "打ち方とか変わってきますよねー",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "ばなな",
            "text": "一昨日、一緒にテニスしました",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "たまご",
            "text": "いいですね！",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "つみれ",
            "text": "素敵ー",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "ばなな",
            "text": "打ち方は違いますよね",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "つみれ",
            "text": "親子で何かできるっていいですね",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "たまご",
            "text": "いまも時々テニスしたくなります",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "ばなな",
            "text": "テニスは楽しいですね",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "たまご",
            "text": "何かまだスポーツしたいです",
            "mention_to": []
        },
        {
            "utterance_id": 103,
            "interlocutor_id": "ばなな",
            "text": "@つみれ スポーツは？",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 104,
            "interlocutor_id": "ばなな",
            "text": "何かされますか？",
            "mention_to": []
        },
        {
            "utterance_id": 105,
            "interlocutor_id": "つみれ",
            "text": "私、運動音痴で",
            "mention_to": []
        }
