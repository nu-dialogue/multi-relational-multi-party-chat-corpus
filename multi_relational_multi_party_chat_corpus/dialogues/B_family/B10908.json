{
    "dialogue_id": "B10908",
    "dialogue_type": "Family",
    "interlocutors": [
        "ポテト",
        "えのき",
        "てばさき"
    ],
    "relationship": [
        "えのき",
        "てばさき"
    ],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "ポテト",
            "text": "おはようございます！",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "えのき",
            "text": "おはようございます",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "てばさき",
            "text": "おはようございます",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "ポテト",
            "text": "朝ごはん、食べましたか？",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "えのき",
            "text": "@ポテト はい、食べました！",
            "mention_to": [
                "ポテト"
            ]
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "てばさき",
            "text": "@ポテト 食べてません",
            "mention_to": [
                "ポテト"
            ]
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "ポテト",
            "text": "あら！",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "てばさき",
            "text": "あ、ルマンドは食べました",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "えのき",
            "text": "お菓子・・・",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "ポテト",
            "text": "@てばさき お菓子ですね？？？？",
            "mention_to": [
                "てばさき"
            ]
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "てばさき",
            "text": "サクサクしていて美味しかったです",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "ポテト",
            "text": "朝ごはんじゃない・・・",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "えのき",
            "text": "てばさんは、寝坊したんだよね",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "ポテト",
            "text": "美味しいけども・・・",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "てばさき",
            "text": "寝坊ではない、粘りである",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "ポテト",
            "text": "@えのき 寒いですからね・・・",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "えのき",
            "text": "二度寝大好きだからね・・・",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "ポテト",
            "text": "二度寝・・・分かります・・・",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "えのき",
            "text": "@てばさき お昼寝も好きだよね",
            "mention_to": [
                "てばさき"
            ]
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "ポテト",
            "text": "二度寝後の方がスヤ・・・って寝れる気もします・・・",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "てばさき",
            "text": "@えのき 惰眠とはこの世の最たる至福だから仕方ないのです…",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "えのき",
            "text": "@てばさき 夜もお風呂の後はすぐ寝てるよね",
            "mention_to": [
                "てばさき"
            ]
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "てばさき",
            "text": "@えのき ソンナコトナイヨー",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "ポテト",
            "text": "@てばさき 結構寝てます・・・？",
            "mention_to": [
                "てばさき"
            ]
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "えのき",
            "text": "いつ起きてるの！？",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "てばさき",
            "text": "なう！",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "ポテト",
            "text": "確かに起きて・・・る！",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "えのき",
            "text": "今朝はたたき起こしました",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "てばさき",
            "text": "母上姦しかった…",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "えのき",
            "text": "起きないから・・・",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "ポテト",
            "text": "@えのき 寝起きいい方ですか？てばさきさんは・・・",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "てばさき",
            "text": "惰眠を貪る方です！",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "えのき",
            "text": "私は早く起きたいので早く寝ます！",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "ポテト",
            "text": "起こされて機嫌悪くなったりしないです？？",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "ポテト",
            "text": "@えのき 正論だ！",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "てばさき",
            "text": "早く寝たとてそれが早く起きれることにはつながらないですよ…",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "ポテト",
            "text": "@てばさき あれあれあれ・・・",
            "mention_to": [
                "てばさき"
            ]
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "えのき",
            "text": "てばさんは、余裕で10時間以上眠れるしね・・・",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "てばさき",
            "text": "@ポテト 寝起きは騒がしいと機嫌悪いです",
            "mention_to": [
                "ポテト"
            ]
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "えのき",
            "text": "なかなか起きないから・・・",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "ポテト",
            "text": "@てばさき 起こすときに神経使うやつ・・・！",
            "mention_to": [
                "てばさき"
            ]
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "えのき",
            "text": "何回も起こす身にもなってほしい・・・",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "ポテト",
            "text": "@えのき 確かに・・・",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "ポテト",
            "text": "転がすしか・・・ない・・・",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "てばさき",
            "text": "どちらかというとお腹空いてた方が虫のいどころは悪いです",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "えのき",
            "text": "転がす・・・",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "てばさき",
            "text": "転がされる…！？",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "ポテト",
            "text": "そっと、転がしましょう・・・",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "ポテト",
            "text": "床に・・・",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "えのき",
            "text": "そっとだときっと起きないですね・・・",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "てばさき",
            "text": "もふもふ毛布に潜り込むのです",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "えのき",
            "text": "布団が寝心地良すぎるのでは？",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "ポテト",
            "text": "じゃあ、仕方ない・・・アクロバティックに転がすしか・・・",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "えのき",
            "text": "毛布を取り上げましょう",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "てばさき",
            "text": "やめてください、寒いではないですか",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "ポテト",
            "text": "@えのき それはあるかも！寝具がいいと寝れます！",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "ポテト",
            "text": "@てばさき だから起きるのです・・・",
            "mention_to": [
                "てばさき"
            ]
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "てばさき",
            "text": "今朝もお姉さまに剥がされましたけれども",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "ポテト",
            "text": "はがされていた・・・",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "てばさき",
            "text": "二度寝は無事決め込みました！",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "えのき",
            "text": "がーん！！",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "えのき",
            "text": "ダメだった",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "ポテト",
            "text": "@てばさき 意地・・・！二度寝の意地すさまじい！",
            "mention_to": [
                "てばさき"
            ]
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "えのき",
            "text": "いまてばさきさん、学校休みだから・・・",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "えのき",
            "text": "余計起きないんですよね～",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "ポテト",
            "text": "あら、そうなんですね？",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "てばさき",
            "text": "休みではない、授業がないだけだ！！",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "えのき",
            "text": "春休みではないんだ？",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "てばさき",
            "text": "多分春休みではある",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "ポテト",
            "text": "学級閉鎖とかですか",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "ポテト",
            "text": "春休みだった",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "てばさき",
            "text": "なんか学期",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "えのき",
            "text": "大学生なので",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "ポテト",
            "text": "え、春休み？？？？",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "てばさき",
            "text": "と学期の間のスキマみたいな",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "えのき",
            "text": "もう1年のカリキュラム終了してます",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "ポテト",
            "text": "早くないですか、春休み！",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "てばさき",
            "text": "履修する授業がない",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "えのき",
            "text": "早いですよ～",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "ポテト",
            "text": "え、もう？！",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "ポテト",
            "text": "履修授業がない？！",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "えのき",
            "text": "2月3月ずっと家にいて邪魔ですよ～",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "ポテト",
            "text": "@えのき じゃまって言っちゃった",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "てばさき",
            "text": "1月は期末試験期間でしたから、それを過ぎた我はフリーダム！！",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "ポテト",
            "text": "ながーい・・・二か月ぅ・・・",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "えのき",
            "text": "大学生って休み長いですよね",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "ポテト",
            "text": "@てばさき 確かに、テストを頑張ったご褒美・・・？",
            "mention_to": [
                "てばさき"
            ]
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "えのき",
            "text": "夏休みも9月中旬まででしたし",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "ポテト",
            "text": "@えのき 長かったですね・・・自分の時も・・・",
            "mention_to": [
                "えのき"
            ]
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "てばさき",
            "text": "これを機に地元のボランティアに応募…する勇気がない",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "えのき",
            "text": "してくればいいのに！",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "てばさき",
            "text": "ええ…",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "ポテト",
            "text": "@てばさき 朝・・・起きれないから・・・",
            "mention_to": [
                "てばさき"
            ]
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "えのき",
            "text": "やりたいボランティアは、夕方からだよね",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "ポテト",
            "text": "地元のボランティアってどんなことするんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "ポテト",
            "text": "夕方ならワンチャン？！",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "てばさき",
            "text": "4時から7時まで子どもたちの面倒をみる活動",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "えのき",
            "text": "放課後、公民館とかで",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "えのき",
            "text": "てばさんも、小さいころ遊んでもらったんだよね",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "てばさき",
            "text": "今まで私が相手してもらったので！",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "ポテト",
            "text": "@てばさき 素敵じゃないですか！",
            "mention_to": [
                "てばさき"
            ]
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "えのき",
            "text": "大学生のボランティアさんが主で",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "ポテト",
            "text": "勇気をふり絞って参加を・・・！",
            "mention_to": []
        }
