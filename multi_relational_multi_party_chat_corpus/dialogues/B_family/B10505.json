    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "りんご",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "えのき",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "てばさき",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "りんご",
            "text": "果物は好きですか？",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "えのき",
            "text": "私は好きです",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "えのき",
            "text": "よく買いますね",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "えのき",
            "text": "みかんとか・・・",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "てばさき",
            "text": "酸っぱいのは苦手ですが甘いのは好きです！",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "りんご",
            "text": "最近、いちごが旬ですよ",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "てばさき",
            "text": "けど食べるまでが面倒なのであんま食べません",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "えのき",
            "text": "いちごはお高い・・・",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "えのき",
            "text": "もう少ししたらいちごも安くなって来ないかなーと思っていますが",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "りんご",
            "text": "確かに！高い埼玉は意外といちごあるんです",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "えのき",
            "text": "いちご狩りとか行きますか？",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "りんご",
            "text": "買う派ですね",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "てばさき",
            "text": "埼玉はブルーベリー狩りもありましたね",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "えのき",
            "text": "ブルーベリー大好きです",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "りんご",
            "text": "ありますね、美味しいですよね",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "えのき",
            "text": "子供が小さい時結構行きましたブルーベリー狩り",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "えのき",
            "text": "現地で買うほうが安いし・・・",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "てばさき",
            "text": "目が良くなるというのは気になるところです、ブルベリアイ",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "りんご",
            "text": "私も大好きで、いっぱいとってジャムとかにします",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "えのき",
            "text": "目が良く・・・なるかなあ",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "りんご",
            "text": "なってるかな、よくたべてるのになあ",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "えのき",
            "text": "小2から眼鏡な近眼なので・・・",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "りんご",
            "text": "えーそんな小さい時から？",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "てばさき",
            "text": "その時点から眼鏡属性持ちです！お得！",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "りんご",
            "text": "@てばさき メガネですか？コンタクト？",
            "mention_to": [
                "てばさき"
            ]
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "てばさき",
            "text": "眼鏡です",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "えのき",
            "text": "うちの家族は全員眼鏡です",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "りんご",
            "text": "私はちょっと老眼になりかけです",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "てばさき",
            "text": "父も最近は老眼です",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "えのき",
            "text": "あー、旦那も老眼なので、跳ね上げ眼鏡にしましたよ",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "りんご",
            "text": "自分がなるとは",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "てばさき",
            "text": "なんかパカパカできる眼鏡使ってます",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "りんご",
            "text": "わかるー！",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "えのき",
            "text": "私は定期健診行ってますが、まだなんとか踏みとどまってます",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "りんご",
            "text": "でも私は近眼もはいっているのでそこまでではなくて主人のが大変みたいです",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "てばさき",
            "text": "実際老眼ってハズキルーペとか使うんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "えのき",
            "text": "お父さんは使ってないよね",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "てばさき",
            "text": "図書館でよく見かけるから…",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "りんご",
            "text": "@てばさき まだ使ってないですけどね、そのうち考えます",
            "mention_to": [
                "てばさき"
            ]
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "えのき",
            "text": "こないだドラマでも話題になっていましたが、50歳あたりで老眼が気になる感じですね・・・",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "えのき",
            "text": "明日はわが身です・・・",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "りんご",
            "text": "年齢には勝てない",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "えのき",
            "text": "寄る年波には・・・",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "てばさき",
            "text": "やっぱし若いうちから飲むべきかな？ブルベりあい…",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "りんご",
            "text": "@てばさき まだまだ先で羨ましいです",
            "mention_to": [
                "てばさき"
            ]
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "えのき",
            "text": "効果のほどはわからない",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "えのき",
            "text": "最近は、老いては子に従え。と言いながら、子供とゲームしたりしてます",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "えのき",
            "text": "子どものキャラのほうが強いので・・・",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "てばさき",
            "text": "わかさ生活の広報担当なかなか愉快な性格してるの最近知ったのでちょっと飲みたいぜ母さん",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "りんご",
            "text": "いいですね！子供には勝てないですよ",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "えのき",
            "text": "なんでわかさ生活にハマっているの・・・",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "えのき",
            "text": "いつの間に・・・",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "えのき",
            "text": "おばあちゃんに、わかさ生活のDM来てた気がする",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "りんご",
            "text": "@てばさき す、すごい！わかさ生活とか",
            "mention_to": [
                "てばさき"
            ]
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "てばさき",
            "text": "実際流行りに乗っかっていく系の企業の寿命は長いと思う",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "えのき",
            "text": "YouTubeで流行ってるんだ",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "りんご",
            "text": "@てばさき 見てる視点がすごすぎ",
            "mention_to": [
                "てばさき"
            ]
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "えのき",
            "text": "最近の企業はウェブマーケティングに余念がないね",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "えのき",
            "text": "若い人にも刺さってる",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "てばさき",
            "text": "錯視の面白さだったり、美人な女上司が出てきたりしてます",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "えのき",
            "text": "へー",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "えのき",
            "text": "錯視は結構好き",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "えのき",
            "text": "トリックアートみたいなのでしょ？",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "てばさき",
            "text": "トリックアートの他にも、残業の有無でバトルしてるのは見ました",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "えのき",
            "text": "トリックアート美術館行ってみたい",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "りんご",
            "text": "行ってみたいですね",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "りんご",
            "text": "那須にあったかな",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "てばさき",
            "text": "エッシャーとか？",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "えのき",
            "text": "@りんご 美術館とか行きますか？",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "りんご",
            "text": "最近は行けてないですね、わんちゃん飼ってからなかなか",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "えのき",
            "text": "こないだ、てばさんと丸紅ギャラリー行ってきました",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "えのき",
            "text": "あそこ着物で行くと割引になるんですよね",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "てばさき",
            "text": "なんでも源氏物語の女房装束の再現したらしいです",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "りんご",
            "text": "へぇー着物でいったんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "えのき",
            "text": "普段着です",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "えのき",
            "text": "着物を着付けるほうが大変・・・",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "りんご",
            "text": "確かに",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "えのき",
            "text": "でも着物の人がいっぱいいました！",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "てばさき",
            "text": "実践女子大学との合同企画らしいんですが、すごいですよね",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "えのき",
            "text": "なんか、有名な先生が講演会してたらしくて",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "てばさき",
            "text": "NHKでも放映されてました",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "りんご",
            "text": "へぇーすごいですね",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "えのき",
            "text": "当時の技術で再現した、とか…",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "えのき",
            "text": "十二単、重そうですよね",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "りんご",
            "text": "そうなんだ！なんか娘と美術館いいなぁ",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "りんご",
            "text": "私が結婚式の時は着ました",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "えのき",
            "text": "おさんぽがてら行ってきました！",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "てばさき",
            "text": "帯なかったんで抑えないと脱げるらしいです",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "えのき",
            "text": "@りんご 結婚式十二単だったんですか？",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "りんご",
            "text": "なんか流行ってたんですよ、そのころ",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "えのき",
            "text": "へー",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "えのき",
            "text": "すごい",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "えのき",
            "text": "髪型も？",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "てばさき",
            "text": "結婚式は白無垢とウェディングドレスしか知らなかった…",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "りんご",
            "text": "たぶん紀子様が結婚した時あたりだったから",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "えのき",
            "text": "あー、そうなんですね",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "りんご",
            "text": "@てばさき 結婚式とか興味あります？",
            "mention_to": [
                "てばさき"
            ]
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "てばさき",
            "text": "@りんご 結婚のつもりはないので見る専にはなりそうですが、一応あります",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "えのき",
            "text": "興味以前に縁が…",
            "mention_to": []
        }
    ],
