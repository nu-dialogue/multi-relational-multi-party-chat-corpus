    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "ポテト",
            "text": "おはようございます！",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "こんぶ",
            "text": "おはようございます",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "ちくわ",
            "text": "おはようございます！",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "ちくわ",
            "text": "年末年始どこかいきました？",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "こんぶ",
            "text": "わたしは、家でゆっくりだったね！",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "こんぶ",
            "text": "私たちか！",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "ちくわ",
            "text": "僕らは帰省せずに家にいたねー！",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "ポテト",
            "text": "年末年始・・・どこか行ったかな・・・もう・・・記憶が・・・",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "ちくわ",
            "text": "@ポテト 帰省しました？",
            "mention_to": [
                "ポテト"
            ]
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "ポテト",
            "text": "私もたぶん家でゆっくりしていたような・・・",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "こんぶ",
            "text": "それが1番ですね！",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "ポテト",
            "text": "@ちくわ 今年はしませんでした！",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "ちくわ",
            "text": "@ポテト それも1番ですね！",
            "mention_to": [
                "ポテト"
            ]
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "ちくわ",
            "text": "僕らも、姉夫婦が子供産まれてこちらに来てくれました！",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "こんぶ",
            "text": "久しぶりに新生児をみたね！",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "こんぶ",
            "text": "小さかった！",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "ポテト",
            "text": "新生児・・・",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "ポテト",
            "text": "最近見ていないです・・・",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "こんぶ",
            "text": "ほやほやでした！",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "ちくわ",
            "text": "赤ちゃんの匂い嗅いでやりました！",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "ポテト",
            "text": "やわいいきもの・・・",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "こんぶ",
            "text": "@ポテト ほんとそうです！",
            "mention_to": [
                "ポテト"
            ]
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "ちくわ",
            "text": "泣き声が懐かしかったです！",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "こんぶ",
            "text": "ね！懐かしかった！",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "ポテト",
            "text": "@こんぶ だっこもう怖いですもん・・・",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "ポテト",
            "text": "首とか",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "こんぶ",
            "text": "怖いですよね！",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "ポテト",
            "text": "ぐらぐらして",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "ちくわ",
            "text": "@ポテト 意外としっかりしてました！",
            "mention_to": [
                "ポテト"
            ]
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "こんぶ",
            "text": "あんだけ小さい頃抱っこしてはずなのに！",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "ちくわ",
            "text": "もう首座ってた？って感じです",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "ポテト",
            "text": "@こんぶ そうそう・・・",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "こんぶ",
            "text": "自分の子どもを！",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "こんぶ",
            "text": "たしかに、しっかりしてたね！",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "ポテト",
            "text": "@ちくわ そんなにしっかり！",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "ちくわ",
            "text": "大きくなったら大きくなったで大変すけど！",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "こんぶ",
            "text": "髪の毛がふさふさでした！",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "ポテト",
            "text": "ふさふさ！",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "こんぶ",
            "text": "うちの子どもは薄毛ちゃんで",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "ちくわ",
            "text": "@こんぶ そうそう！",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "こんぶ",
            "text": "病院で絶対間違えないくらいに",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "こんぶ",
            "text": "1番なかった！",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "ポテト",
            "text": "@こんぶ そんなに？！",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "こんぶ",
            "text": "色も茶色で！",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "ちくわ",
            "text": "それも可愛かった！",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "こんぶ",
            "text": "同じ病院の他の子は",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "こんぶ",
            "text": "ふさふさでしたよ！",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "ポテト",
            "text": "え！ハーフちゃんみたいじゃないですか！",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "ちくわ",
            "text": "分かりやすいように産まれてきたねー",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "こんぶ",
            "text": "そう〜誰に似たんだか〜",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "こんぶ",
            "text": "そうだね！",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "ちくわ",
            "text": "わたくしです。",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "こんぶ",
            "text": "しっかりパパ似だし！",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "ポテト",
            "text": "@ちくわ パパ似なんですね！",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "ちくわ",
            "text": "今でも友達にそっくりと言われます！",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "ポテト",
            "text": "そんなに！",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "こんぶ",
            "text": "@ポテト そっくりすぎます！",
            "mention_to": [
                "ポテト"
            ]
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "ポテト",
            "text": "@こんぶ お二人がいうくらいだから、ほんとにそっくりなんですね！",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "ちくわ",
            "text": "@ポテト お子さんは誰似ですか？",
            "mention_to": [
                "ポテト"
            ]
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "こんぶ",
            "text": "@ポテト そうなんですよ！！！",
            "mention_to": [
                "ポテト"
            ]
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "ポテト",
            "text": "目元は私、性格は旦那ですかね・・・",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "ちくわ",
            "text": "そうかー性格とかも、、、",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "こんぶ",
            "text": "しっかり分かれてるんですね！",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "ポテト",
            "text": "@ちくわ そうですよ！お二人に似てきますよ！",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "ちくわ",
            "text": "娘もどんな性格になるのだろう",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "こんぶ",
            "text": "うちらの子どもの性格はわたしに似そうです。",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "ちくわ",
            "text": "@こんぶ うんうんそうだね！",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "ポテト",
            "text": "@こんぶ どんな性格になりそうです？",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "ポテト",
            "text": "優しそう・・・",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "こんぶ",
            "text": "いやいや",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "こんぶ",
            "text": "強気な女になりそうです",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "こんぶ",
            "text": "とかって。",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "ちくわ",
            "text": "今でも戦ってるもんね！",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "ポテト",
            "text": "強気な女！",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "ちくわ",
            "text": "食事の時とか！",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "こんぶ",
            "text": "そう！今朝も一歳と、戦ってきました",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "ポテト",
            "text": "食事は確かに戦争・・・",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "ちくわ",
            "text": "@こんぶ 戦ってたね！",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "こんぶ",
            "text": "すでに意志強い女子に成長してます",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "こんぶ",
            "text": "@ポテト お子さんもそうでしたか？",
            "mention_to": [
                "ポテト"
            ]
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "ポテト",
            "text": "@こんぶ 食べない！みたいなやつですか？",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "こんぶ",
            "text": "食べるんですけど",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "こんぶ",
            "text": "スプーンをわざとおとしたり！",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "ちくわ",
            "text": "めちゃくちゃ食べます！",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "ちくわ",
            "text": "自分でやる！！て表現したり",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "こんぶ",
            "text": "自分でたべるーって手を出すと、怒られます",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "ポテト",
            "text": "あー・・・",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "こんぶ",
            "text": "そんな時ありましたか？",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "ポテト",
            "text": "困るやつ・・・",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "ちくわ",
            "text": "ほんまに一歳？てなります！",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "こんぶ",
            "text": "そうなんですよ",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "ポテト",
            "text": "落とすとかはなかったんですが、偏食が凄かったです",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "ちくわ",
            "text": "@ポテト 偏食かーーー",
            "mention_to": [
                "ポテト"
            ]
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "こんぶ",
            "text": "@ポテト 偏食も大変だ",
            "mention_to": [
                "ポテト"
            ]
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "ポテト",
            "text": "白米しか食べない！みたいな時期があって心配しました・・・",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "ちくわ",
            "text": "作るのも大変そう！",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "こんぶ",
            "text": "そういう時はどうしたんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "ポテト",
            "text": "@ちくわ おかゆ大変じゃなかったです？！",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "こんぶ",
            "text": "とりあえず白米だけあげる？",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "ポテト",
            "text": "@こんぶ そう、白米だけ与えました・・・",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "ちくわ",
            "text": "@ポテト 本当になんでも食べるんですよ！",
            "mention_to": [
                "ポテト"
            ]
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "こんぶ",
            "text": "@ポテト そうなりますよね",
            "mention_to": [
                "ポテト"
            ]
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "ポテト",
            "text": "@ちくわ えらい！",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 103,
            "interlocutor_id": "こんぶ",
            "text": "でも、とりあえずそれ食べてくれるなら、それでって！",
            "mention_to": []
        },
        {
            "utterance_id": 104,
            "interlocutor_id": "ちくわ",
            "text": "妻のご飯が美味しいってことですね！",
            "mention_to": []
        },
        {
            "utterance_id": 105,
            "interlocutor_id": "こんぶ",
            "text": "そう！なんでもたべるよね！",
            "mention_to": []
        },
        {
            "utterance_id": 106,
            "interlocutor_id": "こんぶ",
            "text": "あら、こーいう時だけ",
            "mention_to": []
        },
        {
            "utterance_id": 107,
            "interlocutor_id": "ポテト",
            "text": "@こんぶ 褒めのターンが来ましたよ！",
            "mention_to": [
                "こんぶ"
            ]
        }
