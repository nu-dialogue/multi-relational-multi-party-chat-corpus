    ],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "ばなな",
            "text": "おはようございます",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "たまご",
            "text": "おはようございます",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "つみれ",
            "text": "おはようございます",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "ばなな",
            "text": "昨日ホワイトデーのチョコを買いに行ったのですけれど",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "たまご",
            "text": "はいー",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "たまご",
            "text": "ホワイトデーですね",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "ばなな",
            "text": "店員さんにチョコは野菜室に入れてくださいと言われました",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "つみれ",
            "text": "ホワイトデーの",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "つみれ",
            "text": "え？野菜室に？",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "たまご",
            "text": "そうなんですか",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "ばなな",
            "text": "野菜室に入れるの知らなかったです",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "たまご",
            "text": "何か変わるんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "つみれ",
            "text": "私も初めて聞きました",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "ばなな",
            "text": "チョコも冷やし過ぎると良くないそうです",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "たまご",
            "text": "あんまチョコ買わないから知らなかったです",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "ばなな",
            "text": "なので野菜室の方が良いみたいですね",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "たまご",
            "text": "そうなんですね",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "つみれ",
            "text": "野菜室の温度が適温なんですね",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "たまご",
            "text": "程よいのが良いんだ",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "たまご",
            "text": "チョコあんま食べないですねー",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "つみれ",
            "text": "次チョコ買ったら野菜室に入れます",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "たまご",
            "text": "自分も入れます",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "ばなな",
            "text": "コーヒーの粉は友人から冷凍庫に入れた方が良いと聞いて",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "たまご",
            "text": "えー！",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "たまご",
            "text": "凍らないんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "つみれ",
            "text": "それも知らなかった",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "ばなな",
            "text": "それぞれに合った保存方法があるんだなと思いました。",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "たまご",
            "text": "コーヒーの粉は凍らないのか",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "ばなな",
            "text": "@たまご 凍らないですよ！",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "たまご",
            "text": "それぞれの保存方法を確認するべきですね",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "たまご",
            "text": "すごい！",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "つみれ",
            "text": "長持ちした方がいいですもんね",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "たまご",
            "text": "1番良い方法で",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "つみれ",
            "text": "鮮度が保てるなら",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "ばなな",
            "text": "きっと間違った方法で保存しているのもあるかも",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "たまご",
            "text": "長持ちするし",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "たまご",
            "text": "美味しいままだし",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "ばなな",
            "text": "出来るだけ鮮度を保ちたいですよね",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "たまご",
            "text": "僕は結構あると思います",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "つみれ",
            "text": "@ばなな 私も勘違いしてる保存方法ありそうです",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "たまご",
            "text": "知識が全然ないので",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "たまご",
            "text": "保存方法チェックしときます",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "たまご",
            "text": "なんか間違ってないか",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "つみれ",
            "text": "半世紀近く生きてても知らないことってまだまだあります",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "たまご",
            "text": "チョコ売ってる人はやっぱ知識あるんですね",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "たまご",
            "text": "いっぱいありそう",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "ばなな",
            "text": "なかなか自分では気が付かないですよね。",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "つみれ",
            "text": "チョコの知識は長けてらっしゃるい",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "たまご",
            "text": "そういう豆知識とか雑学を",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "ばなな",
            "text": "チョコのことは今回初めて聞きました",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "たまご",
            "text": "知るの好きです",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "つみれ",
            "text": "@たまご 雑学王目指して！",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "たまご",
            "text": "いろんな雑学ありますよね",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "たまご",
            "text": "目指したい！",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "ばなな",
            "text": "色々知ると楽しいですよね。",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "たまご",
            "text": "面白いです！",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "ばなな",
            "text": "私はゆで卵をするっと剥く方法知りたいです",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "たまご",
            "text": "雑学くだらないのとかも意外と面白いです",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "たまご",
            "text": "ありそう",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "つみれ",
            "text": "@ばなな この前それ話題になって",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "たまご",
            "text": "冷やすとか",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "ばなな",
            "text": "何か良い方法ありますか？",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "たまご",
            "text": "氷水で",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "ばなな",
            "text": "ボロボロになる時が",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "つみれ",
            "text": "茹でる前に小さい穴を開けるといいらしいです",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "たまご",
            "text": "なんか聞いたことある！",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "ばなな",
            "text": "小さい穴はどうやってあけるのでしょう？",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "つみれ",
            "text": "新しいたまごはボロボロになりやすんですよ",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "たまご",
            "text": "ゆでたまご美味しいですよね",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "たまご",
            "text": "たしかに",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "つみれ",
            "text": "針で開けました",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "たまご",
            "text": "そっから中身出ないの？",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "ばなな",
            "text": "@つみれ 新しいのはボロボロになりやすいんですね",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "たまご",
            "text": "ありえないくらい小さい穴か",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "ばなな",
            "text": "なるほど針で",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "たまご",
            "text": "割れちゃいそう",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "つみれ",
            "text": "＠ぼろぼろなるのは新しい証拠です",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "たまご",
            "text": "ゆで卵食べたくなってきた",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "たまご",
            "text": "そうなんだ",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "ばなな",
            "text": "あと卵の茹で加減もなかなか難しいです",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "つみれ",
            "text": "穴開ける場所も決まってるのでネット見てください",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "たまご",
            "text": "セブンのゆで卵美味しいです",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "たまご",
            "text": "見てみよう",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "たまご",
            "text": "作らず買っちゃいます",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "たまご",
            "text": "コンビニとかで",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "ばなな",
            "text": "@たまご セブンのが美味しいんですね",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "つみれ",
            "text": "@ばなな 茹で加減は私も毎回あやふやです",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "たまご",
            "text": "お店ごとにゆで卵の味違くて",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "たまご",
            "text": "美味しいです",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "つみれ",
            "text": "コンビニならセブンの茹で卵が一番好きです",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "たまご",
            "text": "茹で加減むずそう",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "ばなな",
            "text": "とろっとし過ぎず固すぎずがなかなか",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "たまご",
            "text": "セブンうまい",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "たまご",
            "text": "パサパサになると嫌ですね",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "ばなな",
            "text": "@たまご 気になります。今度買ってみます",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "つみれ",
            "text": "@ばなな 難しいですね",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "たまご",
            "text": "セブンのしょっぱくて美味しい",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "つみれ",
            "text": "塩分が強くて美味しい",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "たまご",
            "text": "良い黄身の柔らかさで",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "ばなな",
            "text": "なるほどしょっぱくて美味しいんですね",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "たまご",
            "text": "食いたくなります",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "つみれ",
            "text": "@ばなな 薄味がお好きならローソンですね",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "たまご",
            "text": "ローソンのも美味しい",
            "mention_to": []
        },
        {
            "utterance_id": 103,
            "interlocutor_id": "つみれ",
            "text": "塩分抑え目で",
            "mention_to": []
        },
        {
            "utterance_id": 104,
            "interlocutor_id": "ばなな",
            "text": "薄味好きなのでローソンが良さそうですね",
            "mention_to": []
        },
        {
            "utterance_id": 105,
            "interlocutor_id": "たまご",
            "text": "けど塩味は少なめ",
            "mention_to": []
        }
