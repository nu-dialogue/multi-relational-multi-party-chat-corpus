{
    "dialogue_id": "B10304",
    "dialogue_type": "Family",
    "interlocutors": [
        "うさぎ",
        "こんぶ",
        "ちくわ"
    ],
    "relationship": [
        "こんぶ",
        "ちくわ"
    ],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "うさぎ",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "こんぶ",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "ちくわ",
            "text": "こんにちは！",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "うさぎ",
            "text": "春はどこか旅行とかいきますか？",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "ちくわ",
            "text": "何も決めていません！",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "こんぶ",
            "text": "先週に横浜いきました",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "うさぎ",
            "text": "おー近いです！",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "ちくわ",
            "text": "@うさぎ どこか行かれますか？",
            "mention_to": [
                "うさぎ"
            ]
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "こんぶ",
            "text": "春休みはどうかな",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "うさぎ",
            "text": "はい、北海道に娘の誕生日にいきます",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "こんぶ",
            "text": "旦那の結婚式およばれついでに",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "こんぶ",
            "text": "北海道！",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "ちくわ",
            "text": "北海道はいいですね！",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "こんぶ",
            "text": "ステキですね！ラベンダー畑？",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "うさぎ",
            "text": "@こんぶ \n何十年かぶりです",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "こんぶ",
            "text": "私東北出身ですが",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "うさぎ",
            "text": "いや札幌と小樽です",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "こんぶ",
            "text": "北海道行ったことないです",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "ちくわ",
            "text": "いくら好きからしたら羨ましい！",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "こんぶ",
            "text": "春だとあったかいですかね？",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "こんぶ",
            "text": "海鮮も美味しい",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "うさぎ",
            "text": "@こんぶ \n私も仙台住んでましたが，周りも同じこと言ってました",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "ちくわ",
            "text": "少しは寒いんやない？",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "うさぎ",
            "text": "雪はさすがに少なくなってるみたいです",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "こんぶ",
            "text": "@うさぎ やっぱりなかなかなんか行けないとうい",
            "mention_to": [
                "うさぎ"
            ]
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "ちくわ",
            "text": "えー仙台にも住んでいたんですね！",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "うさぎ",
            "text": "はい、転勤族なので",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "こんぶ",
            "text": "転勤族なんですね",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "うさぎ",
            "text": "高松、広島、大阪、仙台です",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "ちくわ",
            "text": "転勤族も大変な部分と",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "ちくわ",
            "text": "新たな出会いも！",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "ちくわ",
            "text": "って感じですかね？",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "こんぶ",
            "text": "広島！修学旅行で行きました！",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "ちくわ",
            "text": "本当に色々行かれてるんですね？",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "こんぶ",
            "text": "四国は行ったことないです！",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "うさぎ",
            "text": "@ちくわ \nはい、子供の幼稚園のママ友いまだに繋がってます",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "ちくわ",
            "text": "お子さんも色々と大変だったんですかね？",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "うさぎ",
            "text": "@こんぶ \nおうどん食べにぜひ！",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "ちくわ",
            "text": "学校関係とか！",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "こんぶ",
            "text": "@うさぎ 旦那もうどん好きなんで！",
            "mention_to": [
                "うさぎ"
            ]
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "うさぎ",
            "text": "@ちくわ \n幼稚園だけだったのでそこまで？",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "ちくわ",
            "text": "@うさぎ うどん食べに香川いきましたよ！",
            "mention_to": [
                "うさぎ"
            ]
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "こんぶ",
            "text": "ぜひ行きたい！",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "ちくわ",
            "text": "@うさぎ そうだったんですね！",
            "mention_to": [
                "うさぎ"
            ]
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "こんぶ",
            "text": "そっかそっか",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "うさぎ",
            "text": "@ちくわ \nお蕎麦屋あまりないですよね",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "うさぎ",
            "text": "うどん屋だらけ",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "こんぶ",
            "text": "子どもは意外とすぐに馴染めたりしますからね",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "こんぶ",
            "text": "へーー",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "ちくわ",
            "text": "色んなところにうどん屋がありました！",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "こんぶ",
            "text": "そうなんですね！！",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "うさぎ",
            "text": "@こんぶ \n本人覚えてませんが広島弁になってました",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "ちくわ",
            "text": "安いし！美味しい！",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "うさぎ",
            "text": "「いっけーん」とか",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "ちくわ",
            "text": "ええー広島弁！",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "こんぶ",
            "text": "やっぱり地方の方言はうつるのか",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "ちくわ",
            "text": "可愛い!!",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "うさぎ",
            "text": "はい「ダメ」って意味でいっけーん",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "ちくわ",
            "text": "覚えときます！",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "うさぎ",
            "text": "先生方、バリバリの広島弁で",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "こんぶ",
            "text": "広島では、そういう意味なんでさね",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "ちくわ",
            "text": "いっけーん！って！",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "うさぎ",
            "text": "独特のイントネーションでした",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "ちくわ",
            "text": "大阪でいう",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "ちくわ",
            "text": "あかーーーん！",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "こんぶ",
            "text": "たしかに",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "ちくわ",
            "text": "ですね！",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "うさぎ",
            "text": "それそれ！！",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "うさぎ",
            "text": "あっかーん",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "こんぶ",
            "text": "なかなか聞き取りづらいところもありそう",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "うさぎ",
            "text": "怒ってるみたいです",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "うさぎ",
            "text": "話し方が",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "こんぶ",
            "text": "そういうことか！",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "ちくわ",
            "text": "広島のお好み焼きと",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "ちくわ",
            "text": "大阪のお好み焼きは",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "ちくわ",
            "text": "違いますよね！",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "うさぎ",
            "text": "違いますよね！",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "こんぶ",
            "text": "大阪の人絶対それいうよね！",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "うさぎ",
            "text": "両方すきです",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "うさぎ",
            "text": "広島焼き？",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "ちくわ",
            "text": "@こんぶ 当たり前やん！",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "ちくわ",
            "text": "大阪のやつがお好み焼きと言います！",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "うさぎ",
            "text": "@ちくわ \nわかります！",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "こんぶ",
            "text": "そうだよね",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "ちくわ",
            "text": "って胸張っていいます！",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "うさぎ",
            "text": "両方好きです",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "こんぶ",
            "text": "広島焼きは",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "こんぶ",
            "text": "おそばいり？",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "ちくわ",
            "text": "僕も食べたことありますが、",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "こんぶ",
            "text": "たしかに、どっちも美味しい",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "ちくわ",
            "text": "あれはあれで美味しかったです！",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "うさぎ",
            "text": "高槻に住んでましたが「よしもと」ってお好み焼き屋好きでした",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "ちくわ",
            "text": "あ！",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "うさぎ",
            "text": "地元ネタ",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "ちくわ",
            "text": "聞いたことがある！",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "うさぎ",
            "text": "ほんとですか？",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "こんぶ",
            "text": "あら！",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "ちくわ",
            "text": "はい！",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "うさぎ",
            "text": "しょっちゅう行ってましたー",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "ちくわ",
            "text": "行ったことはないですけど！",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "こんぶ",
            "text": "有名なんですかね？",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "うさぎ",
            "text": "わからないですが美味しかったです",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "ちくわ",
            "text": "地元の人はよく行くとか聞きましたよ",
            "mention_to": []
        },
        {
            "utterance_id": 103,
            "interlocutor_id": "うさぎ",
            "text": "あとマネケン！ワッフル",
            "mention_to": []
        },
        {
            "utterance_id": 104,
            "interlocutor_id": "こんぶ",
            "text": "地元の人がいうなら絶対ぢ",
            "mention_to": []
        },
        {
            "utterance_id": 105,
            "interlocutor_id": "こんぶ",
            "text": "絶対だ！",
            "mention_to": []
        },
        {
            "utterance_id": 106,
            "interlocutor_id": "ちくわ",
            "text": "マネケンはあったらすぐ買います！",
            "mention_to": []
        },
        {
            "utterance_id": 107,
            "interlocutor_id": "うさぎ",
            "text": "ミックスジュース",
            "mention_to": []
        },
        {
            "utterance_id": 108,
            "interlocutor_id": "こんぶ",
            "text": "それ食べました！",
            "mention_to": []
        }
    ],
