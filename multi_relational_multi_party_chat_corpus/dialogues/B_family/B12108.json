    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "ばなな",
            "text": "こんばんは",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "つみれ",
            "text": "こんばんは",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "たまご",
            "text": "こんばんは",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "ばなな",
            "text": "@つみれ どちらの出身ですか",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "つみれ",
            "text": "長野県です",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "ばなな",
            "text": "長野なのですね",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "つみれ",
            "text": "長野県から出たことがないんです",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "たまご",
            "text": "自然豊かです",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "ばなな",
            "text": "何度か行きました",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "つみれ",
            "text": "遊びにですか？",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "ばなな",
            "text": "登山とか",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "つみれ",
            "text": "登山！",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "ばなな",
            "text": "遊びもです",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "つみれ",
            "text": "山多いですからね",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "たまご",
            "text": "スキーとか！",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "ばなな",
            "text": "登山は父の趣味で",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "たまご",
            "text": "山登り放題です",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "ばなな",
            "text": "白馬とか行きました",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "たまご",
            "text": "そうなんですね",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "つみれ",
            "text": "@ばなな お父さんと一緒に登るんですか？",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "ばなな",
            "text": "昔です。",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "つみれ",
            "text": "白馬、夏も冬も遊べなすよね",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "ばなな",
            "text": "最近はないですけれど",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "つみれ",
            "text": "そうなんですね",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "たまご",
            "text": "山登り達成感がありますよね",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "ばなな",
            "text": "白馬は夏も冬も良さそうです",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "つみれ",
            "text": "学校の",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "つみれ",
            "text": "行事で登山があります",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "ばなな",
            "text": "達成感ありますよ",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "たまご",
            "text": "行事で行きました",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "たまご",
            "text": "西駒ヶ岳",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "ばなな",
            "text": "行事で行くのですね",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "たまご",
            "text": "けど延期が続いてほぼロープウェイで行きました",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "つみれ",
            "text": "中学2年生の行事です",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "ばなな",
            "text": "そうでしたか",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "たまご",
            "text": "しかも登山前に山の遭難の映画",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "ばなな",
            "text": "スキーは得意ですか",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "たまご",
            "text": "見せられました",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "たまご",
            "text": "@ばなな スキーは普通くらいです",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "ばなな",
            "text": "遭難の映画は嫌ですね",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "つみれ",
            "text": "スキーは苦手です",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "ばなな",
            "text": "私も駄目です",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "つみれ",
            "text": "若い頃はボードをやりました",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "ばなな",
            "text": "高い所が怖くて",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "たまご",
            "text": "ボードやりたい",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "たまご",
            "text": "そうなんですね",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "ばなな",
            "text": "@つみれ ボード楽しいですか",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "つみれ",
            "text": "この前久しぶりにやったら滑れませんでした",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "つみれ",
            "text": "驚くほど",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "ばなな",
            "text": "スキーとまた違いますよね",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "つみれ",
            "text": "全然違いますね",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "たまご",
            "text": "スキーの方が簡単です",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "ばなな",
            "text": "バランス取るのが難しそう",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "つみれ",
            "text": "@たまご スキーできたっけ？",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "たまご",
            "text": "授業で初めてやって",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "たまご",
            "text": "滑れるようになった",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "ばなな",
            "text": "お二人とも寒いのは大丈夫ですか？",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "たまご",
            "text": "大丈夫なほうです",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "つみれ",
            "text": "だんだん苦手になってきました",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "ばなな",
            "text": "寒いのと暑いのどちらが良いですかね",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "つみれ",
            "text": "寒いのも暑いのも体にこたえます",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "たまご",
            "text": "寒い方が着ればあったまれるので",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "ばなな",
            "text": "寒い方が私は辛いかも",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "つみれ",
            "text": "でも寒いほうがいいかな",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "たまご",
            "text": "寒い方が良いです",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "ばなな",
            "text": "そうなのですね",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "たまご",
            "text": "暑いとき耐えるしかできないので",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "ばなな",
            "text": "昨年の夏は暑かったですね",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "つみれ",
            "text": "@ばなな 出身とちらですか？その違いかな？",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "たまご",
            "text": "すごい暑かったです",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "ばなな",
            "text": "私は東京です",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "たまご",
            "text": "東京なんですね！",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "つみれ",
            "text": "そうなんですね",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "ばなな",
            "text": "はい",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "たまご",
            "text": "自分も月末から東京に引っ越します",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "つみれ",
            "text": "寒さは苦手かもですね",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "ばなな",
            "text": "そうなのですね",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "つみれ",
            "text": "東京の人になるそうです",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "ばなな",
            "text": "@たまご 引っ越し、たいへんですね",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "たまご",
            "text": "まだ都会に慣れてませんが",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "たまご",
            "text": "引っ越し大変です！",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "たまご",
            "text": "準備が",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "ばなな",
            "text": "初めての一人暮らしですか？",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "たまご",
            "text": "今も神奈川に一人暮らししてますー",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "ばなな",
            "text": "そうなのですね",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "たまご",
            "text": "学校卒業するので",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "たまご",
            "text": "引っ越す感じです",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "ばなな",
            "text": "神奈川ならお隣ですね",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "ばなな",
            "text": "なるほど",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "つみれ",
            "text": "引っ越しは親も大変です。",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "たまご",
            "text": "そうなんです",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "つみれ",
            "text": "たまご",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "ばなな",
            "text": "社会人になるのですね",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "たまご",
            "text": "はいー",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "ばなな",
            "text": "@つみれ お母さんもたいへんですね",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "ばなな",
            "text": "@たまご 頑張って下さいね",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "つみれ",
            "text": "たまごの妹も引っ越しなので大変です",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "たまご",
            "text": "ありがとうございます",
