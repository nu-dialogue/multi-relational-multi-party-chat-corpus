        {
            "utterance_id": 101,
            "interlocutor_id": "こんぶ",
            "text": "@ポテト それでも、丸くなるもんですよね",
            "mention_to": [
                "ポテト"
            ]
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "ちくわ",
            "text": "なら美味しいたこ焼きですね！",
