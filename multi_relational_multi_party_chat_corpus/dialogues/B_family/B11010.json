        {
            "utterance_id": 1,
            "interlocutor_id": "ちくわ",
            "text": "こんにちは！",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "こんぶ",
            "text": "こんにちは〜",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "ちくわ",
            "text": "たこ焼きたべたいです！",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "ちくわ",
            "text": "って思ったことあります？",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "ポテト",
            "text": "たこ焼き！",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "こんぶ",
            "text": "たこ焼き美味しいよね",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "ポテト",
            "text": "@ちくわ こないだやりました！",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "ちくわ",
            "text": "お！！",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "ポテト",
            "text": "タコパ！",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "こんぶ",
            "text": "旦那はたこ焼き作るの上手です",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "ちくわ",
            "text": "僕大阪出身なので",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "こんぶ",
            "text": "なんてったって",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "ちくわ",
            "text": "たこ焼き作れますよ！",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "こんぶ",
            "text": "大阪出身なんでね！！！",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "ポテト",
            "text": "@ちくわ 上手そう！",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "ちくわ",
            "text": "本場の！",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "こんぶ",
            "text": "そう！本場の！",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "こんぶ",
            "text": "フワトロで美味しいですよ",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "ポテト",
            "text": "@ちくわ あれ、何を入れるのが正解なんですか？？？タコ以外！",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "こんぶ",
            "text": "あ、あとは、お好み焼きも！",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "ポテト",
            "text": "@こんぶ お好み焼き！",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "ちくわ",
            "text": "粉と水と卵とだしです！",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "ポテト",
            "text": "絶対に美味しいやつ！",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "ちくわ",
            "text": "天かすと青ネギです",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "こんぶ",
            "text": "@ポテト 私も知りませんでしたが、ネギみたいです！",
            "mention_to": [
                "ポテト"
            ]
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "ちくわ",
            "text": "キャベツは入れてはだめですよ！",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "ポテト",
            "text": "え！！！！！！！",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "こんぶ",
            "text": "私はキャベツの、みじん切りいれてました",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "ポテト",
            "text": "本当に？？？",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "こんぶ",
            "text": "@ポテト やっぱりキャベツを？",
            "mention_to": [
                "ポテト"
            ]
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "ちくわ",
            "text": "多分みなさんたこ焼きもキャベツのみじん切り",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "ポテト",
            "text": "@こんぶ ガッツリ入れてました！",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "ちくわ",
            "text": "入れてる人おおいんすよ",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "ちくわ",
            "text": "それは",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "こんぶ",
            "text": "やっぱりそうですよね！！！",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "ちくわ",
            "text": "大阪人からしたら",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "ちくわ",
            "text": "だめですよーーーー",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "ちくわ",
            "text": "ただの丸いお好み焼きです！",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "ポテト",
            "text": "えーーーーーーーーーーーー！",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "こんぶ",
            "text": "なんでか、キャベツだと思ってた",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "ポテト",
            "text": "衝撃の真実・・・・",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "ちくわ",
            "text": "これからは入れないでください！",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "こんぶ",
            "text": "初知りですか？",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "ポテト",
            "text": "@こんぶ レシピも多いですよ！",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "ポテト",
            "text": "初しりです！",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "ポテト",
            "text": "@ちくわ 気をつけます！",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "ポテト",
            "text": "え、今日一びっくり・・・",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "こんぶ",
            "text": "明太子とチーズと",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "ちくわ",
            "text": "びっくりですか？",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "ちくわ",
            "text": "僕は普通だと思いました！",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "こんぶ",
            "text": "お餅もいれるっけ？",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "ポテト",
            "text": "@ちくわ いや、相当びっくりです！",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "ちくわ",
            "text": "お餅も美味しいすよ！",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "ポテト",
            "text": "お餅！",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "こんぶ",
            "text": "ウインナーとか？",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "ちくわ",
            "text": "明太子とチーズもうまいですよ！",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "こんぶ",
            "text": "色々アレンジもできるから美味しいですよね",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "こんぶ",
            "text": "楽しいし！！！",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "ちくわ",
            "text": "@ポテト 家族のみなさんにもお伝えください！",
            "mention_to": [
                "ポテト"
            ]
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "ポテト",
            "text": "@ちくわ 明太子とチーズは間違いなさそう！",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "ポテト",
            "text": "@ちくわ 伝えておきます！",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "こんぶ",
            "text": "@ポテト ぜひ今度は、タコパを",
            "mention_to": [
                "ポテト"
            ]
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "ちくわ",
            "text": "今度、作ってみてくださいね！",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "ポテト",
            "text": "え～衝撃的～",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "こんぶ",
            "text": "お家にたこ焼き機あるんでしたっけ？",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "ポテト",
            "text": "@こんぶ ホットプレートのついてきたやつにあります！",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "ちくわ",
            "text": "いいですね！",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "こんぶ",
            "text": "あー！よくありますよね！",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "ちくわ",
            "text": "ホットプレートのやつ！",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "こんぶ",
            "text": "んじゃ、できますね！！！",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "ちくわ",
            "text": "火力もつよいですし",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "ポテト",
            "text": "でも端っこだけなかなか火が通らない・・・！",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "こんぶ",
            "text": "あーー！それは、わかります",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "ちくわ",
            "text": "端っこはわかります！",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "ポテト",
            "text": "@こんぶ ですよね？？？",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "ちくわ",
            "text": "だから少し形ができたら、",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "こんぶ",
            "text": "うちらのやつも真ん中火力強いです",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "ポテト",
            "text": "@こんぶ そうそう！",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "ちくわ",
            "text": "真ん中のやつと交換すれば",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "ちくわ",
            "text": "いいですよ！",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "ポテト",
            "text": "移動させます！",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "こんぶ",
            "text": "どんどん真ん中に集まっていく",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "ちくわ",
            "text": "移動コロコロですよね！",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "ポテト",
            "text": "@こんぶ そうそう！",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "こんぶ",
            "text": "@ちくわ 移動コロコロ",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "ポテト",
            "text": "移動コロコロ笑",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "ちくわ",
            "text": "液体はホットプレートのギリギリまで流すといいですよ！",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "こんぶ",
            "text": "あとは、たこ焼きの液がすごい",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "こんぶ",
            "text": "ビシャビシャです！",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "ちくわ",
            "text": "そうそう！",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "こんぶ",
            "text": "トロントロンじゃなくて",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "こんぶ",
            "text": "それにも、私はびっくりでした",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "ちくわ",
            "text": "しゃーしゃーな感じです！",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "ポテト",
            "text": "うちもだぱーってやるのが正解と聞いてやってます！",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "ちくわ",
            "text": "目分量ですけどね！",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "ちくわ",
            "text": "いつも！",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "こんぶ",
            "text": "それで、できるの？てくいで",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "ポテト",
            "text": "@こんぶ そう、固まるんか？ってくらいビシャビシャ",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "こんぶ",
            "text": "お！！んじゃ、そこは聞いたことあるんですね！",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "ポテト",
            "text": "そこはクリアしてましたね・・・！",
            "mention_to": []
