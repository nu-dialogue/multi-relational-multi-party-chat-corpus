    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "ばなな",
            "text": "おはようございます。",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "にんじん",
            "text": "おはようございます",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "たまねぎ",
            "text": "おはようございます",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "ばなな",
            "text": "バレンタインはしましたか？",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "たまねぎ",
            "text": "そうでしたね",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "にんじん",
            "text": "今年はできませんでした",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "たまねぎ",
            "text": "バレンタインもらえませんでした",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "ばなな",
            "text": "家は1日遅れで渡しました",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "たまねぎ",
            "text": "つくるつくる詐欺です笑",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "にんじん",
            "text": "@ばなな 素敵ですね",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "ばなな",
            "text": "@たまねぎ 残念でしたね",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "ばなな",
            "text": "買ってきただけですけれど",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "にんじん",
            "text": "私が欲しいです笑",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "たまねぎ",
            "text": "@ばなな それでもいいじゃないですか！",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "たまねぎ",
            "text": "僕にもください笑",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "ばなな",
            "text": "バレンタインはご褒美チョコ買うのが楽しみです",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "にんじん",
            "text": "自分が食べたいやつ選んじゃいますよね",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "ばなな",
            "text": "@にんじん そうなんですよ",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "たまねぎ",
            "text": "生チョコ欲しかった笑",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "ばなな",
            "text": "渡したのに私にも頂戴って",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "にんじん",
            "text": "この前初めてGODIVAのチョコ食べました",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "ばなな",
            "text": "どうでしたか？",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "たまねぎ",
            "text": "あんまし美味しくなかったです",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "にんじん",
            "text": "え！おいしかった！",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "たまねぎ",
            "text": "ハードル上げすぎました",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "ばなな",
            "text": "@たまねぎ 好みではなかったのでしょうの",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "にんじん",
            "text": "@たまねぎ はずれをたべたんじゃ",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "ばなな",
            "text": "苦いのが 駄目とか",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "たまねぎ",
            "text": "@ばなな 甘い物少し苦手で笑",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "たまねぎ",
            "text": "苦い方が得意です",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "にんじん",
            "text": "そもそもチョコが駄目笑",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "ばなな",
            "text": "甘い物が駄目なのですね",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "ばなな",
            "text": "なるほどチョコが",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "にんじん",
            "text": "お酒入ったやつは食べれません",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "たまねぎ",
            "text": "たまーに食べるくらいはいいんですが！",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "たまねぎ",
            "text": "ワインのやつ？",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "にんじん",
            "text": "日本酒とか入ってるやつ",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "たまねぎ",
            "text": "なるへそーです！",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "ばなな",
            "text": "大人ですね",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "ばなな",
            "text": "ちょっと酔いそう",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "にんじん",
            "text": "あれ食べると飲酒になるんですかね？",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "たまねぎ",
            "text": "なるに決まってます",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "にんじん",
            "text": "@たまねぎ そーなの？！",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "たまねぎ",
            "text": "当たり前ですよ笑",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "ばなな",
            "text": "@たまねぎ お酒お好きですか",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "ばなな",
            "text": "もしかしたらと思って",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "たまねぎ",
            "text": "お酒は普段は全く飲みません！",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "ばなな",
            "text": "そうなんですね",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "たまねぎ",
            "text": "でも好きですよ！",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "にんじん",
            "text": "家で晩酌しないですねー",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "にんじん",
            "text": "@ばなな 飲まれますか？？",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "ばなな",
            "text": "@にんじん 弱くなりました",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "にんじん",
            "text": "わかります！",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "ばなな",
            "text": "飲む機会も減って",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "たまねぎ",
            "text": "それは分かりますね！",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "にんじん",
            "text": "コロナもありましたしね！",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "たまねぎ",
            "text": "大人になるに連れて友達との集まりも減りますしね！",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "ばなな",
            "text": "それもありますね",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "にんじん",
            "text": "@ばなな 旦那さんはのみますか？",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "ばなな",
            "text": "@にんじん さっぱり飲めなくて",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "ばなな",
            "text": "食べる専門です",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "たまねぎ",
            "text": "うちは逆です",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "ばなな",
            "text": "@たまねぎ そうなのですね",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "ばなな",
            "text": "少し飲めたらと思いますけれど",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "たまねぎ",
            "text": "俺は食べることのが好きですね",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "たまねぎ",
            "text": "誰か飲む人がいれば全然飲みます！",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "にんじん",
            "text": "お酒飲まないのは安心ですけどね！",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "ばなな",
            "text": "@たまねぎ 食べ物は何が好きですか？",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "たまねぎ",
            "text": "まぁたしかに！",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "たまねぎ",
            "text": "お肉ですね！",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "ばなな",
            "text": "よく行くお店とかありますか？",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "たまねぎ",
            "text": "とくに！グルメではないですよ！笑",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "ばなな",
            "text": "家はファミレスか回転寿司",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "にんじん",
            "text": "最近ココスにはまってます",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "たまねぎ",
            "text": "うちもそんな感じですよ！",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "にんじん",
            "text": "ビーフシチューがおいしくて",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "たまねぎ",
            "text": "困ってるくらいです！",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "にんじん",
            "text": "毎日でも飽きないー",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "ばなな",
            "text": "ビーフシチュー美味しそう",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "にんじん",
            "text": "2月までのメニューだそうです",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "ばなな",
            "text": "ココスたまに行きます。",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "ばなな",
            "text": "2月、あと少しですね",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "にんじん",
            "text": "行き過ぎてプラチナになりました",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "たまねぎ",
            "text": "ココス美味しいですよね！",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "たまねぎ",
            "text": "気持ち悪いです！笑",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "ばなな",
            "text": "それはすごいですね",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "にんじん",
            "text": "プラチナになると10%びきになります笑",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "ばなな",
            "text": "家はジョナサンが多くて",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "ばなな",
            "text": "歩いて行けるので",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "にんじん",
            "text": "近くにないですー",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "たまねぎ",
            "text": "何が上手いですか？",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "ばなな",
            "text": "名前が忘れてしまって",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "たまねぎ",
            "text": "参考にさしてもらおうかと！笑",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "ばなな",
            "text": "タンドリーチキンだったかな",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "たまねぎ",
            "text": "なるほど！",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "ばなな",
            "text": "息子がよく食べてるんです",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "たまねぎ",
            "text": "覚えておきます！",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "にんじん",
            "text": "行ったら食べてみよう！",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "ばなな",
            "text": "カレー味の",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "にんじん",
            "text": "カレー味まちがいないですよね",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "たまねぎ",
            "text": "カレー味か！",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "ばなな",
            "text": "名前違ってたら、すみません",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "ばなな",
            "text": "スパイシーで美味しいです",
            "mention_to": []
        },
        {
            "utterance_id": 103,
            "interlocutor_id": "にんじん",
            "text": "まずジョナサン探さないと！",
            "mention_to": []
        },
        {
            "utterance_id": 104,
            "interlocutor_id": "にんじん",
            "text": "うちは近くてココスとはま寿司です！",
            "mention_to": []
        },
        {
            "utterance_id": 105,
            "interlocutor_id": "ばなな",
            "text": "なるほど",
            "mention_to": []
        },
        {
            "utterance_id": 106,
            "interlocutor_id": "ばなな",
            "text": "うちはかっぱ寿司です",
            "mention_to": []
        }
