        },
        {
            "utterance_id": 40,
            "interlocutor_id": "ポテト",
            "text": "@にんじん たしかに・・・",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "にんじん",
            "text": "もっと早くそうすれば良かった",
