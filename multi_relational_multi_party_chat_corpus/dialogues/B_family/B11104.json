        },
        {
            "utterance_id": 12,
            "interlocutor_id": "ポテト",
            "text": "筋トレも！",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "にんじん",
            "text": "@ポテト 置き換えはしません！",
            "mention_to": [
                "ポテト"
            ]
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "にんじん",
            "text": "ご飯の量減らしてます！",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "ポテト",
            "text": "@にんじん ご飯の量を減らす！",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "にんじん",
            "text": "お母さんって子供の残したもの食べるじゃないですかー",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "ポテト",
            "text": "あ～～～～～～～～",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "にんじん",
            "text": "あれ、ダメだと気づきまして",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "たまねぎ",
            "text": "確かに",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "ポテト",
            "text": "@にんじん ですね・・・",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "にんじん",
            "text": "で、それを旦那に",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "ポテト",
            "text": "ん？",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "たまねぎ",
            "text": "僕が食べます！",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "ポテト",
            "text": "話の流れが",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "ポテト",
            "text": "怪しい・・・",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "たまねぎ",
            "text": "え？？笑",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "ポテト",
            "text": "@たまねぎ 食べてる人が変わってる・・・？！",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "ポテト",
            "text": "気付いて・・・！",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "たまねぎ",
            "text": "最近ダイソンって言われます！",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "ポテト",
            "text": "吸引力・・・",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "たまねぎ",
            "text": "変わらない笑",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "ポテト",
            "text": "食事の吸引力・・・",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "にんじん",
            "text": "すでに4キロ痩せました！",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "にんじん",
            "text": "どんだけ食ってた？！みたいな笑",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "ポテト",
            "text": "@にんじん それ・・・たまねぎさんが４キロ太ってません・・・？",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "にんじん",
            "text": "それが奴は太らないんですよ！",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "たまねぎ",
            "text": "変わりません笑",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "ポテト",
            "text": "ひょ・・・！",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "にんじん",
            "text": "悔しい",
