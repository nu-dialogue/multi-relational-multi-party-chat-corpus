        },
        {
            "utterance_id": 53,
            "interlocutor_id": "くじら",
            "text": "たまにですよ",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "くじら",
            "text": "卵たくさん使うので",
