        },
        {
            "utterance_id": 46,
            "interlocutor_id": "にんじん",
            "text": "笑",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "にんじん",
            "text": "@くじら 作りませんか？",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "くじら",
            "text": "チョコはあまりないですね",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "にんじん",
            "text": "私でも料理嫌いなんですよー",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "くじら",
            "text": "シフォンケーキとかなら",
