    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "ポテト",
            "text": "こんにちは！",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "てばさき",
            "text": "こんにちは！",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "えのき",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "えのき",
            "text": "もうすぐバレンタインですね",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "ポテト",
            "text": "ですね！",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "えのき",
            "text": "チョコレートは買いましたか？",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "ポテト",
            "text": "てばさきさんとえのきさんは何か考えてますか",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "ポテト",
            "text": "まだ買ってないです～",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "えのき",
            "text": "私は池袋西武で買ってきてしまいました！",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "てばさき",
            "text": "父には叩きつける予定です",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "えのき",
            "text": "ちょうど催し物をやっていたので",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "ポテト",
            "text": "娘がキットを買ってましたね！",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "てばさき",
            "text": "3倍返しを請求します",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "えのき",
            "text": "お父さんかわいそう・・・",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "ポテト",
            "text": "三倍返し笑",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "てばさき",
            "text": "キット…その手がありましたね",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "ポテト",
            "text": "池袋、何か催事でですか？",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "えのき",
            "text": "チョコレートフェスティバルに行ったら、ちょうどピエールマルコリーニさんが来てました！",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "えのき",
            "text": "サイン会をやっていたらしいです",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "ポテト",
            "text": "わぁ！",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "ポテト",
            "text": "凄い！",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "てばさき",
            "text": "マルコリーニさん？",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "えのき",
            "text": "日本に二週間も滞在して全国でやってたらしいですよ",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "ポテト",
            "text": "有名な人ですね！？",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "えのき",
            "text": "有名なショコラティエの人です",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "ポテト",
            "text": "お店いった事あります！",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "えのき",
            "text": "お高そうですよね！",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "てばさき",
            "text": "そういえばこの間バレンタインはチョコのコミケだという話を聞きました",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "えのき",
            "text": "催事場、すごかったです！",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "ポテト",
            "text": "凄そうですね～！",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "えのき",
            "text": "各店舗がしのぎを削ってましたね",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "ポテト",
            "text": "えのきさんは、ご自身のためのチョコですか？",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "てばさき",
            "text": "日本企業の商業戦略がここまでになるとは…",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "えのき",
            "text": "職場用と自宅用です",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "てばさき",
            "text": "おこぼれに預かりました",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "ポテト",
            "text": "職場用、凄い金額になりませんか？？",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "えのき",
            "text": "100個入りのお手頃なのを買ってきました",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "ポテト",
            "text": "てばさきさん、いいなぁ・・・",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "ポテト",
            "text": "１００個入り！！",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "てばさき",
            "text": "美味しかったですよ！",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "ポテト",
            "text": "数の規模が違う！",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "えのき",
            "text": "ベルギー王室御用達のらしいです",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "ポテト",
            "text": "絶対に美味しいやつですよね",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "ポテト",
            "text": "王室御用達？！",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "えのき",
            "text": "イートインでも食べてきちゃいました！",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "ポテト",
            "text": "うわぁ・・・食べたい・・・！",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "えのき",
            "text": "池袋西武本店限定。と書かれてあるチョコスイーツを！",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "てばさき",
            "text": "去年母が買ってきた猫？のチョコも美味しかった",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "ポテト",
            "text": "あぁ・・・絶対に並ぶやつ・・・",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "ポテト",
            "text": "猫・・・知ってるかも・・・",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "えのき",
            "text": "だんなと一緒に食べたので、旦那のぶんはもうこれでいいかなと・",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "ポテト",
            "text": "ベロの形のやつのとこかな？",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "えのき",
            "text": "猫のは、どこのだっけ？",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "てばさき",
            "text": "カカオキャットだった気がする",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "えのき",
            "text": "ああ、北海道のチョコレートなんだよね",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "てばさき",
            "text": "コンビニでも見かけます",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "ポテト",
            "text": "カカオキャット！初めて聞きました！",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "えのき",
            "text": "30種類もフレーバーがあって",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "ポテト",
            "text": "え、凄い",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "えのき",
            "text": "こちらもお徳用をアウトレットで買ったんでした",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "てばさき",
            "text": "アウトレットはいい文化です",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "ポテト",
            "text": "チョコにアウトレット？！",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "ポテト",
            "text": "賞味期限とか？？？",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "えのき",
            "text": "パッケージが簡素な代わりに安いみたいな・・・",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "ポテト",
            "text": "なるほど！",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "てばさき",
            "text": "2月14日をすぎると需要が一気に減るから…",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "えのき",
            "text": "バレンタインのチョコ、パッケージも豪華ですしね",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "ポテト",
            "text": "たしかにー！",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "えのき",
            "text": "ポテトさんは、チョコどうしますか？",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "ポテト",
            "text": "ガトーショコラあたりでも・・・",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "ポテト",
            "text": "作りたい・・・希望・・・",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "えのき",
            "text": "手作りとかされるんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "えのき",
            "text": "いいですね！",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "てばさき",
            "text": "ガトーショコラ美味しいですよね",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "えのき",
            "text": "バレンタインに間に合いそうですか？",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "ポテト",
            "text": "今日１３日！",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "えのき",
            "text": "ですね～",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "ポテト",
            "text": "今気が付きました！",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "ポテト",
            "text": "ヤバいですね！",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "えのき",
            "text": "大変！",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "ポテト",
            "text": "何も準備してないです！笑",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "てばさき",
            "text": "私は既製品で済ませようかと思います",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "えのき",
            "text": "知らない間に月日が流れてるんですよね…",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "ポテト",
            "text": "てばさきさんはご自身の買いました？",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "ポテト",
            "text": "月日早すぎますね・・・",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "てばさき",
            "text": "その辺のスーパーでアポロやチロルでも買えば多分いけます",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "てばさき",
            "text": "父なので",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "ポテト",
            "text": "お父さん・・・",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "えのき",
            "text": "自分用には例年ロイズを買っちゃいます",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "えのき",
            "text": "ロイズのチョコレート美味しいです",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "ポテト",
            "text": "ロイズも美味しい！",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "てばさき",
            "text": "ロイズは美味しい",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "えのき",
            "text": "満場一致",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "てばさき",
            "text": "あの並々模様の普通のチョコレートが好きです",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "えのき",
            "text": "近所で買えないので通販なんですが…",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "ポテト",
            "text": "千歳？の空港にパン屋さんもありますね",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "てばさき",
            "text": "ぱりぱりしてます",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "えのき",
            "text": "ロイズのパン屋さんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "ポテト",
            "text": "食べたい・・・",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "ポテト",
            "text": "ですです",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "ポテト",
            "text": "パン屋さん！",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "えのき",
            "text": "千歳空港素晴らしいです…",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "えのき",
            "text": "北海道はなんでも美味しいですよね",
            "mention_to": []
        }
