        {
            "utterance_id": 1,
            "interlocutor_id": "つみれ",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "たまご",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "りんご",
            "text": "お出かけしたら絶対寄りたいとことかありますか？",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "りんご",
            "text": "ご飯屋さんとか",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "たまご",
            "text": "ご飯屋は行きたいですね",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "つみれ",
            "text": "スタバによりがちです",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "つみれ",
            "text": "ドライブスルーですけど",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "たまご",
            "text": "ラーメン屋ばっか行きます",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "りんご",
            "text": "@つみれ スタバ寄りたいですね！",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "たまご",
            "text": "スタバ美味しいー",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "りんご",
            "text": "@たまご この時期ラーメンいいですね",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "たまご",
            "text": "最高です",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "たまご",
            "text": "ラーメン",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "りんご",
            "text": "@つみれ ラーメン好きですか？",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "つみれ",
            "text": "@りんご ついよっちゃいますね",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "つみれ",
            "text": "ラーメンいいですね",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "つみれ",
            "text": "大好きです",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "たまご",
            "text": "スタバなんか引き込まれる",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "りんご",
            "text": "スタバオシャレだしよります",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "りんご",
            "text": "うちの近くもドライブスルーありますよ",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "たまご",
            "text": "限定飲みたくて行きます",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "りんご",
            "text": "でもやったことないけど笑",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "たまご",
            "text": "そうなんですねわ",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "りんご",
            "text": "@たまご 今は何が限定品？",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "りんご",
            "text": "桜とか？",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "たまご",
            "text": "桜抹茶",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "たまご",
            "text": "だった気がします",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "たまご",
            "text": "まだ飲んでないですけど",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "りんご",
            "text": "@たまご 美味しそう！桜のやつ美味しいですよね",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "つみれ",
            "text": "車だとドライブスーのが楽で！",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "つみれ",
            "text": "桜抹茶？",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "たまご",
            "text": "桜系美味しい",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "りんご",
            "text": "@つみれ さむくないし",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "りんご",
            "text": "ドライブスルーいいですね！",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "りんご",
            "text": "@たまご なんだろ、あの桜系の感じ、美味しいですよね",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "つみれ",
            "text": "@りんご はい！全部ドライブスルーになればいいのにと思います",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "たまご",
            "text": "ドライブスルー楽しい",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "たまご",
            "text": "@りんご はい！なんか美味しいです",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "りんご",
            "text": "確かに！買い物とかもね笑",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "つみれ",
            "text": "@りんご 楽ですよ！スーパーとかも。",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "たまご",
            "text": "降りないと楽ですね",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "りんご",
            "text": "@つみれ 降りる寒いから面倒なときあります",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "たまご",
            "text": "大渋滞なりそうだけど",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "りんご",
            "text": "@たまご だね笑",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "つみれ",
            "text": "@りんご 車乗ったら降りたくないです笑",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "たまご",
            "text": "多分3時間くらいかかります",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "つみれ",
            "text": "@りんご 混んで逆に店内に行きたくなるかな",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "りんご",
            "text": "時間かかりすぎたよね笑",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "りんご",
            "text": "たまにマックでそういう事あります",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "たまご",
            "text": "なると思う",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "つみれ",
            "text": "休日のマックのドライブスルーとか地獄です！",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "たまご",
            "text": "中の方が早い",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "りんご",
            "text": "@つみれ 主人は待てない人だから笑",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "つみれ",
            "text": "あんな感じで混んじゃうと嫌になっちゃう",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "りんご",
            "text": "中に行ったほうが早い！とかいって買ってくる",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "つみれ",
            "text": "@りんご じゃあ厳しいですね",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "たまご",
            "text": "みんな選ぶのを待つのがきつい",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "つみれ",
            "text": "@りんご 行ってくれる旦那さんだといいですね",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "りんご",
            "text": "@たまご 早くしてくれ〜と思いますよね",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "たまご",
            "text": "スーパーなんて余計いっぱいありますもん",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "つみれ",
            "text": "@たまご だめだね",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "りんご",
            "text": "@たまご そうなんです、見てると違うもの買っちゃうんです",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "たまご",
            "text": "ささっと終わらせたいです",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "りんご",
            "text": "ついつい見ちゃうんだよね",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "つみれ",
            "text": "決めてたはずなのに",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "りんご",
            "text": "そうなんですよねー",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "つみれ",
            "text": "見ちゃうと別のものに惹かれますよね",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "たまご",
            "text": "家に届くシステムが楽です",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "りんご",
            "text": "私なんてメモ持っていっているのに違うの買ってきてしまう",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "たまご",
            "text": "そうなんですか",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "つみれ",
            "text": "@りんご わかりますー！！",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "りんご",
            "text": "@たまご ありますね、その方が余計なもの買わないかも",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "たまご",
            "text": "分かります",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "りんご",
            "text": "メモのもの忘れるときあるからねー",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "つみれ",
            "text": "買いに行ったもの買い忘れて",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "たまご",
            "text": "決まったものしかこないんで",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "つみれ",
            "text": "別のものを買って帰る",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "つみれ",
            "text": "よくやります",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "りんご",
            "text": "@つみれ 私もです笑",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "つみれ",
            "text": "家に帰ってきて",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "つみれ",
            "text": "あれ？ってなるやつですね",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "りんご",
            "text": "@たまご きっちりしてるんですね、すごい",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "りんご",
            "text": "@つみれ そうそう、よくやるんです",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "たまご",
            "text": "別のものあんま買わないです",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "りんご",
            "text": "@たまご 衝動買いもないタイプ？",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "つみれ",
            "text": "@りんご たまごはブレないんですよ",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "たまご",
            "text": "たまにありますけど",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "たまご",
            "text": "ないです",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "りんご",
            "text": "@たまご 無駄遣いしなくていいです！ぶれないなんてすごい、これから必要ですよ",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "たまご",
            "text": "物欲があんまなくて",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "つみれ",
            "text": "でも、遊びには行っちゃうから",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "りんご",
            "text": "そこはたまごさんのいいとこですね！",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "りんご",
            "text": "@たまご たまには遊ばないとね！",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "たまご",
            "text": "遊びに使いたいです",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "つみれ",
            "text": "結局使っちゃう",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "りんご",
            "text": "どんな遊びですか？",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "たまご",
            "text": "遊園地行ったり飲み行ったりとかです",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "つみれ",
            "text": "釣りに行ったり、サウナにも行ったり",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "つみれ",
            "text": "@たまご ね！",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "たまご",
            "text": "そう！",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "りんご",
            "text": "いいね！どんどん若いうちに行ったほうがいいですよ",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "りんご",
            "text": "釣りもするんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 103,
            "interlocutor_id": "つみれ",
            "text": "今しかできないこともありますからね",
            "mention_to": []
        },
        {
            "utterance_id": 104,
            "interlocutor_id": "たまご",
            "text": "今のうちに行っときます",
            "mention_to": []
        }
