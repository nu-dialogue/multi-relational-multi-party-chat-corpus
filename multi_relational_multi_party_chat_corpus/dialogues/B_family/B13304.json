        },
        {
            "utterance_id": 30,
            "interlocutor_id": "しらたき",
            "text": "@コアラ そうなんですね！",
            "mention_to": [
                "コアラ"
            ]
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "しらたき",
            "text": "@つくね やってみたらいいじゃん！",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "つくね",
