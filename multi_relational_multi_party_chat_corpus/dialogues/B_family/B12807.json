        {
            "utterance_id": 1,
            "interlocutor_id": "こんぶ",
            "text": "おはようございます！",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "ちくわ",
            "text": "おはようございます。",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "コアラ",
            "text": "朝、NHK朝ドラみます？",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "こんぶ",
            "text": "みてないんですよ〜",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "ちくわ",
            "text": "見ないですねー",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "ちくわ",
            "text": "zipみてます笑",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "こんぶ",
            "text": "いつもみられます？",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "コアラ",
            "text": "@こんぶ 何か他の番組？",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "こんぶ",
            "text": "そして、教育番組を、、",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "コアラ",
            "text": "@こんぶ あっ、BSで早めに見てます",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "ちくわ",
            "text": "子供がちいさいんで！",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "こんぶ",
            "text": "@コアラ 娘がいるので！！",
            "mention_to": [
                "コアラ"
            ]
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "コアラ",
            "text": "@こんぶ おっ、お子さん小さい!",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "コアラ",
            "text": "何歳ですか？",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "こんぶ",
            "text": "@コアラ BSて！早くやってるんですもんね！！",
            "mention_to": [
                "コアラ"
            ]
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "こんぶ",
            "text": "そうなんですよ〜",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "ちくわ",
            "text": "朝ドラとかを見るってゆーのがないすね！",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "こんぶ",
            "text": "一歳３ヶ月ですー！",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "コアラ",
            "text": "@ちくわ わかります！うちも散々お世話になりました",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "こんぶ",
            "text": "@コアラ 今の朝ドラ誰ですか？",
            "mention_to": [
                "コアラ"
            ]
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "コアラ",
            "text": "お母さんといっしよ！",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "こんぶ",
            "text": "あれすごいですよね！！！",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "ちくわ",
            "text": "@コアラ ですよね！笑",
            "mention_to": [
                "コアラ"
            ]
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "コアラ",
            "text": "@こんぶ いまは樹里ちゃんです",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "こんぶ",
            "text": "どしてだろ、絶対みるお母さんといっしょ！！",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "コアラ",
            "text": "水谷豊と蘭ちゃんの娘さん",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "ちくわ",
            "text": "教育番組は子供たちのすべてを引き込みますよねー",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "こんぶ",
            "text": "@コアラ 上野樹里ちゃん！！！",
            "mention_to": [
                "コアラ"
            ]
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "ちくわ",
            "text": "@コアラ 親は朝ドラ見てますねー",
            "mention_to": [
                "コアラ"
            ]
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "コアラ",
            "text": "@ちくわ デンデラリューだ？まだやってます？",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "こんぶ",
            "text": "あーー！ちらっとみたことあります！",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "コアラ",
            "text": "小錦とか出てました",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "こんぶ",
            "text": "でてくるばってん",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "ちくわ",
            "text": "@コアラ ん？どうだろー",
            "mention_to": [
                "コアラ"
            ]
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "コアラ",
            "text": "@ちくわ さすがに変わってるはず",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "こんぶ",
            "text": "@コアラ 日本語であそぼうですね！",
            "mention_to": [
                "コアラ"
            ]
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "ちくわ",
            "text": "ほとんどおかあさんといっしょかな？",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "コアラ",
            "text": "10年前なのですみません",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "こんぶ",
            "text": "でも、小錦は出てないかもです",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "ちくわ",
            "text": "妻が娘と一緒に見てますね！",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "コアラ",
            "text": "@こんぶ それそれ！沖縄の",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "こんぶ",
            "text": "お子さんもう大きいんですね！",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "ちくわ",
            "text": "そんなのあるんですね！",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "コアラ",
            "text": "@こんぶ 小錦がゆらゆらゆれてましたよー",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "コアラ",
            "text": "お父さんと一緒は？",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "コアラ",
            "text": "土曜の",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "こんぶ",
            "text": "@コアラ みたことありますよーー！",
            "mention_to": [
                "コアラ"
            ]
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "ちくわ",
            "text": "あ！見てます！",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "ちくわ",
            "text": "おとうさんといっしょ！",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "コアラ",
            "text": "内容違うんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "コアラ",
            "text": "おかあさんと",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "こんぶ",
            "text": "旦那は忙しいながら、、",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "こんぶ",
            "text": "見れる時には一緒にね！",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "こんぶ",
            "text": "なんかちょっと違いますよね！",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "コアラ",
            "text": "@こんぶ いいおとうさん！",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "ちくわ",
            "text": "@こんぶ 見れる時にね！",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "コアラ",
            "text": "イクメンだー",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "コアラ",
            "text": "今は普通なんでしょうね",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "ちくわ",
            "text": "それを見て娘が必死に踊ってるんすよ！",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "こんぶ",
            "text": "@コアラ お褒めの言葉を、、笑",
            "mention_to": [
                "コアラ"
            ]
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "ちくわ",
            "text": "お褒め、、笑",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "こんぶ",
            "text": "まあ、そうですよねー",
