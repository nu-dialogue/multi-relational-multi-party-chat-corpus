        },
        {
            "utterance_id": 94,
            "interlocutor_id": "コアラ",
            "text": "@こんぶ うるさいだけですよ",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "コアラ",
            "text": "@ちくわ なんかお詳しい",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "こんぶ",
