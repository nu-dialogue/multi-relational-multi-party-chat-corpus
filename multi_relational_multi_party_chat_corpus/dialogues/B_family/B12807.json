        },
        {
            "utterance_id": 63,
            "interlocutor_id": "コアラ",
            "text": "@ちくわ きゃー可愛い！",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "こんぶ",
            "text": "今は余計に、協力的じゃないと",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "コアラ",
            "text": "ビデオ撮りますよね？",
