        },
        {
            "utterance_id": 66,
            "interlocutor_id": "こんぶ",
            "text": "やってけないですよね",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "こんぶ",
            "text": "撮りますよ〜〜",
