        },
        {
            "utterance_id": 1,
            "interlocutor_id": "たまねぎ",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "にんじん",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "りんご",
            "text": "春休みとはどこか行きますか？",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "たまねぎ",
            "text": "予定はないですが、大阪行きたいですー",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "りんご",
            "text": "大阪ですかーいいですね",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "にんじん",
            "text": "行ったことないんですよー",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "りんご",
            "text": "UFJとか？",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "たまねぎ",
            "text": "ユニバーサルスタジオジャパン行きたくて",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "たまねぎ",
            "text": "そーです",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "たまねぎ",
            "text": "マリオに会いたくて",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "りんご",
            "text": "いってみたいです",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "りんご",
            "text": "マリオ！",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "たまねぎ",
            "text": "息子がいるんですが、どハマりしまして",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "にんじん",
            "text": "1歳の子なんですが",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "りんご",
            "text": "ですよね、息子はゲームするから",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "りんご",
            "text": "えー！一歳で？",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "にんじん",
            "text": "そうなんです",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "にんじん",
            "text": "マリオの映画見せたらそこから",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "りんご",
            "text": "すごくないですか？",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "たまねぎ",
            "text": "毎日見てます",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "たまねぎ",
            "text": "たまに笑ってます",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "りんご",
            "text": "子供って計り知れないからねー",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "たまねぎ",
            "text": "アンパンマンじゃないのかーって",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "りんご",
            "text": "本当ですね笑",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "にんじん",
            "text": "マリオグッズそんなにないんですよねー",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "たまねぎ",
            "text": "だからUFJ行きたいなって",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "りんご",
            "text": "じゃあ、大阪いって買えますね",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "にんじん",
            "text": "ただ遠くて",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "りんご",
            "text": "楽しそうだしお子さん絶対喜びそう",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "にんじん",
            "text": "なかなか行く計画がたてられません",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "りんご",
            "text": "にんじんさんちは関東ですか？",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "たまねぎ",
            "text": "そうです",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "りんご",
            "text": "確かに大阪遠いですよね",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "たまねぎ",
            "text": "恐らく5時間くらいかと",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "りんご",
            "text": "車でいく感じですか？",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "たまねぎ",
            "text": "そうですね！",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "りんご",
            "text": "家族でいくとなるとそうなりますよね",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "たまねぎ",
            "text": "ですよね",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "りんご",
            "text": "お子さん小さいと車のが気が楽ですもんね",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "たまねぎ",
            "text": "荷物も多いですしね",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "りんご",
            "text": "でもドライブでサービスエリアとか最近はいいとこありそう",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "にんじん",
            "text": "美味しいもの沢山ありますよね",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "たまねぎ",
            "text": "ついつい寄っちゃう",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "りんご",
            "text": "ありますね！",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "りんご",
            "text": "私も好きです！",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "たまねぎ",
            "text": "お腹いっぱいになってメイン食べられなくなります",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "りんご",
            "text": "わかります！なんかちょっと食べちゃう",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "りんご",
            "text": "先週、佐野SA行きました",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "たまねぎ",
            "text": "何が良かったですか？",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "りんご",
            "text": "すごいリューアルされてきれいでしたよ",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "にんじん",
            "text": "へー！",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "にんじん",
            "text": "のぼりとくだりでまた違うからおもしろいですよねー",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "りんご",
            "text": "美味しいそうなお菓子が充実してました",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "たまねぎ",
            "text": "でも私覚えるの苦手だから",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "たまねぎ",
            "text": "どこのSAか忘れます！",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "にんじん",
            "text": "めもっておかないと！",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "りんご",
            "text": "あーあるあるですよね私も主人とあそこで会話してます",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "にんじん",
            "text": "あるあるですね",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "りんご",
            "text": "羽生サービスエリアも建物もかわっていて良かったですよ",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "たまねぎ",
            "text": "充実しすぎて目的地つくまで倍かかりますね笑",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "りんご",
            "text": "そうなんです！",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "にんじん",
            "text": "SA巡りですね",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "たまねぎ",
            "text": "それも楽しそー",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "りんご",
            "text": "意外と楽しめますよね",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "たまねぎ",
            "text": "旅行プランに組み込まないと笑",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "りんご",
            "text": "小さいお子さん向けの遊園地的のもあったりと",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "にんじん",
            "text": "そうなんですか？？",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "たまねぎ",
            "text": "それは楽しそう",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "りんご",
            "text": "こないだいったとこはそうでした",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "りんご",
            "text": "群馬のららん藤岡です",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "たまねぎ",
            "text": "へー！",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "たまねぎ",
            "text": "行ってみたい！",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "りんご",
            "text": "意外といいですよ",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "にんじん",
            "text": "いいですねー",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "りんご",
            "text": "高速からじゃなくても入れし",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "にんじん",
            "text": "そうなんですかー",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "りんご",
            "text": "最近のってそういうとこあるんですよ！",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "りんご",
            "text": "ご飯も食べれるしお買い物もできるし意外とおすすめです",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "たまねぎ",
            "text": "行ってみます！",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "にんじん",
            "text": "わーい！",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "りんご",
            "text": "是非是非！",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "りんご",
            "text": "特に群馬のまわしものではありません笑",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "たまねぎ",
            "text": "笑",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "にんじん",
            "text": "えー！",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "りんご",
            "text": "お子さんの行きたいとこもありますからね",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "たまねぎ",
            "text": "そうですね！",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "りんご",
            "text": "どんなとこ行かれるんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "にんじん",
            "text": "来週TOKYOいきます！",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "りんご",
            "text": "三連休だからかな？いいですね！",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "たまねぎ",
            "text": "結婚式で行きます",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "りんご",
            "text": "なるほど！春になるからそういう季節ですね",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "にんじん",
            "text": "新生活ですよ！",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "りんご",
            "text": "本当、素敵な感じですね！新鮮な響きだわ",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "りんご",
            "text": "お子さんも新生活始まりますか？",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "たまねぎ",
            "text": "はい！2人新生活です！",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "にんじん",
            "text": "へぇー！",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "りんご",
            "text": "二人も？大変ですね",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "たまねぎ",
            "text": "3個違いなので卒業も入学も一緒です",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "りんご",
            "text": "きゃー忙しいですね！",
            "mention_to": []
        }
