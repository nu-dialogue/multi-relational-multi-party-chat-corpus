    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "おおかみ",
            "text": "こんばんは",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "ちくわ",
            "text": "こんばんは",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "こんぶ",
            "text": "こんばんは！",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "おおかみ",
            "text": "好きな音楽とかありますか？",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "おおかみ",
            "text": "よく聞く音楽とか",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "ちくわ",
            "text": "よく聞く音楽はあんまないすねー",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "こんぶ",
            "text": "ん〜私は平井大さんが好きです！",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "ちくわ",
            "text": "けど、体動かす時は",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "こんぶ",
            "text": "知ってますーー？",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "ちくわ",
            "text": "テンポがいい洋楽すね",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "おおかみ",
            "text": "平井大さん！たまに聞きます",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "おおかみ",
            "text": "洋楽もいいですよね",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "こんぶ",
            "text": "@おおかみ ほんとですか！嬉しい",
            "mention_to": [
                "おおかみ"
            ]
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "こんぶ",
            "text": "歌詞がいいですよね〜",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "おおかみ",
            "text": "私も誰ってのは聞か図満遍なく聞くので",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "こんぶ",
            "text": "あー！わかります！",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "おおかみ",
            "text": "@こんぶ わかります",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "ちくわ",
            "text": "自分に合った曲すよね！",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "おおかみ",
            "text": "そうなんです",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "こんぶ",
            "text": "私もそんな感じです！",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "こんぶ",
            "text": "この間、スポーツ観戦にいって",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "おおかみ",
            "text": "だからこの人をずっと推してるとかあんまりないんですよね",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "ちくわ",
            "text": "その方がいいすよ！",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "こんぶ",
            "text": "洋楽ガンガンかかっててめちゃくちゃよかったです笑",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "おおかみ",
            "text": "@こんぶ おお、それは良いですね",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "こんぶ",
            "text": "@おおかみ わかりますーー！",
            "mention_to": [
                "おおかみ"
            ]
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "ちくわ",
            "text": "テンション上がるやつーー！",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "こんぶ",
            "text": "@おおかみ やっぱり洋楽もテンションあがるなって思っちゃいました",
            "mention_to": [
                "おおかみ"
            ]
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "おおかみ",
            "text": "ああいうところで聞く今日、自分のSpotifyで聴きたいのに何かわからない現象が多発します",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "おおかみ",
            "text": "@こんぶ 意味がわからなくてもテンポがいいですよね",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "おおかみ",
            "text": "疾走感というか",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "ちくわ",
            "text": "それはある！",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "こんぶ",
            "text": "あーー！曲名がわからないですよね",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "ちくわ",
            "text": "ただそのリズムを楽しむやつ！",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "おおかみ",
            "text": "そうなんです",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "こんぶ",
            "text": "そうですそうです！！！テンポよくてーー",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "おおかみ",
            "text": "あれ残念だなぁといつも思っています",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "こんぶ",
            "text": "てなると、私たち",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "こんぶ",
            "text": "何でものれちゃうタイプですね笑",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "おおかみ",
            "text": "@こんぶ その通りですね笑",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "おおかみ",
            "text": "ただ私韓流系？があんまり好きではなく",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "ちくわ",
            "text": "@おおかみ 聞きたいのに曲名知らないと残念すよね",
            "mention_to": [
                "おおかみ"
            ]
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "おおかみ",
            "text": "まず顔の見分けとグループの見分けがあんまりつかないという",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "こんぶ",
            "text": "それは、BTSとか？",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "ちくわ",
            "text": "みんな顔一緒ですからね",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "おおかみ",
            "text": "@ちくわ 頑張ってSiriとかに頼むんですけど、だいたいダメですね",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "こんぶ",
            "text": "そーいう韓国発のグループ？",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "おおかみ",
            "text": "@こんぶ そうですそうです",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "おおかみ",
            "text": "その辺一体...",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "ちくわ",
            "text": "@おおかみ でも、音楽ながれてるのをアプリを使って",
            "mention_to": [
                "おおかみ"
            ]
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "こんぶ",
            "text": "確かにみんな一緒にみえる笑",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "ちくわ",
            "text": "曲調べたりできるのありますよ！",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "こんぶ",
            "text": "けど、やっぱり、今って人気ですよねーーー",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "こんぶ",
            "text": "韓国グループ！！！",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "おおかみ",
            "text": "@ちくわ その時に調べることが大事ですね",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "こんぶ",
            "text": "@ちくわ そんなのあるんだ！",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "おおかみ",
            "text": "@こんぶ そうなんです",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "おおかみ",
            "text": "あまりに人気すぎて、ちょっとびっくりしてます",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "ちくわ",
            "text": "@おおかみ その時がいいすよ！",
            "mention_to": [
                "おおかみ"
            ]
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "こんぶ",
            "text": "日本人と韓国人混ざったグループとかも！",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "おおかみ",
            "text": "顔を見分けることすらできてないのに、みんなすごすぎないか？と...",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "こんぶ",
            "text": "何ですかね、魅力とは笑",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "おおかみ",
            "text": "@こんぶ いや全くわからないのです...",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "ちくわ",
            "text": "身長とかで見分けたり笑",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "こんぶ",
            "text": "そして、名前も難しい笑",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "おおかみ",
            "text": "メイク男子も否定するつもりはないですけど、どうなんだろうと...",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "ちくわ",
            "text": "まぁ韓流の良さは",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "こんぶ",
            "text": "あー！特徴的な見分け方でね！笑",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "ちくわ",
            "text": "若い世代しかわからないのかも",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "おおかみ",
            "text": "@ちくわ それはありますね笑",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "こんぶ",
            "text": "それはいえてるね笑",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "おおかみ",
            "text": "必然的にきっと同世代だと思います",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "ちくわ",
            "text": "最近の日本人でさえそういうグループあるし、ら",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "ちくわ",
            "text": "ですね！",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "こんぶ",
            "text": "メイク男子、、私ノーメイク男子の方が好きです",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "おおかみ",
            "text": "@ちくわ ありますね",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "おおかみ",
            "text": "私もノーメイク男子推しですね",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "こんぶ",
            "text": "@ちくわ たしかにたしかに",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "おおかみ",
            "text": "たまに顔を白塗りしすぎちゃう子いまして",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "ちくわ",
            "text": "笑",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "こんぶ",
            "text": "@おおかみ そうですよね〜！！そんなにしなくていいのにーーって",
            "mention_to": [
                "おおかみ"
            ]
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "おおかみ",
            "text": "首と色が違っちゃってて悲しくなります",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "ちくわ",
            "text": "白塗りっていうんすね笑",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "こんぶ",
            "text": "男子は日焼けぐらいがちょうどいいです笑",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "おおかみ",
            "text": "@ちくわ とは言わないんですけど笑、なんかファンデの色間違えちゃってる感じです",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "ちくわ",
            "text": "肌荒れを隠して白塗りかな？笑",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "こんぶ",
            "text": "きっと、私もよりも美容知識の高い男子ばかりだと思います",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "おおかみ",
            "text": "わかります",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "こんぶ",
            "text": "今の若い子は笑",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "おおかみ",
            "text": "日焼けぐらいがいいです",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "ちくわ",
            "text": "なるほど",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "ちくわ",
            "text": "顔違いますからね",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "ちくわ",
            "text": "そういう人たちは",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "ちくわ",
            "text": "写真と本人見たら違うかったり",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "おおかみ",
            "text": "@ちくわ 本当に...",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "ちくわ",
            "text": "怖い世の中です笑",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "こんぶ",
            "text": "たしかに、それはあるね",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "おおかみ",
            "text": "文化を否定するつもりはないですけど、せめても自分に見合ったメイクを...",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "ちくわ",
            "text": "ですね笑",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "こんぶ",
            "text": "まあ、そうですよね〜〜",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "ちくわ",
            "text": "プリクラでも",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "ちくわ",
            "text": "人変わりますからね笑",
            "mention_to": []
        }
    ],
