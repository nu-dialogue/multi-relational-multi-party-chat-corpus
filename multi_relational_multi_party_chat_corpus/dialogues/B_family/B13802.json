        },
        {
            "utterance_id": 5,
            "interlocutor_id": "たまご",
            "text": "めちゃ悪いです",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "くじら",
            "text": "@つみれ 良いですね",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "つみれ",
            "text": "親子でも全然違うんです",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "くじら",
            "text": "@たまご コンタクトしてます？",
            "mention_to": [
                "たまご"
