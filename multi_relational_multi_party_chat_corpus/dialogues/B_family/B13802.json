        },
        {
            "utterance_id": 9,
            "interlocutor_id": "たまご",
            "text": "コンタクト怖くて",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "くじら",
            "text": "私は以前はコンタクトで今は眼鏡です",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "たまご",
            "text": "眼鏡だけです",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "つみれ",
            "text": "学生の頃、メガネに憧れました",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "くじら",
            "text": "この間、友達と視力が良かったら良いよねという話になりました",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "くじら",
            "text": "見えないと不便で",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "つみれ",
            "text": "@くじら ずっと悪いんですか？",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "くじら",
            "text": "小学低学年からで",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "くじら",
            "text": "結構早くに悪くなりました",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "つみれ",
            "text": "そうなんですね",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "くじら",
            "text": "母は視力が良いんです",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "つみれ",
            "text": "私、ずっと2.0で",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "たまご",
            "text": "僕も中学くらいからです",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "くじら",
            "text": "@つみれ そんなに見えるなんて",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "くじら",
            "text": "羨ましいです",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "たまご",
            "text": "いいなー",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "くじら",
            "text": "@たまご 中学からなんですね",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "たまご",
            "text": "映画とかも不便です",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "たまご",
            "text": "@くじら そうなんです",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "たまご",
            "text": "ちょっとずつ悪くなりました",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "くじら",
            "text": "映画も見えづらいですか？",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "たまご",
            "text": "眼鏡ないと",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "たまご",
            "text": "顔ぼやけます",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "くじら",
            "text": "きっと教室の黒板も見るのたいへんでしたよね？",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "たまご",
            "text": "めちゃ大変でした",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "つみれ",
            "text": "@たまご メガネかければ見えるんでしょ？",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "くじら",
            "text": "私も後ろの席だと見えなくて",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "たまご",
            "text": "かけると見える",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "くじら",
            "text": "めがねなしで生活できるのは羨ましいです",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "つみれ",
            "text": "黒板の字が見えづらいって言ってる友達いました",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "たまご",
            "text": "そこです",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "くじら",
            "text": "冬は眼鏡がくもる",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "つみれ",
            "text": "でも・・・",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "たまご",
            "text": "待ち合わせのときも",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "たまご",
            "text": "大変ですもん",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "つみれ",
            "text": "最近は老眼が酷くて",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "くじら",
            "text": "@たまご 見えないとなかなか友達に会えないですね",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "たまご",
            "text": "すれ違っても気付きにくくなっちゃいます",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "つみれ",
            "text": "見えないとそんな支障が・・・",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "たまご",
            "text": "@つみれ そっちか",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "くじら",
            "text": "それは不便ですね",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "くじら",
            "text": "@つみれ 老眼も辛いですね",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "くじら",
            "text": "私も少しずつ",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "くじら",
            "text": "ピントが合わない・・・",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "つみれ",
            "text": "ここ数年で見えないを実感してます",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "つみれ",
            "text": "近いとピントが合わなくて",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "たまご",
            "text": "それも大変だ",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "つみれ",
            "text": "話すと字が小さくて見えない",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "つみれ",
            "text": "離すとです",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "くじら",
            "text": "わかります。",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "くじら",
            "text": "ちょっと疲れますよね",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "たまご",
            "text": "みんないつかかけることになるのks",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "つみれ",
            "text": "昨日、小さな説明書を投げました笑",
