    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "ばなな",
            "text": "こんばんは",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "ちくわ",
            "text": "こんばんは！",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "こんぶ",
            "text": "こんばんは",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "ばなな",
            "text": "今日も暖かでしたね",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "ちくわ",
            "text": "でしたね！",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "こんぶ",
            "text": "気温はそうでしたが！",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "こんぶ",
            "text": "こちらは雨でしたよ！",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "ちくわ",
            "text": "@ばなな 仕事をしてたら汗をかきましたよ！",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "こんぶ",
            "text": "そうなんだ！",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "ばなな",
            "text": "@ちくわ  汗かきましたか",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "ちくわ",
            "text": "そうなんですよ！",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "こんぶ",
            "text": "いい仕事したね！",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "ちくわ",
            "text": "家族のために頑張りました！",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "こんぶ",
            "text": "雨だと気分が下がります",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "こんぶ",
            "text": "あ、ありがとうございます！",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "ちくわ",
            "text": "@ばなな 雨降らなかったんですか？",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "ばなな",
            "text": "@こんぶ 雨だと気分下がりますよね",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "こんぶ",
            "text": "@ばなな 今日はお仕事でしたか？",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "ばなな",
            "text": "はい、降らなかったです",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "こんぶ",
            "text": "@ばなな そうなんですよ",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "ばなな",
            "text": "@こんぶ 仕事していました",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "ちくわ",
            "text": "めちゃくちゃいいですね！",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "こんぶ",
            "text": "皆さんお疲れ様でした！",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "ちくわ",
            "text": "晴れてる方が気持ちもスッキリ！",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "ばなな",
            "text": "夕方はテニスしました",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "こんぶ",
            "text": "私は子どもと一日過ごしました",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "こんぶ",
            "text": "テニス！！！",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "ばなな",
            "text": "息子と",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "こんぶ",
            "text": "趣味ですか？",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "ちくわ",
            "text": "@ばなな お！？息子さんと！！",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "こんぶ",
            "text": "息子さんがいらっしゃるんですね！",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "ばなな",
            "text": "以前は習っていました",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "ちくわ",
            "text": "体動かすことはいいですね！",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "こんぶ",
            "text": "おいくつですか？",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "ばなな",
            "text": "今は息子がテニス部で",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "ばなな",
            "text": "中１です",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "こんぶ",
            "text": "テニス部なんですね！",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "ちくわ",
            "text": "中学のお手伝いみたいな感じですか？",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "こんぶ",
            "text": "お母さんが練習相手なんて",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "こんぶ",
            "text": "素晴らしいですね！",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "ばなな",
            "text": "やりたいとなったので",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "ばなな",
            "text": "今日は付き合いました",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "ちくわ",
            "text": "新しい挑戦はいいことですね！",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "こんぶ",
            "text": "息子さんは中学からテニス部ですか？",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "ばなな",
            "text": "そうです",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "こんぶ",
            "text": "いい汗かきましたね！",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "ばなな",
            "text": "今まではスポーツしていなくて",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "ちくわ",
            "text": "@ばなな テニスやったことあったんですね！",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "こんぶ",
            "text": "そうなんですか！",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "ちくわ",
            "text": "スポーツっていいですよね！",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "こんぶ",
            "text": "スポーツいいよね！",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "ばなな",
            "text": "@ちくわ スポーツ何かしていますか？",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "こんぶ",
            "text": "私たちも体動かすこと大好きです！",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "ちくわ",
            "text": "自分たちの子供にもスポーツしてもらいたいね！",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "こんぶ",
            "text": "ほんとそう！",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "ちくわ",
            "text": "@ばなな バレーボールやってます！",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "ばなな",
            "text": "良いですね",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "ばなな",
            "text": "私はバレーが下手で",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "ちくわ",
            "text": "バレーボールは難しいですよね！",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "こんぶ",
            "text": "私も体育でやったくらいです",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "こんぶ",
            "text": "腕が痛くなる",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "ばなな",
            "text": "@こんぶ 何かスポーツは？",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "ちくわ",
            "text": "妻とは時々外で、バレーボールするんですよ！",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "こんぶ",
            "text": "@ばなな 私は卓球バドミントンなど",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "ちくわ",
            "text": "@こんぶ 腕は痛くなるね！",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "ばなな",
            "text": "外でですか",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "こんぶ",
            "text": "転々としてました",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "ちくわ",
            "text": "青空の中、やりますよ！",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "ばなな",
            "text": "気持ちが良さそう",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "こんぶ",
            "text": "@ちくわ 赤くなるから大変",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "ちくわ",
            "text": "めちゃくちゃ気持ちいいですよ！",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "ばなな",
            "text": "卓球も良いですね",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "こんぶ",
            "text": "外でのバレーもいいね！",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "こんぶ",
            "text": "ラケットスポーツしか経験してないです",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "ちくわ",
            "text": "@ばなな テニスも難しそうですね！",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "ばなな",
            "text": "そうですね",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "こんぶ",
            "text": "たしかに！",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "ばなな",
            "text": "打つしかないかと",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "こんぶ",
            "text": "うまいことあたらないと ボールが返らない",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "ちくわ",
            "text": "シングルスとかだと走らないといけないですもんね！",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "こんぶ",
            "text": "打ち込みあるのみ！",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "こんぶ",
            "text": "ですね！",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "こんぶ",
            "text": "練習熱心は素晴らしい",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "ばなな",
            "text": "今日はかなり走りました",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "こんぶ",
            "text": "本当にいい運動ですね！",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "ちくわ",
            "text": "@ばなな 息子さんの活躍楽しみにしています！",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "ばなな",
            "text": "有難うございます",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "こんぶ",
            "text": "そうだね！楽しみにしてます！",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "ちくわ",
            "text": "スポーツで色んな経験できますもんね！",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "ばなな",
            "text": "そうなんです",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "こんぶ",
            "text": "礼儀とかも学べるもんね！",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "ばなな",
            "text": "なので続けてほしいです",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "こんぶ",
            "text": "そうですよね！",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "ちくわ",
            "text": "@こんぶ そうだね！",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "こんぶ",
            "text": "わかります！！",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "ばなな",
            "text": "@ちくわ お子さんはスポーツはされていますか？",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "ちくわ",
            "text": "自分たちの娘は何をするのかなーと",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "こんぶ",
            "text": "まだ一歳なのでしてないです！",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "ばなな",
            "text": "まだ小さいのですね",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "こんぶ",
            "text": "でも、ベビースイミングに通おうかと！",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "ばなな",
            "text": "これからですね",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "ちくわ",
            "text": "@ばなな ボールを投げるの上手いですよ！",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "こんぶ",
            "text": "@ばなな そうなんですよ！",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 103,
            "interlocutor_id": "こんぶ",
            "text": "たしかに！",
            "mention_to": []
        },
        {
            "utterance_id": 104,
            "interlocutor_id": "ばなな",
            "text": "それは将来が楽しみ",
            "mention_to": []
        },
        {
            "utterance_id": 105,
            "interlocutor_id": "こんぶ",
            "text": "上手にボール投げるね",
            "mention_to": []
        },
        {
            "utterance_id": 106,
            "interlocutor_id": "ちくわ",
            "text": "お風呂で特訓中です！",
            "mention_to": []
        }
