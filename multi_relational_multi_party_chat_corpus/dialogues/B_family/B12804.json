{
    "dialogue_id": "B12804",
    "dialogue_type": "Family",
    "interlocutors": [
        "コアラ",
        "ちくわ",
        "こんぶ"
    ],
    "relationship": [
        "ちくわ",
        "こんぶ"
    ],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "コアラ",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "ちくわ",
            "text": "こんにちは！！！！！！",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "こんぶ",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "コアラ",
            "text": "紅白歌合戦って見ますか？",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "ちくわ",
            "text": "少し見ましたが",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "こんぶ",
            "text": "みてました！！！",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "ちくわ",
            "text": "昔のように",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "こんぶ",
            "text": "けど、今年は、、",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "ちくわ",
            "text": "釘付けではないです",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "コアラ",
            "text": "やはり他の番組も？",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "こんぶ",
            "text": "@ちくわ 何みてたっけ？",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "ちくわ",
            "text": "なんか、僕の姉夫婦がきてて",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "ちくわ",
            "text": "バタバタしてました！",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "コアラ",
            "text": "昨年、逃走中とかやってましたよね？",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "こんぶ",
            "text": "そうですね！つい色んなチャンネルを回しちゃいます",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "ちくわ",
            "text": "@こんぶ なに見たのかもわからない",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "こんぶ",
            "text": "逃走中！！",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "ちくわ",
            "text": "昨年は、、、",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "ちくわ",
            "text": "コロナになってて",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "こんぶ",
            "text": "やってましたよね！あれ、今年もやってましたか？",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "ちくわ",
            "text": "寝てました！",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "ちくわ",
            "text": "たぶんやってたような。",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "コアラ",
            "text": "@ちくわ ええーっ？大丈夫でしたか？",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "コアラ",
            "text": "熱は？",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "ちくわ",
            "text": "@コアラ 家族全滅でした",
            "mention_to": [
                "コアラ"
            ]
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "こんぶ",
            "text": "１ヶ月の娘もなりました！",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "ちくわ",
            "text": "妻が1番ひどくて",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "コアラ",
            "text": "@こんぶ そんなに小さくても？",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "ちくわ",
            "text": "コロナになる前の日の３１日",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "こんぶ",
            "text": "娘が唯一けいしょうでしたよ！",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "ちくわ",
            "text": "ルンルンで",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "コアラ",
            "text": "大晦日？！",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "ちくわ",
            "text": "天ぷら揚げてたら",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "ちくわ",
            "text": "妻が熱出ました！",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "コアラ",
            "text": "年越し蕎麦用？",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "ちくわ",
            "text": "揚げてる場合ちゃうやんかーい",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "こんぶ",
            "text": "@コアラ たぶん、母乳だったので、免疫あったみたいで！",
            "mention_to": [
                "コアラ"
            ]
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "ちくわ",
            "text": "て感じです",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "こんぶ",
            "text": "ほんとそう笑",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "コアラ",
            "text": "@こんぶ 母乳免疫ありそう！",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "ちくわ",
            "text": "天ぷらとお寿司だったんですよ",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "ちくわ",
            "text": "母乳のおかげだったよねー",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "こんぶ",
            "text": "ご飯食べるまでは元気だったんですけど",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "コアラ",
            "text": "@ちくわ いいなー美味しそう",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "ちくわ",
            "text": "感謝です",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "コアラ",
            "text": "えっ？食べられましたか？",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "こんぶ",
            "text": "だんだん、なんか、おかしいなになってきて",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "ちくわ",
            "text": "@コアラ 美味しかったですよ！",
            "mention_to": [
                "コアラ"
            ]
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "こんぶ",
            "text": "旦那は料理が上手です",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "ちくわ",
            "text": "@こんぶ 少し食べてたよね？",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "コアラ",
            "text": "食べてから発症？",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "こんぶ",
            "text": "@ちくわ たべた！たべた！",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "ちくわ",
            "text": "その前に寝てたんすよ！",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "こんぶ",
            "text": "食べてからです！",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "コアラ",
            "text": "@こんぶ いいですね！羨ましいです",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "ちくわ",
            "text": "なんかしんどくてね！",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "こんぶ",
            "text": "けど、ちょっとその前からん？寒気？みたいなのは",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "こんぶ",
            "text": "ありましたけどね！！！",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "ちくわ",
            "text": "料理は常に目分量で適当ですが！",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "こんぶ",
            "text": "そしたらね、まさか。",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "コアラ",
            "text": "@ちくわ うちも昨年夏に主人とコロナでダウンしたのでわかります",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "ちくわ",
            "text": "まさかだったねーー",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "コアラ",
            "text": "味覚とかは？",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "コアラ",
            "text": "匂いとか",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "こんぶ",
            "text": "夏のコロナは大変そうです。。",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "ちくわ",
            "text": "僕は大丈夫でした！",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "こんぶ",
            "text": "味覚だけちょっと、分からない？",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "ちくわ",
            "text": "妻は声がなかなか出なかったです",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "こんぶ",
            "text": "てことありましたけど、すぐに大丈夫に！",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "こんぶ",
            "text": "喉がめちゃくちゃ痛くて！！！",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "こんぶ",
            "text": "声が出なくなりました",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "コアラ",
            "text": "@ちくわ わかります！",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "コアラ",
            "text": "喉！",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "ちくわ",
            "text": "@コアラ うわーー喉はきつい",
            "mention_to": [
                "コアラ"
            ]
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "こんぶ",
            "text": "えーーそんないいタイミングに",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "コアラ",
            "text": "えいやって唾飲み込んでました",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "ちくわ",
            "text": "あ！",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "ちくわ",
            "text": "それはわかる気がする",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "こんぶ",
            "text": "本当そうです。。",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "コアラ",
            "text": "針がささる？",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "ちくわ",
            "text": "我慢しながら飲み込んでた！",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "こんぶ",
            "text": "唾飲むのも痛すぎて",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "コアラ",
            "text": "感じ？",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "こんぶ",
            "text": "めちゃくちゃわかります！！！",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "ちくわ",
            "text": "薬が1番きつかったようなー",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "コアラ",
            "text": "閻魔様とか思い出しました",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "こんぶ",
            "text": "今までも喉の風邪引いてましたけど",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "コアラ",
            "text": "@ちくわ 薬？",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "ちくわ",
            "text": "でもまさか年始になるとはね！",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "ちくわ",
            "text": "って感じです",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "こんぶ",
            "text": "比にならないくらい痛かったです",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "ちくわ",
            "text": "@コアラ 薬飲まされた時に",
            "mention_to": [
                "コアラ"
            ]
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "コアラ",
            "text": "しかも地震？",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "コアラ",
            "text": "揺れませんでした？",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "ちくわ",
            "text": "錠剤が！",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "こんぶ",
            "text": "地震ありましたよね！！",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "ちくわ",
            "text": "あ！その前の年です！",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "コアラ",
            "text": "@ちくわ あっ、錠剤！きつそう",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "ちくわ",
            "text": "2023年になったときです！",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "こんぶ",
            "text": "あ、去年じゃないね！",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "ちくわ",
            "text": "コロナは！",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "ちくわ",
            "text": "間違ってました！",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "コアラ",
            "text": "@ちくわ その前なんですね",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 103,
            "interlocutor_id": "こんぶ",
            "text": "一昨年の話でした！",
            "mention_to": []
        }
