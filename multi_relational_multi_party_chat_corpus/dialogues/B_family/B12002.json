            "interlocutor_id": "たまねぎ",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "ばなな",
            "text": "朝食は何を食べましたか？",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "たまねぎ",
            "text": "今日はポトフです！",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "ばなな",
            "text": "良いですね",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "にんじん",
            "text": "@ばなな ポトフ食べました！",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "たまねぎ",
            "text": "昨日の残りです！",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "ばなな",
            "text": "具は何が入っていますか？",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "たまねぎ",
            "text": "にんじん",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "たまねぎ",
            "text": "たまねぎ",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "たまねぎ",
            "text": "キャベツ",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "たまねぎ",
            "text": "ウィンナー",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "たまねぎ",
            "text": "じゃがいも入れ忘れました",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "ばなな",
            "text": "@たまねぎ たくさん入っていますね",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "たまねぎ",
            "text": "あ、あときのこ！",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "ばなな",
            "text": "私は朝はパンです",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "にんじん",
            "text": "@たまねぎ 体に良さそう",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "たまねぎ",
            "text": "@ばなな 食パンですか？",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "ばなな",
            "text": "@たまねぎ そうです",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "たまねぎ",
            "text": "おいしいですよねー",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "ばなな",
            "text": "シンプルなのが良いですね",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "たまねぎ",
            "text": "@ばなな ですね！",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "たまねぎ",
            "text": "バター塗ってじゅうぶん！",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "ばなな",
            "text": "@たまねぎ バター、良いですね",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "にんじん",
            "text": "メロンパンもすきです",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "ばなな",
            "text": "メロンパン",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "にんじん",
            "text": "少し温めて",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "ばなな",
            "text": "@にんじん かりっとしたのが好きですか？",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "にんじん",
            "text": "固くなる前に食べる！",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "ばなな",
            "text": "私はさくさくした感じが好きです",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "にんじん",
            "text": "@ばなな しっとり派です！",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "にんじん",
            "text": "コーヒーと一緒に",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "ばなな",
            "text": "@にんじん 良いですね",
            "mention_to": [
                "にんじん"
            ]
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "にんじん",
            "text": "合いますよねー",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "ばなな",
            "text": "私もコーヒー好きで",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "にんじん",
            "text": "はい",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "ばなな",
            "text": "眠い時はコーヒー飲みます",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "たまねぎ",
            "text": "@ばなな 私は甘い派です！",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "たまねぎ",
            "text": "苦いのは飲めなくて",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "ばなな",
            "text": "なるほど",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "たまねぎ",
            "text": "主人がブラック飲んでます",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "ばなな",
            "text": "私は苦いのが好きです",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "たまねぎ",
            "text": "おー",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "ばなな",
            "text": "あとはカフェラテとか",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "たまねぎ",
            "text": "緑茶の苦いのとかも？？",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "ばなな",
            "text": "そうですね",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "ばなな",
            "text": "苦すぎるのもですけれど",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "たまねぎ",
            "text": "センブリ茶飲んだことありますか？",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "にんじん",
            "text": "@たまねぎ テレビでしか見たことないけど",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "ばなな",
            "text": "苦いですか",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "にんじん",
            "text": "すごく苦そう",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "ばなな",
            "text": "私もテレビでしか",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "たまねぎ",
            "text": "罰ゲームとかで飲むヤツ",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "ばなな",
            "text": "そうですよね",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "たまねぎ",
            "text": "身体にはよさそうですよね",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "たまねぎ",
            "text": "ダイエットとか",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "ばなな",
            "text": "体に良さそうですね",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "たまねぎ",
            "text": "ですよね",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "ばなな",
            "text": "@たまねぎ ダイエット気になりますか？",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "たまねぎ",
            "text": "@ばなな 気になります！",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "たまねぎ",
            "text": "というかしています！",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "ばなな",
            "text": "夫にダイエットしてもらいたくて",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "たまねぎ",
            "text": "はい",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "ばなな",
            "text": "ダイエットしているのですね",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "たまねぎ",
            "text": "@ばなな そうです",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "ばなな",
            "text": "何か良いのありますか？",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "たまねぎ",
            "text": "続くのはながらダイエットですね",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "ばなな",
            "text": "ながらダイエット？",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "たまねぎ",
            "text": "歯を磨いてる時はストレッチ、",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "たまねぎ",
            "text": "とか決めて",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "ばなな",
            "text": "なるほど",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "たまねぎ",
            "text": "何かするときにセットを作ると",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "たまねぎ",
            "text": "続きます",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "ばなな",
            "text": "確かに続きそうです",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "たまねぎ",
            "text": "洗い物する時は爪先立ちとか",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "にんじん",
            "text": "なるほど",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "ばなな",
            "text": "そういうのもありですね",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "たまねぎ",
            "text": "置き換えはむずかしいです",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "にんじん",
            "text": "@ばなな どれくらい痩せてほしいんですか？",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 80,
