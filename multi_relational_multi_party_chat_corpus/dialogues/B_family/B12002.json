            "text": "はいはい",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "ばなな",
            "text": "本当は10キロ",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "たまねぎ",
            "text": "笑",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "たまねぎ",
            "text": "ここだけの話",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "ばなな",
            "text": "メタボ予備群です",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "たまねぎ",
            "text": "3キロ痩せたけど",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "たまねぎ",
            "text": "4キロって嘘つきました",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "ばなな",
            "text": "はい",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "たまねぎ",
            "text": "笑",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "にんじん",
            "text": "@たまねぎ 嘘かい！",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "ばなな",
            "text": "@たまねぎ すごいですよ",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "たまねぎ",
            "text": "あと15キロ痩せたいです",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "ばなな",
            "text": "1キロでもなかなか",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "たまねぎ",
            "text": "産後太りで",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "ばなな",
            "text": "@たまねぎ そんなにですか？",
            "mention_to": [
                "たまねぎ"
            ]
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "ばなな",
            "text": "なるほど",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "たまねぎ",
            "text": "一年経っても戻りません",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "ばなな",
            "text": "それは辛い",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "たまねぎ",
            "text": "なので初めてダイエット挑戦です！",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "たまねぎ",
            "text": "でも、",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "ばなな",
            "text": "頑張って下さいね",
            "mention_to": []
        },
        {
            "utterance_id": 102,
