    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "くじら",
            "text": "おはようございます",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "つくね",
            "text": "おはようございます！",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "しらたき",
            "text": "おはようございます",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "くじら",
            "text": "今、何か欲しい物ありますか？",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "くじら",
            "text": "買いたいなと思うもの",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "つくね",
            "text": "今欲しいものですか、、、",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "つくね",
            "text": "春服欲しいですね",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "しらたき",
            "text": "@くじら 新しいゲームソフトですね",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "くじら",
            "text": "春服欲しいですね",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "くじら",
            "text": "いつも春と秋が着るものが困って",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "つくね",
            "text": "@くじら 分かります！",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "つくね",
            "text": "一番何着ていいかわからないです",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "つくね",
            "text": "寒い日もあるし",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "くじら",
            "text": "何着たら良いかわからないですよね",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "くじら",
            "text": "最近がそうでした",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "しらたき",
            "text": "服はTシャツとトレーナーあったら大丈夫",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "つくね",
            "text": "@くじら 確かに！急に寒くなるので",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "くじら",
            "text": "@しらたき 重ね着ですね",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "つくね",
            "text": "困りますよね",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "しらたき",
            "text": "@くじら 重ね着ですね",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "くじら",
            "text": "冬っぽいの着ていたらおかしいかという時があって",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "しらたき",
            "text": "ロンTとかは持ってないです笑",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "つくね",
            "text": "うんうん",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "くじら",
            "text": "色で春らしくするとか",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "つくね",
            "text": "色もそうですよね！",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "つくね",
            "text": "大体同じ感じになってしまう",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "つくね",
            "text": "@しらたき ロンT必要だよ！",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "くじら",
            "text": "何故か家にあるのは似たような感じ",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "しらたき",
            "text": "@つくね 買ってください！",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "つくね",
            "text": "@くじら そうなんです！！！",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "つくね",
            "text": "なんでですかね？あれ！笑",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "くじら",
            "text": "ロンT着たことないかも",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "しらたき",
            "text": "@つくね 同じ色コーデ！",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "くじら",
            "text": "どうしでも好きなのを選ぶからかな",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "しらたき",
            "text": "@くじら ロンT着る時あります？笑",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "くじら",
            "text": "@しらたき ないです",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "つくね",
            "text": "@しらたき それな！笑気付いたら同じ色",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "つくね",
            "text": "@くじら あと買うときに目に入りますよね",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "つくね",
            "text": "この色なら似合うかもーってやつ！",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "くじら",
            "text": "他の人に選んでもらったら違うのかな",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "しらたき",
            "text": "@くじら ですよねーだからTシャツかトレーナーの2択！",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "くじら",
            "text": "なるほど",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "つくね",
            "text": "@くじら どうですかね？どっちがいいー？と聞きますが",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "つくね",
            "text": "大体自分の好みの方になります！",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "しらたき",
            "text": "@つくね じゃあ聞かないでくれ！笑",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "くじら",
            "text": "夫とは好みが違うので、母に見てもらったりはします",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "つくね",
            "text": "@しらたき え！？だって選べないから笑",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "つくね",
            "text": "@くじら それありますよね！",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "くじら",
            "text": "昨日、テレビで見たのですが",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "くじら",
            "text": "かまいたちの山内の持っているTシャツ",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "くじら",
            "text": "80万でした",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "つくね",
            "text": "80万！？",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "つくね",
            "text": "やばっ！",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "つくね",
            "text": "そんな服着れないです…笑",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "くじら",
            "text": "そうです！80万",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "しらたき",
            "text": "@くじら 80万はやばいですね",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "しらたき",
            "text": "他に色々買える笑",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "くじら",
            "text": "そんなお金ないので、買えないです",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "くじら",
            "text": "確かに他に色々買えるかも",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "つくね",
            "text": "@くじら 同じくです",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "しらたき",
            "text": "@くじら 他のもの買った方が有意義ですよね笑",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "くじら",
            "text": "80万なら海外旅行にも行ますね",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "つくね",
            "text": "@くじら 確かに！それに80万のTシャツ汚せない笑",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "くじら",
            "text": "海外は行ったことありますか？",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "つくね",
            "text": "@くじら ありますー",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "くじら",
            "text": "@つくね どちらに？",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "しらたき",
            "text": "@くじら 新婚旅行で行きました",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "つくね",
            "text": "@くじら ニュージーランドと韓国です",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "くじら",
            "text": "@しらたき 新婚旅行ですね",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "しらたき",
            "text": "@くじら それが初めての海外旅行でした",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "くじら",
            "text": "2か国行ったのですね",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "くじら",
            "text": "初海外はどうでしたか？",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "つくね",
            "text": "@くじら 韓国は何回か行ってます",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "しらたき",
            "text": "@くじら 飛行機が怖くて笑",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "しらたき",
            "text": "ドキドキしてました",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "くじら",
            "text": "@つくね 韓国良いですね",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "つくね",
            "text": "@くじら 行ったことありますか？",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "くじら",
            "text": "飛行機は怖いですね",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "しらたき",
            "text": "落ちるかもと思ってしまいますよね",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "くじら",
            "text": "@つくね あります何回か",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "つくね",
            "text": "@くじら 韓国でなにしましたか？",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "くじら",
            "text": "事故とかたまにあるので怖いのかと",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "くじら",
            "text": "@つくね すみません、韓国はなくて、海外はあります",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "しらたき",
            "text": "滅多にないと信じたいですが",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "つくね",
            "text": "@くじら あ！すみません！勘違いを…",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "くじら",
            "text": "スペイン、イタリア、オーストラリアに行きました",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "つくね",
            "text": "どこの国に行かれたんですか?",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "つくね",
            "text": "おお！ヨーロッパ方面ですね",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "つくね",
            "text": "オーストラリアもいいですね！",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "くじら",
            "text": "オーストラリアは新婚旅行で",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "くじら",
            "text": "夫は初海外旅行",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "しらたき",
            "text": "@くじら 新婚旅行オーストラリアだったんですね",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "しらたき",
            "text": "同じですね！",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "くじら",
            "text": "夫はどこのお店でも日本語で通してました",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "しらたき",
            "text": "@くじら 俺もです",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "くじら",
            "text": "何となく通じてるだろうって",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "しらたき",
            "text": "全部嫁任せでした笑",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "くじら",
            "text": "話せなくても何とかなりました",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "つくね",
            "text": "@くじら なんとかなります！意外と！笑",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "くじら",
            "text": "たまにホテルのスタッフで日本人の方もいて",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "つくね",
            "text": "@くじら 今多いですよね！日本人スタッフ",
            "mention_to": [
                "くじら"
            ]
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "くじら",
            "text": "偶然同じ苗字でびっくり",
            "mention_to": []
        }
