        },
        {
            "utterance_id": 64,
            "interlocutor_id": "しらたき",
            "text": "受け売りです！",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "コアラ",
            "text": "入居説明の時，言われました",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "つくね",
            "text": "@コアラ なるほど！結構保険適用外のうちもあるみたいですね",
            "mention_to": [
                "コアラ"
