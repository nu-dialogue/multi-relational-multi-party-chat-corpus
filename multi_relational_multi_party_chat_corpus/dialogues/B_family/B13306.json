        },
        {
            "utterance_id": 100,
            "interlocutor_id": "しらたき",
            "text": "煙突から入ってくるって",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "しらたき",
            "text": "言えますねー",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "コアラ",
            "text": "@しらたき 子供さんまだ小さいんですか？",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 103,
            "interlocutor_id": "つくね",
            "text": "@コアラ わかります！海外のお家可愛いし！",
            "mention_to": [
                "コアラ"
            ]
        },
        {
            "utterance_id": 104,
            "interlocutor_id": "コアラ",
            "text": "ですよね！",
            "mention_to": []
        },
        {
            "utterance_id": 105,
            "interlocutor_id": "しらたき",
            "text": "@コアラ 小1と小4です！",
            "mention_to": [
                "コアラ"
            ]
        },
        {
            "utterance_id": 106,
            "interlocutor_id": "コアラ",
            "text": "@しらたき あっ、まだサンタさんくる？！",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 107,
            "interlocutor_id": "しらたき",
            "text": "下の息子はまだまだ騙せます",
