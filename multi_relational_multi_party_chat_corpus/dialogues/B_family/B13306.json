            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "コアラ",
            "text": "床暖",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "つくね",
            "text": "@コアラ そんな弊害があるんですね！？",
