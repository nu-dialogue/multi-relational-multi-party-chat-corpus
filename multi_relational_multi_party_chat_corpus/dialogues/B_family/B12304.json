        },
        {
            "utterance_id": 87,
            "interlocutor_id": "つくね",
            "text": "@すじこ 確かに！笑",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "つくね",
            "text": "こう言う時にリアルタイムで",
            "mention_to": []
        },
