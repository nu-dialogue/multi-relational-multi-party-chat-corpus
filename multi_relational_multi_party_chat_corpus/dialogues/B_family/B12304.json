        },
        {
            "utterance_id": 98,
            "interlocutor_id": "しらたき",
            "text": "信頼してるから大丈夫です",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "つくね",
            "text": "@すじこ はモニタリングしてみたいですか？",
            "mention_to": [
                "すじこ"
