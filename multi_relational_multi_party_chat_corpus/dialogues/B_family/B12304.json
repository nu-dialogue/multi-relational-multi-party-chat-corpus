    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "つくね",
            "text": "こんばんは",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "しらたき",
            "text": "こんばんは",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "すじこ",
            "text": "こんばんは",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "つくね",
            "text": "もうすぐ桜の季節ですが",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "つくね",
            "text": "お花見とか行きますか？",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "すじこ",
            "text": "家族で行く予定です",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "つくね",
            "text": "@すじこ いいですね！",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "しらたき",
            "text": "家族と行きます！",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "つくね",
            "text": "いつも行かれるところがありますか？",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "つくね",
            "text": "@しらたき え？そうなの？笑",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "つくね",
            "text": "@しらたき 初耳です！",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "しらたき",
            "text": "@つくね え！行かないの？",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "すじこ",
            "text": "@つくね 決まってはないですが空いてそうなとこにします笑",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "つくね",
            "text": "@しらたき だって毎年仕事でいないじゃん笑",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "しらたき",
            "text": "@つくね まぁ桜より屋台だけど",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "つくね",
            "text": "@すじこ 空いてそうなところなんてあるんですか？笑",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "つくね",
            "text": "@しらたき 確かにね…",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "すじこ",
            "text": "@しらたき 屋台飯いいですね！",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "しらたき",
            "text": "屋台ではまずイカ焼きたべてー",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "しらたき",
            "text": "焼きそば買って",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "すじこ",
            "text": "@つくね 桜が咲いてればどこでもいいので笑",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "しらたき",
            "text": "ソフトクリーム買って",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "つくね",
            "text": "@すじこ なるほど！",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "つくね",
            "text": "@すじこ ちなみにお住まいどこなんですか？",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "すじこ",
            "text": "@しらたき 楽しそうですね！",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "しらたき",
            "text": "楽しみですね！",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "つくね",
            "text": "@しらたき 連れてってほしいですね！",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "すじこ",
            "text": "@つくね 愛媛県です！どちらですか？",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "しらたき",
            "text": "桜の下でBBQとかしたいなー",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "つくね",
            "text": "@すじこ 愛媛県なんですね！",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "つくね",
            "text": "私は長野県です！",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "すじこ",
            "text": "@しらたき BBQもいいですね！",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "つくね",
            "text": "暖かそうで羨ましい〜",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "しらたき",
            "text": "愛媛はやっぱりみかんがそこらじゅうに",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "しらたき",
            "text": "あるんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "しらたき",
            "text": "蛇口から",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "しらたき",
            "text": "みかんが",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "つくね",
            "text": "ポンジュース！",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "しらたき",
            "text": "出てくるとか！",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "すじこ",
            "text": "@つくね 今年は温かったですね！",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "つくね",
            "text": "@しらたき 蛇口からみかんは出てこないと思う",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "つくね",
            "text": "@すじこ 暖冬だったんですね！",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "しらたき",
            "text": "間違えた！",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "すじこ",
            "text": "@しらたき ありますよ！だんだん畑になって",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "つくね",
            "text": "長野は先日大雪でした笑",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "しらたき",
            "text": "すごーい",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "すじこ",
            "text": "ポンジュースが出てくる蛇口があります！",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "しらたき",
            "text": "子供達喜びそうだなー",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "つくね",
            "text": "@すじこ やっぱりあるんですね！",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "つくね",
            "text": "@すじこ 学校に欲しい！笑",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "すじこ",
            "text": "@つくね 大雪羨ましい！",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "つくね",
            "text": "@すじこ そうですか？",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "つくね",
            "text": "雪かきが大変です…",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "すじこ",
            "text": "@しらたき 確か松山空港に設置されてました！",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "しらたき",
            "text": "行ってみたいなー",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "しらたき",
            "text": "会社の、出張で",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "すじこ",
            "text": "@つくね 大人は大変ですが子どもの反応を見たくて",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "しらたき",
            "text": "愛媛いく予定だったのですが",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "すじこ",
            "text": "@しらたき 是非！",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "つくね",
            "text": "@すじこ そうですよね！",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "しらたき",
            "text": "コロナでなくなり",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "つくね",
            "text": "雪国ですが、子供は大喜びです",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "すじこ",
            "text": "あらら",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "しらたき",
            "text": "それから話がなくなりました",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "つくね",
            "text": "@しらたき 行きたかったよねー！",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "すじこ",
            "text": "@しらたき 残念",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "つくね",
            "text": "1週間！！",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "しらたき",
            "text": "一週間独身になれたのに",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "すじこ",
            "text": "@つくね 毎年のことでも喜ぶんですね笑",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "つくね",
            "text": "@しらたき 浮気か！？笑",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "つくね",
            "text": "@すじこ 喜びます！",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "つくね",
            "text": "登下校がやたら遅いです！",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "すじこ",
            "text": "浮気ダメ絶対",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "しらたき",
            "text": "@すじこ しません！",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "すじこ",
            "text": "@つくね モニタリングしてみたいですね笑",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "つくね",
            "text": "@しらたき 信じてます笑",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "つくね",
            "text": "@すじこ 面白そうですね！",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "しらたき",
            "text": "@すじこ モニタリングはやめてー",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "つくね",
            "text": "@しらたき なんで？笑",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "しらたき",
            "text": "特に無いけど",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "すじこ",
            "text": "@しらたき なにかまずいことでも？笑",
            "mention_to": [
