        },
        {
            "utterance_id": 84,
            "interlocutor_id": "しらたき",
            "text": "え！皆んな敵！？",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "すじこ",
            "text": "監視ですよね笑",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "しらたき",
            "text": "しらたきをいじめないでー",
