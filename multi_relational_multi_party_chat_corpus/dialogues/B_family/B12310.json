    ],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "すじこ",
            "text": "こんばんは",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "しらたき",
            "text": "こんばんは",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "つくね",
            "text": "こんばんは",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "すじこ",
            "text": "最近物忘れが酷くて",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "すじこ",
            "text": "妻の話しをほとんど覚えてません",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "つくね",
            "text": "@すじこ 覚えててください！笑",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "しらたき",
            "text": "@すじこ それは大変だ",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "つくね",
            "text": "そこは流石に！わら",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "すじこ",
            "text": "@つくね 覚えなきゃと思うのに",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "すじこ",
            "text": "真剣に聞いてるのに",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "すじこ",
            "text": "数分後にはもう",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "しらたき",
            "text": "メモメモ",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "つくね",
            "text": "それはかなりヤバい！笑",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "しらたき",
            "text": "書いたら忘れない",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "すじこ",
            "text": "@しらたき 奥様の話し忘れないですか？",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "つくね",
            "text": "メモとる旦那やだなー笑",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "しらたき",
            "text": "@すじこ わすれます",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "つくね",
            "text": "よく忘れられます",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "しらたき",
            "text": "よく忘れて怒られた",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "つくね",
            "text": "家のこと頼んだのに",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "つくね",
            "text": "やってない時とか",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "すじこ",
            "text": "@つくね ほんとにメモ取ろうか悩みました",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "つくね",
            "text": "鬼の形相で怒ります！笑",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "しらたき",
            "text": "すいません",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "つくね",
            "text": "@すじこ いやいや！覚えておきましょ！",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "つくね",
            "text": "特訓ですね！笑",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "しらたき",
            "text": "最近はやってるはず",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "すじこ",
            "text": "スマホにメモしてます笑",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "つくね",
            "text": "@すじこ それいいですね！",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "つくね",
            "text": "@しらたき 当たり前です！",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "しらたき",
            "text": "@すじこ お互い頑張りましょう",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "つくね",
            "text": "私はどちらかと言うと",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "すじこ",
            "text": "もしくは忘れる前にやるを徹底してます笑",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "つくね",
            "text": "ボールペン持ったまま",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "しらたき",
            "text": "@すじこ それが一番いい",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "つくね",
            "text": "移動してそのボールペンを",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "つくね",
            "text": "どこかに置いてきてしまいます。",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "すじこ",
            "text": "@しらたき 奥様に頼み事しますか？",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "つくね",
            "text": "で、めっちゃ探します！",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "しらたき",
            "text": "@すじこ あんまりしないかなー",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "すじこ",
            "text": "@つくね 分かります笑",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "すじこ",
            "text": "僕もスマホやメガネを家庭内で見失います笑",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "つくね",
            "text": "@すじこ よく無くすんですよね…",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "しらたき",
            "text": "大抵灯台下暗しってやつかな",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "つくね",
            "text": "@しらたき そうだね！大体近くにある！",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "すじこ",
            "text": "@しらたき 僕も滅多にしないんですよ笑",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "つくね",
            "text": "@すじこ 良く使うものほど",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "つくね",
            "text": "見失います！笑",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "すじこ",
            "text": "@つくね そうなんですよ！",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "つくね",
            "text": "昨日しらたきが車のキーが無くて",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "つくね",
            "text": "めっちゃ責められました！笑",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "すじこ",
            "text": "だからいつもの場所を作ってるのに",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "つくね",
            "text": "自分のパーカーの中にあったのに笑",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "すじこ",
            "text": "そこに置かない自分がいる",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "つくね",
            "text": "@すじこ 分かります！",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "つくね",
            "text": "なんでですかねー？笑",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "しらたき",
            "text": "@すじこ いつもの場所に置かない自分もいました",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "つくね",
            "text": "凄く不思議笑",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "しらたき",
            "text": "んで嫁を責める",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "しらたき",
            "text": "最低な旦那でした",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "すじこ",
            "text": "@しらたき あえてですか？笑",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "つくね",
            "text": "@しらたき ありえん！笑",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "しらたき",
            "text": "@すじこ 試しました",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "つくね",
            "text": "@しらたき へーそーなんだー",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "つくね",
            "text": "試されてたんだね私！",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "しらたき",
            "text": "@つくね すいません！もうしません！",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "つくね",
            "text": "@しらたき 覚えておけよー！笑",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "すじこ",
            "text": "@しらたき あの突然怒りが醒める感覚面白いですよね笑",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "すじこ",
            "text": "あーあったわ",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "すじこ",
            "text": "みたいな笑",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "しらたき",
            "text": "@すじこ そーですね！面白い",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "つくね",
            "text": "@すじこ 被害者は「は？」ってなります！笑",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "しらたき",
            "text": "見つけた時めっちゃ嬉しいけど",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "すじこ",
            "text": "@つくね そうですよね笑ごめんなさい笑",
            "mention_to": [
                "つくね"
            ]
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "しらたき",
            "text": "被害者まじは？ってなるよね",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "すじこ",
            "text": "あれ男の人に多いのかな？笑",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "つくね",
            "text": "@すじこ ちゃんと自分で探せよーっめ思います！笑",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "つくね",
            "text": "@しらたき わかってるならちゃんと探して！",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "すじこ",
            "text": "@しらたき 自分が探したときはなかったんですよ笑",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "つくね",
            "text": "これも全て年のせいですね！笑",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "しらたき",
            "text": "@すじこ やっぱり第三者が入ると見つかるんですよ！笑",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "すじこ",
            "text": "そういうことにしましょう笑",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "しらたき",
            "text": "そうですね",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "つくね",
            "text": "それが一番しっくりきます！笑",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "しらたき",
            "text": "後部屋を綺麗に保てば",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "すじこ",
            "text": "体育祭で転けるパパも年のせい",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "つくね",
            "text": "@すじこ 張り切りパパですね！",
            "mention_to": [
                "すじこ"
            ]
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "しらたき",
            "text": "パパコケるだけなら",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "しらたき",
            "text": "いいんですが",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "すじこ",
            "text": "ハゲるパパも臭いパパもウザイパパも全て年のせい",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "しらたき",
            "text": "現実頭皮",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "つくね",
            "text": "全て年のせい！いい言葉だ！笑",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "つくね",
            "text": "頭皮！？笑",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "つくね",
            "text": "@しらたき ハゲた！",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "しらたき",
            "text": "逃避！",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "すじこ",
            "text": "頭皮",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "しらたき",
            "text": "間違えた！",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "つくね",
            "text": "パパだけに笑",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "すじこ",
            "text": "ハゲだけに笑",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "つくね",
            "text": "ハゲ散らかさない様にしないと！笑",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "しらたき",
            "text": "え？ツボ入りましたー？笑",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "つくね",
            "text": "@しらたき かなりウケた！笑",
            "mention_to": [
                "しらたき"
            ]
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "つくね",
            "text": "狙える変換ミスじゃない！笑",
            "mention_to": []
        },
        {
            "utterance_id": 103,
            "interlocutor_id": "すじこ",
            "text": "@しらたき バッチリです笑",
            "mention_to": [
                "しらたき"
            ]
        }
