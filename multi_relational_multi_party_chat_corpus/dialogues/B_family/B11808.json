    ],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "ばなな",
            "text": "こんばんは",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "ちくわ",
            "text": "こんばんは！",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "こんぶ",
            "text": "こんばんは！",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "こんぶ",
            "text": "相談なんですが",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "こんぶ",
            "text": "友人が結婚しました",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "こんぶ",
            "text": "何かおすすめのお祝いありますか？",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "ばなな",
            "text": "おすすめのお祝いですね",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "こんぶ",
            "text": "はい！",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "ちくわ",
            "text": "お！それ聞きたいです！！",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "こんぶ",
            "text": "何かあれば教えてください！",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "ばなな",
            "text": "お友達は何が好きなのでしょう？",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "こんぶ",
            "text": "好きなものですね！",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "こんぶ",
            "text": "コーヒーです！",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "こんぶ",
            "text": "よく、カフェに行くようです！",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "ちくわ",
            "text": "あ！言ってたね！",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "ちくわ",
            "text": "よくカフェ巡りするって！",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "こんぶ",
            "text": "@ちくわ そうそう！",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "ばなな",
            "text": "コーヒーの豆はどうでしょう？",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "ばなな",
            "text": "こだわりの",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "こんぶ",
            "text": "なるほど！",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "ちくわ",
            "text": "なんか自分で選んで豆をプレゼントしたりとか",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "ちくわ",
            "text": "あるよね！",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "こんぶ",
            "text": "うんうん！",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "こんぶ",
            "text": "何かでみたことある！",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "ちくわ",
            "text": "@ばなな コーヒーのこだわりとかあります？",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "こんぶ",
            "text": "それいいね！",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "こんぶ",
            "text": "@ばなな ありがとうございます！",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "ちくわ",
            "text": "僕は、年中アイスコーヒーを飲むことです！",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "ばなな",
            "text": "@ちくわ 特にないのですけれど",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "こんぶ",
            "text": "たしかに、寒くても旦那はアイスコーヒー",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "ばなな",
            "text": "@ちくわ 酸味がない方が好きです",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "こんぶ",
            "text": "@ばなな わかります！",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "ばなな",
            "text": "寒くてもアイスなんですね",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "ちくわ",
            "text": "わかります！それ！",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "ちくわ",
            "text": "はい！",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "こんぶ",
            "text": "マイルドというか、スッキリタイプが私も好みです",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "ばなな",
            "text": "ちょっと苦みがあるのが好きです",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "こんぶ",
            "text": "大人ですね",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "ちくわ",
            "text": "昔は砂糖も入れてましたが",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "ちくわ",
            "text": "体重のことを考えると、、",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "こんぶ",
            "text": "ブラックが飲めるようになると",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "こんぶ",
            "text": "大人になった気分です",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "ばなな",
            "text": "私も砂糖はなしです",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "ちくわ",
            "text": "わかる！",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "ちくわ",
            "text": "大人ぶって頼んじゃうよね！",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "こんぶ",
            "text": "@ばなな ミルクはいれますか？",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "ばなな",
            "text": "入れる時と入れない時と",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "こんぶ",
            "text": "気分でですね！",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "ばなな",
            "text": "カフェラテにします",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "ちくわ",
            "text": "ミルクの量で結構味変わるもんなー",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "こんぶ",
            "text": "たしかにね！",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "ばなな",
            "text": "そうですね",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "こんぶ",
            "text": "あなたはだいぶミルクの比率が",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "ちくわ",
            "text": "おしゃれカフェとか近くないから",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "こんぶ",
            "text": "多い気がするわ！",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "ちくわ",
            "text": "行ってみたいです！",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "こんぶ",
            "text": "そうなんですよ",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "こんぶ",
            "text": "私たちの家の近くには、カフェがなくて",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "ちくわ",
            "text": "@こんぶ 牛乳大好きなんです！",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "こんぶ",
            "text": "オシャレなカフェいってみたい！",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "ばなな",
            "text": "それは寂しいですね",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "ちくわ",
            "text": "@ばなな 近くにあるんですか？",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "こんぶ",
            "text": "牛乳の減りが早い",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "ばなな",
            "text": "@ちくわ 近くにはなくて",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "ちくわ",
            "text": "牛乳は２日で一本無くなるよねー",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "ばなな",
            "text": "ちょっと出かけた先で行きます",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "こんぶ",
            "text": "そうそう！",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "こんぶ",
            "text": "それもいいですね！",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "ばなな",
            "text": "牛乳の減り、早いですね",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "ちくわ",
            "text": "僕らの地域はちょっとのところにはないんですよー",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "こんぶ",
            "text": "カフェのコーヒーの匂いに落ち着きます",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "ちくわ",
            "text": "@ばなな 牛乳大好きなんです！",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "こんぶ",
            "text": "冷蔵庫の常備品は、牛乳です",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "ばなな",
            "text": "そうでしたか",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "ちくわ",
            "text": "@こんぶ 部屋の匂いコーヒーにする？",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "こんぶ",
            "text": "切らさないように買い物してます",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "ばなな",
            "text": "そのままだとあまり飲めなくて",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "こんぶ",
            "text": "@ちくわ それはさすがに",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "ちくわ",
            "text": "コーヒーはリラックス効果もあるみたいです！",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "こんぶ",
            "text": "@ばなな 牛乳ですか？",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "こんぶ",
            "text": "そのままだと、苦手？",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "ばなな",
            "text": "牛乳は何かに入れないと",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "ちくわ",
            "text": "なるほど！！",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "ばなな",
            "text": "冷たい牛乳なら飲めます",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "こんぶ",
            "text": "そういう方多いですよね！",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "ちくわ",
            "text": "牛乳の話してたら",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "ちくわ",
            "text": "牧場に行って",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "ばなな",
            "text": "ホットにすると駄目なんです",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "こんぶ",
            "text": "冷たいとゴクゴクのめちゃいます",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "ちくわ",
            "text": "乳搾りしたくなりました！！",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "こんぶ",
            "text": "牧場？いく？",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "ちくわ",
            "text": "小さい時に、よく行ってたので！",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "ばなな",
            "text": "乳しぼりはしたことないですね",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "こんぶ",
            "text": "ホットがだめなんですね",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "こんぶ",
            "text": "私もしたことないです",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "ちくわ",
            "text": "@こんぶ 娘連れて行ってみよう！",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "ばなな",
            "text": "良い経験になりそうです",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "ちくわ",
            "text": "@ばなな 無いんですか？",
            "mention_to": [
                "ばなな"
            ]
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "こんぶ",
            "text": "@ちくわ そうだね！",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "ちくわ",
            "text": "楽しいですよ！",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "こんぶ",
            "text": "なりそうですよね！",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "ちくわ",
            "text": "チーズも作ったりできますし！",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "ばなな",
            "text": "楽しいのですね",
            "mention_to": []
        },
        {
            "utterance_id": 103,
            "interlocutor_id": "こんぶ",
            "text": "自分が搾ったものでチーズをつくるとか",
            "mention_to": []
        },
        {
            "utterance_id": 104,
            "interlocutor_id": "ばなな",
            "text": "チーズ、良いですね",
            "mention_to": []
        },
        {
            "utterance_id": 105,
            "interlocutor_id": "こんぶ",
            "text": "でも、近くに牧場はないから",
            "mention_to": []
        }
