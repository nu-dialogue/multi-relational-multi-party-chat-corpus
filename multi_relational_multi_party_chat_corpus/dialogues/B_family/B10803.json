        },
        {
            "utterance_id": 5,
            "interlocutor_id": "つみれ",
            "text": "よく行きます",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "たまご",
            "text": "今の家がセブンが目の前で",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "りんご",
            "text": "スイーツとか食べますか？",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "たまご",
            "text": "行ってしまいます",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "りんご",
            "text": "@たまご えー便利！",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "たまご",
            "text": "たまに食べます",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "つみれ",
            "text": "コンビニスイーツ好きで良く食べます",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "りんご",
            "text": "@たまご すぐいっちゃいそう",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "たまご",
            "text": "めちゃ便利です",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "たまご",
            "text": "行っちゃいます",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "りんご",
            "text": "@つみれ 私もよくたべます",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "つみれ",
            "text": "@りんご 美味しいですよね",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "つみれ",
            "text": "どこのコンビニがいいとかありますか？",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "たまご",
            "text": "セブン派です",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "りんご",
            "text": "@つみれ セブンですかね、スイーツはローソンかな",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "りんご",
            "text": "@たまご ご飯はセブン美味しいですね",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "たまご",
            "text": "白玉ぜんざいとか好きです",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "たまご",
            "text": "そうなんです！",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "つみれ",
            "text": "@りんご セブンいいですよね!確かにローソンのスイーツ美味しい",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "たまご",
            "text": "ご飯が全体的に美味しいです",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "りんご",
            "text": "@たまご 渋いですね、でもあれ美味しい！",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "たまご",
            "text": "あれをたまに食うくらいです",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "りんご",
            "text": "うちもセブンがちかいからセブン派です",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "つみれ",
            "text": "あれ美味しい",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "りんご",
            "text": "@たまご 気分あがりますね",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "つみれ",
            "text": "うちはセブンもローソンも近くてどっちもよく行きます",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "たまご",
            "text": "近いと行っちゃいます",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "たまご",
            "text": "上がります！",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "りんご",
            "text": "コンビニの作戦にハマってますね笑",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "つみれ",
            "text": "まんまとハマってます笑",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "りんご",
            "text": "最近のコンビニスイーツおいしいよね",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "たまご",
            "text": "ハマってますw",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "りんご",
            "text": "お菓子屋さんより美味しいのある！",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "たまご",
            "text": "美味しいです",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "りんご",
            "text": "値段もたまに高いのあるけどお手頃",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "つみれ",
            "text": "安くて美味しいのが嬉しいです",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "りんご",
            "text": "最近、セブンの桜餅たべました",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "たまご",
            "text": "桜餅美味しい！",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "つみれ",
            "text": "季節ですもんね！",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "たまご",
            "text": "和菓子美味しいですね",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "りんご",
            "text": "セブンの生チョコサンド美味しいですよ",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "つみれ",
            "text": "あれ美味しいです!たまに食べます",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "りんご",
            "text": "すみません、和菓子ではないけど",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "りんご",
            "text": "@つみれ ですよね",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "りんご",
            "text": "@たまご 食べた事ありますか？",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "たまご",
            "text": "ないですー",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "つみれ",
            "text": "@りんご 生チョコとかも美味しいです",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "りんご",
            "text": "なんかチョコパイの生版って感じですよね",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "たまご",
            "text": "チョコ系はあんま食べないです",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "りんご",
            "text": "@たまご 和菓子派なんですね！",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "たまご",
            "text": "はいー",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "つみれ",
            "text": "@りんご ですね！",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "たまご",
            "text": "団子とか好きです",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "つみれ",
            "text": "みたらしとか美味しい",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "りんご",
            "text": "@たまご 団子も美味しいよね",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "たまご",
            "text": "美味しい！！",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "りんご",
            "text": "みたらし団子！これから花見とセット！",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "りんご",
            "text": "お花見とかいきますか？",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "たまご",
            "text": "花見にいい",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "つみれ",
            "text": "お花見毎年行きます!",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "たまご",
            "text": "行きたいですねー",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "りんご",
            "text": "@たまご 友達といきますか？",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "たまご",
            "text": "行こうと思ってます",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "りんご",
            "text": "@つみれ 名所に？",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "たまご",
            "text": "桜見たいです",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "りんご",
            "text": "@たまご 楽しみですね！",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "つみれ",
            "text": "@りんご 近くに綺麗な場所があってそこによく行きますね",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "たまご",
            "text": "楽しみ！",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "りんご",
            "text": "@つみれ 長野はいいとこいっぱいありそう",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "たまご",
            "text": "桜綺麗ですよねp",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "りんご",
            "text": "@たまご なんか桜っていいですよね",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "りんご",
            "text": "日本人って感じ！どんな感じ？",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "つみれ",
            "text": "@りんご 高遠っていう場所がすごくきれいです",
            "mention_to": [
                "りんご"
            ]
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "たまご",
            "text": "そうなんですよー",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "たまご",
            "text": "散ってる感じも好きです",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "りんご",
            "text": "@つみれ すごいいっぱい桜あるんですか？",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "りんご",
            "text": "@たまご 散ってる感じ素敵よね",
            "mention_to": [
                "たまご"
            ]
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "りんご",
            "text": "@つみれ いっぱい桜あるとこで散り始める桜あったらすごいきれいそうですね！",
            "mention_to": [
                "つみれ"
            ]
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "つみれ",
            "text": "@りんご いっぱいあります!その近くにお城の跡地があります",
