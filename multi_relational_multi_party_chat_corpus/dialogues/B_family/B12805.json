    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "コアラ",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "ちくわ",
            "text": "こんにちは！",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "こんぶ",
            "text": "こんにちは！！！",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "コアラ",
            "text": "学生時代や子供の頃に戻りたいですか？",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "ちくわ",
            "text": "んーー",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "ちくわ",
            "text": "大学生に！",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "こんぶ",
            "text": "そうですね〜。",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "こんぶ",
            "text": "私も短大時代に！！！",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "コアラ",
            "text": "9割が戻りたくないって答えたらしいです",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "ちくわ",
            "text": "えーー",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "こんぶ",
            "text": "@コアラ さんは？",
            "mention_to": [
                "コアラ"
            ]
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "ちくわ",
            "text": "そうなんですね！",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "こんぶ",
            "text": "あら、、笑",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "ちくわ",
            "text": "お！気になる！",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "コアラ",
            "text": "お二人、いい学生時代だったんですね！",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "こんぶ",
            "text": "楽しかったですよー！",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "ちくわ",
            "text": "いい学生というか",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "コアラ",
            "text": "@こんぶ 私もお気楽女子大生に戻りたいです",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "ちくわ",
            "text": "もっと色んなことしたかったです",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "こんぶ",
            "text": "でも、もう一度、勉強し直したいです",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "コアラ",
            "text": "@こんぶ うわ！素晴らしい",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "ちくわ",
            "text": "勉強は、、、",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "ちくわ",
            "text": "僕はスポーツばかりだったから",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "こんぶ",
            "text": "@コアラ わかります〜社会人と大学生て全く違いますよね！",
            "mention_to": [
                "コアラ"
            ]
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "ちくわ",
            "text": "なんか違うことしたい！",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "コアラ",
            "text": "@ちくわ なにされてたんですか？",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "こんぶ",
            "text": "保育士の資格取ったんですけど",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "ちくわ",
            "text": "@コアラ バレーボールです",
            "mention_to": [
                "コアラ"
            ]
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "こんぶ",
            "text": "もう少しちゃんと勉強してればと、、",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "コアラ",
            "text": "@こんぶ すごい！働かれてましたか？",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "コアラ",
            "text": "@ちくわ かっこいい！",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "ちくわ",
            "text": "勉強熱心だね！",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "ちくわ",
            "text": "@コアラ いえいえ！",
            "mention_to": [
                "コアラ"
            ]
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "こんぶ",
            "text": "@コアラ はい、保育士してました！",
            "mention_to": [
                "コアラ"
            ]
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "コアラ",
            "text": "@ちくわ 背が高いですか？",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "ちくわ",
            "text": "ずっとバレーボールだから",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "コアラ",
            "text": "@こんぶ おーすごい！！",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "ちくわ",
            "text": "きつかったです！",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "こんぶ",
            "text": "@コアラ 低いです。",
            "mention_to": [
                "コアラ"
            ]
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "ちくわ",
            "text": "ポジションがレシーブのリベロなんで",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "こんぶ",
            "text": "とか言って笑ごめんなさい",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "こんぶ",
            "text": "今の男性にしたら標準くらいでしょうか？",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "コアラ",
            "text": "@こんぶ 何センチくらいですか？",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "ちくわ",
            "text": "@こんぶ あなたが答えなくていい",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "コアラ",
            "text": "聞いてよければ",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "こんぶ",
            "text": "今の男性て、正直あまり大きいイメージなくないですか？",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "ちくわ",
            "text": "170センチはあります！",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "こんぶ",
            "text": "@ちくわ 失礼しました笑",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "ちくわ",
            "text": "でもめちゃくちゃ大きくなくてよかったです！",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "コアラ",
            "text": "@こんぶ 確かに甥っ子、高校生ですが，，周り小さいらしいです",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "こんぶ",
            "text": "たしかに！大きすぎるのは困りそう",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "ちくわ",
            "text": "私服とか靴のサイズが日本にはないので！",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "ちくわ",
            "text": "今の身長だとなんでも選べるし",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "こんぶ",
            "text": "@コアラ ですよね！！！そんなに高身長なイメージがあまり、、",
            "mention_to": [
                "コアラ"
            ]
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "コアラ",
            "text": "@ちくわ 実は主人、ガタイが良くて足は28cmです",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "こんぶ",
            "text": "甥っ子さんは大きいですか？",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "ちくわ",
            "text": "@コアラ でも僕も28センチの靴履きます！",
            "mention_to": [
                "コアラ"
            ]
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "コアラ",
            "text": "@こんぶ やはり170くらいです",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "コアラ",
            "text": "@ちくわ あっ、よかった！あまりなくないですか？",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "こんぶ",
            "text": "@コアラ そうなんですね！",
            "mention_to": [
                "コアラ"
            ]
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "コアラ",
            "text": "主人は水泳やってたので",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "こんぶ",
            "text": "@コアラ 旦那さんの身長は？",
            "mention_to": [
                "コアラ"
            ]
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "こんぶ",
            "text": "水泳を！！！",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "コアラ",
            "text": "@こんぶ 175ですがそんなに大きく見えないです",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "ちくわ",
            "text": "靴以外にありますよ！",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "こんぶ",
            "text": "私も娘とベビースイミング通おうかと思ってます",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "ちくわ",
            "text": "水泳！",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "コアラ",
            "text": "@ちくわ 靴以外？",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "ちくわ",
            "text": "ガタイ良いのわかります！",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "コアラ",
            "text": "意外に？",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "こんぶ",
            "text": "そうなんですね！！",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "ちくわ",
            "text": "@コアラ まぁそんなにないですね！",
            "mention_to": [
                "コアラ"
            ]
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "コアラ",
            "text": "@こんぶ ベビースイミングいいですね！",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "こんぶ",
            "text": "@コアラ は身長いくつです？",
            "mention_to": [
                "コアラ"
            ]
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "コアラ",
            "text": "@ちくわ 海外物とかですね",
            "mention_to": [
                "ちくわ"
            ]
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "こんぶ",
            "text": "私は１６０あります！",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "コアラ",
            "text": "@こんぶ 私は160あったのにちぢんでます",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "コアラ",
            "text": "あっ、同じくらい？",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "ちくわ",
            "text": "まぁ海外物すよね！",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "こんぶ",
            "text": "え、、ちぢむんですね",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "ちくわ",
            "text": "え",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "ちくわ",
            "text": "縮みたくない",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "こんぶ",
            "text": "縮みたくない。。",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "ちくわ",
            "text": "逆に伸ばしたい！",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "コアラ",
            "text": "@こんぶ お二人より年上だと思うのでトホホ",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "こんぶ",
            "text": "わかる！のばしたい！",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "ちくわ",
            "text": "わかる！！",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "コアラ",
            "text": "@こんぶ でも80代の母は伸びてます",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "コアラ",
            "text": "なぜか",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "ちくわ",
            "text": "え？",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "こんぶ",
            "text": "私たちはアラサーと言われる世代です",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "ちくわ",
            "text": "まさかのその歳で成長期？",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "コアラ",
            "text": "@こんぶ おーやはり私上です！",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "こんぶ",
            "text": "@コアラ お母様！！！何かのんでます？",
            "mention_to": [
                "コアラ"
            ]
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "こんぶ",
            "text": "グルコサミン？",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "ちくわ",
            "text": "何か飲んでる、、、",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "ちくわ",
            "text": "笑わせるな！",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "ちくわ",
            "text": "でもお元気な証拠ですね！",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "こんぶ",
            "text": "笑わせにいったつもりは！",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "コアラ",
            "text": "@こんぶ それは飲んでるかもですが、毎週、ストレッチに通うようになってからです",
            "mention_to": [
                "こんぶ"
            ]
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "ちくわ",
            "text": "すごい！",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "コアラ",
            "text": "えっ？ジョークでしたか？笑",
            "mention_to": []
        }
