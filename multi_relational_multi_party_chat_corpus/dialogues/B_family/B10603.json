        {
            "utterance_id": 3,
            "interlocutor_id": "りんご",
            "text": "ドラマとか観てますか？",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "にんじん",
            "text": "最近コードブルー見ましたよ！",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "りんご",
            "text": "そうなんですね！",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "たまねぎ",
            "text": "私見れてないんです",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "にんじん",
            "text": "ドラマは録り溜めて見るタイプです",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "りんご",
            "text": "なかなか忙しいからね",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "たまねぎ",
            "text": "テレビ占領されてる",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "りんご",
            "text": "お子さんのことやったりするとタイムリーは無理ですよね",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "にんじん",
            "text": "暇な時に一気に見たいタイプなので！",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "りんご",
            "text": "なるほど！その方柄効率いいですよね",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "にんじん",
            "text": "そうなんですよ！",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "りんご",
            "text": "なかなか時間が大変ですよね",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "たまねぎ",
            "text": "続き気になりますしね！",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "にんじん",
            "text": "天気悪い日とか！",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "りんご",
            "text": "見たいけど続きがーって感じですよ",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "にんじん",
            "text": "そうなんです！",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "りんご",
            "text": "つい録画とか忘れてCMみてます笑",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "たまねぎ",
            "text": "あ、わかりますー",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "たまねぎ",
            "text": "飛ばせるのに",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "りんご",
            "text": "私は不適切にもほどがあるみてます",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "たまねぎ",
            "text": "あれ人気ですよね",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "りんご",
            "text": "昭和なんですよね",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "たまねぎ",
            "text": "そーそー",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "たまねぎ",
            "text": "気になってました！",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "りんご",
            "text": "あるあるですよね",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "にんじん",
            "text": "俺は知らなくてショックです！",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "りんご",
            "text": "でも今の子たちは冷めた目で見てます笑",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "にんじん",
            "text": "どんなドラマなんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "りんご",
            "text": "昭和にタイムスリップする話しです",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "にんじん",
            "text": "なるほど！",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "りんご",
            "text": "ちょっと今の時代をデスってます",
