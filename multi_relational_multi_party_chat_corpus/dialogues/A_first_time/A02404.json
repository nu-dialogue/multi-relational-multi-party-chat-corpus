        },
        {
            "utterance_id": 10,
            "interlocutor_id": "うどん",
            "text": "@おでん 最近まじめに始めました！",
            "mention_to": [
                "おでん"
            ]
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "うどん",
            "text": "和食ばかりですが...",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "おでん",
            "text": "@うどん まじにとは？",
            "mention_to": [
                "うどん"
            ]
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "うどん",
            "text": "1人暮らしだから、前はあんまりやってなくて...",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "はくまい",
            "text": "@うどん 和食、なに作られるんですか？",
            "mention_to": [
                "うどん"
            ]
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "おでん",
            "text": "そうなんですね",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "おでん",
            "text": "和食いいじゃないですか！",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "うどん",
            "text": "@はくまい 炊き込みごはんとか、茶碗蒸しとか作ります！",
            "mention_to": [
                "はくまい"
            ]
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "うどん",
            "text": "@おでん ありがとうございます！！！",
            "mention_to": [
                "おでん"
            ]
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "おでん",
            "text": "美味しそう",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "はくまい",
            "text": "茶碗蒸し大好きなんです！",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "おでん",
            "text": "美味しいですよねー",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "うどん",
            "text": "@はくまい 茶碗蒸しいいですよね！",
            "mention_to": [
                "はくまい"
            ]
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "うどん",
            "text": "皆さんは何を作りますか？",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "はくまい",
            "text": "チャーハンとか焼きそばとか",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "はくまい",
            "text": "簡単なのばかりです",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "おでん",
            "text": "私は今は主人と2人なので",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "うどん",
            "text": "@はくまい おいしそう！",
            "mention_to": [
                "はくまい"
            ]
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "おでん",
            "text": "週初めに。3品ほど惣菜を作り置きして",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "うどん",
            "text": "@おでん 作り置きいいですね！",
            "mention_to": [
                "おでん"
            ]
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "おでん",
            "text": "それを1週間で使い回しをして",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "はくまい",
            "text": "@うどん 袋麺のスープが余ったりしてるのを使って作ってます",
            "mention_to": [
                "うどん"
            ]
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "おでん",
            "text": "メインだけを変えていく感じです笑笑",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "おでん",
            "text": "@はくまい アイディアですね",
            "mention_to": [
                "はくまい"
            ]
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "うどん",
            "text": "アイディア大事ですよね！",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "おでん",
            "text": "そうですよね",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "はくまい",
            "text": "鍋すると締めのラーメンで余るんですよ",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "うどん",
            "text": "賢く使い回ししないとしんどいし〜",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "うどん",
            "text": "@はくまい なるほど！",
            "mention_to": [
                "はくまい"
            ]
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "おでん",
            "text": "@はくまい なるほど！",
            "mention_to": [
                "はくまい"
            ]
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "はくまい",
            "text": "他にもいろいろう使い道ありそう！",
