            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "こぐま",
            "text": "こんにちわ",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "なると",
            "text": "よろしくお願いします",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "いくら",
            "text": "お願いします！",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "こぐま",
            "text": "よろしくお願いします！！",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "なると",
            "text": "まだ夕飯に何を食べるか",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "なると",
            "text": "決めてないんですが",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "なると",
            "text": "もう決まりましたか？",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "こぐま",
            "text": "私もです",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "いくら",
            "text": "決まらないですよね、、",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "なると",
            "text": "ですよね",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "こぐま",
            "text": "まだ決まってないです",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "いくら",
            "text": "私もまだです！",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "いくら",
            "text": "スーパー行かないと",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "こぐま",
            "text": "@いくら 同じく",
            "mention_to": [
                "いくら"
            ]
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "なると",
            "text": "買出しに行かないとですね",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "いくら",
            "text": "ですね！",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "いくら",
            "text": "まとめ買いとかしたいですが",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "いくら",
            "text": "献立きめれないんですよね",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "なると",
            "text": "はい",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "こぐま",
            "text": "はい",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "なると",
            "text": "まとめ買いの方は",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "なると",
            "text": "献立決めてから",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "こぐま",
            "text": "私は週2回か3回しか買い出しに行かないから",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "なると",
            "text": "買うのですよね",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "いくら",
            "text": "@なると きっとそうですよね！",
            "mention_to": [
                "なると"
            ]
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "なると",
            "text": "@こぐま 毎日行かないですよね",
            "mention_to": [
                "こぐま"
            ]
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "こぐま",
            "text": "@なると いえ、特に考えないですあ",
            "mention_to": [
                "なると"
            ]
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "なると",
            "text": "@いくら やはりそうですよね",
            "mention_to": [
                "いくら"
            ]
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "いくら",
            "text": "@こぐま 特に決めずに買うんですか？",
            "mention_to": [
                "こぐま"
            ]
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "こぐま",
            "text": "はい",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "いくら",
            "text": "あるもので作る感じですかね？",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "こぐま",
            "text": "とりあえず、安いもので",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "こぐま",
            "text": "メインの数だけあれば",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "いくら",
            "text": "凄いです！！",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "こぐま",
            "text": "あとはあるもので…",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "いくら",
            "text": "なるほど、メインを数揃えればいいんですね",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "いくら",
            "text": "参考になります！",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "こぐま",
            "text": "週初めに",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "なると",
            "text": "うんうん",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "こぐま",
            "text": "お惣菜をまとめて作ります",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "なると",
            "text": "それがいいですね",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "こぐま",
            "text": "それを1週間で食べ尽くすという、",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "いくら",
            "text": "作り置きされるんですね！",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "いくら",
            "text": "わたしはすぐこれもうダメかも、と",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "いくら",
            "text": "何日も食べれないのですが",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "いくら",
            "text": "そんなことないですか？？",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "こぐま",
            "text": "なるほど",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "なると",
            "text": "悪くなるんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "なると",
            "text": "飽きる？",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "こぐま",
            "text": "副菜は、煮物系がら多くなりますねー",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "いくら",
            "text": "悪くなってるんじゃないかと思ってしまいます！",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "なると",
            "text": "煮物は持ちそうですよね",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "こぐま",
            "text": "火を通してれば",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "いくら",
            "text": "煮物系は大丈夫そうですよね！",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "いくら",
            "text": "なるほど、、",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "こぐま",
            "text": "はい",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "なると",
            "text": "和食がいいですね",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "いくら",
            "text": "子供が全然野菜食べないんですよね",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "なると",
            "text": "そうなんですね",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "いくら",
            "text": "作りがいがなくて悲しいです",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "こぐま",
            "text": "そうなんですね",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "こぐま",
            "text": "お子さんが小さい時は、私も毎日買い物に行ってました",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "いくら",
            "text": "そうなんですか！",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "なると",
            "text": "へえ",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "こぐま",
            "text": "今は夫婦二人なので、手抜きです…はい",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "なると",
            "text": "確かに",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "いくら",
            "text": "@こぐま なるほど、変わるんですね！",
            "mention_to": [
                "こぐま"
            ]
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "なると",
            "text": "毎日きっちり作るのは大変ですね",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "こぐま",
            "text": "@いくら 家族構成で変わってきますよね",
            "mention_to": [
                "いくら"
            ]
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "こぐま",
            "text": "若い子がいたら、量もいるし",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "いくら",
            "text": "子供は、肉か魚か焼いておけばいい気がしますね",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "こぐま",
            "text": "@いくら そうですよね",
            "mention_to": [
                "いくら"
            ]
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "なると",
            "text": "肉がいいですね",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "こぐま",
            "text": "肉、いいですよねー",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "いくら",
            "text": "@こぐま 野菜を食べないかわりに肉の量がすごいです",
            "mention_to": [
                "こぐま"
            ]
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "なると",
            "text": "いっぱい食べるんですね",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "こぐま",
            "text": "@いくら 笑笑笑",
            "mention_to": [
                "いくら"
            ]
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "いくら",
            "text": "そうなんです！",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "なると",
            "text": "食べ盛り",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "いくら",
            "text": "焼肉屋さんとか行くと金額怖いですね",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "こぐま",
            "text": "食べ盛りですか？",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "いくら",
            "text": "食べ盛りだと思います。",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "こぐま",
            "text": "そっかぁ",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "なると",
            "text": "焼肉屋は怖いですね",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "いくら",
            "text": "でも偏食なので大きくなるといいのですが！",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "なると",
            "text": "野菜食べない",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "こぐま",
            "text": "外食だと、高くつきそうですね",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "なると",
            "text": "スポーツ選手もいますね",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "いくら",
            "text": "外食もう凄いですよね金額が",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "こぐま",
            "text": "@なると そうなんですか？",
            "mention_to": [
                "なると"
            ]
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "いくら",
            "text": "@なると そうなんですか！！",
            "mention_to": [
                "なると"
            ]
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "なると",
            "text": "そうなんですね",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "いくら",
            "text": "大丈夫な気がしてきますね！",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "こぐま",
            "text": "野菜食べなくても、食べるようになると思いますが",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "なると",
            "text": "@いくら @こぐま いますよ",
            "mention_to": [
                "いくら",
                "こぐま"
            ]
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "いくら",
            "text": "わたしも小学生までピーマンだめでした",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "こぐま",
            "text": "食べなくても、ですね笑",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "なると",
            "text": "ピーマンは苦かったですね",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "いくら",
            "text": "でも今では大好きですもんね",
            "mention_to": []
        },
        {
            "utterance_id": 101,
