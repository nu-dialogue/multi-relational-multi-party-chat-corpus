{
    "dialogue_id": "A03304",
    "dialogue_type": "First time",
    "interlocutors": [
        "おでん",
        "がんも",
        "きなこ"
    ],
    "relationship": [],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "おでん",
            "text": "こんばんは",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "がんも",
            "text": "こんばんは。",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "きなこ",
            "text": "こんばんは",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "おでん",
            "text": "よろしくお願いします",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "きなこ",
            "text": "よろしくお願します！",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "がんも",
            "text": "よろしくお願いします。",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "おでん",
            "text": "趣味って何かありますか？",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "きなこ",
            "text": "@おでん 以前は旅行でした",
            "mention_to": [
                "おでん"
            ]
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "おでん",
            "text": "そうなんですね、いいですね",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "おでん",
            "text": "@きなこ 今はあまり行けませんか？",
            "mention_to": [
                "きなこ"
            ]
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "がんも",
            "text": "@おでん 趣味ないですね。",
            "mention_to": [
                "おでん"
            ]
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "きなこ",
            "text": "国内は近場で行きます！海外はいけなくなりました",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "おでん",
            "text": "@がんも あら？これから見つかるかもですね",
            "mention_to": [
                "がんも"
            ]
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "きなこ",
            "text": "@がんも そうですよ！",
            "mention_to": [
                "がんも"
            ]
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "おでん",
            "text": "海外！どこに行かれたことあるんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "がんも",
            "text": "キャンプ好きですが趣味まで行かないです。",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "きなこ",
            "text": "東南アジアやインド、アメリカとか",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "おでん",
            "text": "@がんも キャンプ！今、アウトドア流行りですね",
            "mention_to": [
                "がんも"
            ]
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "おでん",
            "text": "@きなこ あちこち行かれてたんですね",
            "mention_to": [
                "きなこ"
            ]
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "きなこ",
            "text": "@がんも 素敵！私は焚火も趣味です",
            "mention_to": [
                "がんも"
            ]
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "がんも",
            "text": "@きなこ すごい！",
            "mention_to": [
                "きなこ"
            ]
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "おでん",
            "text": "焚火が趣味？笑",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "おでん",
            "text": "それ、どういうことですか？",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "きなこ",
            "text": "そうですよ～焚火いいですよ～",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "きなこ",
            "text": "薪を割って、火を起こして、",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "がんも",
            "text": "焚火いいですよね。",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "おでん",
            "text": "うんうん",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "きなこ",
            "text": "気をくべて、眺めるだけ",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "きなこ",
            "text": "@がんも いいですよね",
            "mention_to": [
                "がんも"
            ]
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "おでん",
            "text": "へぇ～それがいいんですね",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "がんも",
            "text": "心休まる感じがして。",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "きなこ",
            "text": "そうなんです！無心になれます！",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "おでん",
            "text": "あー、アロマキャンドル的な、安らぎがあるんでしょうか？",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "きなこ",
            "text": "もしかしたらあるかもしれませんね！",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "がんも",
            "text": "火を見てるだけで落ち着くんです。",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "おでん",
            "text": "そんなのもあって、アウトドアも流行ってるのかもですね",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "がんも",
            "text": "ですね。",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "きなこ",
            "text": "みんな、忙しくて疲れてるのかも",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "おでん",
            "text": "キャンプ用品とかの専門店、増えてますもんね",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "がんも",
            "text": "多いですね。",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "きなこ",
            "text": "沢山ありますね！ほしくなってしまいます",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "おでん",
            "text": "この前、一人暮らしの息子のとこに行くのに",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "おでん",
            "text": "寝袋を買いに行ったら",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "おでん",
            "text": "ほんとピンキリでした！お値段が！",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "きなこ",
            "text": "笑笑",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "きなこ",
            "text": "高いのはびっくりする値段ですよね",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "おでん",
            "text": "外じゃないので、安いのを買いました",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "おでん",
            "text": "ちゃんとしたのは、高すぎて",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "がんも",
            "text": "高いですね。",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "おでん",
            "text": "びっくりする値段でした",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "きなこ",
            "text": "高いのだとお布団の方が安いかもですね",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "おでん",
            "text": "そうなんです！",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "おでん",
            "text": "その通りです！",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "がんも",
            "text": "それ、ありますね。",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "きなこ",
            "text": "キャンプ用品は、お値段しますよね",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "おでん",
            "text": "@がんも 少しづつ、揃えていったりしてますか？",
            "mention_to": [
                "がんも"
            ]
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "がんも",
            "text": "１０年くらい掛けて揃えてます。",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "がんも",
            "text": "気付いた田",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "おでん",
            "text": "ほぉー",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "きなこ",
            "text": "それはベテランですね！！",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "がんも",
            "text": "まちがえた。",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "おでん",
            "text": "ですね",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "おでん",
            "text": "@がんも 大丈夫です",
            "mention_to": [
                "がんも"
            ]
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "きなこ",
            "text": "大きなテントあこがれます",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "おでん",
            "text": "キャンプ用品って、一つ一つが高いから",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "がんも",
            "text": "気付いたらオイルランタン４個ありました。",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "おでん",
            "text": "10年計画なんですね",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "おでん",
            "text": "笑笑あら",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "きなこ",
            "text": "本格的！",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "きなこ",
            "text": "オイルって明かりが優しくていいですよね",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "おでん",
            "text": "そうなんですか",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "がんも",
            "text": "ランタンお勧めです。",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "おでん",
            "text": "キャンプって、子供の頃にしたっきりです",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "おでん",
            "text": "親がそういうの好きだと、子供は楽しいですね",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "がんも",
            "text": "久しぶりに去年行きました。",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "がんも",
            "text": "思い切って",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "きなこ",
            "text": "おお！どちらに行かれましたか？",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "おでん",
            "text": "わぁ、いいですね",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "がんも",
            "text": "テント新調しました。",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "きなこ",
            "text": "すごい！！",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "おでん",
            "text": "今って、いいテントありそうですね",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "きなこ",
            "text": "テントの値段に驚いたことあります！",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "おでん",
            "text": "昔は三角の、感じでした",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "がんも",
            "text": "@きなこ 長野の駒ヶ根のキャンプ場です。",
            "mention_to": [
                "きなこ"
            ]
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "おでん",
            "text": "長野県！いいなぁ",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "きなこ",
            "text": "私のイメージも三角です",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "きなこ",
            "text": "長野うらやましい！星空過ごそう",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "きなこ",
            "text": "凄そう",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "おでん",
            "text": "ねー、水も綺麗だし",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "おでん",
            "text": "空気も、",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "がんも",
            "text": "星奇麗でしたよ",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "おでん",
            "text": "ですよねぇ  ",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "きなこ",
            "text": "星、みたいですね～",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "おでん",
            "text": "思い切って行かれて良かったですね",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "がんも",
            "text": "長野に阿智村ってあるんですが",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "おでん",
            "text": "うんうん",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "きなこ",
            "text": "初めて聞きました",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "がんも",
            "text": "日本一星がきれいだそうです",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "おでん",
            "text": "私も初めて聞きました",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "きなこ",
            "text": "行ってみたいです！",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "おでん",
            "text": "へぇ～それはいいですね",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "おでん",
            "text": "満天の星空なんですね",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "きなこ",
            "text": "阿智村、メモしました笑",
            "mention_to": []
        }
