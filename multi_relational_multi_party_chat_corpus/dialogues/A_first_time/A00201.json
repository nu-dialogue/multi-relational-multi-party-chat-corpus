{
    "dialogue_id": "A00201",
    "dialogue_type": "First time",
    "interlocutors": [
        "しじみ",
        "おでん",
        "しらす"
    ],
    "relationship": [],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "しじみ",
            "text": "はじめまして、よろしくお願いいたします！",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "おでん",
            "text": "よろしくお願いします。",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "しらす",
            "text": "はじめまして。よろしくお願いいたします。",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "しじみ",
            "text": "みなさん三連休はどう過ごされましたか？",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "おでん",
            "text": "私は土曜は仕事でした、",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "しらす",
            "text": "三連休は子どもと公園めぐりをしていました。",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "しじみ",
            "text": "おでんさんお疲れ様でした、",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "おでん",
            "text": "ありがとう",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "しじみ",
            "text": "しらすさん、公園めぐりいいですね",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "しらす",
            "text": "おでんさんお疲れ様です",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "おでん",
            "text": "ありがとうございます",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "しらす",
            "text": "子どもが公園が好きなので休みの度に行っています。",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "おでん",
            "text": "公園巡り、いいお天気でしたか？",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "しじみ",
            "text": "連休中は公園遊びにいいお天気でしたか？",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "しらす",
            "text": "3連休天気がよかったので楽しかったです。",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "おでん",
            "text": "お子さんも楽しかったでしょうね",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "しじみ",
            "text": "私の地域も良いお天気でした！",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "おでん",
            "text": "私のところは夕方に少し雨が降ったりしてました",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "しらす",
            "text": "子どもも楽しそうでした。私もリフレッシュできました。",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "しじみ",
            "text": "夕立ちですかね？",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "おでん",
            "text": "そんな感じです",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "しじみ",
            "text": "お洗濯ものが濡れたりしませんでしか？",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "おでん",
            "text": "あー、乾いてなかったです",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "おでん",
            "text": "屋根があるので、何となくセーフ？",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "おでん",
            "text": "笑",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "しじみ",
            "text": "それなら良かったです！",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "おでん",
            "text": "親子でリフレッシュ、いいですね",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "しらす",
            "text": "最近乾燥がすごいのでこっちはずっと部屋干しです笑",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "しじみ",
            "text": "寒いと、外に出しても乾かないし迷いますよね",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "おでん",
            "text": "部屋干しで加湿も兼ねて、いいですね",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "しじみ",
            "text": "私は、去年の冬に浴室乾燥機を常に回しっぱなしにしたんですが",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "しらす",
            "text": "部屋中洗濯ものだらけで部屋が大変なことになっていますが笑",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "しじみ",
            "text": "電気代が大変なことになり、",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "しじみ",
            "text": "今年はなるべく外に干しています",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "おでん",
            "text": "あら、浴室乾燥は電気代高いって言いますもんね",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "しらす",
            "text": "最近電気代高いから余計つらいですね",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "おでん",
            "text": "ですよね",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "しじみ",
            "text": "電気代がちょうど高騰した時期だったので、",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "しじみ",
            "text": "余計に高かったですね〜",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "しじみ",
            "text": "しらすさんは、部屋干しで",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "しじみ",
            "text": "サーキュレーターなど使われてますか？",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "しらす",
            "text": "エアコンの下に干しています",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "しじみ",
            "text": "なるほど、一石二鳥ですね",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "おでん",
            "text": "ほんと、それはいいですね",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "しらす",
            "text": "エアコンに引っかける物干しがあるのでずっとそこに干しています笑",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "しじみ",
            "text": "おでんさんは、干し場所に屋根があるなら便利ですね！",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "おでん",
            "text": "うちは、海が近く",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "おでん",
            "text": "冬でも風で乾きます",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "おでん",
            "text": "それは有難く思います",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "しらす",
            "text": "いいですね。",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "しじみ",
            "text": "エアコンに引っ掛ける物干し、",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "しじみ",
            "text": "気になります！",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "おでん",
            "text": "私も",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "しじみ",
            "text": "ネットで調べてみよう！",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "しらす",
            "text": "結構便利ですよ",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "おでん",
            "text": "便利なものがあるんですね",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "おでん",
            "text": "しじみさんは三連休どう過ごされましたか？",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "しじみ",
            "text": "良い情報が知れました！",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "しらす",
            "text": "梅雨時期にも活躍します",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "しじみ",
            "text": "私は確定申告準備をしたり、",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "しじみ",
            "text": "子供とショッピングモールに行きました！",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "おでん",
            "text": "ショッピングモールもすごい人じゃなかっですか？",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "しらす",
            "text": "確定申告、準備から進んでいないです笑",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "しらす",
            "text": "ショッピングモールいいですね。",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "しじみ",
            "text": "ショッピングモールは混んでいたので、すぐに退散しました！笑",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "しじみ",
            "text": "確定申告、気が重いですよね〜",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "しらす",
            "text": "しばらく外に買い物に行けていないので行きたいです",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "おでん",
            "text": "買い物も、ストレス発散になるますもんね",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "しじみ",
            "text": "買い物は、気分転換にちょうどいいですよね！",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "おでん",
            "text": "楽しいし",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "しらす",
            "text": "見ているだけでも楽しいです",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "しじみ",
            "text": "私は海なし県民なので、",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "おでん",
            "text": "そうなんですね",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "しじみ",
            "text": "おでんさんのように海を気分転換に見に行きたいですねー",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "しらす",
            "text": "私も出身は海のないところでした",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "しじみ",
            "text": "そうなんですね！",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "おでん",
            "text": "そんないい風景でも⋯汗",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "しじみ",
            "text": "あら、笑",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "しらす",
            "text": "小学校くらいまで海を見たことなかったです笑",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "おでん",
            "text": "そーなんですか！",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "しじみ",
            "text": "そしたら、",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "しらす",
            "text": "最初、波に驚きましたね",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "しじみ",
            "text": "初めての海は感動しましたか？",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "おでん",
            "text": "笑",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "しじみ",
            "text": "なるほど、波！",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "しらす",
            "text": "近くの川とは比べものにならなかったです笑",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "おでん",
            "text": "海の歌詞通りでしたか？",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "しじみ",
            "text": "海は広いな、",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "しじみ",
            "text": "大きいな、ですね！",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "しらす",
            "text": "そうです、まさに歌のとおり",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "しらす",
            "text": "子どもながら感動しました",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "おでん",
            "text": "笑、可愛いですね、",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "しじみ",
            "text": "ですね！ほっこりします",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "おでん",
            "text": "何となく想像すると癒される話ですね",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "しじみ",
            "text": "冬の海はきっと寒そうですね",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "おでん",
            "text": "何となく日本海は寒そうな",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "しらす",
            "text": "確かに",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "おでん",
            "text": "うちは瀬戸内なので",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "しじみ",
            "text": "暖かいんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "おでん",
            "text": "温暖ですね。",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "しらす",
            "text": "あー、温かそうですね",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "しじみ",
            "text": "いいですね〜！",
            "mention_to": []
        }
    ],
