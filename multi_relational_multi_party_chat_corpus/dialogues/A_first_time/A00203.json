    "relationship": [],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "しじみ",
            "text": "こんにちは、",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "しじみ",
            "text": "よろしくお願いします",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "おでん",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "しらす",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "しらす",
            "text": "よろしくお願いいたします。",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "おでん",
            "text": "よろしくお願いいたします",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "しじみ",
            "text": "明日はバレンタインですね！",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "しらす",
            "text": "忘れていました笑",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "おでん",
            "text": "あー、笑",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "しじみ",
            "text": "あら！笑",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "おでん",
            "text": "今日、買いに行きます笑",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "しじみ",
            "text": "チョコつくりますか？",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "しじみ",
            "text": "おでんさんは、買いに行かれるんですね！",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "おでん",
            "text": "私は作りませんが、作られますか？",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "しらす",
            "text": "買いますね～",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "しじみ",
            "text": "娘とつくります！",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "しじみ",
            "text": "うまく行くといいんですが…",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "おでん",
            "text": "ご主人に？",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "しじみ",
            "text": "そうです！",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "しらす",
            "text": "作るの楽しそうですね",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "おでん",
            "text": "親子で作るっていいですね",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "しらす",
            "text": "うちは男だらけなので",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "しじみ",
            "text": "きっと、キッチンがベタベタになります！笑",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "しらす",
            "text": "二人の好きなもの買って渡します",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "おでん",
            "text": "しじみさんのご主人は嬉しいでしょうね",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "しじみ",
            "text": "しらすさんのお子さんは、貰う側なのですね！",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "しらす",
            "text": "もらう側です",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "しじみ",
            "text": "おでんさんは、ご主人にですか？",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "おでん",
            "text": "貰って帰ってくるかもですね",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "しらす",
            "text": "もうこれがいいって言われています笑",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "おでん",
            "text": "そうですね、主人にです",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "しじみ",
            "text": "指定あり！すごい！笑",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "おでん",
            "text": "笑",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "しらす",
            "text": "今、可愛いチョコたくさんありますからね",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "しじみ",
            "text": "みなさんのご家族は、",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "しじみ",
            "text": "甘いものお好きですか？",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "しらす",
            "text": "買い物行く度これがいいって笑",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "おでん",
            "text": "うちはあんまり甘いものは好きじゃないです",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "しじみ",
            "text": "しっかり買い物中にリサーチしてるんですね！笑",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "しらす",
            "text": "うちはみんな甘党です",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "しじみ",
            "text": "甘いものお好きじゃないと、",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "しじみ",
            "text": "チョコにするか迷いますね",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "おでん",
            "text": "欲しいって言ってくれるの、可愛いです",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "しじみ",
            "text": "可愛いですね！",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "おでん",
            "text": "うちはビールなら間違いないので笑",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "しじみ",
            "text": "なるほど！笑",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "おでん",
            "text": "もう、バレンタイン関係ないやん！って感じです",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "しらす",
            "text": "ビールいいですね！",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "しじみ",
            "text": "チョコ味のビールとかあるんですかね？？",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "しじみ",
            "text": "さすがにないかな",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "おでん",
            "text": "あるんでしょうか？笑",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "おでん",
            "text": "おふたりは飲まれますか？",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "しじみ",
            "text": "お酒は好きですが、",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "しらす",
            "text": "チョコ味のビールは聞いたことないです",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "しじみ",
            "text": "普段はなかなか飲む機会がないですね",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "しらす",
            "text": "お酒好きです",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "しじみ",
            "text": "おでんさんも、飲まれますか？",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "おでん",
            "text": "私は飲まないんです",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "おでん",
            "text": "しらすさんは好きなんですね",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "しじみ",
            "text": "そうなんですね！",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "しらす",
            "text": "でも飲む量は減りました",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "おでん",
            "text": "お酒を飲める人が羨ましいです",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "しじみ",
            "text": "飲むと気持ち悪くなるタイプですか？",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "おでん",
            "text": "私はアレルギーで",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "しらす",
            "text": "次の日気持ち悪くなるタイプです",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "おでん",
            "text": "じんましんが出るんです",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "しじみ",
            "text": "あら〜、それは切ないですね",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "しじみ",
            "text": "蕁麻疹！大変だ！",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "しらす",
            "text": "あら、それはおつらい",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "おでん",
            "text": "しらすさんは好きなのに、次の日がダメなんですね",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "おでん",
            "text": "色々あるんですね",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "しじみ",
            "text": "昨日は、久々に男梅サワーを飲みました！",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "しらす",
            "text": "なぜか次の日体調悪くなります",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "おでん",
            "text": "おー、美味しかったですか？",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "しらす",
            "text": "休みの前の日しか飲めません笑",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "しじみ",
            "text": "男梅サワー、美味しいんです！",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "おでん",
            "text": "笑笑",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "しらす",
            "text": "男梅サワー、おいしそう",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "しじみ",
            "text": "しらすさん、大変ですね",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "しじみ",
            "text": "男梅の、あの顔を見ながら晩酌しました。笑",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "おでん",
            "text": "楽しそう笑",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "おでん",
            "text": "渋い顔ですもんね",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "しじみ",
            "text": "梅干しは、みなさんお好きですか？",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "しらす",
            "text": "私も今度買ってみます",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "おでん",
            "text": "大好きです",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "しらす",
            "text": "梅干苦手です",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "しじみ",
            "text": "しらすさん、ぜひ！おすすめです！",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "しじみ",
            "text": "おー、意見が分かれましたね",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "しらす",
            "text": "梅風味とか梅酒は好きですが",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "しらす",
            "text": "梅の食感が苦手です",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "おでん",
            "text": "特に好きなのははちみつ梅干しです",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "しじみ",
            "text": "食感、独特ですもんね",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "しじみ",
            "text": "はちみつ梅干し、美味しいですよね！",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "おでん",
            "text": "食べやすいです",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "おでん",
            "text": "そっかぁ、食感がダメなんですね",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "しらす",
            "text": "カリカリ梅とかならいけます笑",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "しじみ",
            "text": "カリカリ梅、美味しいですよね！",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "しじみ",
            "text": "気づくと永遠に食べちゃう！笑",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "しらす",
            "text": "分かります",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "おでん",
            "text": "カリカリ梅は",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "おでん",
            "text": "おやつですか？",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "しじみ",
            "text": "おやつかな？",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "しらす",
            "text": "おやつですね",
            "mention_to": []
        }
