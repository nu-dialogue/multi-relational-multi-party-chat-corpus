        },
        {
            "utterance_id": 96,
            "interlocutor_id": "しじみ",
            "text": "確かに！",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "なると",
            "text": "コラボしまくりですね",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "しじみ",
            "text": "結構びっくりするようなコラボしてますよね！",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "なると",
            "text": "そうですね",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "しじみ",
            "text": "そことしちゃうの？！みたいな",
            "mention_to": []
        },
