            "interlocutor_id": "ししとう",
            "text": "こんばんは！ ",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "あずき",
            "text": "こんばんは ",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "たらこ",
            "text": "こんばんは ",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "ししとう",
            "text": "今日は暖かかったですね！ ",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "あずき",
            "text": "ですね ",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "たらこ",
            "text": "暖かかったです ",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "あずき",
            "text": "@ししとう 嬉しいです",
            "mention_to": [
                "ししとう"
            ]
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "あずき",
            "text": "@たらこ 毎日暖かいといいですね",
            "mention_to": [
                "たらこ"
            ]
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "あずき",
            "text": "花粉はすごいのかな？ ",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "たらこ",
            "text": "@あずき ですね！",
            "mention_to": [
                "あずき"
            ]
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "あずき",
            "text": "花粉症ですか？ ",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "たらこ",
            "text": "もう飛んでますかね？ ",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "ししとう",
            "text": "@あずき 今日はそうでもなかったですが、心配です。",
            "mention_to": [
                "あずき"
            ]
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "あずき",
            "text": "@たらこ 何か対策していますか？",
            "mention_to": [
                "たらこ"
            ]
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "たらこ",
            "text": "@あずき 私は花粉症ではないです",
            "mention_to": [
                "あずき"
            ]
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "あずき",
            "text": "@たらこ そうでしたか！",
            "mention_to": [
                "たらこ"
            ]
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "ししとう",
            "text": "@たらこ うらやましいです！",
            "mention_to": [
                "たらこ"
            ]
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "あずき",
            "text": "私も今のところ大丈夫です ",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "あずき",
            "text": "@ししとう ひどいですか？",
            "mention_to": [
                "ししとう"
            ]
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "たらこ",
            "text": "@ししとう 花粉症ですか？",
            "mention_to": [
                "ししとう"
            ]
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "ししとう",
            "text": "@あずき @たらこ",
            "mention_to": [
                "あずき",
                "たらこ"
            ]
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "ししとう",
            "text": "花粉症です。 ",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "あずき",
            "text": "そうなんですね ",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "あずき",
            "text": "辛いですね ",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "たらこ",
            "text": "薬のんでるんですか？ ",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "ししとう",
            "text": "でも今日は症状ないのでこれからなのかなとドキドキしてます。 ",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "あずき",
            "text": "うちも娘がひどいです ",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "たらこ",
            "text": "そうなんですね ",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "ししとう",
            "text": "@たらこ 薬はまだ今年は飲んでないです。",
            "mention_to": [
                "たらこ"
            ]
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "ししとう",
            "text": "@あずき 娘さん！大変ですね",
            "mention_to": [
                "あずき"
            ]
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "あずき",
            "text": "@ししとう ひどくならいないといいですね",
            "mention_to": [
                "ししとう"
            ]
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "たらこ",
            "text": "@ししとう ほんとにです",
            "mention_to": [
                "ししとう"
            ]
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "あずき",
            "text": "そうなんです。 ",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "あずき",
            "text": "薬飲んでます ",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "あずき",
            "text": "春も秋も花粉症です ",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "たらこ",
            "text": "大変な子は小さい時から ",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "ししとう",
            "text": "@あずき 薬は市販薬ですか？病院のですか？",
            "mention_to": [
                "あずき"
            ]
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "たらこ",
            "text": "しんどそうですもんね ",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "あずき",
            "text": "@ししとう 病院に行ってます",
            "mention_to": [
                "ししとう"
            ]
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "あずき",
            "text": "耳鼻科です ",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "たらこ",
            "text": "うちも娘がひどい花粉症で ",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "あずき",
            "text": "鼻水げひどくて眠れないようです ",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "ししとう",
            "text": "そうなんですね！ ",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "たらこ",
            "text": "専用メガネかかせないです ",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "たらこ",
            "text": "鼻水つらいですね ",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "あずき",
            "text": "@たらこ 専用メガネ！！",
            "mention_to": [
                "たらこ"
            ]
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "あずき",
            "text": "@たらこ 辛いですね",
            "mention_to": [
                "たらこ"
            ]
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "ししとう",
            "text": "@たらこ 専用メガネ！すごいですね！",
            "mention_to": [
                "たらこ"
            ]
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "たらこ",
            "text": "@あずき そうなんです",
            "mention_to": [
                "あずき"
            ]
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "あずき",
            "text": "メガネは効果ありますか？ ",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "たらこ",
            "text": "@ししとう 今は可愛い色もあるみたいで",
            "mention_to": [
                "ししとう"
            ]
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "たらこ",
            "text": "メガネあるほうが楽みたいです ",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "あずき",
            "text": "@たらこ いいですね！かわいい色",
            "mention_to": [
                "たらこ"
            ]
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "あずき",
            "text": "やっぱり違うんですね ",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "たらこ",
            "text": "みたいです！ ",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "あずき",
            "text": "今年は飛散量がすごいのかな ",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "あずき",
            "text": "怖いですね ",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "ししとう",
            "text": "@たらこ おお！可愛い色いいですね！見た目も大事ですね！",
            "mention_to": [
                "たらこ"
            ]
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "たらこ",
            "text": "@ししとう そうなんです！",
            "mention_to": [
                "ししとう"
            ]
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "あずき",
            "text": "二月も半分過ぎました ",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "ししとう",
            "text": "@あずき 暖かいから余計に花粉量心配ですね",
            "mention_to": [
                "あずき"
            ]
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "あずき",
            "text": "速いですね ",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "たらこ",
            "text": "やっぱり暖かいとひどくなるんですかね？ ",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "あずき",
            "text": "@たらこ ひどそうですよね",
            "mention_to": [
                "たらこ"
            ]
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "あずき",
            "text": "まだ２月なのに ",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "たらこ",
            "text": "暖かいと母としては洗濯がすぐ乾くので ",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "たらこ",
            "text": "助かりますよね ",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "あずき",
            "text": "@たらこ 洗濯乾くのはいいですよね",
            "mention_to": [
                "たらこ"
            ]
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "たらこ",
            "text": "花粉症さんはつらいですけど ",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "ししとう",
            "text": "確かに洗濯物助かります ",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "あずき",
            "text": "気分も春は上がりますね ",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "ししとう",
            "text": "冬があっという間な感じですね ",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "あずき",
            "text": "冬が短いですね ",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "たらこ",
            "text": "@あずき あがります！",
            "mention_to": [
                "あずき"
            ]
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "あずき",
            "text": "今年はかなり暖冬ですよね ",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "たらこ",
            "text": "最近秋がなくなってきたような ",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "たらこ",
            "text": "気がします ",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "あずき",
            "text": "@たらこ わかります",
            "mention_to": [
                "たらこ"
            ]
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "ししとう",
            "text": "@たらこ 確かに",
            "mention_to": [
                "たらこ"
            ]
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "あずき",
            "text": "秋が短い ",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "たらこ",
            "text": "ね～！ ",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "あずき",
            "text": "夏が長い ",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "たらこ",
            "text": "春はお花見とかしますか？ ",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "ししとう",
            "text": "@たらこ お散歩程度です",
            "mention_to": [
                "たらこ"
            ]
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "あずき",
            "text": "近所に桜並木があるので散歩します ",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "たらこ",
            "text": "桜並木ですか！ ",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "たらこ",
            "text": "いいですね！ ",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "ししとう",
            "text": "@あずき いいですね！",
            "mention_to": [
                "あずき"
            ]
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "あずき",
            "text": "@たらこ そうなんですよ",
            "mention_to": [
                "たらこ"
            ]
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "ししとう",
            "text": "今年はお弁当持ってでかけようかな？ ",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "あずき",
            "text": "観光客も来ます ",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "あずき",
            "text": "@ししとう お弁当いいですね",
            "mention_to": [
                "ししとう"
            ]
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "たらこ",
            "text": "@ししとう お散歩いいですね",
            "mention_to": [
                "ししとう"
            ]
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "あずき",
            "text": "お花見弁当美味しそう ",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "たらこ",
            "text": "@あずき 長い桜並木ですか？",
            "mention_to": [
                "あずき"
            ]
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "あずき",
            "text": "@たらこ そうですね　長いです",
            "mention_to": [
                "たらこ"
            ]
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "ししとう",
            "text": "@あずき ステキだろうな～",
            "mention_to": [
                "あずき"
            ]
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "あずき",
            "text": "ソメイヨシノが咲きます ",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "たらこ",
            "text": "素敵そうですよね～ ",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "あずき",
            "text": "映えますよ ",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "たらこ",
            "text": "写真スポットですね！ ",
