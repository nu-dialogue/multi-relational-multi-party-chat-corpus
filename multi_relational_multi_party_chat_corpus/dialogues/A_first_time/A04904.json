    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "おでん",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "げんまい",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "おでん",
            "text": "よろしくお願いします",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "まりも",
            "text": "こんにちは～",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "げんまい",
            "text": "はじめまして",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "まりも",
            "text": "よろしくお願いします",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "おでん",
            "text": "兄弟姉妹っていますか？",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "げんまい",
            "text": "妹がいます。",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "まりも",
            "text": "兄が1人います",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "おでん",
            "text": "私も兄が",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "げんまい",
            "text": "@おでん います？",
            "mention_to": [
                "おでん"
            ]
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "おでん",
            "text": "1人",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "おでん",
            "text": "います",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "おでん",
            "text": "性格とか全然違って",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "げんまい",
            "text": "お二人はお兄さんがいらっしゃるんですねー",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "げんまい",
            "text": "兄弟でも個性ありますよねー",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "おでん",
            "text": "同じ親なのに、なんでこんなに違うんだろうって",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "まりも",
            "text": "同性のきょうだいって憧れます",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "げんまい",
            "text": "あるある",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "げんまい",
            "text": "うちの子見ててもそう思います。",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "まりも",
            "text": "@おでん それは本当に思います",
            "mention_to": [
                "おでん"
            ]
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "おでん",
            "text": "@まりも 私も！",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "げんまい",
            "text": "妹とは、",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "げんまい",
            "text": "小さい時はよく喧嘩したけど、",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "おでん",
            "text": "お姉ちゃんか、妹欲しかった",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "げんまい",
            "text": "今は、仲良しです。",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "まりも",
            "text": "大人になってからいいですよね",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "おでん",
            "text": "歳とってから、いいっ言いますよね",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "おでん",
            "text": "一緒に出かけたり",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "まりも",
            "text": "兄とは基本連絡とることないです",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "まりも",
            "text": "うんうん",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "げんまい",
            "text": "あら、寂しい。",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "おでん",
            "text": "@まりも 同じくです",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "げんまい",
            "text": "妹と一緒にライブに行ったりします。",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "まりも",
            "text": "遺恨がいろいろありまして",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "おでん",
            "text": "笑  実は私もです笑",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "まりも",
            "text": "@げんまい 仲良しですね～",
            "mention_to": [
                "げんまい"
            ]
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "げんまい",
            "text": "あらー",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "おでん",
            "text": "@げんまい いいですねー",
            "mention_to": [
                "げんまい"
            ]
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "げんまい",
            "text": "京都と東京で離れているのでめったに会いませんがね",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "おでん",
            "text": "あー、遠いんですね",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "まりも",
            "text": "そうなんですか",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "げんまい",
            "text": "LINEでやりとりはしますね。",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "げんまい",
            "text": "時々",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "おでん",
            "text": "でも、お互い、行き来が出来たら",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "おでん",
            "text": "最高の居住地ですねー",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "げんまい",
            "text": "ですねー",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "おでん",
            "text": "羨ましい",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "げんまい",
            "text": "お兄さんとは、会う機会ってどれくらいですか？",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "おでん",
            "text": "私は、今年はまだ会ってません笑",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "まりも",
            "text": "何もなければまったく会わないです",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "まりも",
            "text": "入院とか法事とか",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "おでん",
            "text": "冠婚葬祭ぐらいでしょうか笑",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "げんまい",
            "text": "まだ今年はじまったばかりですから",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "げんまい",
            "text": "なるほどー",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "おでん",
            "text": "でも、まぁ、行く機会も段々減ってきたしねー",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "まりも",
            "text": "法事、前回は日程知らされなくて",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "まりも",
            "text": "怒りました",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "げんまい",
            "text": "え？",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "おでん",
            "text": "えー",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "げんまい",
            "text": "それは、",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "まりも",
            "text": "そういう感じです",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "おでん",
            "text": "そらは",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "おでん",
            "text": "それは",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "げんまい",
            "text": "そういう感じですか。",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "げんまい",
            "text": "距離を感じますね",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "おでん",
            "text": "ダメですね",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "まりも",
            "text": "ダメなんですよね…",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "おでん",
            "text": "今はLINEあると",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "おでん",
            "text": "遠くても",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "おでん",
            "text": "遠く感じませんね",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "げんまい",
            "text": "そうそう。",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "げんまい",
            "text": "すぐ連絡します。",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "げんまい",
            "text": "つまらない事で",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "おでん",
            "text": "電話も無料でできたり",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "まりも",
            "text": "それが大事ですよね～",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "げんまい",
            "text": "ですよねー",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "まりも",
            "text": "普段からの関係が",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "おでん",
            "text": "なんなら、ビデオ通話もできるし",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "おでん",
            "text": "その点はいい時代ですね",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "げんまい",
            "text": "必要ならビデオ通話もしますね。",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "まりも",
            "text": "ですね",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "げんまい",
            "text": "この服いる？とか",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "おでん",
            "text": "いいなぁ",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "まりも",
            "text": "いいですね～",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "おでん",
            "text": "ウンウン",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "げんまい",
            "text": "でも、遠いから仲良しでいられるような気もします",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "まりも",
            "text": "ある程度距離あったほうが",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "おでん",
            "text": "それも言えるかも",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "まりも",
            "text": "大事にできるかも？",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "げんまい",
            "text": "母と妹は一緒に住んでいるので、時々喧嘩します。",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "おでん",
            "text": "親子って",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "まりも",
            "text": "近すぎると",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "げんまい",
            "text": "距離は大事ですね。",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "まりも",
            "text": "いろいろ目につくんですかね",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "おでん",
            "text": "言いたいこと言っちゃいますもんね",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "げんまい",
            "text": "そうそう",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "おでん",
            "text": "遠慮なくね",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "げんまい",
            "text": "そう。遠慮がないから。",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "おでん",
            "text": "でも、それでもやっぱり",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "おでん",
            "text": "肉親でしょうね",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "げんまい",
            "text": "でも、すぐ普通に戻りますけどね。",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "おでん",
            "text": "ウンウン",
            "mention_to": []
