        },
        {
            "utterance_id": 14,
            "interlocutor_id": "きなこ",
            "text": "私はウサギ飼ってます",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "きなこ",
            "text": "文鳥ですか？",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "おでん",
            "text": "そうです",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "きなこ",
            "text": "あ！！",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "おでん",
            "text": "子供が飼い始めて、",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "まりも",
            "text": "うさぎもいいですねぇ",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "きなこ",
            "text": "なるほど。猫と鳥か",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "おでん",
            "text": "私が世話してます",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "きなこ",
            "text": "わかります・・",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "おでん",
            "text": "うさぎ可愛いですよね",
