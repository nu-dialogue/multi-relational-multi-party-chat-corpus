    "relationship": [],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "あずき",
            "text": "おはようございます！",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "がんも",
            "text": "おはようございます。",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "きなこ",
            "text": "おはようございます！",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "あずき",
            "text": "午後は娘のバスケの試合を見に行くんです",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "きなこ",
            "text": "お！素敵ですね",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "あずき",
            "text": "自分は運動しないんですが",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "がんも",
            "text": "いいですね",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "きなこ",
            "text": "私、中高バスケしてました",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "きなこ",
            "text": "遠い昔ですが",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "あずき",
            "text": "@きなこ そうなんですね！",
            "mention_to": [
                "きなこ"
            ]
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "あずき",
            "text": "私はバスケできなくて",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "がんも",
            "text": "バッシュに憧れてました。",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "あずき",
            "text": "教えてあげられません",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "あずき",
            "text": "@がんも 憧れていたんですね",
            "mention_to": [
                "がんも"
            ]
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "あずき",
            "text": "わかります！",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "きなこ",
            "text": "ちょっと憧れますよね",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "あずき",
            "text": "かっこいいですよね",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "がんも",
            "text": "かっこいいですよね",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "あずき",
            "text": "バスケってかっこいいですよね",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "あずき",
            "text": "娘はまだ始めたばかりなので",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "あずき",
            "text": "上手くないんですが",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "きなこ",
            "text": "@あずき これからですよ！",
            "mention_to": [
                "あずき"
            ]
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "あずき",
            "text": "周りの子はすごいです",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "きなこ",
            "text": "今の子は小さいころからやりますからね",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "きなこ",
            "text": "でも大丈夫",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "あずき",
            "text": "@きなこ 今でもバスケしますか？",
            "mention_to": [
                "きなこ"
            ]
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "きなこ",
            "text": "やりたい気持ちもありますが・・",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "きなこ",
            "text": "最近はスキーくらいです",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "きなこ",
            "text": "スポーツは",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "あずき",
            "text": "@きなこ スキーいいですね",
            "mention_to": [
                "きなこ"
            ]
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "あずき",
            "text": "小学校で毎年スキー教室があります",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "がんも",
            "text": "スキーも憧れていました",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "あずき",
            "text": "私も子供の頃はスキーしてました",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "あずき",
            "text": "今は転んだら怪我しそうで",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "あずき",
            "text": "できません",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "がんも",
            "text": "恐いですよね",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "きなこ",
            "text": "意外にけがは大丈夫ですよ～",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "あずき",
            "text": "@きなこ 家族で行くんですか？",
            "mention_to": [
                "きなこ"
            ]
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "きなこ",
            "text": "そうです",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "きなこ",
            "text": "子供たちつれて",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "あずき",
            "text": "@きなこ 家族で滑るの憧れます",
            "mention_to": [
                "きなこ"
            ]
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "あずき",
            "text": "晴れた日は最高ですね！",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "きなこ",
            "text": "そうですね～",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "きなこ",
            "text": "気持ちいいですね！",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "きなこ",
            "text": "ただ・・",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "きなこ",
            "text": "スキー場遠くて",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "がんも",
            "text": "何処に行きますか？",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "あずき",
            "text": "そうなんですね",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "きなこ",
            "text": "群馬あたりでしょうか",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "あずき",
            "text": "@きなこ 群馬ですか",
            "mention_to": [
                "きなこ"
            ]
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "きなこ",
            "text": "がんばって新潟",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "あずき",
            "text": "新潟！！",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "きなこ",
            "text": "そうです！",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "がんも",
            "text": "遠いですね",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "あずき",
            "text": "すごい！",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "あずき",
            "text": "雪質いいですか？",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "きなこ",
            "text": "遠くて・・",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "きなこ",
            "text": "いいです！雪質！",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "あずき",
            "text": "そうなんですねー",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "きなこ",
            "text": "今年はいけなくて",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "きなこ",
            "text": "残念",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "あずき",
            "text": "それは残念",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "がんも",
            "text": "残念",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "きなこ",
            "text": "暖冬でしたしね",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "あずき",
            "text": "なかなか行くのも大変ですよね",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "あずき",
            "text": "準備とかあるし",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "がんも",
            "text": "雪ないですもんね",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "きなこ",
            "text": "そうなんですよ",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "きなこ",
            "text": "雪はないし、",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "あずき",
            "text": "@がんも 雪がないのかー",
            "mention_to": [
                "がんも"
            ]
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "あずき",
            "text": "雨ですもね",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "あずき",
            "text": "今年は雨が多い気がします",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "がんも",
            "text": "確かに",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "きなこ",
            "text": "そうですね。雨かな",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "あずき",
            "text": "雨だと滑れない",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "あずき",
            "text": "子供のスキー教室も中止になりました",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "きなこ",
            "text": "え！",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "がんも",
            "text": "残念",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "きなこ",
            "text": "残念ですね・・",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "あずき",
            "text": "天気が悪くて",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "あずき",
            "text": "中止になったんです",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "あずき",
            "text": "バス代やら物価の上昇で",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "あずき",
            "text": "来年はやるかどうか",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "あずき",
            "text": "わからないみたいです",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "きなこ",
            "text": "そうなんですか・・悲しいですね",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "きなこ",
            "text": "何でも高いですよね",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "がんも",
            "text": "子どもたちはやりたいですよね",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "きなこ",
            "text": "ですよね。",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "あずき",
            "text": "@がんも 悲しんでました",
            "mention_to": [
                "がんも"
            ]
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "あずき",
            "text": "連れて行ってあげたいんですけどね",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "あずき",
            "text": "なかなか予定が合わなくて",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "きなこ",
            "text": "@あずき スキー場は近いですか？",
            "mention_to": [
                "あずき"
            ]
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "あずき",
            "text": "@きなこ こちらは近いです",
            "mention_to": [
                "きなこ"
            ]
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "あずき",
            "text": "といっても2時間くらい",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "あずき",
            "text": "車で",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "あずき",
            "text": "長野なので近いですよ",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "きなこ",
            "text": "いいですね！長野！",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "あずき",
            "text": "寒いですー",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "きなこ",
            "text": "子供のスキー教室、菅平でした",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "あずき",
            "text": "おお！！いいですね",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "あずき",
            "text": "夏もいいですよ",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "がんも",
            "text": "いいですね",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "あずき",
            "text": "外国人観光客",
            "mention_to": []
        },
        {
            "utterance_id": 103,
