            "mention_to": [
                "きなこ"
            ]
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "しじみ",
            "text": "うんうん",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "きなこ",
            "text": "そう！外国の様にしたい欲求から・・",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "しじみ",
            "text": "面白い",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "きなこ",
            "text": "かっこつけてパンにしてます",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "なると",
            "text": "かっこいいですね",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "しじみ",
            "text": "理由がおもしろいですね！",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "きなこ",
            "text": "笑ですかね～",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "なると",
            "text": "朝ならするかもしれません",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "しじみ",
            "text": "でもパンもあいますよね！",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "しじみ",
            "text": "むしろパンのほうがあうかも",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "なると",
            "text": "洋食ですものね",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "きなこ",
            "text": "一度パンにすると",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "きなこ",
            "text": "やめられなくなって",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "しじみ",
            "text": "ふむふむ",
            "mention_to": []
        },
        {
            "utterance_id": 99,
