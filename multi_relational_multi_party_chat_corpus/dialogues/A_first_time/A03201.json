        "うどん"
    ],
    "relationship": [],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "まりも",
            "text": "こんにちは～",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "きなこ",
            "text": "こんにちは！",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "うどん",
            "text": "こんにちは！",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "まりも",
            "text": "今日あったかいですね",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "うどん",
            "text": "あったか〜い！！！",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "きなこ",
            "text": "暑いくらいでした！",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "うどん",
            "text": "すっかり春ですね〜！",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "まりも",
            "text": "でも明日からまた寒くなるとか",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "きなこ",
            "text": "ええ・・",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "まりも",
            "text": "体がついてかないですね",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "うどん",
            "text": "また寒くなるんだ！",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "うどん",
            "text": "風邪ひきそうですね",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "きなこ",
            "text": "体調に気を付けないと・・",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "うどん",
            "text": "寒暖差でやられちゃいますね",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "まりも",
            "text": "ね～",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "うどん",
            "text": "春は旅行とかお花見とか、何かご予定ありますか？",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "まりも",
            "text": "全然考えてないです",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "きなこ",
            "text": "@うどん お出かけはしたいなと思ってます",
            "mention_to": [
                "うどん"
            ]
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "まりも",
            "text": "あーでも",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "うどん",
            "text": "お出かけしたいですね！",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "きなこ",
            "text": "どこに行くか決めてませんが",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "まりも",
            "text": "美術展のチケット買ってあるので",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "うどん",
            "text": "お！",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "まりも",
            "text": "期限内に行かないと",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "きなこ",
            "text": "いいですね～！",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "きなこ",
            "text": "美術展！",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "うどん",
            "text": "美術大好き！",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "まりも",
            "text": "2月3月忙しくて",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "まりも",
            "text": "行ける日をなんとか捻出しないと",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "まりも",
            "text": "印象派展なんですけど",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "うどん",
            "text": "印象派展！",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "まりも",
            "text": "なんだったかな　ウスター展？",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "うどん",
            "text": "おもしろそう！",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "きなこ",
            "text": "素敵！",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "うどん",
            "text": "調べてみます！",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "きなこ",
            "text": "私も！",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "まりも",
            "text": "去年モネ展見て",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "まりも",
            "text": "そのときに知ったんだったかな",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "うどん",
            "text": "モネ好き！",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "まりも",
            "text": "で、悩んで前売り買いました",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "うどん",
            "text": "おぉー！",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "まりも",
            "text": "今大阪行ってるはずですモネ展",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "うどん",
            "text": "そうなんですか！",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "まりも",
            "text": "@うどん モネ好きならモネ展はおすすめです！",
            "mention_to": [
                "うどん"
            ]
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "うどん",
            "text": "大阪行く機会ありそうなので",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "うどん",
            "text": "行ってみます！",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "うどん",
            "text": "@まりも ありがとうございますー！",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "まりも",
            "text": "好きな画家の作品だけ並んでるって",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "まりも",
            "text": "贅沢ですね",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "うどん",
            "text": "存分に楽しめますね",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "まりも",
            "text": "@うどん ぜひぜひ！",
            "mention_to": [
                "うどん"
            ]
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "きなこ",
            "text": "なるほど～素敵な趣味ですね",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "まりも",
            "text": "同じ場所にキャンバスたくさん並べて",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "まりも",
            "text": "時間の経過を描いてたらしいです",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "うどん",
            "text": "@まりも そうなんだ！",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "うどん",
            "text": "心が癒されます",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "まりも",
            "text": "モネ、作品数が多くてよかったなと思いました",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "きなこ",
            "text": "なるほど～",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "まりも",
            "text": "あと生きてる間に売れないとなかなか",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "うどん",
            "text": "モネ展調べてみたら俄然行きたくなりました",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "まりも",
            "text": "お金に苦労して描けなかったり",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "うどん",
            "text": "亡くなってから評価されるって切ない",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "まりも",
            "text": "そうですよね～",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "きなこ",
            "text": "たしかに・・",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "まりも",
            "text": "貧乏でろうそくの絵ばっかり描いてた人とか",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "まりも",
            "text": "たしかいますね",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "うどん",
            "text": "えー！",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "きなこ",
            "text": "悲しい・・",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "うどん",
            "text": "蝋燭の絵は極まりそう",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "うどん",
            "text": "見てみたい",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "まりも",
            "text": "名前が思い出せない～",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "うどん",
            "text": "背景まで知ってみると、また違って見えてきますね",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "まりも",
            "text": "ですね！",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "きなこ",
            "text": "うんうん！",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "まりも",
            "text": "印象派でも繋がりのある人とか知ると",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "まりも",
            "text": "これは誰の影響受けてる？とか",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "まりも",
            "text": "推察できて楽しいです",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "きなこ",
            "text": "そういう楽しみ方があるのですね",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "うどん",
            "text": "その視点もって観てみます！",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "まりも",
            "text": "クールベっぽいモネの絵見て",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "まりも",
            "text": "1人ではしゃいでました",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "まりも",
            "text": "クールベも印象派の人で",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "うどん",
            "text": "1人ではしゃいじゃうのわかる！",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "まりも",
            "text": "深緑色の荒々しい波とか有名なのかな",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "まりも",
            "text": "でも見るときはあんまり難しいこと考えてないです",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "きなこ",
            "text": "奥深いですね",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "うどん",
            "text": "フィーリングも大切ですね！",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "まりも",
            "text": "美術展でそういう背景とか文字で書かれてたりして",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "うどん",
            "text": "うんうん",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "まりも",
            "text": "その場その場で知ってます",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "うどん",
            "text": "しっかり読むと、いろいろ学べますね",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "きなこ",
            "text": "初心者でも楽しめますか？",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "まりも",
            "text": "初心者のほうが楽しめるかも？",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "きなこ",
            "text": "おお！",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "うどん",
            "text": "深い！",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "きなこ",
            "text": "安心しました！",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "まりも",
            "text": "とりあえずなんかいいなと思う絵見かけたら",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "まりも",
            "text": "行ってみるとか",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "まりも",
            "text": "実物見るとやっぱ違うので",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "きなこ",
            "text": "そうですね！",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "まりも",
            "text": "わけわかんないもの見るのも",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "まりも",
            "text": "楽しいかもしれないですが",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "きなこ",
            "text": "わからなくても、",
            "mention_to": []
        },
        {
            "utterance_id": 103,
            "interlocutor_id": "きなこ",
            "text": "考えすぎず鑑賞してみようかな",
            "mention_to": []
        },
        {
            "utterance_id": 104,
            "interlocutor_id": "まりも",
            "text": "音声ガイドとかもありますし",
            "mention_to": []
        }
