    ],
    "relationship": [],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "しらす",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "ねぎとろ",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "まりも",
            "text": "こんにちは～",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "しらす",
            "text": "よろしくお願いします",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "ねぎとろ",
            "text": "よろしくお願いいたします",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "しらす",
            "text": "皆さん、お酒飲まれます？",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "まりも",
            "text": "よろしくお願いします～",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "まりも",
            "text": "まったく飲めないですね～",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "ねぎとろ",
            "text": "飲みます",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "まりも",
            "text": "@しらす よく飲むんですか？",
            "mention_to": [
                "しらす"
            ]
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "しらす",
            "text": "@まりも 休みの日限定です",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "しらす",
            "text": "平日は飲みません",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "ねぎとろ",
            "text": "そうなんですね。",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "まりも",
            "text": "なるほど～",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "しらす",
            "text": "休みの日の楽しみというか",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "ねぎとろ",
            "text": "わかる～",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "まりも",
            "text": "毎日飲むと体によくないし",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "まりも",
            "text": "そういう飲み方のほうがよさそうですね",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "ねぎとろ",
            "text": "ですね。",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "しらす",
            "text": "平日飲んだら次の日起きれません笑",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "まりも",
            "text": "どういうお酒が好きですか？",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "しらす",
            "text": "@まりも 氷結無糖が好きです",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "ねぎとろ",
            "text": "@しらす 量はどれくらい飲まれますか？",
            "mention_to": [
                "しらす"
            ]
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "まりも",
            "text": "@しらす ほ～！",
            "mention_to": [
                "しらす"
            ]
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "しらす",
            "text": "@ねぎとろ あまり強くないので多くても500缶1本で満足します",
            "mention_to": [
                "ねぎとろ"
            ]
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "しらす",
            "text": "飲みながらダラダラ夜更かしして",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "ねぎとろ",
            "text": "@しらす ちょうどいい感じですね。",
            "mention_to": [
                "しらす"
            ]
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "しらす",
            "text": "YouTubeとか見てるのが楽しいです",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "まりも",
            "text": "飲めない人からすると十分なような？",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "まりも",
            "text": "まったりタイム的な感じですか",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "ねぎとろ",
            "text": "わかる。お休みの前の日の夜って好きです。",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "しらす",
            "text": "@まりも そうです!!まったりしてます",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "ねぎとろ",
            "text": "まったり、",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "しらす",
            "text": "@ねぎとろ そうそう",
            "mention_to": [
                "ねぎとろ"
            ]
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "ねぎとろ",
            "text": "明日を気にせず",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "しらす",
            "text": "次の日のこと考えなくていい",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "ねぎとろ",
            "text": "自由な時間",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "ねぎとろ",
            "text": "最高",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "しらす",
            "text": "最高ですね",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "しらす",
            "text": "でも現実は…",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "しらす",
            "text": "二日酔いのときに限って",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "しらす",
            "text": "子どもに叩き起されます笑",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "まりも",
            "text": "二日酔いすることもあるんですか",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "まりも",
            "text": "それは…笑",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "ねぎとろ",
            "text": "あちゃ",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "しらす",
            "text": "@まりも かなりの確率で二日酔いです",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "しらす",
            "text": "でもその朝布団でダラダラするのも気持ちいい",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "まりも",
            "text": "@しらす あら～　あんまり強くないってそういうことですかね",
            "mention_to": [
                "しらす"
            ]
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "しらす",
            "text": "@まりも そうなんです、次の日にきますね笑",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "まりも",
            "text": "ねぎとろさんはお強いですか？",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "ねぎとろ",
            "text": "@しらす お布団だらだら、気持ちがいいですね。",
            "mention_to": [
                "しらす"
            ]
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "しらす",
            "text": "@ねぎとろ 気持ちいいです",
            "mention_to": [
                "ねぎとろ"
            ]
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "ねぎとろ",
            "text": "@まりも あまり強くはないですが、すきです。",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "しらす",
            "text": "@ねぎとろ どんなの飲みます？",
            "mention_to": [
                "ねぎとろ"
            ]
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "ねぎとろ",
            "text": "@しらす 赤ワインかスパークリングワイン",
            "mention_to": [
                "しらす"
            ]
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "ねぎとろ",
            "text": "です。",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "まりも",
            "text": "なんか素敵～",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "しらす",
            "text": "@ねぎとろ いいですね",
            "mention_to": [
                "ねぎとろ"
            ]
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "ねぎとろ",
            "text": "クリームチーズと一緒に飲むのがすきです。",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "まりも",
            "text": "さらに素敵感が",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "しらす",
            "text": "ほんと素敵",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "ねぎとろ",
            "text": "休みの前なら更に最高",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "ねぎとろ",
            "text": "ですね。",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "まりも",
            "text": "お酒を食事に合わせたりするのいいですよね",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "まりも",
            "text": "憧れます",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "ねぎとろ",
            "text": "強くないので、お酒だけでは飲めません",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "ねぎとろ",
            "text": "何か食べるものないと。",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "まりも",
            "text": "ほうほう",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "しらす",
            "text": "@ねぎとろ 分かります",
            "mention_to": [
                "ねぎとろ"
            ]
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "ねぎとろ",
            "text": "@しらす ですよね。",
            "mention_to": [
                "しらす"
            ]
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "まりも",
            "text": "お酒だけよりも何か合わせたほうがお腹によさそう",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "ねぎとろ",
            "text": "お酒だけ飲むひとは、本当の酒飲みかと",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "しらす",
            "text": "私もお酒だけだと胃にきますね",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "ねぎとろ",
            "text": "胃にきます。",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "ねぎとろ",
            "text": "酔いも早い",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "しらす",
            "text": "たしかに",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "まりも",
            "text": "お酒強いとアルコール依存になりやすかったり",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "まりも",
            "text": "肝臓悪くしたりするらしいので",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "まりも",
            "text": "ほどほどが一番ですね",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "しらす",
            "text": "私のイメージでは",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "ねぎとろ",
            "text": "うんうん",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "しらす",
            "text": "強い人より弱い人の方が依存性になりそうな気がします",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "まりも",
            "text": "なるほど飲めないんじゃないかと",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "しらす",
            "text": "強い人はお酒の飲み方が上手",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "ねぎとろ",
            "text": "なるほど",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "ねぎとろ",
            "text": "たしかに",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "しらす",
            "text": "限度が分かるというか",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "ねぎとろ",
            "text": "そうですね。",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "しらす",
            "text": "逆に弱い人は飲めればいいって感じ",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "まりも",
            "text": "酔いが表に出ないだけで",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "まりも",
            "text": "内臓とかのアルコール許容量は",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "まりも",
            "text": "そこまで変わらないのかなと",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "しらす",
            "text": "そう思います",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "まりも",
            "text": "分解が早いだけ、とか",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "ねぎとろ",
            "text": "そうですね。",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "まりも",
            "text": "言うて自分は飲めないんですけどね",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "ねぎとろ",
            "text": "ぜんぜんダメですか？",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "ねぎとろ",
            "text": "梅酒たかも？",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "まりも",
            "text": "楽しい気分は味わってみたかったような",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "まりも",
            "text": "ぜんぜんダメです",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "ねぎとろ",
            "text": "そうなんですね。",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "まりも",
            "text": "一口二口で",
            "mention_to": []
        }
