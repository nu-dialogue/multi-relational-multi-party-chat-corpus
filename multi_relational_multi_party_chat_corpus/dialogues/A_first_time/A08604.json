    "relationship": [],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "しらす",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "しじみ",
            "text": "こんにちは！",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "あまえび",
            "text": "こんにちは！",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "しらす",
            "text": "よろしくお願いします",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "あまえび",
            "text": "よろしくお願いします。",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "しじみ",
            "text": "よろしくお願いします〜",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "しらす",
            "text": "今日は私の住んでるところ",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "しじみ",
            "text": "ほうほう",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "しらす",
            "text": "雨降って寒いです",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "あまえび",
            "text": "うんうん",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "しじみ",
            "text": "@しらす あらー！",
            "mention_to": [
                "しらす"
            ]
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "あまえび",
            "text": "こちらもです～",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "しじみ",
            "text": "こちらはいい天気です",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "あまえび",
            "text": "@しじみ そうなんだ！",
            "mention_to": [
                "しじみ"
            ]
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "しじみ",
            "text": "雨ってテンション下がりますよね",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "あまえび",
            "text": "いいですね",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "しらす",
            "text": "@しじみ 羨ましい",
            "mention_to": [
                "しじみ"
            ]
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "あまえび",
            "text": "@しじみ 下がります。",
            "mention_to": [
                "しじみ"
            ]
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "しじみ",
            "text": "うんうん",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "しらす",
            "text": "日が当たらないのでテンションあがりませんね",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "しじみ",
            "text": "@しらす ですよねぇ",
            "mention_to": [
                "しらす"
            ]
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "あまえび",
            "text": "冷たい雨は嫌いです。",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "しじみ",
            "text": "結構気分が違いますよね",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "しじみ",
            "text": "@あまえび 冷たい雨は嫌ですね！",
            "mention_to": [
                "あまえび"
            ]
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "しらす",
            "text": "でも乾燥から解放されたので",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "しじみ",
            "text": "おお！なるほど！",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "しらす",
            "text": "そこは良かったです",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "しじみ",
            "text": "確かに！乾燥は敵！",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "あまえび",
            "text": "@しらす なるほど。言い面もありますね。",
            "mention_to": [
                "しらす"
            ]
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "しじみ",
            "text": "うんうん",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "あまえび",
            "text": "いい面",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "しじみ",
            "text": "ですね！",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "しらす",
            "text": "@あまえび そんなんですよ",
            "mention_to": [
                "あまえび"
            ]
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "しらす",
            "text": "口の周りがカピカピで",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "あまえび",
            "text": "目の端が乾燥でいたい。",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "しじみ",
            "text": "あ〜〜",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "しじみ",
            "text": "わかります",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "しらす",
            "text": "@あまえび あまえびさんは目なんですね",
            "mention_to": [
                "あまえび"
            ]
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "しじみ",
            "text": "私は唇がカピカピ",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "あまえび",
            "text": "@しらす そうなんです。",
            "mention_to": [
                "しらす"
            ]
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "しらす",
            "text": "みんな分かれますね",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "しじみ",
            "text": "@あまえび 目の端って痛そうですねぇ",
            "mention_to": [
                "あまえび"
            ]
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "しじみ",
            "text": "@しらす ですね！個人差が結構でますね！",
            "mention_to": [
                "しらす"
            ]
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "あまえび",
            "text": "@しじみ ヒリヒリするんですよ！",
            "mention_to": [
                "しじみ"
            ]
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "しらす",
            "text": "口開けると口の周りがピリピリなります",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "しじみ",
            "text": "ヒリヒリは結構つらいですねぇ",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "しじみ",
            "text": "ピリピリもお辛そうだわぁ",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "あまえび",
            "text": "そうなんです",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "しらす",
            "text": "化粧水でヒリヒリしません？",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "しらす",
            "text": "私だけかな",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "あまえび",
            "text": "します。しみます。",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "しじみ",
            "text": "@しらす 春先とかはヒリヒリする事ありますね！",
            "mention_to": [
                "しらす"
            ]
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "しじみ",
            "text": "そして、目の端も口周りも",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "しらす",
            "text": "うんうん",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "あまえび",
            "text": "@しじみ 乾燥は大敵ですね。",
            "mention_to": [
                "しじみ"
            ]
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "しじみ",
            "text": "クリームたっぷり塗るには躊躇する箇所ですよね",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "あまえび",
            "text": "そうそう！",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "しらす",
            "text": "そうなんですよね",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "しじみ",
            "text": "ですよねぇ",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "しじみ",
            "text": "難しい！",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "しらす",
            "text": "塗りすぎるとベタつくし",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "しじみ",
            "text": "うんうん",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "しらす",
            "text": "塗らないと痛い",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "しじみ",
            "text": "目や口に入ってほしくないですしね…",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "あまえび",
            "text": "どんな基礎化粧品がいいのかもわからないし。",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "しらす",
            "text": "本当に難しい",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "しじみ",
            "text": "難しい…",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "しらす",
            "text": "@あまえび いろいろありますもんね",
            "mention_to": [
                "あまえび"
            ]
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "あまえび",
            "text": "@しらす ありすぎて選べないですよ笑",
            "mention_to": [
                "しらす"
            ]
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "しじみ",
            "text": "確かに、今種類多いですよね！",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "しじみ",
            "text": "ドラッグストアのものでも、",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "しらす",
            "text": "いいやつはいいって言いますけど、お金がもたない笑",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "しじみ",
            "text": "良さそうなもの沢山あるし！",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "あまえび",
            "text": "情報に惑わされる",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "しじみ",
            "text": "@しらす 継続するとなると、",
            "mention_to": [
                "しらす"
            ]
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "しらす",
            "text": "うんうん",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "しじみ",
            "text": "デパコスとかは無理ですよね涙",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "しじみ",
            "text": "@あまえび そうそう！",
            "mention_to": [
                "あまえび"
            ]
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "しらす",
            "text": "無理無理笑",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "しじみ",
            "text": "今はネットもあるから、情報量過多になりますよねぇ",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "しらす",
            "text": "本当、肌は人それぞれだから",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "しらす",
            "text": "これが絶対がないですもんね",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "しじみ",
            "text": "ですよねぇ！肌こそ個人差！",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "あまえび",
            "text": "そうなんですよね。",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "あまえび",
            "text": "悩ましい",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "しらす",
            "text": "だから探すのむずかしい",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "しじみ",
            "text": "私は結構キュレル信者です",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "しじみ",
            "text": "冬はキュレル大量に塗っとけ",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "しじみ",
            "text": "って感じでやり過ごしてます",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "しらす",
            "text": "キュレルのバームつかってます",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "あまえび",
            "text": "@しじみ いいですか？",
            "mention_to": [
                "しじみ"
            ]
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "あまえび",
            "text": "@しらす さんも使ってるんですね。",
            "mention_to": [
                "しらす"
            ]
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "しじみ",
            "text": "@しらす おぉ！バーム使った事ないです！",
            "mention_to": [
                "しらす"
            ]
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "しじみ",
            "text": "@しらす クリームが結構いいですー",
            "mention_to": [
                "しらす"
            ]
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "しらす",
            "text": "@しじみ クリーム!!今度試してみます",
            "mention_to": [
                "しじみ"
            ]
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "しじみ",
            "text": "ぜひぜひ！",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "しじみ",
            "text": "でもすぐなくなるし。",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "しじみ",
            "text": "微妙に高いんです…",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "しらす",
            "text": "@しじみ たしかになくなるの早いですね",
            "mention_to": [
                "しじみ"
            ]
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "あまえび",
            "text": "そうなんだ～",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "しじみ",
            "text": "もうちょい容量増やして欲しい…",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "あまえび",
            "text": "冬は、よく使いますよね。",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "しじみ",
            "text": "みなさんのおすすめありますか？？",
            "mention_to": []
        },
        {
            "utterance_id": 103,
            "interlocutor_id": "あまえび",
            "text": "いま変えようか迷ってるんですが、わたしはエリクシールを使ってます。",
            "mention_to": []
        },
        {
            "utterance_id": 104,
            "interlocutor_id": "しじみ",
            "text": "エリクシール！",
            "mention_to": []
        }
