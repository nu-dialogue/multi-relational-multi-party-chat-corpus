{
    "dialogue_id": "A01305",
    "dialogue_type": "First time",
    "interlocutors": [
        "きなこ",
        "まりも",
        "おでん"
    ],
    "relationship": [],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "きなこ",
            "text": "こんにちは！",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "まりも",
            "text": "こんにちは～",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "おでん",
            "text": "こんにちは！",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "まりも",
            "text": "先週末ぐらいに",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "おでん",
            "text": "はい",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "きなこ",
            "text": "うんうん",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "まりも",
            "text": "背中にできものができまして",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "きなこ",
            "text": "痛そう",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "おでん",
            "text": "あら",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "まりも",
            "text": "最初ニキビかな～と思ってたんですけど",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "まりも",
            "text": "粉瘤ってやつでした",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "きなこ",
            "text": "粉瘤？？",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "おでん",
            "text": "え？なんて読むんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "まりも",
            "text": "ふんりゅう　ですね",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "おでん",
            "text": "それはどんなものなんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "まりも",
            "text": "良性の腫瘍です",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "きなこ",
            "text": "痛いですか？",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "おでん",
            "text": "腫瘍ですか",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "まりも",
            "text": "膿とか溜まってぽこっと膨らむ感じ？ですかね",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "まりも",
            "text": "炎症起こすと多少痛いです",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "まりも",
            "text": "ほっておいても治らなくて",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "おでん",
            "text": "背中だと、当たること多そうですね",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "きなこ",
            "text": "なるほど、大変ですね・・",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "まりも",
            "text": "根っこ的なの切除しないと",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "きなこ",
            "text": "切除！！",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "おでん",
            "text": "治療されましたか？",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "まりも",
            "text": "膿を出すだけだとまた復活するらしいです",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "まりも",
            "text": "先日皮膚科行ってきまして",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "おでん",
            "text": "元を絶つってことですね、",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "きなこ",
            "text": "大変だあ",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "まりも",
            "text": "炎症起こしてる間は切除できないので",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "おでん",
            "text": "へぇ～",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "まりも",
            "text": "抗生物質でとりあえず炎症抑えてから、ということに",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "きなこ",
            "text": "そうなのですね。",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "おでん",
            "text": "じゃ、まだあるんですね",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "まりも",
            "text": "すごいサイズになることもあるらしいです",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "まりも",
            "text": "そうなんですよ～",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "おでん",
            "text": "こわいですね",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "まりも",
            "text": "肩甲骨と背骨の間ぐらいの場所で",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "まりも",
            "text": "自分で見えない撮れない笑",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "きなこ",
            "text": "背中だと外から見えないからまだ良かったですね",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "まりも",
            "text": "一番薬塗りづらいですね",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "おでん",
            "text": "また、困ったところにですね",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "まりも",
            "text": "あ～　顔とかだとそれはそれで",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "きなこ",
            "text": "本当・・大変だ。早く切除できるといいですね",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "おでん",
            "text": "来週ぐらいに切除でしょうか？",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "まりも",
            "text": "炎症治まるまで１ヶ月ぐらいが目安って",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "まりも",
            "text": "言われました",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "きなこ",
            "text": "結構かかりますね",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "おでん",
            "text": "そうなんですね",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "まりも",
            "text": "さくっと切っちゃいたいです",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "きなこ",
            "text": "おお！",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "おでん",
            "text": "早く無くしたいですね",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "まりも",
            "text": "ちょうど知り合いの旦那さんが",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "おでん",
            "text": "うんうん",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "まりも",
            "text": "10年ぐらい育てた粉瘤を切ったらしくて",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "きなこ",
            "text": "ええ！！そんなに！",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "まりも",
            "text": "ほっといたらすごいサイズになってて",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "おでん",
            "text": "えー、それは長くそだてたんですね",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "まりも",
            "text": "びっくりして病院に連れて行ったって",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "おでん",
            "text": "見えないとこだったのかな？",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "まりも",
            "text": "切るのに何日かかかったそうです",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "おでん",
            "text": "わぁー痛そう、大変！",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "まりも",
            "text": "背中にできることが多いみたいですね",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "まりも",
            "text": "あと男性の方が多いとか",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "きなこ",
            "text": "背中って気づきにくいですよね",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "おでん",
            "text": "そうなんですね、まだ大丈夫って、思うんでしょうね",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "まりも",
            "text": "炎症起こしてなければ",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "おでん",
            "text": "うんうん",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "まりも",
            "text": "たぶんただのこぶ…的な？",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "まりも",
            "text": "感覚かもしれないですね",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "きなこ",
            "text": "それだと病院行かないかも",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "おでん",
            "text": "あー痛くなかったら、流しますよね",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "まりも",
            "text": "そうですよね～",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "おでん",
            "text": "@まりも 痛くて病院に行ったんですか？",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "まりも",
            "text": "病院も混んでましたし",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "まりも",
            "text": "@おでん 炎症起こしてるのと、切らないと治らないの知ってたので",
            "mention_to": [
                "おでん"
            ]
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "まりも",
            "text": "観念して行きました",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "きなこ",
            "text": "混みますよね‥病院大変でしたね",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "おでん",
            "text": "良かった、観念してくれて",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "まりも",
            "text": "受付開始ぐらいに行ったら",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "まりも",
            "text": "外まで並んでました…",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "おでん",
            "text": "そういうのは、皮膚科？外科？",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "まりも",
            "text": "私は皮膚科行きました",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "おでん",
            "text": "そうなんですね",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "まりも",
            "text": "見えるとこだと形成外科とか行くんですかね",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "きなこ",
            "text": "診療科悩みますよね",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "きなこ",
            "text": "顔は形成ですよね",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "まりも",
            "text": "初めてかかるとこだったんで",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "まりも",
            "text": "レビュー見て",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "まりも",
            "text": "受付に怖い人がいる～みたいな書き込みが",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "おでん",
            "text": "@きなこ 顔は形成なんですか？",
            "mention_to": [
                "きなこ"
            ]
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "まりも",
            "text": "ビビってたけど、行ってみたらすごく普通でした",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "おでん",
            "text": "@まりも それは怖い笑笑",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "きなこ",
            "text": "@おでん そうなんです。首から上は形成外科です",
            "mention_to": [
                "おでん"
            ]
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "おでん",
            "text": "@きなこ 勉強になります",
            "mention_to": [
                "きなこ"
            ]
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "まりも",
            "text": "@おでん @きなこ そうなんですね！",
            "mention_to": [
                "おでん",
                "きなこ"
            ]
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "きなこ",
            "text": "@まりも 受付怖いのいやですよね・・病人なのに",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "おでん",
            "text": "ほんと！同感笑笑",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "きなこ",
            "text": "優しくしてほしい！",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "まりも",
            "text": "@きなこ 他の病院も結構そういうレビュー多くて",
            "mention_to": [
                "きなこ"
            ]
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "おでん",
            "text": "クチコミとか、見ちゃいますよね",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "まりも",
            "text": "結局わからん！と思いました",
            "mention_to": []
        },
        {
            "utterance_id": 103,
            "interlocutor_id": "きなこ",
            "text": "@まりも わかります・・",
            "mention_to": [
                "まりも"
            ]
        }
