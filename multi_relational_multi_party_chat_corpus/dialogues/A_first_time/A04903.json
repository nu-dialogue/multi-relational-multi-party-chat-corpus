    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "おでん",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "まりも",
            "text": "こんにちは～",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "おでん",
            "text": "よろしくお願いします",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "げんまい",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "まりも",
            "text": "よろしくお願いします",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "げんまい",
            "text": "よろしくお願いします",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "おでん",
            "text": "最近、",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "げんまい",
            "text": "はい",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "おでん",
            "text": "寝ても寝ても眠いです",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "げんまい",
            "text": "あー眠いんですねー",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "まりも",
            "text": "私もよく寝てます",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "げんまい",
            "text": "お疲れなのかなー",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "おでん",
            "text": "寝てますか？",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "まりも",
            "text": "気温が乱高下してると",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "おでん",
            "text": "ウンウン",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "まりも",
            "text": "睡眠がめちゃめちゃになります",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "げんまい",
            "text": "睡眠時間でいうと、7時間ぐらいかな。",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "おでん",
            "text": "あー、そうなんかもです",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "げんまい",
            "text": "気温に左右されちゃうんですね。",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "おでん",
            "text": "7時間って、1番",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "まりも",
            "text": "自律神経がダメなので",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "おでん",
            "text": "いいって言いますよね",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "げんまい",
            "text": "7時間、いいんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "げんまい",
            "text": "へー",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "まりも",
            "text": "気温とか気圧とかの影響受けやすいです",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "げんまい",
            "text": "あらー",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "おでん",
            "text": "ウンウン、でも、私はもっと寝てます",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "げんまい",
            "text": "気圧の変化がわかるアプリありますよね。",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "おでん",
            "text": "自律神経って、色んなところに",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "おでん",
            "text": "影響ありますよね",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "まりも",
            "text": "@げんまい ありますね～　一時期使ってました",
            "mention_to": [
                "げんまい"
            ]
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "おでん",
            "text": "@げんまい そんなのあるんですね",
            "mention_to": [
                "げんまい"
            ]
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "げんまい",
            "text": "@まりも　今は使ってないですか？",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "おでん",
            "text": "結構、使い勝手いいもんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "げんまい",
            "text": "気温と気圧の予想が見られて、おもしろいです。アプリ。",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "まりも",
            "text": "@おでん 睡眠はしょっちゅう乱れてますね",
            "mention_to": [
                "おでん"
            ]
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "おでん",
            "text": "いい睡眠って、難しい",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "まりも",
            "text": "@げんまい 使っても使わなくても変わらないので",
            "mention_to": [
                "げんまい"
            ]
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "まりも",
            "text": "見なくなってしまいました",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "おでん",
            "text": "笑",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "げんまい",
            "text": "たしかに",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "おでん",
            "text": "そりゃそうですね笑",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "げんまい",
            "text": "使ってもかわらないか。ｗｗｗ",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "まりも",
            "text": "爆弾低気圧～とかって言われてるときより",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "げんまい",
            "text": "睡眠って個人さも大きいし、",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "おでん",
            "text": "でも、気圧まで分かるって、",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "おでん",
            "text": "面白いかも",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "まりも",
            "text": "そのちょっと前のほうが調子悪い気がします",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "げんまい",
            "text": "睡眠の質も大事って言いますよね",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "おでん",
            "text": "ウンウン",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "げんまい",
            "text": "ポケモンスリープで睡眠の質がわかるって聞きましたよ。",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "おでん",
            "text": "一時期、ヤクルト1000が流行ったじゃないですか？",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "まりも",
            "text": "睡眠の質が悪いから調子が悪いのか",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "げんまい",
            "text": "はやりましたね。１０００",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "まりも",
            "text": "調子が悪いから睡眠が悪くなるのか",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "おでん",
            "text": "ウンウン",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "まりも",
            "text": "ヤクルト1000気になってました",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "げんまい",
            "text": "今は、普通に売ってますね。",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "げんまい",
            "text": "一時期、売り切れてたけど、",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "まりも",
            "text": "悪夢見るって噂が",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "おでん",
            "text": "私は普通のヤクルトを飲んでますが、",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "げんまい",
            "text": "え？！悪夢？",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "おでん",
            "text": "え？",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "げんまい",
            "text": "私も普通のヤクルトは飲んでます。",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "まりも",
            "text": "普通に売ってますか？今はあるのかな～",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "おでん",
            "text": "悪夢！",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "げんまい",
            "text": "この前、駅の販売機とかで",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "おでん",
            "text": "ウンウン",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "げんまい",
            "text": "見ました　ヤクルト１０００",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "おでん",
            "text": "私も最近、あちこちで見ます",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "まりも",
            "text": "昔の職場ではヤクルト販売員さんが来てました",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "まりも",
            "text": "もう流通してるんですかね～",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "おでん",
            "text": "あの、売り切れはなんだったのだろうかと、",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "げんまい",
            "text": "ヤクルトレディー！",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "おでん",
            "text": "ヤクルトレディさんですね",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "まりも",
            "text": "ヤクルト400が好きで",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "まりも",
            "text": "よく買ってました",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "おでん",
            "text": "ウンウン",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "げんまい",
            "text": "飲み続けるといいのかもですね。",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "おでん",
            "text": "400はやっぱり違いますか？",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "まりも",
            "text": "私はお通じがよくなりました",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "おでん",
            "text": "色々ありますよね",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "まりも",
            "text": "400、なんかおいしいです",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "おでん",
            "text": "そうなんだ",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "げんまい",
            "text": "味もちがうんですか",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "まりも",
            "text": "なぜかはわからないんですが",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "まりも",
            "text": "おいしいと思いました",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "げんまい",
            "text": "400はスーパーで売ってないですよね。",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "おでん",
            "text": "ライトとか、500とか、カロリーハーフとか",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "げんまい",
            "text": "ヤクルトさんから買わないと",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "まりも",
            "text": "そうそう、直売じゃないと",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "おでん",
            "text": "そうなんだ",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "まりも",
            "text": "ないですよね",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "おでん",
            "text": "特別なのかな？",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "げんまい",
            "text": "昔、妹がヤクルトレディをやってました。",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "おでん",
            "text": "へぇー",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "まりも",
            "text": "へ～！",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "げんまい",
            "text": "で、子どもが毎日ヤクルトを託児所で飲んでいて",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "おでん",
            "text": "結構、成分とか色々教えてもらえたりしたんじゃないですか？",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "げんまい",
            "text": "ですね。",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "まりも",
            "text": "ヤクルトちょっと高くて",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "まりも",
            "text": "躊躇してたけど",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "まりも",
            "text": "久々に飲みたい",
            "mention_to": []
        },
        {
            "utterance_id": 103,
            "interlocutor_id": "おでん",
            "text": "ウンウン",
            "mention_to": []
        },
        {
            "utterance_id": 104,
            "interlocutor_id": "げんまい",
            "text": "ヤクルトを止めたら、子どもがよく熱を出すようになったと",
            "mention_to": []
        }
    ],
