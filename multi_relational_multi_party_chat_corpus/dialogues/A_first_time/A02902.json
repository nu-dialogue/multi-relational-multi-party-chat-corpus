        },
        {
            "utterance_id": 1,
            "interlocutor_id": "しじみ",
            "text": "こんにちは！",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "なると",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "しらす",
            "text": "よろしくお願いします",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "しじみ",
            "text": "よろしくお願いします！",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "なると",
            "text": "よろしくお願いします。",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "しらす",
            "text": "カレンダー見て気づいたのですけど",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "しじみ",
            "text": "なんでしょう",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "しらす",
            "text": "今年ってオリンピックある年でしたね",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "しじみ",
            "text": "え！",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "なると",
            "text": "そうでしたか！",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "しじみ",
            "text": "今言われて気づきました！",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "なると",
            "text": "知りませんでした。",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "しらす",
            "text": "え？違いましたかね",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "しじみ",
            "text": "閏年って",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "しじみ",
            "text": "オリンピックのある年でしたっけ？",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "なると",
            "text": "分からないですね。",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "なると",
            "text": "冬季もあって分からなくなりますね。",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "しじみ",
            "text": "開催地もわからないかも",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "しらす",
            "text": "たしかそうではなかったですかね",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "しじみ",
            "text": "@なると 冬季とごっちゃになりますね〜",
            "mention_to": [
                "なると"
            ]
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "なると",
            "text": "毎年何か見てる気がします。",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "しらす",
            "text": "東京オリンピックが延期になって分からなくなってきました",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "しじみ",
            "text": "@しらす あとで調べてみます！",
            "mention_to": [
                "しらす"
            ]
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "しらす",
            "text": "私も調べます",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "しじみ",
            "text": "コロナ禍を挟み、",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "しじみ",
            "text": "わけがわからなくなりますよね〜",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "しらす",
            "text": "うんうん",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "なると",
            "text": "この前は日本でしたよね。",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "しじみ",
            "text": "@なると でしたね！",
            "mention_to": [
                "なると"
            ]
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "しらす",
            "text": "日本でしたね",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "しじみ",
            "text": "リオジャネイロでしたっけ？",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "しじみ",
            "text": "フランス？？",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "なると",
            "text": "フランスとかでしたっけ？",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "しらす",
            "text": "気になってきた",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "しじみ",
            "text": "@しらす わかります！",
            "mention_to": [
                "しらす"
            ]
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "なると",
            "text": "真剣に見てないと分からなくなります。",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "しじみ",
            "text": "気になってきた",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "しじみ",
            "text": "ちなみに、皆さんは",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "しじみ",
            "text": "なんの競技を見ますか？",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "なると",
            "text": "なんだろう",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "しらす",
            "text": "@しじみ 私、東京でハマったのですが",
            "mention_to": [
                "しじみ"
            ]
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "しじみ",
            "text": "@しらす ふむふむ",
            "mention_to": [
                "しらす"
            ]
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "しらす",
            "text": "スケボーおもしろいです",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "しじみ",
            "text": "あー！！",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "しじみ",
            "text": "めちゃくちゃわかります！",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "なると",
            "text": "スケボーかっこいい",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "しじみ",
            "text": "スケボーかっこいいですやね！",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "しらす",
            "text": "見ててかっこいい",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "しじみ",
            "text": "わかりますわかります！",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "しらす",
            "text": "見てて飽きない",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "しじみ",
            "text": "日本すごかったですよね！",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "なると",
            "text": "そうなんですね",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "しらす",
            "text": "すごかったです",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "しじみ",
            "text": "もみじちゃんと、",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "しじみ",
            "text": "優勝した男の子のインスタ、競技後にフォローしちゃいました",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "なると",
            "text": "すごいですね",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "しらす",
            "text": "堀米くんでしたっけ？",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "しじみ",
            "text": "@しらす そうでした！",
            "mention_to": [
                "しらす"
            ]
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "しじみ",
            "text": "@なると 平均年齢がすごく若くて、",
            "mention_to": [
                "なると"
            ]
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "しじみ",
            "text": "胸が熱くなりました！",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "なると",
            "text": "10代ですかね",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "しじみ",
            "text": "ですです！",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "しらす",
            "text": "みんな若いですよね",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "しじみ",
            "text": "みんなまだ子供なのに、",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "しらす",
            "text": "失敗してもみんなで励ましてたり",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "しじみ",
            "text": "こんなに一生懸命頑張っててすごい！\nと親戚の気持ちです",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "なると",
            "text": "スポーツははまりますね",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "しらす",
            "text": "国関係なく仲良くしているのが胸熱でした",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "しらす",
            "text": "分かります",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "しじみ",
            "text": "そうそう！スケボーって解説者もフレンドリー！",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "なると",
            "text": "そうなんですね",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "しらす",
            "text": "ギスギスしてないというか",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "しじみ",
            "text": "@なると ぜひ次回見て欲しいです！",
            "mention_to": [
                "なると"
            ]
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "なると",
            "text": "はい、見てみます",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "しじみ",
            "text": "ちなみに私は、",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "しらす",
            "text": "うんうん",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "しじみ",
            "text": "開会式が一番好きです",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "しらす",
            "text": "分かります",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "なると",
            "text": "面白いですね",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "しらす",
            "text": "演出毎回気になります",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "しじみ",
            "text": "おー！わかっていただけて嬉しい",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "しじみ",
            "text": "各国の衣装も素敵ですよね",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "しらす",
            "text": "素敵です",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "しじみ",
            "text": "アフリカ地域って、",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "しじみ",
            "text": "カラフルなお洋服が多くてつい見入りますね",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "なると",
            "text": "綺麗ですよね",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "しらす",
            "text": "色使いとか綺麗ですよね",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "しじみ",
            "text": "華やかですね！",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "しじみ",
            "text": "国民性がでますよね",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "しらす",
            "text": "確かに",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "なると",
            "text": "そうですね",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "しじみ",
            "text": "もし開催地フランスだと",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "しじみ",
            "text": "花火とかすごそう",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "なると",
            "text": "盛大そうですね",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "しらす",
            "text": "綺麗そうですね",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "しじみ",
            "text": "ですよね！",
            "mention_to": []
        },
        {
            "utterance_id": 97,
