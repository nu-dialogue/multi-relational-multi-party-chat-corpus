{
    "dialogue_id": "A01301",
    "dialogue_type": "First time",
    "interlocutors": [
        "きなこ",
        "まりも",
        "おでん"
    ],
    "relationship": [],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "きなこ",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "まりも",
            "text": "こんにちは～",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "おでん",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "まりも",
            "text": "今日寒いですね",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "まりも",
            "text": "昨日暑いぐらいだったのに",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "きなこ",
            "text": "昨日と違って寒いです",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "おでん",
            "text": "寒いです、風があります",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "まりも",
            "text": "気温差についていけない…",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "きなこ",
            "text": "わかります！",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "まりも",
            "text": "風強いですね",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "おでん",
            "text": "ほんとに、着るものに迷います",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "まりも",
            "text": "朝すごかった",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "まりも",
            "text": "風の音が",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "おでん",
            "text": "そうなんですね",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "きなこ",
            "text": "そうでしたよね！音がすごかった",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "まりも",
            "text": "窓がガタガタ言いますね",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "おでん",
            "text": "昨日は春一番とか言ってましたもんね",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "まりも",
            "text": "ね～",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "まりも",
            "text": "嘘みたいに寒い",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "きなこ",
            "text": "もう春になるのかな～と思っていました",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "おでん",
            "text": "うんうん",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "まりも",
            "text": "みなさんどの辺にお住まいですか？",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "おでん",
            "text": "私も思ってました",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "まりも",
            "text": "私は関東です",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "きなこ",
            "text": "先ほどテレビで週末雪かも！？って言ってましたよ",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "おでん",
            "text": "私は関西です。",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "きなこ",
            "text": "私もです",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "きなこ",
            "text": "あ、関東です",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "まりも",
            "text": "雪はちょっとつらい～",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "おでん",
            "text": "@きなこ 雪ですか！",
            "mention_to": [
                "きなこ"
            ]
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "きなこ",
            "text": "テレビでそう話してました・・",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "まりも",
            "text": "ちょっと前に関東で雪降りましたよね",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "おでん",
            "text": "関東って、結構雪降りますよね",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "まりも",
            "text": "朝起きて空模様が怪しいな～と思ったら",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "きなこ",
            "text": "降らない年はまったく降らないんですけどね",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "まりも",
            "text": "結構がっつり降って",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "まりも",
            "text": "そうですね",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "おでん",
            "text": "雪だと、困りますか？",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "まりも",
            "text": "何年かおきに降る気がします",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "まりも",
            "text": "あんまり対策がないので",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "おでん",
            "text": "私のところは積もらないなぁ",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "きなこ",
            "text": "雪だとちょっと困ります・・マンションの前の雪かきが・・",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "おでん",
            "text": "慣れてないと、歩くのも怖いですよね",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "まりも",
            "text": "ですね～",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "まりも",
            "text": "普通の運動靴しかないので",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "きなこ",
            "text": "私も長くつしかないです",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "おでん",
            "text": "長靴！",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "まりも",
            "text": "それは　滑りますね",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "おでん",
            "text": "そうか、そゆうのがいいかもですね",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "まりも",
            "text": "雪降る地域って備えが違いますよね",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "おでん",
            "text": "そうですよね",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "まりも",
            "text": "関東は電車止まっちゃうし",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "きなこ",
            "text": "そうですよね。",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "おでん",
            "text": "わかります",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "きなこ",
            "text": "@おでん 関西は雪あまり降らないですか？",
            "mention_to": [
                "おでん"
            ]
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "まりも",
            "text": "数年に1回のために万全の備えできないですし",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "おでん",
            "text": "@きなこ 降らないですねー",
            "mention_to": [
                "きなこ"
            ]
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "おでん",
            "text": "特に瀬戸内に面してるので",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "きなこ",
            "text": "@まりも ですよ！わかります！",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "おでん",
            "text": "温暖です",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "きなこ",
            "text": "@おでん 素敵！",
            "mention_to": [
                "おでん"
            ]
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "まりも",
            "text": "@おでん 瀬戸内、いいですね～",
            "mention_to": [
                "おでん"
            ]
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "おでん",
            "text": "そうですね、",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "まりも",
            "text": "住み暮らすなら温暖がいいですね",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "きなこ",
            "text": "私もそう思います",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "おでん",
            "text": "台風とかも当たりが弱い気がします",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "まりも",
            "text": "へええ",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "きなこ",
            "text": "知らなかった！",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "おでん",
            "text": "四国に守られてるというか笑笑",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "まりも",
            "text": "四国がガーディアンですか笑",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "きなこ",
            "text": "なるほど！確かに",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "おでん",
            "text": "@まりも 笑笑、カーディガン",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "きなこ",
            "text": "ガーディアンって！",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "おでん",
            "text": "@きなこ @まりも 関東は海側ですか？街中ですか？山側ですか？",
            "mention_to": [
                "まりも",
                "きなこ"
            ]
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "まりも",
            "text": "@おでん うちは埼玉県です",
            "mention_to": [
                "おでん"
            ]
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "きなこ",
            "text": "私、街中です！東京ですが真ん中です",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "まりも",
            "text": "平地ですね～",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "おでん",
            "text": "関東と言っても、広いですねー",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "きなこ",
            "text": "そうでですね～",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "まりも",
            "text": "東京の真ん中！アクセスいいですか？",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "おでん",
            "text": "良さそう",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "まりも",
            "text": "最近気になる美術展多くて",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "きなこ",
            "text": "いいですよ！",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "まりも",
            "text": "東京に出るのしんどい～と思いながら",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "おでん",
            "text": "あー、沢山ありますよね",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "まりも",
            "text": "でかけてます",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "おでん",
            "text": "いいなぁ",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "きなこ",
            "text": "でも、うちは多摩地域なので都会ではないです",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "おでん",
            "text": "電車で行ける距離感、羨ましい",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "まりも",
            "text": "去年モネ展に行って",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "まりも",
            "text": "上野の森美術館だったんですけど",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "きなこ",
            "text": "おお！いいですね",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "まりも",
            "text": "上野のあっち側って美術展とか博物館とか多いですね",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "まりも",
            "text": "動物園もあるし",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "おでん",
            "text": "えー！私来週、大阪のモネ展に行く予定なんです",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "まりも",
            "text": "あらあら！",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "まりも",
            "text": "すごくよかったですよ！",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "きなこ",
            "text": "すごい偶然！私も行ってみようかな！",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "まりも",
            "text": "もう大阪始まってるんですね",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "おでん",
            "text": "楽しみ",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "きなこ",
            "text": "心落ち着きますよね～",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "まりも",
            "text": "私モネの雪の絵が好きで",
            "mention_to": []
        }
