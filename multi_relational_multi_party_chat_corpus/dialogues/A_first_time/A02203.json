{
    "dialogue_id": "A02203",
    "dialogue_type": "First time",
    "interlocutors": [
        "おでん",
        "はくさい",
        "げんまい"
    ],
    "relationship": [],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "おでん",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "はくさい",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "おでん",
            "text": "よろしくお願いします",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "はくさい",
            "text": "お願いします！！",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "おでん",
            "text": "最近、友達に教えてもらったんですが",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "おでん",
            "text": "いちごを炭酸水に入れて、",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "はくさい",
            "text": "うんうん",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "げんまい",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "おでん",
            "text": "1日置くと、すごく美味しいって",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "げんまい",
            "text": "それ、聞いたことあります",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "はくさい",
            "text": "そうなんですか！",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "おでん",
            "text": "@げんまい ごめんなさい、先に話始めてしまいました",
            "mention_to": [
                "げんまい"
            ]
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "はくさい",
            "text": "初めて聞きました",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "げんまい",
            "text": "シュワシュワするとかしないとか？",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "おでん",
            "text": "ね、私、昨日はじめてして",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "げんまい",
            "text": "どうでした？",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "おでん",
            "text": "今朝食べたら、シュワシュワして美味しかったです",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "はくさい",
            "text": "ちょうどイチゴも炭酸水もある",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "はくさい",
            "text": "うちも試してみようかな",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "げんまい",
            "text": "ほんとにシュワシュワするんですねっ",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "おでん",
            "text": "してみてください",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "げんまい",
            "text": "1日漬けておくんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "おでん",
            "text": "炭酸水もほのかにいちごの味がして、飲むと美味しかったです",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "おでん",
            "text": "そう、器に入れて、蓋をして、1日置くらしいです",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "げんまい",
            "text": "ほう。炭酸水までおいしくなるとは！",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "はくさい",
            "text": "おもしろい！！",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "げんまい",
            "text": "炭酸、抜けないですか？",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "おでん",
            "text": "微炭酸になります",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "げんまい",
            "text": "そうですか",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "おでん",
            "text": "1度試して見てください",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "げんまい",
            "text": "やってみます！",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "はくさい",
            "text": "やってみます！",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "おでん",
            "text": "他の果物でもいいらしいです",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "げんまい",
            "text": "いちご、そろそろ時期ですね",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "げんまい",
            "text": "ぶどうでやるのも聞いたことあります。",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "おでん",
            "text": "それも美味しいって",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "はくさい",
            "text": "@げんまい ぶどうもおいしそう",
            "mention_to": [
                "げんまい"
            ]
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "おでん",
            "text": "パインとかも",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "はくさい",
            "text": "ほんとに何でもいいんですね",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "げんまい",
            "text": "パイン！ちょっと想像つかないけど、おいしいかも。",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "げんまい",
            "text": "この後スーパーに行くので、買っちゃうかも",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "おでん",
            "text": "うんうん、果物だったらいい感じなのかもです",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "おでん",
            "text": "果物、何が好きですか？",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "げんまい",
            "text": "炭酸水は、味がついてないやつですか？",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "おでん",
            "text": "@げんまい そうです",
            "mention_to": [
                "げんまい"
            ]
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "おでん",
            "text": "蓋付きの容器がいいみたいですよ",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "はくさい",
            "text": "@おでん 時期じゃないけどなしが好きです",
            "mention_to": [
                "おでん"
            ]
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "げんまい",
            "text": "私も梨！あと柿",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "おでん",
            "text": "なし！どんな種類の？",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "げんまい",
            "text": "全然季節じゃないけど",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "げんまい",
            "text": "ジューシーならなんでも。＜なし",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "おでん",
            "text": "笑笑どちらも美味しいですよねー",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "はくさい",
            "text": "梨は二十世紀が好きです",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "おでん",
            "text": "私も！",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "はくさい",
            "text": "ちょっとわかめのシャキシャキが好き",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "おでん",
            "text": "私もです",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "げんまい",
            "text": "シャキシャキいいですね！",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "おでん",
            "text": "なんでもシャキシャキが好きです",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "おでん",
            "text": "柿も",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "はくさい",
            "text": "私もですー",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "げんまい",
            "text": "柿、わたしはジュクジュクい熟したやつが好きです",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "おでん",
            "text": "柔かーい柿は苦手です",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "はくさい",
            "text": "逆に熟したのは苦手かも",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "げんまい",
            "text": "やばそうなぐらい熟したやつ。",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "おでん",
            "text": "おーそうなんですね",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "げんまい",
            "text": "分かれましたね。",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "はくさい",
            "text": "かなりジュクジュクですね",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "おでん",
            "text": "甘味は増してるから、美味しいんだろけど",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "おでん",
            "text": "柿もシャキシャキがいいかなー",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "げんまい",
            "text": "私の食べごろになるまでに家族に先に食べられてしまいます。",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "おでん",
            "text": "笑笑そうなりそうですね",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "げんまい",
            "text": "家族は、お二人と一緒でシャキシャキ派なので、",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "おでん",
            "text": "うんうん",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "げんまい",
            "text": "付箋に名前を書いて貼っておきます",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "げんまい",
            "text": "ふふふ",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "おでん",
            "text": "笑笑頑張ってますね",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "はくさい",
            "text": "取られないように大事です",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "げんまい",
            "text": "必死ですｗｗ",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "おでん",
            "text": "大事ですね",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "げんまい",
            "text": "自分用のお菓子に名前書いたりしませんか？",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "おでん",
            "text": "お菓子なら、隠します笑笑",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "げんまい",
            "text": "隠す！！",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "おでん",
            "text": "隠す！",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "おでん",
            "text": "取られたら悲しいですもん",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "はくさい",
            "text": "@げんまい 自分用には書かないですが、子どもたちは喧嘩になるから書きますね",
            "mention_to": [
                "げんまい"
            ]
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "げんまい",
            "text": "絶対安全な場所に？",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "おでん",
            "text": "そそ、家族が思いつかないところ",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "げんまい",
            "text": "ケンカは面倒ですもんね。",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "げんまい",
            "text": "@おでん では、食べる時も1人でこっそりですね",
            "mention_to": [
                "おでん"
            ]
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "おでん",
            "text": "@はくさい 喧嘩の仲裁が大変ですもんね",
            "mention_to": [
                "はくさい"
            ]
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "おでん",
            "text": "@げんまい そうです笑笑",
            "mention_to": [
                "げんまい"
            ]
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "はくさい",
            "text": "@おでん そうなんですよね",
            "mention_to": [
                "おでん"
            ]
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "げんまい",
            "text": "私は名前書きます。お菓子",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "はくさい",
            "text": "そうしたら食べられないですか？",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "おでん",
            "text": "でも、兄弟でも、食べる速さとか、違わないでか？",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "げんまい",
            "text": "うちの2人息子は、食べるペースがちがって、",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "おでん",
            "text": "うんうん",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "げんまい",
            "text": "お兄ちゃんはすぐ食べて、弟はいつまでも残っています。おかし",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "げんまい",
            "text": "名前書いてあると、お兄ちゃんに食べられないです。",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "おでん",
            "text": "笑笑仕方ないですよね",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "はくさい",
            "text": "みんなそうなんですね",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "げんまい",
            "text": "お兄ちゃんはアルバイトしてるから、自分で買ったりもしますんでね。",
            "mention_to": []
        }
    ],
