{
    "dialogue_id": "A08202",
    "dialogue_type": "First time",
    "interlocutors": [
        "いくら",
        "くまのみ",
        "まぐろ"
    ],
    "relationship": [],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "いくら",
            "text": "こんにちはー！",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "くまのみ",
            "text": "こんにちは！",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "まぐろ",
            "text": "こんにちは！",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "いくら",
            "text": "8時間ダイエットてご存知です？",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "くまのみ",
            "text": "@いくら 初めてききました",
            "mention_to": [
                "いくら"
            ]
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "まぐろ",
            "text": "@いくら いやー知らないです！",
            "mention_to": [
                "いくら"
            ]
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "くまのみ",
            "text": "どんなダイエットですか？",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "いくら",
            "text": "8時間だけなんでも自由に食べていいんです！",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "いくら",
            "text": "でも残りの16時間何も食べれなくて、",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "くまのみ",
            "text": "@いくら それは良さそうですね",
            "mention_to": [
                "いくら"
            ]
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "まぐろ",
            "text": "あ、想像と逆でした！",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "くまのみ",
            "text": "@いくら それは辛いですね",
            "mention_to": [
                "いくら"
            ]
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "まぐろ",
            "text": "8時間食べないかと",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "いくら",
            "text": "よいですよね！",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "くまのみ",
            "text": "16時間って長いですね",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "いくら",
            "text": "食事制限がないとできそうな感じが！",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "いくら",
            "text": "@まぐろ わかります！いつもどっちだっけ？てなります！",
            "mention_to": [
                "まぐろ"
            ]
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "くまのみ",
            "text": "8時間食べ続けるわけにもいきませんもんね",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "いくら",
            "text": "たくさん寝るとへっちゃらなんですけどね！",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "まぐろ",
            "text": "@くまのみ 笑",
            "mention_to": [
                "くまのみ"
            ]
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "いくら",
            "text": "ですね、2回くらいしか食べれませんね！",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "くまのみ",
            "text": "@いくら 試してみる予定ですか？",
            "mention_to": [
                "いくら"
            ]
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "いくら",
            "text": "@くまのみ 試して見たのですが、なかなか家族がいると合わせるの難しくて、、",
            "mention_to": [
                "くまのみ"
            ]
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "いくら",
            "text": "お弁当とか作りながら味見しちゃったり",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "くまのみ",
            "text": "@いくら そうですよね、１人じゃないと出来ないですよね。",
            "mention_to": [
                "いくら"
            ]
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "いくら",
            "text": "なので10時間だけ食べないダイエットにしました！笑",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "まぐろ",
            "text": "確かに、家族に合わせちゃうから・・むずかしいかも",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "くまのみ",
            "text": "@いくら しますよねー。",
            "mention_to": [
                "いくら"
            ]
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "いくら",
            "text": "@くまのみ なかなか難しかったです！",
            "mention_to": [
                "くまのみ"
            ]
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "まぐろ",
            "text": "10時間も意外に長いですから",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "くまのみ",
            "text": "@いくら 10時間なら睡眠含めたら出来そうですね",
            "mention_to": [
                "いくら"
            ]
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "いくら",
            "text": "そうなんです！",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "まぐろ",
            "text": "睡眠か！",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "いくら",
            "text": "寝てたら意外とあっという間で！",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "くまのみ",
            "text": "@いくら そうなんですね。難しかったんですね。",
            "mention_to": [
                "いくら"
            ]
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "いくら",
            "text": "ちょっと最近食べ過ぎだったので",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "いくら",
            "text": "痩せれるといいのですが",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "くまのみ",
            "text": "寒い時は体が欲しますもんね",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "まぐろ",
            "text": "寒いし、ついつい食べ過ぎちゃいますよね・・",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "いくら",
            "text": "そうなんですよー冬だからですよね！",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "くまのみ",
            "text": "@いくら 頑張って下さい！",
            "mention_to": [
                "いくら"
            ]
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "いくら",
            "text": "ありがとうございます！",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "いくら",
            "text": "ダイエットとかされます？",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "まぐろ",
            "text": "@いくら いつも",
            "mention_to": [
                "いくら"
            ]
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "いくら",
            "text": "いつも！",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "くまのみ",
            "text": "私はお腹まわりをなんとかしたいです",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "まぐろ",
            "text": "ダイエットしているつもりです笑",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "くまのみ",
            "text": "@まぐろ 笑",
            "mention_to": [
                "まぐろ"
            ]
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "いくら",
            "text": "お腹周りわかります！！肉が、、",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "いくら",
            "text": "@まぐろ それはやばいですね笑",
            "mention_to": [
                "まぐろ"
            ]
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "まぐろ",
            "text": "お腹周りってどうしたらいいのだろう・・",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "くまのみ",
            "text": "体重は標準なんですけど、ザ中年体形です",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "いくら",
            "text": "腹筋とか？",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "くまのみ",
            "text": "@まぐろ 一番おちないですよね",
            "mention_to": [
                "まぐろ"
            ]
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "いくら",
            "text": "お風呂の全身鏡が最近しんどくて",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "まぐろ",
            "text": "わかります～！",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "くまのみ",
            "text": "@いくら わかります",
            "mention_to": [
                "いくら"
            ]
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "いくら",
            "text": "ですよねー！",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "いくら",
            "text": "全身いらないですよね！笑",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "くまのみ",
            "text": "外出時ふいに見る鏡も",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "まぐろ",
            "text": "私も外で鏡に映った自分を・・どこのおばさん？って勘違いしたことありです！！",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "まぐろ",
            "text": "自分だった・・",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "くまのみ",
            "text": "@まぐろ 全く同じです！",
            "mention_to": [
                "まぐろ"
            ]
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "いくら",
            "text": "ほんとわかります、、",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "まぐろ",
            "text": "私の中の自分と、現実が違いすぎて笑",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "いくら",
            "text": "写真や動画に写った自分にもびっくりします",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "くまのみ",
            "text": "家ではおすまし顔をして鏡見てるので、外で気を抜いていたらびっくりします",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "まぐろ",
            "text": "もう・・わかりすぎます・・",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "いくら",
            "text": "@くまのみ おすまし！！",
            "mention_to": [
                "くまのみ"
            ]
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "いくら",
            "text": "悲しいですねえ",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "いくら",
            "text": "若々しいママでいたい",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "くまのみ",
            "text": "@いくら ほんとそうですよね",
            "mention_to": [
                "いくら"
            ]
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "いくら",
            "text": "やっぱり運動ですかね？食事より",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "くまのみ",
            "text": "整骨院の先生も運動っていってました",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "まぐろ",
            "text": "動くのかー。やはりそこかな",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "いくら",
            "text": "そうなんですね！",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "くまのみ",
            "text": "新陳代謝をあげたらお腹まわりもましになるって",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "いくら",
            "text": "おしりが上がらないですよね、、",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "くまのみ",
            "text": "踏み台昇降いいらしいですよ",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "いくら",
            "text": "新陳代謝！なるほどです",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "くまのみ",
            "text": "おうちの階段で",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "いくら",
            "text": "踏み台！意外と簡単に実践出来ますね！",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "くまのみ",
            "text": "けど、10分やらなきゃいけないらしくて出来てません",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "いくら",
            "text": "10分はしんどい！",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "まぐろ",
            "text": "マンションなので・・駅の階段を往復するか・・",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "いくら",
            "text": "階段に10分、、",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "くまのみ",
            "text": "@いくら 簡単なんですけど、10分はなかなかですよね",
            "mention_to": [
                "いくら"
            ]
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "まぐろ",
            "text": "10分・・",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "いくら",
            "text": "駅の階段も人に見られながら辛いですね",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "くまのみ",
            "text": "@まぐろ 何か台があれば出来るかもですが、危ないかな",
            "mention_to": [
                "まぐろ"
            ]
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "いくら",
            "text": "なるほど、脚立みたいなものでもいいってことですもんね！",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "まぐろ",
            "text": "踏み台かあ！",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "まぐろ",
            "text": "駅でうろうろするよりいいかも！",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "いくら",
            "text": "それならテレビみながらとかできますよね！",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "いくら",
            "text": "駅でうろうろ怪しいですね笑",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "くまのみ",
            "text": "駅は冬、寒いですしね",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "まぐろ",
            "text": "近所でうわさになりそう笑",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "くまのみ",
            "text": "@いくら テレビ見ながらはあんまりっていってはりました",
            "mention_to": [
                "いくら"
            ]
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "いくら",
            "text": "寒いですよね、、",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "いくら",
            "text": "@くまのみ そうなんですか！！",
            "mention_to": [
                "くまのみ"
            ]
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "くまのみ",
            "text": "集中して動かしてるっていう意識も大事だと",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "いくら",
            "text": "あー、なるほどです、、",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "くまのみ",
            "text": "ますますハードル高くなりますよね",
            "mention_to": []
        }
