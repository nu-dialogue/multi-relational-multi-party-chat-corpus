    "relationship": [],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "てんぷら",
            "text": "こんにちは！",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "まりも",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "かにたま",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "てんぷら",
            "text": "ホットプレート買おうか迷っています",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "まりも",
            "text": "ほお",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "てんぷら",
            "text": "持ってますか？",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "かにたま",
            "text": "大小あります",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "まりも",
            "text": "うちはないですね～",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "てんぷら",
            "text": "大小って！",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "まりも",
            "text": "片付けが面倒そうで",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "まりも",
            "text": "大小？",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "てんぷら",
            "text": "そうそう、どうやって洗うんだろう",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "かにたま",
            "text": "小さいのは副菜があるとき",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "まりも",
            "text": "同時に使い分けですか",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "かにたま",
            "text": "@てんぷら プレートとれますよ",
            "mention_to": [
                "てんぷら"
            ]
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "てんぷら",
            "text": "おおお",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "かにたま",
            "text": "@まりも 使い分けてます",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "てんぷら",
            "text": "お料理好きな方とみました",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "まりも",
            "text": "@かにたま すごい上級者感！",
            "mention_to": [
                "かにたま"
            ]
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "かにたま",
            "text": "@てんぷら 大好きです",
            "mention_to": [
                "てんぷら"
            ]
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "てんぷら",
            "text": "結婚して欲しいです",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "まりも",
            "text": "ね～",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "てんぷら",
            "text": "あ、わたし女性です",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "かにたま",
            "text": "わあー ",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "まりも",
            "text": "誰かに料理作ってもらえるっていいですよね",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "てんぷら",
            "text": "最高ですね",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "かにたま",
            "text": "周りがお料理上手で",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "まりも",
            "text": "自分で自分の食べる分作るだけだと",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "てんぷら",
            "text": "ホットプレート、焼きそばでも焼うどんでも盛り上がりそうだなと目論んでいます",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "かにたま",
            "text": "朝から揚げ物したり",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "まりも",
            "text": "張り合いなくて",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "てんぷら",
            "text": "朝から揚げ物！",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "まりも",
            "text": "朝から揚げ物…",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "まりも",
            "text": "揚げ物自体しない…",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "てんぷら",
            "text": "油が飛びますから！",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "かにたま",
            "text": "周りの友達が、、です",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "まりも",
            "text": "お友達すごいですね",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "かにたま",
            "text": "@てんぷら 油めんどうですよね",
            "mention_to": [
                "てんぷら"
            ]
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "かにたま",
            "text": "@まりも 私は朝からはやらないです、、",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "てんぷら",
            "text": "野球部の高校生2人がいるお宅では朝から何キロか唐揚げ揚げるって",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "かにたま",
            "text": "面倒くさいといったら白い目で見られてしまいました",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "まりも",
            "text": "洗い物とか片付けが面倒なんですよね",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "まりも",
            "text": "作るのはいいけど",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "まりも",
            "text": "キロ！",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "かにたま",
            "text": "@てんぷら たいへーん",
            "mention_to": [
                "てんぷら"
            ]
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "まりも",
            "text": "運動部の高校生ってすごいんですね",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "てんぷら",
            "text": "@かにたま おりょうり好きにも上がいるということですね",
            "mention_to": [
                "かにたま"
            ]
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "かにたま",
            "text": "時間もかかりますね",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "てんぷら",
            "text": "自分が中学生高校生のときも",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "てんぷら",
            "text": "確かに1日5食くらい食べていた気がしますもん",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "かにたま",
            "text": "@てんぷら えっ",
            "mention_to": [
                "てんぷら"
            ]
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "まりも",
            "text": "量要るときは揚げ物のほうが楽なのかな",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "てんぷら",
            "text": "おやつに肉まんとあんまんとか",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "かにたま",
            "text": "おやつも入れて？",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "かにたま",
            "text": "あー",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "まりも",
            "text": "なるほど",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "かにたま",
            "text": "@てんぷら 私もおやつばかり食べてました",
            "mention_to": [
                "てんぷら"
            ]
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "てんぷら",
            "text": "顔も肉まんみたいでした",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "てんぷら",
            "text": "今もか？",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "かにたま",
            "text": "@てんぷら 同じです",
            "mention_to": [
                "てんぷら"
            ]
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "てんぷら",
            "text": "お見せ出来なくて残念だわ",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "まりも",
            "text": "最近お菓子食べすぎてます",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "てんぷら",
            "text": "季節限定のお菓子とか気になります",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "かにたま",
            "text": "@まりも 私もです",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "まりも",
            "text": "ピスタチオのチョコとか見かけるとつい",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "てんぷら",
            "text": "美味しそう",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "かにたま",
            "text": "@まりも ピスタチオ美味しいですよね",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "まりも",
            "text": "ドラッグストア行った時に見かけてしまって…",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "かにたま",
            "text": "@まりも 買っちゃいました？",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "まりも",
            "text": "@かにたま ですよね！ピスタチオそのもの食べたことないのに",
            "mention_to": [
                "かにたま"
            ]
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "まりも",
            "text": "買っちゃいました",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "かにたま",
            "text": "@まりも わかるわかるー",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "まりも",
            "text": "パクパク食べてしまいました",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "てんぷら",
            "text": "買っちゃったんだ",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "かにたま",
            "text": "@まりも 大福も美味しいです",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "てんぷら",
            "text": "和風だ",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "まりも",
            "text": "ミロと一緒に食べました",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "てんぷら",
            "text": "ミロは鉄分とカルシウムがありますから",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "まりも",
            "text": "@かにたま 最近知り合いがフルーツ大福食べてて",
            "mention_to": [
                "かにたま"
            ]
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "かにたま",
            "text": "@てんぷら うちにもあるー",
            "mention_to": [
                "てんぷら"
            ]
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "まりも",
            "text": "めっちゃいいなと思ってました",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "てんぷら",
            "text": "フルーツ大福ってイチゴ以外ですか？",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "かにたま",
            "text": "@まりも めっちゃ美味しいです",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "まりも",
            "text": "みかんだったかな？",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "まりも",
            "text": "まるごと入ってるの",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "かにたま",
            "text": "@てんぷら いろいろありますよー",
            "mention_to": [
                "てんぷら"
            ]
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "かにたま",
            "text": "うんうん",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "てんぷら",
            "text": "みなさんお詳しい",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "まりも",
            "text": "映えそうなやつでしたね",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "まりも",
            "text": "おいしいもの見るだけで",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "まりも",
            "text": "心が豊かになります",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "かにたま",
            "text": "萌断",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "てんぷら",
            "text": "マスカットとか？",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "てんぷら",
            "text": "萌え断面！",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "かにたま",
            "text": "と言って、糸で切ると",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "かにたま",
            "text": "可愛い",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "まりも",
            "text": "へえ～！",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "てんぷら",
            "text": "あー！",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "まりも",
            "text": "ちょっとあとで調べてみます",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "かにたま",
            "text": "@てんぷら キウイとか",
            "mention_to": [
                "てんぷら"
            ]
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "てんぷら",
            "text": "糸で切ったらイチゴが切れていなかった失敗動画みました",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "てんぷら",
            "text": "それかー",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "かにたま",
            "text": "@まりも ぜひぜひ",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 103,
            "interlocutor_id": "まりも",
            "text": "切れてないの悲しい",
            "mention_to": []
        },
        {
            "utterance_id": 104,
            "interlocutor_id": "かにたま",
            "text": "@てんぷら そうです！",
            "mention_to": [
                "てんぷら"
            ]
        },
        {
            "utterance_id": 105,
            "interlocutor_id": "てんぷら",
            "text": "笑い声も入っていて良かったです",
            "mention_to": []
        },
        {
            "utterance_id": 106,
            "interlocutor_id": "てんぷら",
            "text": "ほっこり動画",
            "mention_to": []
        },
        {
            "utterance_id": 107,
            "interlocutor_id": "まりも",
            "text": "いちご大好きなんで",
            "mention_to": []
        },
        {
            "utterance_id": 108,
            "interlocutor_id": "てんぷら",
            "text": "いま季節ですよね～",
            "mention_to": []
        },
        {
            "utterance_id": 109,
            "interlocutor_id": "まりも",
            "text": "この時期スーパー行っては買うかどうか",
            "mention_to": []
        },
        {
            "utterance_id": 110,
            "interlocutor_id": "かにたま",
            "text": "私もいちご好き",
            "mention_to": []
        },
        {
            "utterance_id": 111,
            "interlocutor_id": "まりも",
            "text": "迷います",
            "mention_to": []
        }
