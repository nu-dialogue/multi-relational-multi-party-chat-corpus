    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "いくら",
            "text": "こんにちは！",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "しじみ",
            "text": "こんにちは！",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "あずき",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "いくら",
            "text": "普段マスクつけられてますか？",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "しじみ",
            "text": "あー、",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "あずき",
            "text": "つけてますよー",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "しじみ",
            "text": "最近またつけてます！",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "いくら",
            "text": "コロナ禍抜けてもマスクを手放せない",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "しじみ",
            "text": "@いくら つけてますか？",
            "mention_to": [
                "いくら"
            ]
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "あずき",
            "text": "外出時ですよね？",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "いくら",
            "text": "外出時です！",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "あずき",
            "text": "うんうん",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "しじみ",
            "text": "うんうん",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "あずき",
            "text": "化粧が面倒で",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "しじみ",
            "text": "つけますねー",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "いくら",
            "text": "@しじみ もう外せないです",
            "mention_to": [
                "しじみ"
            ]
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "いくら",
            "text": "@あずき ほんとそれです！！！",
            "mention_to": [
                "あずき"
            ]
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "あずき",
            "text": "眉毛だけ書いてます",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "しじみ",
            "text": "@あずき わかります",
            "mention_to": [
                "あずき"
            ]
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "しじみ",
            "text": "わかります〜",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "いくら",
            "text": "わかります、眉毛だけ書きますよね",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "しじみ",
            "text": "めちゃくちゃ共感",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "あずき",
            "text": "楽なんですよね",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "いくら",
            "text": "もう全然化粧してなくてやばいです",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "しじみ",
            "text": "うんうん！",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "あずき",
            "text": "今は防寒？にもなるし",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "しじみ",
            "text": "そうそう！",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "いくら",
            "text": "防寒！でも蒸れますよね",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "あずき",
            "text": "夏は外してたかも",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "あずき",
            "text": "蒸れますね",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "しじみ",
            "text": "確かに",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "いくら",
            "text": "夏は苦しくもなりますよね",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "しじみ",
            "text": "夏はあつい",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "しじみ",
            "text": "ですね！",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "あずき",
            "text": "暑い",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "いくら",
            "text": "布の洗えるマスクのほうが蒸れないのかな？とか思いますね",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "しじみ",
            "text": "あー、",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "いくら",
            "text": "不織布しかつけてないのですが",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "あずき",
            "text": "そっかー",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "あずき",
            "text": "確かに蒸れないかも",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "しじみ",
            "text": "私も不織布派です",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "あずき",
            "text": "私も使い捨てです",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "いくら",
            "text": "そうなんですかね！布にしてみようかな",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "あずき",
            "text": "いいかもですね",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "しじみ",
            "text": "布は、菌が防げるか",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "いくら",
            "text": "ですよねー！清潔感もありますよね",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "しじみ",
            "text": "気になっちゃいますよね！",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "あずき",
            "text": "確かにー",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "しじみ",
            "text": "うんうん",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "いくら",
            "text": "菌！ただのすっぴん隠しになりますね",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "しじみ",
            "text": "ですね！",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "あずき",
            "text": "@いくら ですね",
            "mention_to": [
                "いくら"
            ]
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "いくら",
            "text": "今色んな菌がありますよね",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "あずき",
            "text": "うんうん",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "あずき",
            "text": "コロナもインフルも",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "しじみ",
            "text": "流行ってますね！",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "いくら",
            "text": "今日も学級閉鎖になってるとこあります",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "あずき",
            "text": "花粉もですかねー",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "しじみ",
            "text": "うんうん",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "しじみ",
            "text": "あらー",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "あずき",
            "text": "そうなんですねー",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "しじみ",
            "text": "今年は、",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "あずき",
            "text": "学校は落ち着いてます",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "いくら",
            "text": "花粉もでてきたみたいですね！",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "しじみ",
            "text": "年間通してじわじわ",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "あずき",
            "text": "嫌ですねー",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "しじみ",
            "text": "流行ってますよね",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "いくら",
            "text": "落ち着いてるんですか！いいですねー！",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "あずき",
            "text": "これから凄そうですよね",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "あずき",
            "text": "花粉",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "いくら",
            "text": "いつ我が子がなるかヒヤヒヤです",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "しじみ",
            "text": "確かに、",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "あずき",
            "text": "@いくら 落ち着いてます",
            "mention_to": [
                "いくら"
            ]
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "しじみ",
            "text": "花粉きそう",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "いくら",
            "text": "花粉症ありますか？",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "あずき",
            "text": "私はないです",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "しじみ",
            "text": "@いくら ないです！",
            "mention_to": [
                "いくら"
            ]
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "しじみ",
            "text": "おお！",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "いくら",
            "text": "@あずき うらやましいです！",
            "mention_to": [
                "あずき"
            ]
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "いくら",
            "text": "あら、みんなない、、",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "しじみ",
            "text": "あら、あるんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "いくら",
            "text": "わたしもないんです！",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "あずき",
            "text": "ありますか？",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "しじみ",
            "text": "おお！",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "あずき",
            "text": "おお！！",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "あずき",
            "text": "それはよかった",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "いくら",
            "text": "すごい、",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "あずき",
            "text": "子供はあります",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "しじみ",
            "text": "なりたくないですよね！",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "しじみ",
            "text": "@あずき あらー",
            "mention_to": [
                "あずき"
            ]
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "あずき",
            "text": "耳鼻科で薬もらいますよ",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "しじみ",
            "text": "大変ですよね",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "いくら",
            "text": "子供あるんですか！",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "あずき",
            "text": "そうなんですよー",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "しじみ",
            "text": "結構ひどめですか？",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "いくら",
            "text": "薬処方されるんですよね！",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "しじみ",
            "text": "そうなんですね",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "あずき",
            "text": "春と秋も飲んでます",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "あずき",
            "text": "薬",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "しじみ",
            "text": "秋も！",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "いくら",
            "text": "そういえば、子供目がひどくて、眼科行きました去年",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "しじみ",
            "text": "それは大変！",
            "mention_to": []
        },
        {
            "utterance_id": 102,
