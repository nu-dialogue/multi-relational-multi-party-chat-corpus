{
    "dialogue_id": "A01405",
    "dialogue_type": "First time",
    "interlocutors": [
        "いくら",
        "おでん",
        "はくさい"
    ],
    "relationship": [],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "いくら",
            "text": "こんにちはー！",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "おでん",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "はくさい",
            "text": "こんにちは！",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "いくら",
            "text": "子供が小学校行きたがらないんですよね",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "いくら",
            "text": "そんなことありましたか？",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "おでん",
            "text": "何かあるんでしょうか？",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "いくら",
            "text": "勉強が嫌いみたいで、、",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "おでん",
            "text": "うちはなかったですけど",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "おでん",
            "text": "勉強！嫌ですよね",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "いくら",
            "text": "わたしも学校が好きだったので分からなくて、、",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "はくさい",
            "text": "うちの子は歩いていくのがいやだと、最近ごねてます",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "いくら",
            "text": "歩いて行くの嫌なのも言いますね笑",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "おでん",
            "text": "笑笑",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "いくら",
            "text": "10分とかかかるんですよね、",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "はくさい",
            "text": "うちは小一時間かかるんです。。。",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "おでん",
            "text": "幼稚園、保育園は近かったんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "おでん",
            "text": "1時間",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "おでん",
            "text": "遠いですねー",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "いくら",
            "text": "はくさいさん、それはすごいですね！",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "はくさい",
            "text": "田舎なのでしんどいところです。。。",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "はくさい",
            "text": "最近は寒いから余計に嫌がるんです",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "いくら",
            "text": "幼稚園だったのですが、",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "いくら",
            "text": "車で送り迎えでした、、笑",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "おでん",
            "text": "そうなんですね",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "いくら",
            "text": "ほんと朝とか寒いですよね",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "おでん",
            "text": "朝は寒いけど、昼間はどうですか？",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "いくら",
            "text": "こちらは結構暖かいですね！",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "おでん",
            "text": "私のところも暖かいです。",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "はくさい",
            "text": "こちらは寒いです。。。",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "いくら",
            "text": "公園を見ると半袖の子とかいますね笑",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "おでん",
            "text": "えーすごい！",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "いくら",
            "text": "はくさいさん、雪とかも降る感じですか？",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "はくさい",
            "text": "こちらで半袖はまだまだ先ですね",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "いくら",
            "text": "すごいですよね、",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "はくさい",
            "text": "この間大雪で大変でした",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "いくら",
            "text": "みてて寒いなと思うこともありますよね笑",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "いくら",
            "text": "大雪は大変ですね",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "おでん",
            "text": "雪積もるんですね",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "いくら",
            "text": "わたしのとこも今年は積もりました！",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "はくさい",
            "text": "久しぶりに積もりましたねー",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "おでん",
            "text": "私は去年の暮れに",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "いくら",
            "text": "積もるとお子さんさらに大変ですね",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "はくさい",
            "text": "子どもたちは大喜びで遊んでましたけど",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "おでん",
            "text": "チラチラしただけで",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "いくら",
            "text": "お子さん大喜びなんですね！笑",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "はくさい",
            "text": "大人は雪かきで筋肉痛です",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "おでん",
            "text": "楽しいですよね。",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "おでん",
            "text": "雪かきをするところなんですね、",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "いくら",
            "text": "雪は少し降るくらいがいい気がします！",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "おでん",
            "text": "ほんと、降ったあとが大変ですもんね",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "はくさい",
            "text": "ほんとそうです！！！",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "はくさい",
            "text": "日陰の雪が溶けなくて、駐車場が凍ってて",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "はくさい",
            "text": "今朝車が滑ってしまって、出すのに苦労しました",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "いくら",
            "text": "日陰の雪って残りますよね",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "いくら",
            "text": "もう１週間も経ってるはずなのに残ってるとこ見ると",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "おでん",
            "text": "うんうん",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "いくら",
            "text": "なんか恋しくなりますね雪が、、",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "いくら",
            "text": "雪は車怖いですよね！",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "はくさい",
            "text": "全然恋しくない。。",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "おでん",
            "text": "笑笑",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "いくら",
            "text": "そうですよね笑",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "はくさい",
            "text": "無いものねだりですね",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "おでん",
            "text": "ほんとそうですね",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "いくら",
            "text": "雪が降ってる中でも子供って遊ぶんですよね",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "いくら",
            "text": "風邪引くんじゃないかとヒヤヒヤで",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "おでん",
            "text": "笑笑",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "いくら",
            "text": "今インフルがすごい流行ってるんですよね、、",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "おでん",
            "text": "子供さんは珍しいから嬉しいんでしょうね",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "おでん",
            "text": "インフル、流行ってるんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "いくら",
            "text": "年１ほどしか積もらないので",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "いくら",
            "text": "わかるんですけどね",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "いくら",
            "text": "インフルすごいみたいです",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "はくさい",
            "text": "スキーウェアで走り回っています",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "いくら",
            "text": "学級閉鎖になったりもしてて、",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "はくさい",
            "text": "インフルすごいみたいですね",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "いくら",
            "text": "スキーウェアあるといいですよね！！",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "おでん",
            "text": "集団生活してると、どうしてもね",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "はくさい",
            "text": "学級閉鎖って困りますよね",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "いくら",
            "text": "帽子とかも防水なんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "はくさい",
            "text": "帽子は毛糸です笑",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "いくら",
            "text": "うつっちゃいますよね、、",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "いくら",
            "text": "帽子は毛糸でいけるんですね！",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "いくら",
            "text": "来年は被せてあげよう、、笑",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "おでん",
            "text": "まだ、お二方の学級はインフル大丈夫ですか？",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "いくら",
            "text": "まだ幸いにも平気でして、",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "はくさい",
            "text": "うちの学校はまだ大丈夫ですね",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "おでん",
            "text": "それは良かったです",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "いくら",
            "text": "急に学級閉鎖になると",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "いくら",
            "text": "お昼ご飯悩みますよね",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "はくさい",
            "text": "お昼めんどくさいやつですね笑",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "おでん",
            "text": "そうですよね、急にってのが困りますね",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "いくら",
            "text": "自分だけだとほんと適当で笑",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "おでん",
            "text": "同じくです",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "はくさい",
            "text": "ほんとですよ",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "いくら",
            "text": "今日はもうお昼食べられましたか？",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "おでん",
            "text": "はい、食べました",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "はくさい",
            "text": "食べました",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "いくら",
            "text": "何食べたんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "おでん",
            "text": "いくらさんも食べられました？",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "いくら",
            "text": "わたしも食べました！",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "いくら",
            "text": "ラーメンを、、",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "はくさい",
            "text": "職場なのでコンビニですが、パスタサラダでしたね",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "おでん",
            "text": "私は肉まんと昨日の残りの味噌汁です",
            "mention_to": []
        },
        {
            "utterance_id": 103,
            "interlocutor_id": "いくら",
            "text": "そうなんですね！",
            "mention_to": []
        }
