{
    "dialogue_id": "A05603",
    "dialogue_type": "First time",
    "interlocutors": [
        "まぐろ",
        "ねぎとろ",
        "はくまい"
    ],
    "relationship": [],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "まぐろ",
            "text": "こんにちは！",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "ねぎとろ",
            "text": "こんにちは！",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "はくまい",
            "text": "こんにちはー",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "まぐろ",
            "text": "よろしくおねがいします",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "はくまい",
            "text": "よろしくお願いします",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "ねぎとろ",
            "text": "お願いいたします！",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "まぐろ",
            "text": "子供がいるのですが、",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "ねぎとろ",
            "text": "うんうん",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "はくまい",
            "text": "おぉー！",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "まぐろ",
            "text": "子供が私をママ呼びします。１７才。",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "まぐろ",
            "text": "いつ変えたらいいか・・",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "まぐろ",
            "text": "本人が困ってます",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "はくまい",
            "text": "なるほど…",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "ねぎとろ",
            "text": "そうなんですね。",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "まぐろ",
            "text": "ちょっとした悩みです。親子ともに。",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "ねぎとろ",
            "text": "ちなみに、男の子さんですか？女の子さんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "ねぎとろ",
            "text": "お差し支えなければ",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "まぐろ",
            "text": "男の子なんです",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "はくまい",
            "text": "自分は小学校くらいからお母さん呼びでした",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "ねぎとろ",
            "text": "それは難しいですね。",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "まぐろ",
            "text": "@はくまい そこ！切り替える重要なタイミングでしたね",
            "mention_to": [
                "はくまい"
            ]
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "ねぎとろ",
            "text": "ママから何に変わるかも気になるところですね。",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "はくまい",
            "text": "ですよね",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "まぐろ",
            "text": "@ねぎとろ 確かに・・",
            "mention_to": [
                "ねぎとろ"
            ]
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "まぐろ",
            "text": "急におふくろとか困る・・",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "ねぎとろ",
            "text": "友達は、お袋は嫌だと行ってました。",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "ねぎとろ",
            "text": "そうそう",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "まぐろ",
            "text": "ですよね！！",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "はくまい",
            "text": "お袋、",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "ねぎとろ",
            "text": "@まぐろ やっぱりお母さんはそうなんですね。",
            "mention_to": [
                "まぐろ"
            ]
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "まぐろ",
            "text": "さすがに・・ママ～おふくろは",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "ねぎとろ",
            "text": "@まぐろ なんて呼ばれたいですか？",
            "mention_to": [
                "まぐろ"
            ]
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "はくまい",
            "text": "友達で母上呼びがいますね笑",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "まぐろ",
            "text": "お母さんをはさんでほしい笑",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "まぐろ",
            "text": "@はくまい いいですね！笑",
            "mention_to": [
                "はくまい"
            ]
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "ねぎとろ",
            "text": "母上！",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "まぐろ",
            "text": "それ、有力候補にしたい",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "ねぎとろ",
            "text": "ママ上は？",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "ねぎとろ",
            "text": "なんちゃって",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "はくまい",
            "text": "ママ上笑",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "まぐろ",
            "text": "@ねぎとろ それも、段階踏むのにいいかも！",
            "mention_to": [
                "ねぎとろ"
            ]
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "ねぎとろ",
            "text": "かわいいです。",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "ねぎとろ",
            "text": "息子がいたら、呼ばれたい",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "はくまい",
            "text": "結構ありかも？",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "まぐろ",
            "text": "なんだか、いい案がたくさん！",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "ねぎとろ",
            "text": "おふくろはなんとしても阻止しましょう！",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "はくまい",
            "text": "ですね！",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "まぐろ",
            "text": "本当・・おふくろは・・きつい",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "まぐろ",
            "text": "間違ってはいませんが・・",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "ねぎとろ",
            "text": "お察しします！",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "まぐろ",
            "text": "呼び方って小さい時に決めておくべきでしたね",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "はくまい",
            "text": "小さい頃、本当に重要ですよね",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "ねぎとろ",
            "text": "そうなのですね。",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "まぐろ",
            "text": "今更実感します。",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "まぐろ",
            "text": "照れくさくて変えられなくなるみたいで",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "ねぎとろ",
            "text": "@まぐろ 照れくさいって、男ならますますありそうですね。",
            "mention_to": [
                "まぐろ"
            ]
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "ねぎとろ",
            "text": "男の子",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "はくまい",
            "text": "17歳だと余計にそう思いますよね",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "まぐろ",
            "text": "やっぱり年齢もありますかね～",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "はくまい",
            "text": "ですねー",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "ねぎとろ",
            "text": "彼は、お友だちの前では何と言ってるんだろう、とふと思いました。",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "まぐろ",
            "text": "た・・たしかに",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "はくまい",
            "text": "気になる！",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "まぐろ",
            "text": "ママはなさそうだから・・おかん？？",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "はくまい",
            "text": "母親とか？",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "ねぎとろ",
            "text": "母親が有力候補かも！",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "まぐろ",
            "text": "母親ならいいかな！",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "まぐろ",
            "text": "女の子ならこんな悩みはなかったかな",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "ねぎとろ",
            "text": "@まぐろ おかんはないと思いますよ、予想では。",
            "mention_to": [
                "まぐろ"
            ]
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "はくまい",
            "text": "裏でお袋と呼ばれてるかもしれない…",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "ねぎとろ",
            "text": "なるほど",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "まぐろ",
            "text": "わあ！そのパターンつらい",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "まぐろ",
            "text": "影でおふくろ",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "ねぎとろ",
            "text": "でも、おふくろって、ちょっとオールドファッションのような。",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "はくまい",
            "text": "@まぐろ 女の子ならママでも全然可愛いですよね",
            "mention_to": [
                "まぐろ"
            ]
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "はくまい",
            "text": "オールドファッション笑",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "まぐろ",
            "text": "オールド笑",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "ねぎとろ",
            "text": "母親か、おかあさんだと思う。",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "はくまい",
            "text": "そうですねー",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "まぐろ",
            "text": "あとで・・聞いてみます笑",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "ねぎとろ",
            "text": "お子さんがいると、いろんな気になることがありますね。",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "ねぎとろ",
            "text": "@まぐろ そうですか！頑張って！",
            "mention_to": [
                "まぐろ"
            ]
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "はくまい",
            "text": "学校ではどんな会話してるんだろうとか？",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "まぐろ",
            "text": "気になりますね～",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "ねぎとろ",
            "text": "@はくまい それ、かなり気になりますね。",
            "mention_to": [
                "はくまい"
            ]
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "まぐろ",
            "text": "余計な事話してそう",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "はくまい",
            "text": "ですよね",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "はくまい",
            "text": "親なら気になります",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "はくまい",
            "text": "余計なこと笑",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "まぐろ",
            "text": "うち、友達に自慢で・・",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "ねぎとろ",
            "text": "うんうん",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "まぐろ",
            "text": "お父さん年収80万って言ってきた！って言われたことあります・・",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "ねぎとろ",
            "text": "おーーー",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "ねぎとろ",
            "text": "あ、年収",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "はくまい",
            "text": "お父さん…",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "まぐろ",
            "text": "嘘なら大きくついてきて！と思いました・・",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "ねぎとろ",
            "text": "ですね。月収と間違えた",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "ねぎとろ",
            "text": "かわいいですね。",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "はくまい",
            "text": "結構現実味ある笑",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "ねぎとろ",
            "text": "なんだか、ほっこりしました。",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "まぐろ",
            "text": "いや、素直に年収80が高いと思ってるみたいでした",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "ねぎとろ",
            "text": "かわいい！",
            "mention_to": []
        }
