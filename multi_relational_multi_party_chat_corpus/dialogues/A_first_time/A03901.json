        },
        {
            "utterance_id": 37,
            "interlocutor_id": "こまつな",
            "text": "私もない",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "かにたま",
            "text": "羨ましい、、",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "まりも",
            "text": "金縛りっていっても",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "まりも",
            "text": "頭起きてる体寝てる…って感じだったので",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "かにたま",
            "text": "軽い感じ？",
