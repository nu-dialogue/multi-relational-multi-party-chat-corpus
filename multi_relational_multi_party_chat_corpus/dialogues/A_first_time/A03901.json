            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "かにたま",
            "text": "声も出せず",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "かにたま",
            "text": "@まりも 長く感じました",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "まりも",
            "text": "びっくりしますよね",
