            "mention_to": [
                "こまつな"
            ]
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "かにたま",
            "text": "殺されるー",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "かにたま",
            "text": "って思って",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "こまつな",
            "text": "きゃーです",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "かにたま",
            "text": "うー",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "かにたま",
            "text": "って思いっきり叫びたく",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "かにたま",
            "text": "でもできず",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "かにたま",
            "text": "よく朝娘が",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "こまつな",
            "text": "@かにたま 自然に解けたんですか？",
            "mention_to": [
                "かにたま"
            ]
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "かにたま",
            "text": "ママ、唸り声怖過ぎた！",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "かにたま",
            "text": "って",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "まりも",
            "text": "唸り声…",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "こまつな",
            "text": "うなってたんだ",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "かにたま",
            "text": "@こまつな 無理矢理体捩りましたー",
            "mention_to": [
                "こまつな"
            ]
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "かにたま",
            "text": "心霊体験ありますか？",
