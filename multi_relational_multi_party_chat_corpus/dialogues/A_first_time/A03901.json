        },
        {
            "utterance_id": 42,
            "interlocutor_id": "まりも",
            "text": "軽さはよくわからないですけど",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "かにたま",
            "text": "押さえつけられて",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "まりも",
            "text": "怖いというより夢なのかどうか",
