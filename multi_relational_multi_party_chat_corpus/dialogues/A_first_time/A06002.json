            "text": "おはようございます",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "あざらし",
            "text": "おはようございます！",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "きりん",
            "text": "おはようございます～",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "こぐま",
            "text": "よろしくです",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "あざらし",
            "text": "よろしくです",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "きりん",
            "text": "よろしくお願いします",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "こぐま",
            "text": "もうすぐ",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "こぐま",
            "text": "ひな祭りですが",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "あざらし",
            "text": "そうですね！",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "こぐま",
            "text": "何かしたりとかありますか？",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "きりん",
            "text": "ちらしずしおいしそうだなと",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "あざらし",
            "text": "雛人形出しました",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "きりん",
            "text": "思っているけど何もせず",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "あざらし",
            "text": "@きりん 美味しそうです",
            "mention_to": [
                "きりん"
            ]
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "こぐま",
            "text": "美味しそうですね",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "こぐま",
            "text": "@あざらし 飾られるんですね",
            "mention_to": [
                "あざらし"
            ]
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "きりん",
            "text": "@あざらし 女の子がいるんですか？",
            "mention_to": [
                "あざらし"
            ]
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "あざらし",
            "text": "@こぐま 子供が飾りたいと",
            "mention_to": [
                "こぐま"
            ]
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "あざらし",
            "text": "言います",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "こぐま",
            "text": "女の子がいるご家庭は",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "あざらし",
            "text": "@きりん そうなんですよ",
            "mention_to": [
                "きりん"
            ]
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "こぐま",
            "text": "華やかでいいですね",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "あざらし",
            "text": "一気に華やかになります",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "きりん",
            "text": "どんなお雛様ですか",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "あざらし",
            "text": "春ですね",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "あざらし",
            "text": "3段飾りです",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "こぐま",
            "text": "春間近を、感じますね",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "きりん",
            "text": "ほほ～",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "あざらし",
            "text": "子供とあれこれ言いながら",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "こぐま",
            "text": "@あざらし 3段！！",
            "mention_to": [
                "あざらし"
            ]
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "あざらし",
            "text": "飾る時間も楽しいです",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "きりん",
            "text": "ああいうの小物とか",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "きりん",
            "text": "好きです",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "あざらし",
            "text": "@きりん かわいいですよね",
            "mention_to": [
                "きりん"
            ]
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "こぐま",
            "text": "おままごとみたいですよね",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "あざらし",
            "text": "私も好きです",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "あざらし",
            "text": "雅な感じ",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "あざらし",
            "text": "おままごとみたいですよね",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "きりん",
            "text": "子供の頃ちょっと壊しちゃった記憶があります",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "あざらし",
            "text": "小さいけど重厚感あって",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "きりん",
            "text": "お雛様の飾り",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "あざらし",
            "text": "わかります",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "こぐま",
            "text": "小さい頃、おままごとに使って、怒られた記憶が",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "あざらし",
            "text": "子供がもっと小さい頃に",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "あざらし",
            "text": "抱っこして",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "あざらし",
            "text": "髪が乱れてます",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "こぐま",
            "text": "ふんふん",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "こぐま",
            "text": "可愛いー",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "きりん",
            "text": "かわいい思い出ですね",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "あざらし",
            "text": "人形のおもちゃと勘違いしますよね",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "こぐま",
            "text": "お子様がしたこと、怒れないですね",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "きりん",
            "text": "触っちゃうのしょうがないですよね",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "あざらし",
            "text": "そうなんですよー",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "あざらし",
            "text": "ぼんぼりとか振り回して",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "こぐま",
            "text": "まぁ、その子のお雛様ですもんね",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "あざらし",
            "text": "いました",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "あざらし",
            "text": "そうですねー",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "きりん",
            "text": "ぼんぼりは",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "こぐま",
            "text": "@あざらし 笑活発ですね",
            "mention_to": [
                "あざらし"
            ]
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "あざらし",
            "text": "娘のですもんね",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "きりん",
            "text": "わんぱくですね！",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "あざらし",
            "text": "@きりん わんぱくですね笑",
            "mention_to": [
                "きりん"
            ]
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "こぐま",
            "text": "ね！ですね！",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "こぐま",
            "text": "片付けるのは、終わったらすぐにですか？",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "あざらし",
            "text": "@こぐま そうですね",
            "mention_to": [
                "こぐま"
            ]
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "あざらし",
            "text": "片付けるのも大変です",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "こぐま",
            "text": "片付けは早くしないと⋯ってありますよね",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "あざらし",
            "text": "ずっと出しておくと",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "きりん",
            "text": "片付けのほうが",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "こぐま",
            "text": "はい",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "あざらし",
            "text": "お嫁に行けない",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "あざらし",
            "text": "とかいうんで",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "あざらし",
            "text": "早く片付けます",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "きりん",
            "text": "気分的にちょっとめんどくさそうです",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "こぐま",
            "text": "やっぱり、そう言いますよね",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "あざらし",
            "text": "@きりん そうなんですよー",
            "mention_to": [
                "きりん"
            ]
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "きりん",
            "text": "@あざらし よく言われてますよね",
            "mention_to": [
                "あざらし"
            ]
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "あざらし",
            "text": "適当に出来ないし",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "あざらし",
            "text": "気を使いますね",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "きりん",
            "text": "そうですよね",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "こぐま",
            "text": "出すのは楽しいけど、片付けは大変そうです",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "あざらし",
            "text": "@こぐま 片付けは子供も",
            "mention_to": [
                "こぐま"
            ]
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "あざらし",
            "text": "手伝ってくれない",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "あざらし",
            "text": "私が頑張って",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "こぐま",
            "text": "なるほど",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "あざらし",
            "text": "片付けます",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "きりん",
            "text": "あら…そうなんですか",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "あざらし",
            "text": "悲しい笑",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "きりん",
            "text": "一緒にやってくれたらいいですね",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "こぐま",
            "text": "出すのは娘さんと一緒にですか？",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "あざらし",
            "text": "出すのは一緒に出しました",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "こぐま",
            "text": "そうなんだ",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "きりん",
            "text": "楽しいとこだけ？",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "あざらし",
            "text": "もう早く見たい触りたいで",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "こぐま",
            "text": "ちゃっかりされてますね",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "あざらし",
            "text": "楽しいところだけですよ",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "あざらし",
            "text": "調子いいですよね",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "こぐま",
            "text": "可愛らしい、子供らしいです",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "あざらし",
            "text": "確かに笑",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "きりん",
            "text": "子供の頃はそんな感じだったかも",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "あざらし",
            "text": "@きりん 私もです",
            "mention_to": [
                "きりん"
            ]
        },
