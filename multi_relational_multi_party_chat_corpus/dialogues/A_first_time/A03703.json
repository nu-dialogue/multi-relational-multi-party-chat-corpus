    "relationship": [],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "げんまい",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "かにたま",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "なると",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "げんまい",
            "text": "よろしくお願いします。",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "かにたま",
            "text": "お願いします",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "なると",
            "text": "お願いします",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "げんまい",
            "text": "ところで花粉症は大丈夫ですか？",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "かにたま",
            "text": "大丈夫です！",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "げんまい",
            "text": "うらやましい",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "なると",
            "text": "花粉なのか鼻がかゆいですね",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "かにたま",
            "text": "家族はひどいですが、、",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "げんまい",
            "text": "@なると 診断はされてないんですね",
            "mention_to": [
                "なると"
            ]
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "かにたま",
            "text": "今年早いですよね？",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "なると",
            "text": "@げんまい そうですね",
            "mention_to": [
                "げんまい"
            ]
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "げんまい",
            "text": "うーん。早いような。",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "なると",
            "text": "そうなんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "げんまい",
            "text": "でも、毎年そんな感じかなとも思ったり",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "かにたま",
            "text": "1月から",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "げんまい",
            "text": "私は1月から薬飲んでます。",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "なると",
            "text": "@げんまい 薬を飲むくらいなんですね",
            "mention_to": [
                "げんまい"
            ]
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "かにたま",
            "text": "@げんまい 家族も一緒〜",
            "mention_to": [
                "げんまい"
            ]
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "げんまい",
            "text": "薬なしでは乗り越えられません。。。",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "かにたま",
            "text": "わかるわかる",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "なると",
            "text": "大変ですね",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "げんまい",
            "text": "早めに飲みはじめるとそのシーズンは楽だったりします。",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "なると",
            "text": "やっぱり効くんですね",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "かにたま",
            "text": "そうなんですね",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "げんまい",
            "text": "耳鼻科で処方してもらってるので、効きます。",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "かにたま",
            "text": "3ヶ月分まとめて貰ってましたー",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "なると",
            "text": "そうなんですね",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "げんまい",
            "text": "@かにたま 市販の薬ですか？",
            "mention_to": [
                "かにたま"
            ]
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "かにたま",
            "text": "@げんまい 病院です！",
            "mention_to": [
                "げんまい"
            ]
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "げんまい",
            "text": "病院って1ヶ月分しか出してくれないんですけど、",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "げんまい",
            "text": "言えば3か月もらるのかな？",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "かにたま",
            "text": "先生が面倒くさいっていって",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "なると",
            "text": "1か月ごとは面倒ですね",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "げんまい",
            "text": "先生が？！",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "かにたま",
            "text": "@げんまい そうなんです",
            "mention_to": [
                "げんまい"
            ]
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "かにたま",
            "text": "びっくりしましたー",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "げんまい",
            "text": "来週あたり、薬がなくなるので、もらいに行ってきます。",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "げんまい",
            "text": "まとめて出してほしいって、言ってみます。",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "なると",
            "text": "そうですね",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "かにたま",
            "text": "@げんまい いいと思いますよ",
            "mention_to": [
                "げんまい"
            ]
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "げんまい",
            "text": "3か月分ももらえたら、1シーズンもちますね。",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "かにたま",
            "text": "そうなんですよ",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "なると",
            "text": "春先から大変なんですかね",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "げんまい",
            "text": "これから１～2ヶ月がピークかなー",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "なると",
            "text": "そうなんですね",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "かにたま",
            "text": "今もすごいですよね",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "げんまい",
            "text": "桜が散る頃に、落ち着きます。",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "かにたま",
            "text": "うんうん",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "なると",
            "text": "1シーズンでおさまるんですかね",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "かにたま",
            "text": "見ていて可哀想になる…",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "げんまい",
            "text": "毎年もれなくやってきますけどね。",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "なると",
            "text": "嫌ですね",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "かにたま",
            "text": "花粉症って大変ですよね",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "げんまい",
            "text": "妊娠中、授乳中は薬が飲めなくて、ほんと大変でした",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "かにたま",
            "text": "どうしてたんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "かにたま",
            "text": "我慢？",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "げんまい",
            "text": "ただ、耐えていました。",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "かにたま",
            "text": "キツイですねー",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "げんまい",
            "text": "ティッシュのボックスかかえて",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "かにたま",
            "text": "可哀想に、、",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "げんまい",
            "text": "今は、薬で抑えられるからまだいいです。",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "なると",
            "text": "大変だ",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "げんまい",
            "text": "ジャンジャン飲みますｗｗｗ",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "かにたま",
            "text": "笑",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "かにたま",
            "text": "わかるわかる",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "かにたま",
            "text": "爆笑しちゃいました",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "げんまい",
            "text": "洗濯物は外に干す派ですか？",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "かにたま",
            "text": "中です",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "なると",
            "text": "普段は外ですね",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "げんまい",
            "text": "私は外に干しちゃう。",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "かにたま",
            "text": "夏は外",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "なると",
            "text": "天気悪い時は中です",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "げんまい",
            "text": "家族が花粉症じゃないから。",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "なると",
            "text": "花粉がつくんですね",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "げんまい",
            "text": "ですです",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "かにたま",
            "text": "付きますね",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "なると",
            "text": "それもつらいですね",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "げんまい",
            "text": "@かにたま 室内で乾きますか？",
            "mention_to": [
                "かにたま"
            ]
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "げんまい",
            "text": "乾燥機とか？",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "かにたま",
            "text": "乾きますよ",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "げんまい",
            "text": "そうなんだ。",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "かにたま",
            "text": "半々で使います！",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "かにたま",
            "text": "あと",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "かにたま",
            "text": "暖房で乾いちゃう",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "なると",
            "text": "暖房の時期は",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "げんまい",
            "text": "そうですね。",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "なると",
            "text": "乾きやすいですね",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "かにたま",
            "text": "はい",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "げんまい",
            "text": "冬場は暖房でけっこう乾きますよね。",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "げんまい",
            "text": "今日は、都内、雨で乾かないので、",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "なると",
            "text": "そうですよね",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "げんまい",
            "text": "コインランドリーに持て行きました",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "なると",
            "text": "都内の方は中のイメージです",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "かにたま",
            "text": "@なると 都内です！",
            "mention_to": [
                "なると"
            ]
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "げんまい",
            "text": "外に干したほうが早く乾くので、基本外です。",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "なると",
            "text": "そうなんですね",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "かにたま",
            "text": "花粉の時期以外は外かな",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "なると",
            "text": "都会の方が花粉も多いんですかね",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "げんまい",
            "text": "どうなんでしょうね。",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "かにたま",
            "text": "どうなんだろ？",
            "mention_to": []
        }
