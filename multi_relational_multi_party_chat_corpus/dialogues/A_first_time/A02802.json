{
    "dialogue_id": "A02802",
    "dialogue_type": "First time",
    "interlocutors": [
        "てんぷら",
        "ねぎとろ",
        "ハンバーグ"
    ],
    "relationship": [],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "てんぷら",
            "text": "こんにちは！",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "ねぎとろ",
            "text": "こんにちは！",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "ハンバーグ",
            "text": "こんにちは！",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "てんぷら",
            "text": "朝ごはん、食べましたか？",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "ハンバーグ",
            "text": "まだです",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "てんぷら",
            "text": "あらあら",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "ハンバーグ",
            "text": "お腹空きました",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "ねぎとろ",
            "text": "まだです！",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "てんぷら",
            "text": "えええ！",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "てんぷら",
            "text": "まさかの全員朝ごはん抜き",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "ハンバーグ",
            "text": "ええええ",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "てんぷら",
            "text": "私もコーヒーしか食べていません",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "ねぎとろ",
            "text": "おなかすいて、さっきチョコたべました。",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "てんぷら",
            "text": "手軽に栄養補給ですね",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "ねぎとろ",
            "text": "あまり良くないとは思いつつ",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "てんぷら",
            "text": "脳にダイレクトに効く気がします、チョコ",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "ねぎとろ",
            "text": "正にそうです！",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "ハンバーグ",
            "text": "りんごジュース飲みました",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "ねぎとろ",
            "text": "狙いは",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "ねぎとろ",
            "text": "健康的",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "てんぷら",
            "text": "@ハンバーグ 身体に良さそうです",
            "mention_to": [
                "ハンバーグ"
            ]
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "てんぷら",
            "text": "健康オタクの友人の名台詞が",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "ねぎとろ",
            "text": "うんうん",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "てんぷら",
            "text": "健康のためなら死んでもいい",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "ハンバーグ",
            "text": "りんごジュース、オレンジジュースはよく朝飲みます",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "ねぎとろ",
            "text": "すごい",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "ねぎとろ",
            "text": "本末転倒",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "ハンバーグ",
            "text": "子供だなってよく言われます",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "てんぷら",
            "text": "本末転倒ってこのことかと思いました",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "てんぷら",
            "text": "いや、ジュース良さそうじゃないですか",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "てんぷら",
            "text": "身体に",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "ねぎとろ",
            "text": "@ハンバーグ 健康的ですね",
            "mention_to": [
                "ハンバーグ"
            ]
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "てんぷら",
            "text": "コーヒーだけより良さそうです",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "ハンバーグ",
            "text": "健康的かはわかりませんがよく飲んでます",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "てんぷら",
            "text": "何年か前に、ジューサー買いました",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "ハンバーグ",
            "text": "おー",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "てんぷら",
            "text": "リンゴと人参の",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "ねぎとろ",
            "text": "おーー",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "てんぷら",
            "text": "ジュースを配偶者がよく作ってくれました",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "ハンバーグ",
            "text": "自分は買ってめんどくさくなって使わなくなりそう",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "てんぷら",
            "text": "今あのジューサー、どこにあるんだろう？",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "ねぎとろ",
            "text": "おいしそうで、健康的。",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "ねぎとろ",
            "text": "え？",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "てんぷら",
            "text": "@ハンバーグ まさにそれです",
            "mention_to": [
                "ハンバーグ"
            ]
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "ハンバーグ",
            "text": "最初は使うけど",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "ハンバーグ",
            "text": "ですよね",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "てんぷら",
            "text": "まさにその良い例です",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "てんぷら",
            "text": "私は作ってもらうだけだったので",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "ハンバーグ",
            "text": "じゃ今日探して作ってみてください",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "てんぷら",
            "text": "ほんとです",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "ねぎとろ",
            "text": "使うのはいいけど、洗ったりするのが面倒そう",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "てんぷら",
            "text": "ちなみに、炭酸を作るマシーンがあります",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "ハンバーグ",
            "text": "へぇー",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "てんぷら",
            "text": "@ねぎとろ 洗うのは私の役目でした",
            "mention_to": [
                "ねぎとろ"
            ]
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "ねぎとろ",
            "text": "@てんぷら いいですね、それ。",
            "mention_to": [
                "てんぷら"
            ]
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "てんぷら",
            "text": "洗い物は好きです",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "ハンバーグ",
            "text": "自分も買おうと思ったことあります",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "てんぷら",
            "text": "炭酸作るシリンダーの交換が面倒ですが",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "ねぎとろ",
            "text": "@てんぷら うちにいらっしゃいませんか。",
            "mention_to": [
                "てんぷら"
            ]
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "てんぷら",
            "text": "@ねぎとろ 洗い物係ですか、よろこんで",
            "mention_to": [
                "ねぎとろ"
            ]
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "ハンバーグ",
            "text": "面倒なのは嫌だけど興味あります",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "てんぷら",
            "text": "食洗器があるのですが、たまに手で洗いたくなって",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "てんぷら",
            "text": "洗っちゃいます",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "てんぷら",
            "text": "料理は苦手です",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "ハンバーグ",
            "text": "自分、料理はするけど洗い物やってもらいます",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "ねぎとろ",
            "text": "食洗機の前の予備洗いが面倒で、、、",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "ねぎとろ",
            "text": "でもしないと気持ち悪いし",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "てんぷら",
            "text": "乾いた皿にチーズがカピカピについていたりすると",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "ハンバーグ",
            "text": "予備洗いするなら自分で洗っちゃいそうです",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "ねぎとろ",
            "text": "@ハンバーグ いいな",
            "mention_to": [
                "ハンバーグ"
            ]
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "てんぷら",
            "text": "あらあら、食洗器め、と思います",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "てんぷら",
            "text": "お昼ご飯どうしようかな",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "てんぷら",
            "text": "作ろうか食べに行っちゃおうか",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "ハンバーグ",
            "text": "あさは食べない感じですか？",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "てんぷら",
            "text": "今日は食べない感じになりそうです",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "ハンバーグ",
            "text": "お昼にがっつりですね",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "てんぷら",
            "text": "がっつりいきたいです",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "てんぷら",
            "text": "デザートまで行きたいですね",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "ハンバーグ",
            "text": "おやつつままないとお昼までもたなそう",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "てんぷら",
            "text": "このあいだ、朝食べずに昼も食べ損ねて",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "ねぎとろ",
            "text": "おやつ、食べちゃいますね。",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "てんぷら",
            "text": "昼過ぎに仕事で失敗して落ち込んで",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "てんぷら",
            "text": "夜も食べられなかった日がありました",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "ハンバーグ",
            "text": "空腹はつらい",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "てんぷら",
            "text": "手が震えました",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "てんぷら",
            "text": "低血糖か？",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "ねぎとろ",
            "text": "@てんぷら それは大変でしたね。",
            "mention_to": [
                "てんぷら"
            ]
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "てんぷら",
            "text": "次の日には立ち直りました",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "ねぎとろ",
            "text": "ですね。",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "てんぷら",
            "text": "お腹すきすぎてたので",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "てんぷら",
            "text": "ええ。",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "ハンバーグ",
            "text": "立ち直ったんですね",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "ねぎとろ",
            "text": "低血糖で、気を失ったことあります。",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "てんぷら",
            "text": "わあ！",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "ハンバーグ",
            "text": "それは危険",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "ねぎとろ",
            "text": "正に、失恋で何も食べられず",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "ハンバーグ",
            "text": "ちゃんと食べて下さい",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "てんぷら",
            "text": "わあ！！",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "ハンバーグ",
            "text": "なるほど",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "ねぎとろ",
            "text": "今は、そんなことはありませんが。",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "ハンバーグ",
            "text": "その気持ちわかります",
            "mention_to": []
        }
    ],
