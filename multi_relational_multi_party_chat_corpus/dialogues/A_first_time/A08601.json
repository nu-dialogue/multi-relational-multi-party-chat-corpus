        },
        {
            "utterance_id": 40,
            "interlocutor_id": "あまえび",
            "text": "焼き肉いいですね！",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "しじみ",
            "text": "肉といえば焼肉ですね！",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "しらす",
            "text": "@しじみ しませんね、汚れとにおいが怖い笑",
