    "relationship": [],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "おにぎり",
            "text": "こんにちは。",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "げんまい",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "てんぷら",
            "text": "こんにちはー",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "おにぎり",
            "text": "皆さんは、最近何かお買い物しましたか？",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "げんまい",
            "text": "買い物かーネットで買う事が多いです。",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "てんぷら",
            "text": "卒業式に着る服を物色していますが",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "てんぷら",
            "text": "まだ買っていません",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "げんまい",
            "text": "卒業式！",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "おにぎり",
            "text": "てんぷらさんは、卒業式なんですね！",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "げんまい",
            "text": "もうすぐですね",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "てんぷら",
            "text": "中三です",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "てんぷら",
            "text": "受験が終わっていないんですけど",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "おにぎり",
            "text": "あれ、受験ですか？！",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "おにぎり",
            "text": "2月ですか？",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "てんぷら",
            "text": "来週かな？",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "げんまい",
            "text": "決まるまで、落ち着きませんね",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "おにぎり",
            "text": "追い込みですね～",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "げんまい",
            "text": "がんばれー",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "てんぷら",
            "text": "服くらい決めてしまいたいという",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "てんぷら",
            "text": "私の服ですけど",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "げんまい",
            "text": "決められる事は決めたいですね。w",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "おにぎり",
            "text": "親の服は、ダークな色が多いですか？",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "てんぷら",
            "text": "卒業式は暗めっていいますね",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "おにぎり",
            "text": "紺色とかかな。",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "げんまい",
            "text": "入学式よりは暗めですよね",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "げんまい",
            "text": "私は、華やかなワンピース着ちゃうけど",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "てんぷら",
            "text": "リボンタイみたいなのが付いたブラウスが流行ってて",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "おにぎり",
            "text": "スーツは、長く着たいし迷いますね。",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "げんまい",
            "text": "買うなら吟味した",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "げんまい",
            "text": "い",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "おにぎり",
            "text": "なるほど。",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "てんぷら",
            "text": "そう、普段も使いたいです",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "おにぎり",
            "text": "だんだん、昔よりはカジュアルになってきた気もしますね。",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "げんまい",
            "text": "着物の人も少ないですしね",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "おにぎり",
            "text": "着物は、ほとんど見ないですね。。",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "てんぷら",
            "text": "ひざ丈のタイトスカートもあまり見かけません",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "おにぎり",
            "text": "確かに！",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "げんまい",
            "text": "短いのも流行ってませんね",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "おにぎり",
            "text": "パンツも多いですよね。",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "てんぷら",
            "text": "ハイヒールよりはローファーとか",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "げんまい",
            "text": "あーそうかも。靴",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "おにぎり",
            "text": "去年の娘の卒業式で、ブラウスっぽいのだけの方とかいて",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "おにぎり",
            "text": "楽でいいな、と思いました。",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "げんまい",
            "text": "ま、自由ですからね。",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "てんぷら",
            "text": "そういえばスーツの女子もいました、小学校の卒業式",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "げんまい",
            "text": "主役は親じゃないしw",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "てんぷら",
            "text": "そうそう",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "げんまい",
            "text": "スーツ女子はめずらしい",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "げんまい",
            "text": "AKBみたいな子が多いなーという印象でした",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "おにぎり",
            "text": "こちらは、小学校の卒業式、中学の制服なんです。",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "てんぷら",
            "text": "それはそれで楽ですねえ",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "おにぎり",
            "text": "びっくりしました。",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "げんまい",
            "text": "制服なら考えなくていいですね",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "げんまい",
            "text": "どこの学校に行くのか制服でわかっちゃうな",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "おにぎり",
            "text": "そうなんですよ。",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "おにぎり",
            "text": "私立とか行く子、全員にわかってしまう笑",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "げんまい",
            "text": "わー目立つ。",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "おにぎり",
            "text": "私は、最近スープジャーっていうんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "おにぎり",
            "text": "お弁当の時持ってける保温の？",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "げんまい",
            "text": "あーお弁当箱。保温のありますね",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "おにぎり",
            "text": "娘に頼まれ買ったのですが、使いこなせなくて。",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "げんまい",
            "text": "去年の冬はヘビロテでしたあったか弁当",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "おにぎり",
            "text": "げんまいさんのは、お弁当箱ですか？",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "げんまい",
            "text": "保温のお弁当箱も使っています",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "おにぎり",
            "text": "うちのは、スープとか液体を入れるもので。",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "げんまい",
            "text": "スープジャーも使います",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "おにぎり",
            "text": "何を入れてます？！",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "おにぎり",
            "text": "スープなんでしょうけど笑",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "てんぷら",
            "text": "友達がレトルトカレー入れていましたけど",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "げんまい",
            "text": "そうですね。",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "げんまい",
            "text": "スープです",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "おにぎり",
            "text": "朝作っていれてます？",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "おにぎり",
            "text": "カレーか！",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "てんぷら",
            "text": "保温弁当に白米",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "げんまい",
            "text": "けっこう熱めにしておかないと、冷めますよね。",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "てんぷら",
            "text": "冷めるのですね",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "おにぎり",
            "text": "保温弁当箱もあるんですか？！",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "てんぷら",
            "text": "冷めたレトルトカレーはちょっといやだな",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "げんまい",
            "text": "保温のお弁当箱は、熱湯で容器を温めて、",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "おにぎり",
            "text": "へぇ！",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "げんまい",
            "text": "容器にいれたご飯をレンチンして熱々にして蓋しめます",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "てんぷら",
            "text": "それいいですね",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "おにぎり",
            "text": "腐りやすくないのかな、って心配なんですが。。",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "げんまい",
            "text": "ぬるいと悪くなると思います。",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "げんまい",
            "text": "だから、熱々にします。",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "てんぷら",
            "text": "おおお",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "おにぎり",
            "text": "熱々がポイントですね。",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "げんまい",
            "text": "でもね、かさばるので今年は使っていません。",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "てんぷら",
            "text": "かさばりそうです。重いですか？",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "おにぎり",
            "text": "そうなんですよ！",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "げんまい",
            "text": "そう。かさばるし重いし",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "おにぎり",
            "text": "飲み終わって、そのまま夜持って帰ってきたら、こびりついてそうだし。",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "げんまい",
            "text": "最近は、曲げワッパを愛用",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "げんまい",
            "text": "冷めてもご飯がおいしいです。",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "てんぷら",
            "text": "あ、気になりますまげわっぱ",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "おにぎり",
            "text": "ワッパ、いいですよね！",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "げんまい",
            "text": "軽くていいです",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "おにぎり",
            "text": "うちもです！",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "おにぎり",
            "text": "そうそう、なんだか美味しそうにみえますよね。",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "げんまい",
            "text": "おにぎりさんも！",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "げんまい",
            "text": "そうそう。おいしそうに見えるのはポイント高い",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "てんぷら",
            "text": "皆さん、試行錯誤されていますね",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "おにぎり",
            "text": "漏れるのだけ、心配でしたが、意外と大丈夫ですね。",
            "mention_to": []
        },
        {
            "utterance_id": 103,
            "interlocutor_id": "おにぎり",
            "text": "てんぷらさんは、弁当作りはないですか？",
            "mention_to": []
        },
        {
            "utterance_id": 104,
            "interlocutor_id": "てんぷら",
            "text": "息子の受験が終わったら、弁当作りが始まります",
            "mention_to": []
        }
