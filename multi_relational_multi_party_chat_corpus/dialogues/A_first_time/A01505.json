        },
        {
            "utterance_id": 35,
            "interlocutor_id": "きなこ",
            "text": "うんうん",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "きなこ",
            "text": "ラーメンは国民食ですね",
