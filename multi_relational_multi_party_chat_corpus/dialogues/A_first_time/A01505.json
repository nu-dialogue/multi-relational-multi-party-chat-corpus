        },
        {
            "utterance_id": 13,
            "interlocutor_id": "きなこ",
            "text": "うちもです",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "きなこ",
            "text": "色々な国の料理もでるみたいですね",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "あずき",
            "text": "@きなこ そうなんですね",
