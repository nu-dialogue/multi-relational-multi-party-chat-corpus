            "interlocutor_id": "きなこ",
            "text": "@がんも おいしそう！",
            "mention_to": [
                "がんも"
            ]
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "あずき",
            "text": "ただのラーメンじゃないところが",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "あずき",
            "text": "いいですね",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "きなこ",
            "text": "あんかけで温まりそう",
