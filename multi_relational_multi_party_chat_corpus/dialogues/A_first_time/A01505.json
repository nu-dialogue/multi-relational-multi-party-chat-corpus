            ]
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "あずき",
            "text": "多国籍ですね",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "きなこ",
            "text": "家で作れないものだった記憶があります",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "あずき",
            "text": "メロンパンとか",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "あずき",
            "text": "パンも充実してるみたいで",
