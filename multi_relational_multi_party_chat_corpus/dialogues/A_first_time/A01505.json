        },
        {
            "utterance_id": 20,
            "interlocutor_id": "きなこ",
            "text": "いいなあ～",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "あずき",
            "text": "昔はコッペパンだけでした",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "あずき",
            "text": "@きなこ いいですよねー",
            "mention_to": [
                "きなこ"
            ]
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "きなこ",
            "text": "わかります～",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "がんも",
            "text": "昨日は五目ラーメンだったみたいです",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "あずき",
            "text": "@がんも 美味しそう",
            "mention_to": [
                "がんも"
            ]
        },
        {
