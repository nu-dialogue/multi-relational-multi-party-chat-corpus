        },
        {
            "utterance_id": 30,
            "interlocutor_id": "あずき",
            "text": "うんうん",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "がんも",
            "text": "栄養も考えられていますね",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "あずき",
            "text": "冬にいいですね",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "あずき",
            "text": "@がんも 考えられてますよねー",
            "mention_to": [
                "がんも"
            ]
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "あずき",
