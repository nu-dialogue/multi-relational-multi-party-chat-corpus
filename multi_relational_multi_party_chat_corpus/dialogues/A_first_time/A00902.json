    "relationship": [],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "あずき",
            "text": "こんにちは！",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "しじみ",
            "text": "こんにちはー",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "つぶあん",
            "text": "こんにちは！",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "あずき",
            "text": "今日はこれから参観日なんです",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "しじみ",
            "text": "あら！",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "あずき",
            "text": "楽しみです",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "つぶあん",
            "text": "忙しいですね！",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "しじみ",
            "text": "@あずき お忙しいですね",
            "mention_to": [
                "あずき"
            ]
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "しじみ",
            "text": "小学校ですか？",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "あずき",
            "text": "少し疲れちゃいますけどね",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "あずき",
            "text": "@しじみ 小学校です",
            "mention_to": [
                "しじみ"
            ]
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "あずき",
            "text": "懇談会もあるので長いです",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "しじみ",
            "text": "懇談会大変そう〜",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "つぶあん",
            "text": "@あずき 何年生ですか？",
            "mention_to": [
                "あずき"
            ]
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "あずき",
            "text": "@つぶあん 5年です",
            "mention_to": [
                "つぶあん"
            ]
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "つぶあん",
            "text": "おお！授業も難しそう！",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "しじみ",
            "text": "懇談会って何されるんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "あずき",
            "text": "立って聞いてるのが大変",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "あずき",
            "text": "懇談会は先生のお話です",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "しじみ",
            "text": "@つぶあん 確かに、授業難しそう！",
            "mention_to": [
                "つぶあん"
            ]
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "あずき",
            "text": "聞いてるだけねんですけどね",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "しじみ",
            "text": "@あずき そうなんですね！",
            "mention_to": [
                "あずき"
            ]
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "しじみ",
            "text": "我が家は幼稚園児なので、",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "しじみ",
            "text": "未知の世界です",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "つぶあん",
            "text": "@あずき うちは懇談会座ります。",
            "mention_to": [
                "あずき"
            ]
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "しじみ",
            "text": "@つぶあん 場所によって違うんですね",
            "mention_to": [
                "つぶあん"
            ]
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "あずき",
            "text": "今日は体育館で発表するらしいです",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "つぶあん",
            "text": "@しじみ 幼稚園の参観はたのしいです〜ね！",
            "mention_to": [
                "しじみ"
            ]
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "しじみ",
            "text": "体育館、広いから寒くないといいですね！",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "つぶあん",
            "text": "@あずき 体育館ですか！",
            "mention_to": [
                "あずき"
            ]
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "あずき",
            "text": "@つぶあん 幼稚園、懐かしいです",
            "mention_to": [
                "つぶあん"
            ]
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "しじみ",
            "text": "幼稚園児の参観日はただただ可愛いです！",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "あずき",
            "text": "本当ですね〜",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "あずき",
            "text": "癒されます",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "しじみ",
            "text": "来年ランドセル選びなんですが、",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "つぶあん",
            "text": "ですね！",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "あずき",
            "text": "おお！",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "あずき",
            "text": "いいですね！",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "しじみ",
            "text": "みなさんどうやって選びました？",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "あずき",
            "text": "決まりました？",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "つぶあん",
            "text": "同じです！",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "しじみ",
            "text": "@あずき まだまだです！",
            "mention_to": [
                "あずき"
            ]
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "あずき",
            "text": "私が好きなのを選びました",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "しじみ",
            "text": "なるほど〜",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "つぶあん",
            "text": "おお！",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "しじみ",
            "text": "お色は？",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "あずき",
            "text": "うちはワインレッド？みたいな色です",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "しじみ",
            "text": "へー！",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "あずき",
            "text": "赤の暗め",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "しじみ",
            "text": "オシャレですね！",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "つぶあん",
            "text": "今はたくさん色ありますよね！",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "しじみ",
            "text": "長く使うのに良さそうな色味ですね！",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "あずき",
            "text": "茶色系も素敵ですよね",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "しじみ",
            "text": "沢山ありすぎて、",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "あずき",
            "text": "あり過ぎて悩みますよね",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "しじみ",
            "text": "私が目移りしちゃいます",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "つぶあん",
            "text": "こどもに選ばせるのドキドキですよね？！",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "あずき",
            "text": "お子さんの好きな色はあるのかな？",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "しじみ",
            "text": "茶色もオシャレですね",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "しじみ",
            "text": "水色希望みたいです",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "つぶあん",
            "text": "なるほど！",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "しじみ",
            "text": "みなさんのお子さんは",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "あずき",
            "text": "いいですね！綺麗！",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "しじみ",
            "text": "ランドセルの色に飽きたりしていますか？",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "つぶあん",
            "text": "そんな色が好きですよね？！",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "あずき",
            "text": "全然気にしてないです",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "しじみ",
            "text": "@つぶあん 水色人気ですよね！",
            "mention_to": [
                "つぶあん"
            ]
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "あずき",
            "text": "あんまり色とか気にしないかも",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "しじみ",
            "text": "気にしてないのは心強いお言葉！",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "あずき",
            "text": "もう5年も使っていると",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "あずき",
            "text": "子供の話題にならない",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "つぶあん",
            "text": "なるほど！慣れてくるのかな？",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "しじみ",
            "text": "なるほど〜",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "あずき",
            "text": "多分、子供が好きな色が一番ですよ",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "しじみ",
            "text": "なるほど！",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "しじみ",
            "text": "とても参考になります！",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "あずき",
            "text": "でも、楽しい時間ですね",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "しじみ",
            "text": "確かに",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "あずき",
            "text": "また一緒に選びたいです",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "つぶあん",
            "text": "そうですね！",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "しじみ",
            "text": "もし選び直すなら、",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "あずき",
            "text": "入学式楽しみですね",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "しじみ",
            "text": "何を重視しますか？",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "つぶあん",
            "text": "何色選ぶか楽しみです。",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "あずき",
            "text": "容量ですかね？たくさん入るようね",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "しじみ",
            "text": "楽しみですね！",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "つぶあん",
            "text": "わかります！",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "しじみ",
            "text": "なるほど！容量ですね！",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "つぶあん",
            "text": "荷物多いですよね！",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "あずき",
            "text": "スリムなランドセルは手荷物になります",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "しじみ",
            "text": "あー",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "しじみ",
            "text": "なるほど！",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "あずき",
            "text": "教科書もたくさんあるし、",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "しじみ",
            "text": "ふむふむ",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "つぶあん",
            "text": "いつもパンパンです。",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "しじみ",
            "text": "あらー",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "あずき",
            "text": "タブレットもあるんで",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "つぶあん",
            "text": "そうでした〜！",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "しじみ",
            "text": "タブレット！",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "しじみ",
            "text": "現代の子ですね〜",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "あずき",
            "text": "今の小学生は荷物多いですよ",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "しじみ",
            "text": "置き勉は禁止ですか？",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "つぶあん",
            "text": "@あずき 高学年だと教科書多そう！",
            "mention_to": [
                "あずき"
            ]
        },
        {
            "utterance_id": 103,
