{
    "dialogue_id": "A02305",
    "dialogue_type": "First time",
    "interlocutors": [
        "ししとう",
        "てんぷら",
        "つぶあん"
    ],
    "relationship": [],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "ししとう",
            "text": "こんばんは！",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "てんぷら",
            "text": "こんばんは！",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "つぶあん",
            "text": "こんばんは！",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "ししとう",
            "text": "今、インフルとか流行ってますよね～",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "ししとう",
            "text": "大丈夫ですか？",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "てんぷら",
            "text": "流行っていますね～いろいろ",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "つぶあん",
            "text": "流行ってます。",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "てんぷら",
            "text": "ちょっと落ち着いてきました",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "つぶあん",
            "text": "学級閉鎖とか怖いです。",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "ししとう",
            "text": "@てんぷら おお！そうなんですね！",
            "mention_to": [
                "てんぷら"
            ]
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "てんぷら",
            "text": "ありましたありました学級閉鎖",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "てんぷら",
            "text": "年末にもあったのに",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "てんぷら",
            "text": "年明けにもありました",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "つぶあん",
            "text": "おお！",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "ししとう",
            "text": "学級閉鎖大変！",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "つぶあん",
            "text": "突然くるから困りますよね。",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "ししとう",
            "text": "保護者も大変ですよね！",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "てんぷら",
            "text": "ほんとですねえ",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "てんぷら",
            "text": "先週は、長男の先生が熱で休んでいました",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "てんぷら",
            "text": "先生も大変",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "つぶあん",
            "text": "先生も大変！",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "ししとう",
            "text": "@てんぷら きゃー！それも大変",
            "mention_to": [
                "てんぷら"
            ]
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "てんぷら",
            "text": "マスクが復活していました",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "つぶあん",
            "text": "流行っているクラスにいれば",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "つぶあん",
            "text": "うつりますよね！",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "ししとう",
            "text": "@つぶあん ですよね～",
            "mention_to": [
                "つぶあん"
            ]
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "ししとう",
            "text": "うちは幼稚園児もいるので",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "ししとう",
            "text": "いろいろもらってきます…",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "てんぷら",
            "text": "幼稚園児は大変ですね",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "てんぷら",
            "text": "保育園児も大変",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "つぶあん",
            "text": "ほんとうですね！",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "ししとう",
            "text": "@てんぷら 子ども同士の密着率が高いです",
            "mention_to": [
                "てんぷら"
            ]
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "つぶあん",
            "text": "わかります！",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "てんぷら",
            "text": "頭くっつけて遊んだりしていますもんねえ",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "つぶあん",
            "text": "めちゃ近いですよね！",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "てんぷら",
            "text": "かわいいけど",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "ししとう",
            "text": "仲良くしてくれるのはいいけど…",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "つぶあん",
            "text": "わかります！",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "てんぷら",
            "text": "中学生くらいになると、距離感ありますけどねえ",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "つぶあん",
            "text": "距離明けるの無理ですよね。",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "ししとう",
            "text": "@てんぷら 中学生は適度な距離感になりますよね",
            "mention_to": [
                "てんぷら"
            ]
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "てんぷら",
            "text": "中学生、家族も距離感ありますね",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "つぶあん",
            "text": "@てんぷら 中学校で学級閉鎖でしたか？",
            "mention_to": [
                "てんぷら"
            ]
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "ししとう",
            "text": "@てんぷら 思春期だ～",
            "mention_to": [
                "てんぷら"
            ]
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "てんぷら",
            "text": "中学校は大丈夫でした",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "てんぷら",
            "text": "小学校低学年の子のクラスが閉鎖していました",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "てんぷら",
            "text": "小学校も、１，２年生はまだ近いかも",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "つぶあん",
            "text": "やはり、そうですよね！",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "ししとう",
            "text": "@てんぷら そうなんですね！",
            "mention_to": [
                "てんぷら"
            ]
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "ししとう",
            "text": "１、２年生の距離の近さ…わかる～",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "つぶあん",
            "text": "マスクもなかなか。。。",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "てんぷら",
            "text": "自分も、頭くっつけてお絵描きしていましたから",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "てんぷら",
            "text": "マスク、取ってましたよねえ、しばらく",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "ししとう",
            "text": "マスクしてても役目果たしてないですよね～",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "つぶあん",
            "text": "一度外してしまったから。。。",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "てんぷら",
            "text": "気にする子はずっと付けていたみたいです",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "ししとう",
            "text": "鼻マスクだったり、裏返したり…",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "つぶあん",
            "text": "そうなんですね？！",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "てんぷら",
            "text": "コロナのほんとに始まりの、一斉休校あったじゃないですか",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "つぶあん",
            "text": "@てんぷら 逆にマスクとれなくなってしまったり？",
            "mention_to": [
                "てんぷら"
            ]
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "てんぷら",
            "text": "そうそう",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "てんぷら",
            "text": "あのあと入園だったんですよ、うち",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "つぶあん",
            "text": "最初からマスクだったんですね！",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "てんぷら",
            "text": "初めての幼稚園、異様な雰囲気で、全員マスクで",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "ししとう",
            "text": "@てんぷら わわ～大変なタイミングでしたね！",
            "mention_to": [
                "てんぷら"
            ]
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "つぶあん",
            "text": "子供大変ですよね",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "てんぷら",
            "text": "お互いの顔をあまり知らないまま卒園しましたが",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "つぶあん",
            "text": "ほんと、マスクしていると",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "つぶあん",
            "text": "顔覚えられないですよね！",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "ししとう",
            "text": "@つぶあん それそれ！",
            "mention_to": [
                "つぶあん"
            ]
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "ししとう",
            "text": "わからないですよね！",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "てんぷら",
            "text": "勝手にマスクの中の顔、作りますよね、想像で",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "てんぷら",
            "text": "外されると、違和感があったり",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "ししとう",
            "text": "@てんぷら 違和感もわかる～！",
            "mention_to": [
                "てんぷら"
            ]
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "つぶあん",
            "text": "@てんぷら わかります！",
            "mention_to": [
                "てんぷら"
            ]
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "つぶあん",
            "text": "マスクとると誰かわからないですよね？",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "てんぷら",
            "text": "あれ？って思います",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "てんぷら",
            "text": "同じように思われるの怖いから、",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "てんぷら",
            "text": "私はマスク美人ということで",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "てんぷら",
            "text": "永遠にマスクでいいです！",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "つぶあん",
            "text": "自信なくします。さん？かな？？",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "つぶあん",
            "text": "みたいな。",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "ししとう",
            "text": "マスクしてなくても人の顔覚えられないから",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "ししとう",
            "text": "マスクしてたり外したりすると混乱～",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "てんぷら",
            "text": "なおさら覚えにくいですね、マスクしていると",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "つぶあん",
            "text": "行事や集まりも少なくて覚えられないです。",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "ししとう",
            "text": "@つぶあん 本当ぐっと減りましたよね～",
            "mention_to": [
                "つぶあん"
            ]
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "ししとう",
            "text": "小学校の運動会も午前開催のままだし",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "つぶあん",
            "text": "@ししとう 減りました！",
            "mention_to": [
                "ししとう"
            ]
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "ししとう",
            "text": "ありがたいけど。",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "つぶあん",
            "text": "わかります！",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "てんぷら",
            "text": "そうそう、半日開催、ちょっと助かります",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "てんぷら",
            "text": "このままでいいです",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "つぶあん",
            "text": "行事短縮のいいところもありますよね！",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "ししとう",
            "text": "そうですよね！",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "ししとう",
            "text": "このまま簡略化できるところ削って",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "てんぷら",
            "text": "子どもも、全校集会がテレビモニターになったの喜んでいました",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "ししとう",
            "text": "@てんぷら ハイテクになってますよね！",
            "mention_to": [
                "てんぷら"
            ]
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "てんぷら",
            "text": "体育館で立ちっぱなし、がなくなって",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "つぶあん",
            "text": "はいはい、便利ですよね！",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "てんぷら",
            "text": "リモート授業もいいみたいです",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "てんぷら",
            "text": "出席停止だけど元気な時とか",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "ししとう",
            "text": "@てんぷら リモート授業進んでるんですね！",
            "mention_to": [
                "てんぷら"
            ]
        },
        {
            "utterance_id": 103,
            "interlocutor_id": "ししとう",
            "text": "うち田舎だからこの辺まだまだかも",
            "mention_to": []
        }
