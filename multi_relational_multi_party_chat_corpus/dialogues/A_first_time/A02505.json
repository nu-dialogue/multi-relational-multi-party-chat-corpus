            "mention_to": [
                "きなこ"
            ]
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "つぶあん",
            "text": "@きなこ 正直者です！",
            "mention_to": [
                "きなこ"
            ]
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "きなこ",
            "text": "安くても高く感じちゃいますが、",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "きなこ",
            "text": "必需品ですからね",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "つぶあん",
            "text": "うんうん",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "しらす",
            "text": "そうなんですよね",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "つぶあん",
            "text": "そうですね。",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "きなこ",
            "text": "大人もスマホなしが難しいですよね",
