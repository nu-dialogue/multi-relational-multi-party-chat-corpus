        },
        {
            "utterance_id": 90,
            "interlocutor_id": "しらす",
            "text": "分かります",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "きなこ",
            "text": "ついついみちゃいます",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "つぶあん",
            "text": "本当にそうです。",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "しらす",
            "text": "見ますよね",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "つぶあん",
            "text": "携帯ないの想像できませんよね。",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "きなこ",
            "text": "ですよね",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "つぶあん",
            "text": "便利ですし！",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "つぶあん",
            "text": "なんでも調べらるし",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "きなこ",
            "text": "そうそう！",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "しらす",
            "text": "スマホ一つで出かけられますもん",
            "mention_to": []
        },
