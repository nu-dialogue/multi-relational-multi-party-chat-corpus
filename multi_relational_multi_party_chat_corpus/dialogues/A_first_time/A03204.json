{
    "dialogue_id": "A03204",
    "dialogue_type": "First time",
    "interlocutors": [
        "まりも",
        "きなこ",
        "うどん"
    ],
    "relationship": [],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "まりも",
            "text": "こんにちは～",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "きなこ",
            "text": "こんにちは～",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "うどん",
            "text": "こんにちは〜",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "まりも",
            "text": "比較的近くにピザハットができたみたいで",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "まりも",
            "text": "気になっています",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "まりも",
            "text": "お持ち帰りだとすごく安いですね",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "きなこ",
            "text": "近いとつい利用したくなります！",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "きなこ",
            "text": "持ち帰り一択で",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "まりも",
            "text": "ピザの生地、何選びますか？",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "うどん",
            "text": "ピザいいですね！！",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "まりも",
            "text": "私いつもクリスピー選んじゃうんですが",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "まりも",
            "text": "ふっくらパンピザっていうのを",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "うどん",
            "text": "クリスピーおいしいですよね",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "まりも",
            "text": "友達から激推しされてて",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "うどん",
            "text": "気になる",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "まりも",
            "text": "そう、カリカリが好きで…",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "きなこ",
            "text": "それもおいしそう！",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "うどん",
            "text": "カリカリおいしそう！",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "うどん",
            "text": "迷うなぁ！",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "まりも",
            "text": "ふっくらパンピザ",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "まりも",
            "text": "厚みがあってサクっとふわふわな食感で",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "うどん",
            "text": "ほー！",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "まりも",
            "text": "ハンドトスとクリスピーのいいとこ取りみたいな",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "まりも",
            "text": "感じらしいです",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "きなこ",
            "text": "なんかお得な感じ！",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "うどん",
            "text": "いいとこどりいいですね！",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "きなこ",
            "text": "両方のいいとこどり",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "まりも",
            "text": "満足感もしっかりあるらしいです",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "うどん",
            "text": "満足感大事ですよね",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "まりも",
            "text": "ピザハット行かなきゃ",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "うどん",
            "text": "わたしも〜！",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "きなこ",
            "text": "うちは・・近くにドミノしかないです～",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "まりも",
            "text": "好きなトッピングありますか？",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "まりも",
            "text": "@きなこ うちの近くはドミノないかも…？",
            "mention_to": [
                "きなこ"
            ]
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "うどん",
            "text": "うちもドミのないんです...",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "まりも",
            "text": "ピザーラしか配達範囲内になかったかも",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "きなこ",
            "text": "ふっくらパンピザ・・の為にピザハット探します",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "まりも",
            "text": "ドミノはパンピサないんですかね～",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "まりも",
            "text": "ピサじゃないピザ",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "きなこ",
            "text": "笑",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "うどん",
            "text": "笑",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "まりも",
            "text": "斜塔になってしまった",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "きなこ",
            "text": "パンピザではないかも",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "きなこ",
            "text": "ほんとだ！",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "きなこ",
            "text": "笑笑",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "まりも",
            "text": "最近スーパーで",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "まりも",
            "text": "手作りピザ系の材料が安売りしてて",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "うどん",
            "text": "手作りいいですね！！",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "まりも",
            "text": "小さいピザ生地と",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "まりも",
            "text": "ピザソースと",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "きなこ",
            "text": "いいですね！",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "まりも",
            "text": "チーズ買いました",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "うどん",
            "text": "焼きたてピザ美味しそう！",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "まりも",
            "text": "それでもう十分おいしいですね",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "きなこ",
            "text": "@まりも 手作りピザには他に何をのせますか？",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "まりも",
            "text": "でも欲を言うならペパロニがほしい",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "きなこ",
            "text": "いいですね！",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "うどん",
            "text": "ペパロニ？",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "まりも",
            "text": "@きなこピザソースとチーズで食べちゃってますね",
            "mention_to": [
                "きなこ"
            ]
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "まりも",
            "text": "サラミみたいなやつ…？",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "うどん",
            "text": "@まりも なるほど！",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "きなこ",
            "text": "おいしいですよね",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "まりも",
            "text": "サラミとはちょっと違うんだったか",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "うどん",
            "text": "おあたしそうだなぁ！",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "きなこ",
            "text": "ちょっと辛いやつですよね？",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "うどん",
            "text": "おいしそう",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "きなこ",
            "text": "ペパロニ",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "まりも",
            "text": "たぶんそんな感じ！",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "うどん",
            "text": "辛いの大好き！",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "まりも",
            "text": "スーパーでのペバロニの在り処を知らない",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "きなこ",
            "text": "私は・・ドライトマトかな",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "まりも",
            "text": "あ～いいですね",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "きなこ",
            "text": "トマト好きです",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "まりも",
            "text": "トマト私も好きです",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "うどん",
            "text": "カルディとかもピザ関係置いてそう",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "うどん",
            "text": "ドライトマトいいですね！",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "まりも",
            "text": "カルディありそうですね！",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "うどん",
            "text": "生のトマトとは違うおいしさ",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "きなこ",
            "text": "そうそう！",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "うどん",
            "text": "おいしいですよね！！",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "まりも",
            "text": "ドライトマトか～試したい～",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "きなこ",
            "text": "年に数回だけ成城石井に行くのですが",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "きなこ",
            "text": "ドライトマトのマリネがおいしいです",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "うどん",
            "text": "成城石井大好きです！！",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "まりも",
            "text": "すごく素敵っぽい…",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "うどん",
            "text": "ドライトマトのマリネ！！",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "きなこ",
            "text": "おいしいですよ～！",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "まりも",
            "text": "さすが成城石井",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "うどん",
            "text": "今度買ってみます！！！",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "きなこ",
            "text": "高くて時々しか行きませんが",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "まりも",
            "text": "残念ながらうちの近くにはないかも…",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "うどん",
            "text": "成城石井、凝った料理多いですね！",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "きなこ",
            "text": "@うどん おすすめです",
            "mention_to": [
                "うどん"
            ]
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "うどん",
            "text": "@きなこ ありがとうございます！",
            "mention_to": [
                "きなこ"
            ]
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "きなこ",
            "text": "@まりも どこかで見かけたら見てみてください！",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "きなこ",
            "text": "おいしくて凝った料理多いですよね",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "うどん",
            "text": "そうなんですよねー！",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "まりも",
            "text": "@きなこ 今度成城石井見かけたら探してみます！",
            "mention_to": [
                "きなこ"
            ]
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "うどん",
            "text": "凝ってて、かつ美味しい",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "きなこ",
            "text": "そうそう！",
            "mention_to": []
        }
