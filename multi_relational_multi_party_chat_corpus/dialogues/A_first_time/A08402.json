        },
        {
            "utterance_id": 2,
            "interlocutor_id": "いそべ",
            "text": "こんにちは！",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "きりん",
            "text": "最近ブラジルの人とチャットする機会がありました",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "いくら",
            "text": "ええ",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "いそべ",
            "text": "おおお",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "いくら",
            "text": "それはすごい機会ですね！",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "きりん",
            "text": "日本語がすごく上手で",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "いそべ",
            "text": "日本語なんですか！",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "いそべ",
            "text": "すごい",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "きりん",
            "text": "日本語でお話してます",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "いくら",
            "text": "チャットは日本語でされるんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "いくら",
            "text": "そうなんですね！",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "きりん",
            "text": "@いくら そうそう、そうなんですー",
            "mention_to": [
                "いくら"
            ]
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "いくら",
            "text": "変換とかされてるんですかね？",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "きりん",
            "text": "いや、何年か勉強してるそうです",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "きりん",
            "text": "すごいですよね",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "いくら",
            "text": "すごいですねー！",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "いそべ",
            "text": "日本語難しいのにすごいですね",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "きりん",
            "text": "そんなに違和感なく会話してます",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "いくら",
            "text": "知り合いなんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "きりん",
            "text": "共通のゲームが好きで知り合いました",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "いそべ",
            "text": "おおお！",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "いくら",
            "text": "ゲーム！",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "きりん",
            "text": "デルタルーンっていうゲームなんですが",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "いくら",
            "text": "ゲーム内でチャットされるんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "いくら",
            "text": "デルタルーン聞いたことあります！",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "いそべ",
            "text": "私もあります！",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "いくら",
            "text": "アンダーテールでしたっけ？",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "きりん",
            "text": "@いくら オンラインゲームではないので、チャットは別ツール使ってます",
            "mention_to": [
                "いくら"
            ]
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "いくら",
            "text": "息子が好きなんですよね",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "いそべ",
            "text": "アンダーテールと同じ作者ですよね",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "きりん",
            "text": "そうそう、アンダーテールと同じクリエイターさんのゲームです！",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "いくら",
            "text": "@きりん そこまで仲良くなるのすごいです！",
            "mention_to": [
                "きりん"
            ]
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "いそべ",
            "text": "僕もデルタルーンプレイしました",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "いくら",
            "text": "そうなんですね！また違うゲームなんですね！",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "きりん",
            "text": "その日英翻訳動画を作りたいらしくて",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "きりん",
            "text": "日本語のフィードバックがほしいと",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "いくら",
            "text": "@いそべ 面白かったですか？",
            "mention_to": [
                "いそべ"
            ]
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "いそべ",
            "text": "面白いです",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "いそべ",
            "text": "ストーリーが",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "きりん",
            "text": "質問がなかなか難しくて、でもおもしろいですね",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "いくら",
            "text": "@きりん なるほど！そんなきっかけがあるんですね！",
            "mention_to": [
                "きりん"
            ]
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "いくら",
            "text": "ストーリーが面白いんですね！",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "きりん",
            "text": "デルタルーンはまだチャプター2までしか公開されてないです",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "いくら",
            "text": "@きりん ブラジルの方からの質問は面白そうですね！",
            "mention_to": [
                "きりん"
            ]
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "いくら",
            "text": "日本語訳についてですもんね！",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "いそべ",
            "text": "もうそろそろチャプター3でるんですかね？",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "きりん",
            "text": "全部でチャプター5まで？あるらしいです",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "いそべ",
            "text": "結構チャプター２から時間がたっている気がする",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "きりん",
            "text": "3はそろそろ…な雰囲気ありますよね",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "いくら",
            "text": "まだ続きがあるんですね！",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "きりん",
            "text": "そうなんですー早く続きを知りたいです",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "いそべ",
            "text": "そうですね",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "いくら",
            "text": "息子に3が出そうだと伝えたら喜んでました",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "きりん",
            "text": "アンダーテールもそうですが、曲がいいので",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "きりん",
            "text": "好きです",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "いそべ",
            "text": "曲いいですよね！",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "いくら",
            "text": "曲、たくさん聞きました！！",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "きりん",
            "text": "@いくら 息子さんプレイしてるんですね！",
            "mention_to": [
                "いくら"
            ]
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "いくら",
            "text": "ピアノひかれてるかた、多いですよね！",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "きりん",
            "text": "曲もシナリオもゲームも作れるのすごいですよね",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "いくら",
            "text": "@きりん そうなんです！好きみたいで、",
            "mention_to": [
                "きりん"
            ]
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "いそべ",
            "text": "多才ですよね",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "いくら",
            "text": "わたしはアンダーテールやら、ガーデンオブバンバンやら、",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "いくら",
            "text": "頭がこんがらがってますね！笑",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "いそべ",
            "text": "バンバン！",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "きりん",
            "text": "@いくら ガーデンオブバンバン？",
            "mention_to": [
                "いくら"
            ]
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "いそべ",
            "text": "youtubeでみます",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "いくら",
            "text": "@きりん 1人の方が作られてるんですか！！すごい",
            "mention_to": [
                "きりん"
            ]
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "きりん",
            "text": "@いくら アンダーテールはほぼ1人で作ったそうです",
            "mention_to": [
                "いくら"
            ]
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "いくら",
            "text": "@きりん あまりよく分からないんですが、色々探してくるんですよね、、",
            "mention_to": [
                "きりん"
            ]
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "いくら",
            "text": "@いそべ YouTubeなんですね！！",
            "mention_to": [
                "いそべ"
            ]
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "いそべ",
            "text": "ホラーゲームですよね？",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "いくら",
            "text": "@きりん ええ、すごい！！",
            "mention_to": [
                "きりん"
            ]
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "きりん",
            "text": "ホラーゲームなんですか",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "いくら",
            "text": "ホラーなんですか！知らなかった！！",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "いそべ",
            "text": "たしか",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "きりん",
            "text": "タイトルからは想像できませんでした、バンバン",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "いそべ",
            "text": "キャラクターはかわいいですけれど",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "いくら",
            "text": "バンバン",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "いくら",
            "text": "小3なんですが、悪影響とか無ければいいなと思います。",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "いそべ",
            "text": "バンバンなら多分大丈夫だと思います！",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "きりん",
            "text": "ホラー好きな子は好きですもんね",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "いくら",
            "text": "おお！それは良かったです！！",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "いくら",
            "text": "そうなんですよ、家族誰もホラー見ないのに！",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "いそべ",
            "text": "ホラーゲームはプレイするのは嫌だけれど他の人がやっているのを見るのは好きです",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "きりん",
            "text": "友達の影響とかもありますよね",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "いくら",
            "text": "そういう人多いですよね！",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "いくら",
            "text": "そうなんですよね、流行ってるみたいですねー、",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "いそべ",
            "text": "流行っていますね！",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "きりん",
            "text": "今は好きな配信者の影響とかもありそうですね",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "きりん",
            "text": "ちょっとあとで調べてみます、バンバン",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "いくら",
            "text": "流行ってますよね！変わった名前ばかりで暗号みたいな会話してます。",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "きりん",
            "text": "暗号みたいなw",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "いくら",
            "text": "@きりん 配信されてて初めて知るってこと多いですよね！",
            "mention_to": [
                "きりん"
            ]
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "いそべ",
            "text": "変な名前が多いですよね",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "いくら",
            "text": "暗号みたいなんですよ笑",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "いくら",
            "text": "面白いですよね！",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "きりん",
            "text": "変な名前、どんなのですかね",
