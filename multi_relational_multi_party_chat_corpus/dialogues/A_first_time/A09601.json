    "relationship": [],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "てんぷら",
            "text": "こんばんは！",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "みたらし",
            "text": "こんばんは！",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "らっこ",
            "text": "こんばんはー",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "てんぷら",
            "text": "今日、ひな祭りでした",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "らっこ",
            "text": "あ、そうでしたね。",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "てんぷら",
            "text": "すっかり忘れていました…",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "みたらし",
            "text": "そうですね",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "みたらし",
            "text": "何もしてませんが。。。",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "らっこ",
            "text": "家、息子二人なので、",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "てんぷら",
            "text": "私が女ですので",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "てんぷら",
            "text": "ケーキを買ってきました",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "らっこ",
            "text": "自分に！いいですねー",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "みたらし",
            "text": "いいですね",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "みたらし",
            "text": "私のところも",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "らっこ",
            "text": "そっか。私も女だから自分に何かしてもいいんですね。",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "みたらし",
            "text": "息子3人です",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "てんぷら",
            "text": "おおお",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "らっこ",
            "text": "@みたらし ひな祭り関係ないですね",
            "mention_to": [
                "みたらし"
            ]
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "てんぷら",
            "text": "子供の日は、祝うのですか",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "みたらし",
            "text": "ひな祭りは縁がないです",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "らっこ",
            "text": "兜飾りは出してましたが、、、",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "らっこ",
            "text": "最近さぼってます",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "てんぷら",
            "text": "クリスマスツリーも手間です",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "みたらし",
            "text": "@てんぷら 息子も大きいので、",
            "mention_to": [
                "てんぷら"
            ]
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "みたらし",
            "text": "もう何もしてないです",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "らっこ",
            "text": "小さい間はいろいろやってあげようと思うんですけどね",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "てんぷら",
            "text": "@みたらし おいくつなんですか",
            "mention_to": [
                "みたらし"
            ]
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "みたらし",
            "text": "@らっこ ですよね",
            "mention_to": [
                "らっこ"
            ]
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "らっこ",
            "text": "すぐ、どこかへ遊びに行っちゃうから。ｗｗｗ",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "らっこ",
            "text": "高校生2人です。",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "みたらし",
            "text": "@てんぷら 今度大学と高校と",
            "mention_to": [
                "てんぷら"
            ]
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "みたらし",
            "text": "中2です",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "てんぷら",
            "text": "皆さん、大きいですね・・・",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "みたらし",
            "text": "ですね",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "らっこ",
            "text": "一緒ぐらいですね。",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "てんぷら",
            "text": "ごはん、何合炊きますか",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "らっこ",
            "text": "ごはんｗｗｗ",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "みたらし",
            "text": "子供の日も卒業です",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "らっこ",
            "text": "子どもの日は成人までかな？",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "みたらし",
            "text": "4合を",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "みたらし",
            "text": "１日2回です",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "らっこ",
            "text": "4合！2回！",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "てんぷら",
            "text": "食べますね～",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "らっこ",
            "text": "すごい。",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "みたらし",
            "text": "ですね",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "てんぷら",
            "text": "パンだと、すごそうです",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "らっこ",
            "text": "お米の消費すごいですね。",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "みたらし",
            "text": "夕飯食べた後にカップめんとかも食べますね",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "らっこ",
            "text": "あー、うちもあります。",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "みたらし",
            "text": "ですよね",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "てんぷら",
            "text": "足りないんですね",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "らっこ",
            "text": "食後すぐ、食品庫をゴソゴソする。",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "らっこ",
            "text": "冷蔵庫とかも。",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "てんぷら",
            "text": "買い物、大変そうですが",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "みたらし",
            "text": "もう胃に穴が空いてるのかと",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "てんぷら",
            "text": "週に何回行きます？",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "らっこ",
            "text": "穴ｗｗ",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "みたらし",
            "text": "買い物は1日置きに買ってます",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "らっこ",
            "text": "うちも1日おきぐらいかなー",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "てんぷら",
            "text": "それも大変そうです・・・",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "みたらし",
            "text": "土日に結構買うのですが足りないですね",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "てんぷら",
            "text": "最近、物価も高いじゃないですか",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "てんぷら",
            "text": "レジで、え？ってなるんです",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "みたらし",
            "text": "高いですね",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "らっこ",
            "text": "最近、業務スーパーで買うようにしてます。いろいろ",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "てんぷら",
            "text": "業務スーパーいいですね",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "てんぷら",
            "text": "近くにマルエツがあって、",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "てんぷら",
            "text": "高いです",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "らっこ",
            "text": "マルエツ高そうです。",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "みたらし",
            "text": "スーパーをあちこち回ってます",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "らっこ",
            "text": "安いところを回りたくなりますよねー",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "みたらし",
            "text": "割引あるところに",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "らっこ",
            "text": "チラシ見たりして？",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "てんぷら",
            "text": "ポイントとか？",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "みたらし",
            "text": "ネットチラシですね",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "らっこ",
            "text": "私もネットチラシチェックしますー",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "みたらし",
            "text": "大体月で3割引デーが",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "らっこ",
            "text": "うんうん",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "てんぷら",
            "text": "3割は大きい",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "みたらし",
            "text": "載ってるのでそこにあわせて買いにいきます",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "らっこ",
            "text": "そこで、いっぱい買っておきたいですね。",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "みたらし",
            "text": "ですね",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "らっこ",
            "text": "まとめ買いしようと思うと、けっこう先の献立考えないとだめでしょ？",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "てんぷら",
            "text": "あー",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "てんぷら",
            "text": "いつもごはんのこと考えています",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "みたらし",
            "text": "買った後に考えてる感じですかね",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "てんぷら",
            "text": "なるほど",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "らっこ",
            "text": "なるほどー買った後にね",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "てんぷら",
            "text": "安いものを買ってから",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "みたらし",
            "text": "そう安いのかって",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "らっこ",
            "text": "それがいいですねー",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "てんぷら",
            "text": "明日の朝ごはん決まっていません",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "てんぷら",
            "text": "旦那は、コメ炊いとくよと",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "みたらし",
            "text": "朝はパンか目玉焼きです",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "らっこ",
            "text": "コメはとりあえず炊くとして、後どうする？ってね",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "てんぷら",
            "text": "決まってるといいですね",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "てんぷら",
            "text": "そう、ふりかけごはんオンリー",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "てんぷら",
            "text": "味噌汁作る・・・か・・・？とか",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "みたらし",
            "text": "ふりかけいいですよね",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "てんぷら",
            "text": "鮭フレークかふりかけです",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "てんぷら",
            "text": "シリアルも買わないと",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "らっこ",
            "text": "鮭フレークおいしいですね",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "みたらし",
            "text": "ウチのはのり玉業務用買ってます",
            "mention_to": []
        },
        {
            "utterance_id": 103,
            "interlocutor_id": "らっこ",
            "text": "業務用！！すごい。",
            "mention_to": []
        },
        {
            "utterance_id": 104,
            "interlocutor_id": "らっこ",
            "text": "でも、のりたま美味しいですね。",
            "mention_to": []
        }
