    "relationship": [],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "おでん",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "げんまい",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "はくさい",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "おでん",
            "text": "よろしくお願いします",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "げんまい",
            "text": "よろしくお願いします",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "はくさい",
            "text": "お願いします",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "おでん",
            "text": "最近、どこかに出かけたりしてますか？",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "おでん",
            "text": "お友達ととか",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "げんまい",
            "text": "1人ででかける事が多いですね",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "おでん",
            "text": "そうなんですね、",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "はくさい",
            "text": "年始に友達家族とレゴランド行きました",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "げんまい",
            "text": "レゴランド！楽しそう",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "おでん",
            "text": "楽しそうですね",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "げんまい",
            "text": "一日遊べる感じですか？",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "はくさい",
            "text": "1日遊べましたね",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "おでん",
            "text": "@はくさい 友達家族とは、奥さんの友達？",
            "mention_to": [
                "はくさい"
            ]
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "はくさい",
            "text": "ディズニーとちがって時間が短いんです",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "げんまい",
            "text": "待ち時間がないってことですか？",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "はくさい",
            "text": "@おでん そうですね。職場の友達3家族13人でした",
            "mention_to": [
                "おでん"
            ]
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "げんまい",
            "text": "大勢で楽しそうです。",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "はくさい",
            "text": "@げんまい 待ち時間もですが、10時から17時までだったかな",
            "mention_to": [
                "げんまい"
            ]
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "げんまい",
            "text": "なるほどー",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "おでん",
            "text": "ねー、子供なんて、すごく喜ばれたでしょうね",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "げんまい",
            "text": "お子さん同士の年齢が近いのですか？",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "はくさい",
            "text": "子どもたちは楽しんでましたねー",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "おでん",
            "text": "遊園地ってくくりであってます？",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "はくさい",
            "text": "@げんまい 結構近いです",
            "mention_to": [
                "げんまい"
            ]
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "はくさい",
            "text": "@おでん 遊園地です",
            "mention_to": [
                "おでん"
            ]
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "おでん",
            "text": "レゴランドは",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "おでん",
            "text": "何がメインなんだろ？",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "げんまい",
            "text": "乗り物がレゴで出来てたり？しないですよね？ｗｗ",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "おでん",
            "text": "笑笑  それも楽しそうだけど",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "はくさい",
            "text": "乗り物はレゴっぽい感じでした",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "はくさい",
            "text": "装飾が本物のレゴでできてて",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "はくさい",
            "text": "創作時間と",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "げんまい",
            "text": "レゴっぽいだけで、レゴじゃなくてよかったです。",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "おでん",
            "text": "色合いがカラフルそうですね",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "はくさい",
            "text": "何ピース使ったか書いてありました",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "おでん",
            "text": "へぇー",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "はくさい",
            "text": "かなりカラフルでした",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "げんまい",
            "text": "レゴの大作が見られるんですね！すごーい",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "おでん",
            "text": "レゴのお店には、必ず大作があるじゃないですか",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "おでん",
            "text": "あれってすごいですよね",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "はくさい",
            "text": "ジオラマみたいなのありました",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "おでん",
            "text": "ランドの？",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "はくさい",
            "text": "いろんな名所の",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "おでん",
            "text": "それも見るの楽しそうですね",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "はくさい",
            "text": "スカイツリーとか",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "げんまい",
            "text": "おお！それはおもしろそう。名所再現",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "はくさい",
            "text": "清水寺とか",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "おでん",
            "text": "へぇー",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "げんまい",
            "text": "へー",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "げんまい",
            "text": "自分で作ったりもできるんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "おでん",
            "text": "日本各地の名所ですね",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "はくさい",
            "text": "@げんまい 作れるところもありましたね",
            "mention_to": [
                "げんまい"
            ]
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "おでん",
            "text": "@はくさい 作りましたか？",
            "mention_to": [
                "はくさい"
            ]
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "はくさい",
            "text": "@おでん 大人が見入っちゃって進めない",
            "mention_to": [
                "おでん"
            ]
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "げんまい",
            "text": "大人がｗｗｗ",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "おでん",
            "text": "特に、お父さんとか好きそう",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "はくさい",
            "text": "@おでん 私は創造力がないので作るのは苦手です。。。",
            "mention_to": [
                "おでん"
            ]
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "おでん",
            "text": "そうなんですね",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "はくさい",
            "text": "お父さんはいいですよね",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "おでん",
            "text": "もう、少年にかえって楽しいんじゃないですか？",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "げんまい",
            "text": "ありがち。お父さんが一番楽しんじゃう",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "はくさい",
            "text": "うんうん",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "おでん",
            "text": "笑笑ねー子供と一緒",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "げんまい",
            "text": "小学生以下の子どもが多いですか？",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "はくさい",
            "text": "そうですね",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "おでん",
            "text": "@はくさい 泊まりで行かれたんですか？",
            "mention_to": [
                "はくさい"
            ]
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "はくさい",
            "text": "@おでん 泊まりでした",
            "mention_to": [
                "おでん"
            ]
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "げんまい",
            "text": "大勢で泊りは、すごく楽しそう",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "はくさい",
            "text": "ホテルの部屋は別々でしたが",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "おでん",
            "text": "ずっと、はしゃいじゃいますね",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "はくさい",
            "text": "みんなで飲めたのが楽しかった",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "げんまい",
            "text": "大人は、夜は宴会？",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "はくさい",
            "text": "大人が",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "おでん",
            "text": "うんうん",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "はくさい",
            "text": "飲み屋でごはんで飲んだくれましたね",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "げんまい",
            "text": "帰りの電車とかいろいろ気にせず飲めるって楽しいですね。",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "はくさい",
            "text": "そうなんですよね",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "おでん",
            "text": "笑笑飲んだくれたんですね",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "はくさい",
            "text": "こっちは車社会なので、誰かが我慢することが多くて",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "はくさい",
            "text": "ゆっくり飲めないんです",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "げんまい",
            "text": "あー、誰かが我慢はしんどいですね",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "おでん",
            "text": "帰らなきゃとなると、そうなりますよね",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "げんまい",
            "text": "気を使いそう。",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "はくさい",
            "text": "気にしなくていいって最高ですね",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "おでん",
            "text": "その点、泊まりだと、みんな楽しめるし飲めるし、良かったですね",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "げんまい",
            "text": "子どもも大人も楽しい時間でしたね！",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "はくさい",
            "text": "ほんと楽しかったです",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "おでん",
            "text": "また、行きたいってなりますね",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "げんまい",
            "text": "修学旅行みたい。",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "おでん",
            "text": "ほんと、そんな感じですね",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "はくさい",
            "text": "@おでん また行きたいって話してます",
            "mention_to": [
                "おでん"
            ]
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "げんまい",
            "text": "今度行くとしたら、違う場所ですか？",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "おでん",
            "text": "そりゃみんな同じですよね",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "はくさい",
            "text": "団体なんで結構点呼しました笑",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "おでん",
            "text": "番号ー！？笑笑",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "はくさい",
            "text": "@おでん みんないるかーって",
            "mention_to": [
                "おでん"
            ]
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "おでん",
            "text": "笑笑ホームアローンみたいに忘れたら大変",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "げんまい",
            "text": "1人ぐらいいなくても気づかないｗｗｗ",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "はくさい",
            "text": "@げんまい 今度行くとしたら違うところですね、きっと",
            "mention_to": [
                "げんまい"
            ]
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "げんまい",
            "text": "違う場所も楽しいですね。",
            "mention_to": []
        }
