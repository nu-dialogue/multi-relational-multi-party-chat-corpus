            "mention_to": [
                "はくさい"
            ]
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "あずき",
            "text": "吉高ゆりこ主演です",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "あずき",
            "text": "紫式部の話です",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "あずき",
            "text": "平安時代の",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "きなこ",
            "text": "そうなんですね！",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "はくさい",
            "text": "@あずき そうなんですね",
            "mention_to": [
                "あずき"
            ]
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "あずき",
            "text": "恋愛ありで",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "あずき",
            "text": "女性向きかと",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "はくさい",
            "text": "紫式部って意外って思いました",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "あずき",
            "text": "思われます",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "きなこ",
            "text": "楽しめそう！",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "はくさい",
            "text": "ちょっと気になってきました",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "あずき",
            "text": "子供がハマったのが意外でした",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "あずき",
            "text": "百人一首もやるように",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "はくさい",
            "text": "お子さん大きいですか？",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "あずき",
            "text": "すっかりハマってます",
