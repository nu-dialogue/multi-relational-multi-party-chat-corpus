            "mention_to": [
                "はくさい"
            ]
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "はくさい",
            "text": "@あずき 大河って何でしたっけ？",
            "mention_to": [
                "あずき"
            ]
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "あずき",
