{
    "dialogue_id": "A02204",
    "dialogue_type": "First time",
    "interlocutors": [
        "おでん",
        "げんまい",
        "はくさい"
    ],
    "relationship": [],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "おでん",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "げんまい",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "はくさい",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "おでん",
            "text": "よろしくお願いします",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "げんまい",
            "text": "よろしくお願いします",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "はくさい",
            "text": "お願いします！",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "おでん",
            "text": "おふたりは、ウィンタースポーツってしますか？",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "げんまい",
            "text": "昔はしましたけどねー",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "はくさい",
            "text": "しないですねー",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "げんまい",
            "text": "@おでん します？",
            "mention_to": [
                "おでん"
            ]
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "おでん",
            "text": "私は縁がなくて、スキーとか1回2回しかしたことなくて",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "おでん",
            "text": "@げんまい 昔はスキーですか？",
            "mention_to": [
                "げんまい"
            ]
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "げんまい",
            "text": "スキーとかスノボとか",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "げんまい",
            "text": "20年以上前です。",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "おでん",
            "text": "スノボって、最近じゃないんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "おでん",
            "text": "20年前？",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "げんまい",
            "text": "スノボがはやり出した時代ですね。",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "おでん",
            "text": "そうなんですね",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "おでん",
            "text": "カッコイイ",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "げんまい",
            "text": "ちょっとやったけど、難しいので無理でした。",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "おでん",
            "text": "あら、ではスキーですね",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "はくさい",
            "text": "スノボやってみたかったですね",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "げんまい",
            "text": "今からでもいかがですか？スノボ",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "おでん",
            "text": "@はくさい スケートとかもしないですか？",
            "mention_to": [
                "はくさい"
            ]
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "げんまい",
            "text": "アイススケートは好きです！",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "おでん",
            "text": "今からしたら、確実に入院ですね笑笑",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "げんまい",
            "text": "入院は困りますね。。。",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "はくさい",
            "text": "小学校で田んぼに氷貼ってスケート少ししました",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "おでん",
            "text": "私は下手なんですが、スケートは好きなんです",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "げんまい",
            "text": "おお！自然の？！",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "はくさい",
            "text": "@げんまい そうです",
            "mention_to": [
                "げんまい"
            ]
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "おでん",
            "text": "@はくさい えー！それはすごい",
            "mention_to": [
                "はくさい"
            ]
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "げんまい",
            "text": "寒い地域にお住まいなんですね。すごいなー",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "はくさい",
            "text": "でも、スピードスケートの靴なんで",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "おでん",
            "text": "ねー、凄いですよね",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "はくさい",
            "text": "難しかったです",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "はくさい",
            "text": "寒いの嫌です",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "おでん",
            "text": "えースピードスケートの靴！",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "げんまい",
            "text": "スピードスケートは、刃が長いのかな？",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "はくさい",
            "text": "刃がとがっててバランスが取れなくて",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "おでん",
            "text": "先っぽの？ギザギザがないのかな？",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "はくさい",
            "text": "かなりグラグラしてました",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "げんまい",
            "text": "難しいですね。",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "おでん",
            "text": "それはもちろんマイ靴なんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "はくさい",
            "text": "当時はそうでしたね",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "げんまい",
            "text": "気になる。みんな持ってるのですか？",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "げんまい",
            "text": "靴",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "おでん",
            "text": "ねー気になりますね",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "はくさい",
            "text": "いまでもやっている学校は自分でもってるのかな",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "げんまい",
            "text": "子どもってすぐにサイズアウトするから、ピッタリサイズは大変そう",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "おでん",
            "text": "すごい！みんな滑れるんでしょうね",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "はくさい",
            "text": "休日に行く場合は貸してくれるみたいです",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "げんまい",
            "text": "アイススケート場は、普通貸し靴ですよね。",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "おでん",
            "text": "これだけ暖冬だと、もう、田んぼとか、氷はりませんか？",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "はくさい",
            "text": "今年は暖冬でできませんでしたね",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "おでん",
            "text": "@げんまい ですよね",
            "mention_to": [
                "げんまい"
            ]
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "げんまい",
            "text": "あー、残念",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "げんまい",
            "text": "寒いのいやだけど、ｗｗｗ",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "はくさい",
            "text": "@げんまい 同じく",
            "mention_to": [
                "げんまい"
            ]
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "げんまい",
            "text": "もうこのまま春になっちゃいますかね。",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "おでん",
            "text": "ほんと、ちょっとしたところに、影響出てるんですね",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "はくさい",
            "text": "早く春が来てほしいです",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "おでん",
            "text": "子供の楽しみが減っちゃいますね",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "はくさい",
            "text": "確かに",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "げんまい",
            "text": "都内、来週、雪マーク出てますが。。。降らないかな？",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "はくさい",
            "text": "都内も出てるんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "おでん",
            "text": "私のところは、今日は暑いぐらいです",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "げんまい",
            "text": "来週、雪だるまのマークついてました。",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "おでん",
            "text": "雪マーク！",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "おでん",
            "text": "寒くなるんですね",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "げんまい",
            "text": "今日は、暖かいです。",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "おでん",
            "text": "同じくです",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "げんまい",
            "text": "子どもが、雪！とよろこんでいますが、",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "げんまい",
            "text": "先日のようには積もらないかもしれません",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "おでん",
            "text": "嬉しいですよね、子供は",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "はくさい",
            "text": "この間の雪は大変でした",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "はくさい",
            "text": "子どもは関係ないですけどね",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "げんまい",
            "text": "@はくさい けっこう積もりました？",
            "mention_to": [
                "はくさい"
            ]
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "おでん",
            "text": "@はくさい 雪かきとかありますか？",
            "mention_to": [
                "はくさい"
            ]
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "はくさい",
            "text": "うちの周りで20センチくらいですかね",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "げんまい",
            "text": "20センチは雪かきしないとですねぇ",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "おでん",
            "text": "おー結構積もったんですね",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "げんまい",
            "text": "大人は大変",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "はくさい",
            "text": "@げんまい たまにしか降らないですが、雪かきは必須です",
            "mention_to": [
                "げんまい"
            ]
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "おでん",
            "text": "うんうん",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "はくさい",
            "text": "雪かきで筋肉痛がつらい",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "げんまい",
            "text": "先日東京で数センチ積もって、ちょっと雪かきしました。",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "おでん",
            "text": "子供と犬だけでしょうか？嬉しいの",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "げんまい",
            "text": "筋肉痛になるほどは、大変だ。",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "おでん",
            "text": "雪は、溶けかけも怖いですよね",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "はくさい",
            "text": "怖いです",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "げんまい",
            "text": "溶けて、凍ると滑りますよね",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "おでん",
            "text": "凍るのが、こわいですね",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "はくさい",
            "text": "道路がつるつるに凍ってしまって",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "げんまい",
            "text": "車、怖い",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "おでん",
            "text": "わぁー危ない",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "はくさい",
            "text": "ビクビクしながら運転してます",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "げんまい",
            "text": "運転されるんですね",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "おでん",
            "text": "それでも運転されるんだ",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "はくさい",
            "text": "車がないと生活できない田舎なもので",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "おでん",
            "text": "慣れてらっしゃるんですね",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "はくさい",
            "text": "何とかがんばってる感じです",
            "mention_to": []
        }
    ],
