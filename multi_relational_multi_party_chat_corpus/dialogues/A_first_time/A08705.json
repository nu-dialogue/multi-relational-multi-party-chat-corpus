    "relationship": [],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "つぶあん",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "おにぎり",
            "text": "こんにちは。",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "アルパカ",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "つぶあん",
            "text": "お願いします",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "おにぎり",
            "text": "よろしくお願いいたします！",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "アルパカ",
            "text": "お願いします",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "つぶあん",
            "text": "みなさん冬のスポーツ",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "つぶあん",
            "text": "何かやりますか？",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "アルパカ",
            "text": "スノボーしたいですね",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "つぶあん",
            "text": "おお！",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "おにぎり",
            "text": "うーん、最近はしていないですねぇ。",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "アルパカ",
            "text": "最近は出来ていないですが",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "おにぎり",
            "text": "@アルパカ 滑れるんですね！",
            "mention_to": [
                "アルパカ"
            ]
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "つぶあん",
            "text": "したいですね！",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "アルパカ",
            "text": "転びながらですが結構滑れましたね",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "おにぎり",
            "text": "私は、若い頃ちょっとやったっきり、ですねぇ。",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "つぶあん",
            "text": "雪山行くだけで景色もよいですよね！",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "おにぎり",
            "text": "スノボは転ぶと痛い！",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "アルパカ",
            "text": "雪山素敵です",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "アルパカ",
            "text": "子供連れて行きたいんですが）",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "アルパカ",
            "text": "大変ですよね",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "つぶあん",
            "text": "はい！",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "つぶあん",
            "text": "わかります！",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "アルパカ",
            "text": "@おにぎり スノボ痛いですよねおしり",
            "mention_to": [
                "おにぎり"
            ]
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "つぶあん",
            "text": "@おにぎり 全身筋肉痛ですね",
            "mention_to": [
                "おにぎり"
            ]
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "おにぎり",
            "text": "スノボとか、まず荷物の準備が大変ですよね。",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "アルパカ",
            "text": "レンタルとかにしたら減るんですかね",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "つぶあん",
            "text": "@おにぎり 準備たいへん",
            "mention_to": [
                "おにぎり"
            ]
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "おにぎり",
            "text": "スキーの方が安全な気がします。",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "おにぎり",
            "text": "完全レンタルだったら良さそうですね！",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "つぶあん",
            "text": "@アルパカ 子供のサイズがすぐ変わると思うと",
            "mention_to": [
                "アルパカ"
            ]
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "つぶあん",
            "text": "買うのにためらいます",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "おにぎり",
            "text": "身一つで、行くだけでOKかも！",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "つぶあん",
            "text": "@おにぎり お手軽ですね！",
            "mention_to": [
                "おにぎり"
            ]
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "アルパカ",
            "text": "子供のサイズ、それは変わりますね",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "おにぎり",
            "text": "成長期は、毎年買う位の勢いですよね。。",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "アルパカ",
            "text": "レンタル一択ですね",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "つぶあん",
            "text": "本当ですね。",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "アルパカ",
            "text": "インスタ見てると",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "つぶあん",
            "text": "はい",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "アルパカ",
            "text": "結構行ってる人多いんですよね",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "アルパカ",
            "text": "家族連れで",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "つぶあん",
            "text": "そうなんですね！",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "おにぎり",
            "text": "タイヤみたいのに乗っかって滑るだけでも楽しい！",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "つぶあん",
            "text": "連れて行ってあげたい",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "アルパカ",
            "text": "そうなんです、連れて行ってあげたい",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "おにぎり",
            "text": "小さい子でも、楽しめますよね。",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "つぶあん",
            "text": "そりも楽しめますかね",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "アルパカ",
            "text": "@おにぎり そりでも楽しそうです",
            "mention_to": [
                "おにぎり"
            ]
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "アルパカ",
            "text": "そりですよね",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "おにぎり",
            "text": "小さい子だと、天気次第かもしれませんね。",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "つぶあん",
            "text": "なるほど",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "おにぎり",
            "text": "あまりにも、雪降っていると、大変かも。。",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "つぶあん",
            "text": "それはありますね",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "おにぎり",
            "text": "そうそう、そり遊び！",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "アルパカ",
            "text": "雪降ってたら嫌ですね",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "つぶあん",
            "text": "天気は晴れがいいですね！",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "おにぎり",
            "text": "小さい頃、子供をスクール入れたら、真面目にやってて、かわいかったです笑",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "つぶあん",
            "text": "おお",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "アルパカ",
            "text": "スクール！すてきですね",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "アルパカ",
            "text": "真面目にやってると可愛いですよね",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "つぶあん",
            "text": "スクールに入れるのがいいですよね！",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "アルパカ",
            "text": "美容院とかで大人しく座ってるとこみて同じこと思います",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "おにぎり",
            "text": "スキーでしたが、止まる練習のみ、みたいな笑",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "つぶあん",
            "text": "なるほど！",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "おにぎり",
            "text": "美容院の子供もかわいい！！",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "つぶあん",
            "text": "@アルパカ かわいい！",
            "mention_to": [
                "アルパカ"
            ]
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "アルパカ",
            "text": "止まる練習大事ですね",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "アルパカ",
            "text": "可愛いですよねー",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "つぶあん",
            "text": "@おにぎり 止まるのさえ覚えればいいんですかね！",
            "mention_to": [
                "おにぎり"
            ]
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "つぶあん",
            "text": "止まるの大事！",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "おにぎり",
            "text": "そういえば、靴が硬くて、嫌がったかも笑",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "アルパカ",
            "text": "靴は硬いですよね",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "つぶあん",
            "text": "なるほど！",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "アルパカ",
            "text": "分かります",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "つぶあん",
            "text": "確かに普通の靴とは違う",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "おにぎり",
            "text": "すぐ、そり遊びに移行した記憶が。。",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "つぶあん",
            "text": "違和感ありまくりですね",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "アルパカ",
            "text": "スキーはあんまりしたことないんですよね",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "つぶあん",
            "text": "おお！",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "アルパカ",
            "text": "そりが1番いいですね",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "アルパカ",
            "text": "安全ですし",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "おにぎり",
            "text": "スノボ派ですね！",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "つぶあん",
            "text": "ずっとスノボで？",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "アルパカ",
            "text": "スノボしか経験無いです！",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "つぶあん",
            "text": "子供に教えるのにスノボかスキーか悩みませんか？",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "アルパカ",
            "text": "子供の頃あるみたいですが覚えてないんですよね",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "おにぎり",
            "text": "子供なら、スノボも楽々出来そう。",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "つぶあん",
            "text": "ふむふむ",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "アルパカ",
            "text": "子供の頃なんてそんなものですかね",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "おにぎり",
            "text": "@つぶあん 確かに、悩むかも。",
            "mention_to": [
                "つぶあん"
            ]
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "アルパカ",
            "text": "悩みますね！",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "つぶあん",
            "text": "スクールはスキーが多いのかな？",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "アルパカ",
            "text": "スキーのほうが安全そうですよね",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "おにぎり",
            "text": "スキーの方が、大きくなってからでも、始めやすい？",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "おにぎり",
            "text": "スノボの方が難しくないですか？",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "アルパカ",
            "text": "スノボ支えが無いですもんね",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "つぶあん",
            "text": "なるほど",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "つぶあん",
            "text": "スティックですね",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "おにぎり",
            "text": "最近は、スノボのスクールもあるのではないかな。",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "つぶあん",
            "text": "安心感が",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "アルパカ",
            "text": "安心感ありますよねあの棒",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "おにぎり",
            "text": "そうねー。棒の違いかー！",
            "mention_to": []
        },
        {
            "utterance_id": 103,
            "interlocutor_id": "つぶあん",
            "text": "ですね！",
            "mention_to": []
        },
        {
            "utterance_id": 104,
            "interlocutor_id": "アルパカ",
            "text": "スクールは結構あるんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 105,
            "interlocutor_id": "おにぎり",
            "text": "だったら、子供の内にスノボを教えちゃうとか？",
            "mention_to": []
        },
        {
            "utterance_id": 106,
            "interlocutor_id": "つぶあん",
            "text": "なるほど！",
            "mention_to": []
        },
        {
            "utterance_id": 107,
            "interlocutor_id": "おにぎり",
            "text": "スクール、あると思います！",
            "mention_to": []
        },
        {
            "utterance_id": 108,
            "interlocutor_id": "アルパカ",
            "text": "子供のうちに！いいですね",
            "mention_to": []
        }
