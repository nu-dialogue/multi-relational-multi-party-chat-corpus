        "まりも"
    ],
    "relationship": [],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "きなこ",
            "text": "こんにちは～",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "おでん",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "まりも",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "まりも",
            "text": "今日ピザハットのチラシが",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "まりも",
            "text": "ポストに入ってて",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "おでん",
            "text": "そうなんですね",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "まりも",
            "text": "わりと近くにオープンしたみたいです",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "おでん",
            "text": "食べたくなりましたか？",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "まりも",
            "text": "はい笑",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "きなこ",
            "text": "私、さっきまで今日の夕飯のメニュー考えていました！",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "まりも",
            "text": "そういう時間ですよね～",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "おでん",
            "text": "あら？小腹が空く頃？",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "まりも",
            "text": "オープニングでだいぶ安くて",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "おでん",
            "text": "良いですね",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "まりも",
            "text": "ピザハットって",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "きなこ",
            "text": "オープニングはそそられますね",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "まりも",
            "text": "ふっくらパンピザって生地があるの",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "まりも",
            "text": "知ってますか？",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "きなこ",
            "text": "初めて知りました",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "おでん",
            "text": "そうなんですね、知りませんでした",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "まりも",
            "text": "友人から激推しされてるんですよ",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "おでん",
            "text": "それは食べなきゃですね",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "まりも",
            "text": "ふわふわしてるけど表面はサクサクらしくて",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "きなこ",
            "text": "おいしそう！",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "まりも",
            "text": "ずっと気になってるんです",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "おでん",
            "text": "食べなきゃ",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "きなこ",
            "text": "薄いピザ生地もおいしいですが、ふっくらも好きです",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "まりも",
            "text": "あと北海道ではパンピザ選ぶ人が多いとか",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "きなこ",
            "text": "地域差があるんですかね～",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "まりも",
            "text": "あったかさが持続するから～みたいな理由で",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "おでん",
            "text": "@きなこ @まりも どんなトッピングが、好きですか？",
            "mention_to": [
                "まりも",
                "きなこ"
            ]
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "まりも",
            "text": "私はクリスピーがあったらクリスピーにしてしまう",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "おでん",
            "text": "クリスピーは薄い生地？",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "まりも",
            "text": "@おでん 前はマルゲリータがばっかり食べてました",
            "mention_to": [
                "おでん"
            ]
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "まりも",
            "text": "薄いカリカリのやつですね",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "おでん",
            "text": "マルゲリータ、美味しいですよね",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "きなこ",
            "text": "私は肉系でふっくらです！",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "まりも",
            "text": "最近ビスマルクが好きです",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "まりも",
            "text": "半熟卵？が乗ってるやつ",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "きなこ",
            "text": "ビ・・ビスマルクとは？",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "おでん",
            "text": "ビスマルクって",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "おでん",
            "text": "どんなん？",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "きなこ",
            "text": "しらなかった！おいしそう！",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "まりも",
            "text": "だいたい卵が中央にのってて",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "まりも",
            "text": "切り分けるのが難しい",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "おでん",
            "text": "そうなんですね、",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "きなこ",
            "text": "確かに笑",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "まりも",
            "text": "おふたりはクワトロフォルマッジ食べたことあります？",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "おでん",
            "text": "半熟だと、タラーと落ちそうですもんね",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "まりも",
            "text": "そうなんですよ～",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "まりも",
            "text": "難易度が高い",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "おでん",
            "text": "クワトロ⋯なんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "きなこ",
            "text": "ウワトロフォルマッジ・・ないです！",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "まりも",
            "text": "はちみつかけて食べるらしいんですけど",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "おでん",
            "text": "へぇ～甘いんですね",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "まりも",
            "text": "チーズどーん！って感じのやつ？ですね",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "きなこ",
            "text": "チーズピザにはちみつですかね",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "おでん",
            "text": "@まりも 食べたことないんですか？",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "まりも",
            "text": "はちみつかけるとデザート寄りになると",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "まりも",
            "text": "噂に聞いて",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "おでん",
            "text": "うんうん、そんな感じしますね",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "まりも",
            "text": "@おでん 気になってるシリーズです",
            "mention_to": [
                "おでん"
            ]
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "おでん",
            "text": "笑笑なるほどー",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "まりも",
            "text": "チーズの匂いがきついイメージあるんで",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "きなこ",
            "text": "私、どこかのお店で食べたことあります",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "まりも",
            "text": "おぉ",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "おでん",
            "text": "@きなこ どうでしたか？",
            "mention_to": [
                "きなこ"
            ]
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "きなこ",
            "text": "甘じょっぱくておいしかったです！",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "きなこ",
            "text": "くせになる？",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "まりも",
            "text": "あまじょっぱい",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "おでん",
            "text": "あまじょっぱいんですか",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "まりも",
            "text": "なるほど",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "きなこ",
            "text": "そんな感じでした！",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "おでん",
            "text": "美味しかったんですね",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "まりも",
            "text": "興味はあります！",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "おでん",
            "text": "試したいけど、",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "おでん",
            "text": "いざとなった、頼めなさそ笑笑",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "まりも",
            "text": "一緒に行く人がいれば…！",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "きなこ",
            "text": "そうそう！私もお友達いたから",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "まりも",
            "text": "1人では冒険できない",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "おでん",
            "text": "そうですね、分けて食べれるぐらいなら、試せますね",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "きなこ",
            "text": "そこですよね～一人じゃ難しい",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "おでん",
            "text": "メモしときます",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "まりも",
            "text": "誰か道連れを探さないと",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "おでん",
            "text": "道連れ笑笑",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "まりも",
            "text": "いやおいしいかもしれないですけど",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "きなこ",
            "text": "いいですね！おいしい道連れですよ！",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "おでん",
            "text": "でも、美味しかったそうだから、大丈夫ですね",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "まりも",
            "text": "謎のプレッシャーが",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "おでん",
            "text": "ちゃんと感想聞けて良かった",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "おでん",
            "text": "あまじょっぱい",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "きなこ",
            "text": "そうそう笑",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "まりも",
            "text": "あんまり想像できないんですよね",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "きなこ",
            "text": "おいしいですから！",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "まりも",
            "text": "ピザにはちみつが",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "きなこ",
            "text": "好みはありますけど・・",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "まりも",
            "text": "いずれ必ず",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "まりも",
            "text": "チャレンジします！",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "きなこ",
            "text": "@まりも 甘いもの好きですか？",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "おでん",
            "text": "ピザ屋さんが近くだと、友達が来た時とかに便利でいいですよね",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "まりも",
            "text": "@きなこ 急にすごく食べたいときと全然要らないときとあります",
            "mention_to": [
                "きなこ"
            ]
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "おでん",
            "text": "私も甘いものは好きだけど、",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "おでん",
            "text": "なかなか冒険が出来ません。",
            "mention_to": []
        },
        {
            "utterance_id": 103,
            "interlocutor_id": "まりも",
            "text": "どんなのが好きですか？",
            "mention_to": []
        },
        {
            "utterance_id": 104,
            "interlocutor_id": "まりも",
            "text": "チョコとか？",
            "mention_to": []
        }
