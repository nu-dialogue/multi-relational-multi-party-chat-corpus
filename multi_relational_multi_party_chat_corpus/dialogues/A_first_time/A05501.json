        },
        {
            "utterance_id": 89,
            "interlocutor_id": "あずき",
            "text": "大喜びでしたもん",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "あずき",
            "text": "雨でも雪でも",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "まりも",
            "text": "そうなんですか！かわいい",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "あずき",
            "text": "関係ないですよね",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "あずき",
            "text": "寒くないのかな",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "まぐろ",
            "text": "犬可愛いですよね～",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "あずき",
            "text": "こっちが心配になる",
