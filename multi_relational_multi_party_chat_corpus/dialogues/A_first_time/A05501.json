    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "あずき",
            "text": "こんにちはー",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "まりも",
            "text": "こんにちは～",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "まぐろ",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "あずき",
            "text": "今日も寒いですね",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "あずき",
            "text": "こちらは雪が降ってます",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "まりも",
            "text": "寒いですね～",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "まぐろ",
            "text": "凄く寒かった",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "まりも",
            "text": "わあ",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "まりも",
            "text": "雪降る地域ですか",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "あずき",
            "text": "数日前まで暖かかったのに",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "あずき",
            "text": "@まりも そうなんです",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "まりも",
            "text": "一度暑くなったのに",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "まぐろ",
            "text": "そうそう暖かかったね～",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "あずき",
            "text": "逆戻りですよね",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "あずき",
            "text": "悲しい",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "まりも",
            "text": "今日は湯たんぽ必須です",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "あずき",
            "text": "早く暖かく",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "あずき",
            "text": "なってほしい",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "あずき",
            "text": "@まりも 湯たんぽいいですね",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "あずき",
            "text": "寝る時ですか？",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "まぐろ",
            "text": "私はタイツに戻りました・・",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "まりも",
            "text": "@あずき 雪かき必要な感じですか？",
            "mention_to": [
                "あずき"
            ]
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "あずき",
            "text": "@まりも そこまでは大丈夫そうです",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "あずき",
            "text": "うっすら積もってます",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "あずき",
            "text": "雪かき大変なんで",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "あずき",
            "text": "嫌なんですよね",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "まりも",
            "text": "@あずき 雪かき大変そうですよね",
            "mention_to": [
                "あずき"
            ]
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "あずき",
            "text": "体が痛くなりますよ",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "あずき",
            "text": "筋肉痛です",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "まりも",
            "text": "北海道の知人が腰痛めながら",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "あずき",
            "text": "重い雪だと特に辛い",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "まりも",
            "text": "雪かきしてましたね",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "あずき",
            "text": "うんうん",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "あずき",
            "text": "そうなんですねー",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "まぐろ",
            "text": "大変ですね",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "あずき",
            "text": "北海道は大変だ",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "あずき",
            "text": "解けないですよね",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "あずき",
            "text": "きっと",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "まりも",
            "text": "うちは関東平野なので",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "まりも",
            "text": "積もることは稀ですが",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "あずき",
            "text": "そうなんですね",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "あずき",
            "text": "@まりも 積もると慣れてないから",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "まりも",
            "text": "この前は結構降りましたね",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "あずき",
            "text": "大変ですね",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "あずき",
            "text": "ですよねー",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "まぐろ",
            "text": "大変でしたね",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "まりも",
            "text": "@あずき そうなんです、対策してないから",
            "mention_to": [
                "あずき"
            ]
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "あずき",
            "text": "この前は大変でした",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "あずき",
            "text": "ツルツルでしたね",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "あずき",
            "text": "車もスタットレスタイヤ",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "まりも",
            "text": "雪というより路面凍ると危ないですね",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "あずき",
            "text": "じゃないですよね",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "あずき",
            "text": "そうなんですよー",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "あずき",
            "text": "歩くのも怖いです",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "まりも",
            "text": "@まぐろ 雪降る地域ですか？",
            "mention_to": [
                "まぐろ"
            ]
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "まぐろ",
            "text": "@まりも 私も関東なので・・降らないですね～",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "あずき",
            "text": "@まりも いいですね",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "あずき",
            "text": "@まぐろ いいですねー",
            "mention_to": [
                "まぐろ"
            ]
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "あずき",
            "text": "雪がよく降るので",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "あずき",
            "text": "積もっても",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "まりも",
            "text": "たまに雪で電車止まったりすると",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "まりも",
            "text": "通勤通学の人大変そうだなと思います",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "あずき",
            "text": "自転車で通勤している人も",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "あずき",
            "text": "見かけます",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "まりも",
            "text": "自転車？滑らないんですかね",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "まぐろ",
            "text": "雪の通勤通学大変！",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "あずき",
            "text": "@まりも危険ですよね",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "あずき",
            "text": "私はできないですけど",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "あずき",
            "text": "強者もいます笑",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "まりも",
            "text": "@あずき そうですよね？ツルーンといかないのか",
            "mention_to": [
                "あずき"
            ]
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "まりも",
            "text": "ツワモノですね",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "あずき",
            "text": "ブレーキできないですよね？",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "あずき",
            "text": "普通に雪の上、乗ってました笑",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "まりも",
            "text": "筋力でねじ伏せてるとか…？",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "まりも",
            "text": "バランスいい人なのかな",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "あずき",
            "text": "すごいテクニックですね",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "あずき",
            "text": "うんうん",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "あずき",
            "text": "バランス感覚いいのかも",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "まぐろ",
            "text": "みんなすごいですね～",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "まりも",
            "text": "犬ぞりとかたまに動画で見かけますが",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "あずき",
            "text": "うんうん",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "まりも",
            "text": "飼い主も上手じゃないと",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "まりも",
            "text": "転びそう",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "あずき",
            "text": "@まりも 確かに",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "あずき",
            "text": "自分が転びそう",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "あずき",
            "text": "でも犬は雪、好きですよねー",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "まりも",
            "text": "そうですよね",
            "mention_to": []
        },
        {
            "utterance_id": 87,
