    "relationship": [],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "ねぎとろ",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "あひる",
            "text": "こんにちは！",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "みたらし",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "ねぎとろ",
            "text": "最近、オットーという男という映画をみて",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "あひる",
            "text": "ほぉ",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "ねぎとろ",
            "text": "とてもよかったんですが、",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "みたらし",
            "text": "映画いいですね",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "ねぎとろ",
            "text": "最近、映画観ましたか？",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "みたらし",
            "text": "最近見に行ってないな",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "ねぎとろ",
            "text": "そうなんですね。",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "あひる",
            "text": "最近観れてないなぁ！",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "ねぎとろ",
            "text": "@みたらし 映画館派",
            "mention_to": [
                "みたらし"
            ]
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "ねぎとろ",
            "text": "ですか？",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "みたらし",
            "text": "昔はそうでした",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "あひる",
            "text": "映画館の雰囲気、",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "あひる",
            "text": "いいですよね！",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "みたらし",
            "text": "近年はプライム系ですね",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "ねぎとろ",
            "text": "そうですよね。",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "あひる",
            "text": "@みたらし 同じくです！",
            "mention_to": [
                "みたらし"
            ]
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "ねぎとろ",
            "text": "@あひる ちょっと特別感ありました。",
            "mention_to": [
                "あひる"
            ]
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "みたらし",
            "text": "プライム便利すぎて、、、",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "あひる",
            "text": "便利...！",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "みたらし",
            "text": "映画館いいですよね",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "ねぎとろ",
            "text": "便利ですね",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "みたらし",
            "text": "映画館の雰囲気いいですよね",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "あひる",
            "text": "ささやかな非日常",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "ねぎとろ",
            "text": "子どもの頃、親につれていて貰ったの思い出します。",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "みたらし",
            "text": "いいですね。",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "あひる",
            "text": "@ねぎとろ たしかに！",
            "mention_to": [
                "ねぎとろ"
            ]
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "みたらし",
            "text": "@ねぎとろ うちの親も映画館好きだったので",
            "mention_to": [
                "ねぎとろ"
            ]
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "ねぎとろ",
            "text": "うんうん",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "みたらし",
            "text": "小さい頃はよく連れて行ってくれました",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "ねぎとろ",
            "text": "みなさん、一番初めに観た映画、覚えてますか？",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "みたらし",
            "text": "なんだろう？",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "あひる",
            "text": "ファインディングニモ！",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "みたらし",
            "text": "ドラえもん？",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "ねぎとろ",
            "text": "アニメですね、やっぱり",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "みたらし",
            "text": "ですね",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "あひる",
            "text": "ほんとだ！",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "みたらし",
            "text": "ニモもいいですね",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "ねぎとろ",
            "text": "ファインディングニモって、つい最近ですね。",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "あひる",
            "text": "ディズニー好きで！",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "ねぎとろ",
            "text": "わたしも好きです。",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "みたらし",
            "text": "@ねぎとろ いつでしたっけ？",
            "mention_to": [
                "ねぎとろ"
            ]
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "ねぎとろ",
            "text": "@みたらし いつかはわからないのですが。",
            "mention_to": [
                "みたらし"
            ]
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "あひる",
            "text": "親に連れて行ってもらったのに、親が寝ちゃってた思い出が",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "ねぎとろ",
            "text": "すっかり大人になっていたのは確かです",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "みたらし",
            "text": "そうなんですね。",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "ねぎとろ",
            "text": "@あひる あるあるですね",
            "mention_to": [
                "あひる"
            ]
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "みたらし",
            "text": "私も二十歳のころ妹を映画に連れて行ったら",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "あひる",
            "text": "笑",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "あひる",
            "text": "うんうん",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "ねぎとろ",
            "text": "うんうん",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "みたらし",
            "text": "寝てしまいました",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "ねぎとろ",
            "text": "笑",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "あひる",
            "text": "暗いですし、疲れてたりするとね 笑",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "あひる",
            "text": "寝ちゃいます...",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "みたらし",
            "text": "寝ますね",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "みたらし",
            "text": "なんだか気持ちいいですよね",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "あひる",
            "text": "そうなんですよね",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "あひる",
            "text": "音はしてるのに、不思議と",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "ねぎとろ",
            "text": "映画館と、プラネタリウムはよく寝られます。",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "あひる",
            "text": "寝てしまう",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "あひる",
            "text": "プラネタリウムは寝るのに向いてそうですね！",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "みたらし",
            "text": "結構大音量なのに不思議ですね",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "あひる",
            "text": "ねー！",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "ねぎとろ",
            "text": "みなさん、お疲れでしょうか・",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "みたらし",
            "text": "プラネタリウムいいですね。",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "みたらし",
            "text": "好きなんですよ",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "ねぎとろ",
            "text": "@みたらし たまに行くと、いいんですよ！",
            "mention_to": [
                "みたらし"
            ]
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "ねぎとろ",
            "text": "寝ちゃうけど",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "あひる",
            "text": "プラネタリウム好き！",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "みたらし",
            "text": "ですね。",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "みたらし",
            "text": "こんど行ってみよう",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "ねぎとろ",
            "text": "ぜひぜひ",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "ねぎとろ",
            "text": "ナレーションの男性の声が、眠気をさらにさそう",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "みたらし",
            "text": "5~6年は行ってないので、、",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "あひる",
            "text": "ナレーションたしかに！",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "みたらし",
            "text": "@ねぎとろ 確かに",
            "mention_to": [
                "ねぎとろ"
            ]
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "ねぎとろ",
            "text": "もちろん、星を楽しみにいってますけど！",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "あひる",
            "text": "久々に行きたいな",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "あひる",
            "text": "@ねぎとろ うんうん！！",
            "mention_to": [
                "ねぎとろ"
            ]
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "みたらし",
            "text": "ですね",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "あひる",
            "text": "眠くなるのも含めて癒しってことで...",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "ねぎとろ",
            "text": "ですね！",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "みたらし",
            "text": "@あひる 癒しいいですね",
            "mention_to": [
                "あひる"
            ]
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "ねぎとろ",
            "text": "癒やされたい",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "ねぎとろ",
            "text": "皆さんにとっても癒やしとは？",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "あひる",
            "text": "癒しだいじですね",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "ねぎとろ",
            "text": "とっての",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "ねぎとろ",
            "text": "大事です",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "みたらし",
            "text": "うーん、なんだろう",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "あひる",
            "text": "最近メイクしてるときが癒されます",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "ねぎとろ",
            "text": "そうなんですね！",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "あひる",
            "text": "なんかちょっと、",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "ねぎとろ",
            "text": "うんうん",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "あひる",
            "text": "違う自分になれるというか",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "みたらし",
            "text": "自然に囲まれると癒されますね",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "あひる",
            "text": "@みたらし いいですね！",
            "mention_to": [
                "みたらし"
            ]
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "あひる",
            "text": "わかる！",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "ねぎとろ",
            "text": "@あひる なるほど",
            "mention_to": [
                "あひる"
            ]
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "あひる",
            "text": "森林浴とか好きです！",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "ねぎとろ",
            "text": "@みたらし わかります！",
            "mention_to": [
                "みたらし"
            ]
        },
        {
            "utterance_id": 103,
            "interlocutor_id": "みたらし",
            "text": "露天温泉かな",
            "mention_to": []
        },
        {
            "utterance_id": 104,
            "interlocutor_id": "あひる",
            "text": "おんせん〜！！",
            "mention_to": []
        },
        {
            "utterance_id": 105,
            "interlocutor_id": "あひる",
            "text": "最高",
            "mention_to": []
        },
        {
            "utterance_id": 106,
            "interlocutor_id": "ねぎとろ",
            "text": "どっちも癒やされそう",
            "mention_to": []
        }
