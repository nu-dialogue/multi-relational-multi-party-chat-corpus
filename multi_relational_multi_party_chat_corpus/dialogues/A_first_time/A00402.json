    "relationship": [],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "おにぎり",
            "text": "こんにちは。",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "げんまい",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "おにぎり",
            "text": "今日は、バレンタインですね。",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "げんまい",
            "text": "それ、言おうと思ってた。バレンタインデーW",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "おにぎり",
            "text": "何か、しますか？",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "おにぎり",
            "text": "私は、何もしませんが笑",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "げんまい",
            "text": "家族と職場に配布しました",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "てんぷら",
            "text": "すっかり忘れていました",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "げんまい",
            "text": "職場は今日休みなので、昨日のうちに配布済",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "てんぷら",
            "text": "いくらくらいの買いました？",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "おにぎり",
            "text": "職場にも渡すのですね！",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "げんまい",
            "text": "夫には200円ぐらいの板チョコ",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "おにぎり",
            "text": "板チョコ！",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "てんぷら",
            "text": "リーズナブル！",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "おにぎり",
            "text": "でも、優しい！",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "げんまい",
            "text": "あげても「ん」しか言わないんだもん",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "おにぎり",
            "text": "そんな！",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "げんまい",
            "text": "十分です",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "てんぷら",
            "text": "職場のほうが高くつきましたか",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "げんまい",
            "text": "息子にはリクエスト通りにアルファベットチョコ",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "げんまい",
            "text": "質より量だそうです。",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "おにぎり",
            "text": "アルファベットチョコとは？",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "てんぷら",
            "text": "アルファベットチョコってなんでしたっけ",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "げんまい",
            "text": "職場には無印の塩チョコパイ",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "げんまい",
            "text": "大袋入り",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "おにぎり",
            "text": "塩チョコパイ美味しそう～",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "てんぷら",
            "text": "あー、アルファベットが四角いチョコに印字されているやつ",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "げんまい",
            "text": "キューブ型のチョコがネジネジしてあるやつ",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "げんまい",
            "text": "そうです。",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "おにぎり",
            "text": "え、わからない！",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "てんぷら",
            "text": "舌で文字をなぞった覚えが",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "げんまい",
            "text": "舌で？！",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "てんぷら",
            "text": "子どものころの記憶です",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "おにぎり",
            "text": "スーパーにありますか？",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "げんまい",
            "text": "私はガリガリ噛む派",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "てんぷら",
            "text": "当てるんですよ、これはAだなとか",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "げんまい",
            "text": "スーパーによくあるやつです",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "おにぎり",
            "text": "探してみよう。",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "てんぷら",
            "text": "ありますね、大袋で",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "おにぎり",
            "text": "あ！ひとつづつ包まれているのかな？",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "てんぷら",
            "text": "そうですそうです",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "げんまい",
            "text": "そうです！",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "おにぎり",
            "text": "わかったかも！",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "おにぎり",
            "text": "確かに、量ですね笑",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "てんぷら",
            "text": "昔は雑誌で調べて都内まで買いにいったりしました",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "おにぎり",
            "text": "高級チョコですね！",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "げんまい",
            "text": "都内。百貨店はすごい人ですよね",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "おにぎり",
            "text": "お勧めチョコありますか？",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "てんぷら",
            "text": "ほんとです、商業戦略にのせられて",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "げんまい",
            "text": "たくさんあって、迷っちゃいます",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "おにぎり",
            "text": "私は、詳しくなくて。",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "てんぷら",
            "text": "形がかわいいのが好きなので、葉っぱの形のやつとか",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "おにぎり",
            "text": "ミントの味のですか？",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "げんまい",
            "text": "見た目でも買っちゃいますねー",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "てんぷら",
            "text": "そうそう、ミントのやつです",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "おにぎり",
            "text": "白金ですっけ？",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "てんぷら",
            "text": "すごい、そうです",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "げんまい",
            "text": "おにぎりさん、詳しいですね",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "てんぷら",
            "text": "詳しいです！",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "おにぎり",
            "text": "食べてみたい、って思い続けてうん十年。。",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "てんぷら",
            "text": "まさか拾ってもらえるとは",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "げんまい",
            "text": "すごい",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "てんぷら",
            "text": "なるほど、狙っていたのですね",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "おにぎり",
            "text": "結婚前までは、神奈川だったんです。",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "げんまい",
            "text": "自分用においしいのを買うのもいいですね",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "げんまい",
            "text": "神奈川からだったら、白金近いですね",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "てんぷら",
            "text": "そうなんですね",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "おにぎり",
            "text": "最近の子はほんと、男の子にあげないですよね。",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "てんぷら",
            "text": "ああ、友チョコとか",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "おにぎり",
            "text": "息子さんは、もらってくるかしら？",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "げんまい",
            "text": "そうなんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "げんまい",
            "text": "もらってきます。息子",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "おにぎり",
            "text": "わぁ！いいな！",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "てんぷら",
            "text": "息子さんおいくつですか",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "げんまい",
            "text": "高校3年生は彼女いるし",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "てんぷら",
            "text": "わー！青春",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "おにぎり",
            "text": "彼女いるんですね～",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "げんまい",
            "text": "高校1年せいは、彼女いないけど去年2個",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "げんまい",
            "text": "楽しそうですよ。青春",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "おにぎり",
            "text": "モテますね！",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "てんぷら",
            "text": "それは楽しそうですね",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "おにぎり",
            "text": "楽しそうだな。。",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "げんまい",
            "text": "お返しするのに、ものすごく悩んで大変そうではあります",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "おにぎり",
            "text": "それも楽しい悩み！",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "てんぷら",
            "text": "ね、楽しそうです",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "げんまい",
            "text": "悩むのも楽しいですね",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "おにぎり",
            "text": "げんまいさんもアドバイスするんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "げんまい",
            "text": "いえいえ",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "げんまい",
            "text": "私には相談されません",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "げんまい",
            "text": "下の子は相談されて、一緒に買いにいったけど",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "おにぎり",
            "text": "あらら、恥ずかしいのですかね？！",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "おにぎり",
            "text": "お買い物、楽しすぎる！",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "げんまい",
            "text": "まぁお母さんの意見なんて参考にならないって思ってるんですよ",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "げんまい",
            "text": "下の子は頼ってくれて、うれしいです。",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "おにぎり",
            "text": "そんなことは！",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "てんぷら",
            "text": "女性としては先輩なのに",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "おにぎり",
            "text": "そうだそうだ！",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "げんまい",
            "text": "そうですよね！先輩です！",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "てんぷら",
            "text": "大先輩ですわ",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "おにぎり",
            "text": "うちの娘たちは男の子の影が全くなく。。",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "げんまい",
            "text": "こちらから、息子にアピールしておきますw",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "てんぷら",
            "text": "娘さん、おいくつですか",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "げんまい",
            "text": "お嬢さん。",
            "mention_to": []
        },
        {
            "utterance_id": 103,
            "interlocutor_id": "おにぎり",
            "text": "姉は女子高なので、仕方ないのかな？！",
            "mention_to": []
        },
        {
            "utterance_id": 104,
            "interlocutor_id": "おにぎり",
            "text": "妹は中一ですが、やはり影無し。",
            "mention_to": []
        }
