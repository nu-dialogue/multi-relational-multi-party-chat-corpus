    "relationship": [],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "さんま",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "くまのみ",
            "text": "こんにちは！",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "さんま",
            "text": "よろしくお願いします",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "くまのみ",
            "text": "よろしくおねがいします",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "ちくわぶ",
            "text": "よろしくお願いします！",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "くまのみ",
            "text": "お願いします",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "さんま",
            "text": "最近ですね",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "くまのみ",
            "text": "どうされましたか",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "ちくわぶ",
            "text": "はい",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "さんま",
            "text": "イヤホンがこわれまして",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "ちくわぶ",
            "text": "ほうほう",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "くまのみ",
            "text": "それは大変ですね",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "くまのみ",
            "text": "@さんま 何に使うイヤホンですか",
            "mention_to": [
                "さんま"
            ]
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "さんま",
            "text": "ブツブツとぎれまして",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "さんま",
            "text": "出かけるとき音楽きかないといやので",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "くまのみ",
            "text": "@さんま 微妙な感じですか？",
            "mention_to": [
                "さんま"
            ]
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "ちくわぶ",
            "text": "とぎれるの気になるなー",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "くまのみ",
            "text": "@さんま それはつらいですね",
            "mention_to": [
                "さんま"
            ]
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "さんま",
            "text": "買い換えねばと",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "くまのみ",
            "text": "@さんま 決断しましたか？",
            "mention_to": [
                "さんま"
            ]
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "さんま",
            "text": "これがないとつらいものってありますか？",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "ちくわぶ",
            "text": "あー",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "ちくわぶ",
            "text": "なんだろん",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "くまのみ",
            "text": "@さんま 最近ではコーヒーかな",
            "mention_to": [
                "さんま"
            ]
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "ちくわぶ",
            "text": "@くまのみ コーヒー？",
            "mention_to": [
                "くまのみ"
            ]
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "ちくわぶ",
            "text": "まいにち飲むんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "くまのみ",
            "text": "@ちくわぶ はい、そうなんです",
            "mention_to": [
                "ちくわぶ"
            ]
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "さんま",
            "text": "@くまのみ コーヒー好きですか？",
            "mention_to": [
                "くまのみ"
            ]
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "くまのみ",
            "text": "@ちくわぶ 毎日４杯ぐらい飲んでいます",
            "mention_to": [
                "ちくわぶ"
            ]
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "ちくわぶ",
            "text": "結構！",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "くまのみ",
            "text": "@さんま はい、好きなんです",
            "mention_to": [
                "さんま"
            ]
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "ちくわぶ",
            "text": "ブラック？",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "さんま",
            "text": "@くまのみ たくさん！",
            "mention_to": [
                "くまのみ"
            ]
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "くまのみ",
            "text": "@ちくわぶ 結構飲んでます。ブラックです。",
            "mention_to": [
                "ちくわぶ"
            ]
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "ちくわぶ",
            "text": "@くまのみ ブラックなら身体にいいですね！",
            "mention_to": [
                "くまのみ"
            ]
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "くまのみ",
            "text": "あまりよくないと思って、１日２杯にしてたんですけど、最近増えました",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "さんま",
            "text": "あらまあ",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "ちくわぶ",
            "text": "僕もブラック派です！",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "くまのみ",
            "text": "@ちくわぶ そうなんですか？　知りませんでした",
            "mention_to": [
                "ちくわぶ"
            ]
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "くまのみ",
            "text": "@ちくわぶ 同じですね。",
            "mention_to": [
                "ちくわぶ"
            ]
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "ちくわぶ",
            "text": "妻は甘いのなので",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "ちくわぶ",
            "text": "ダイエット中だから控えてます",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "くまのみ",
            "text": "@ちくわぶ 頑張ってらっしゃるんですね",
            "mention_to": [
                "ちくわぶ"
            ]
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "ちくわぶ",
            "text": "妻も毎日飲んでました",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "ちくわぶ",
            "text": "ブラック勧めたんですが",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "さんま",
            "text": "@ちくわぶ そうなんですね",
            "mention_to": [
                "ちくわぶ"
            ]
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "ちくわぶ",
            "text": "飲んでくれません",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "さんま",
            "text": "あはは",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "ちくわぶ",
            "text": "苦いみたいで",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "くまのみ",
            "text": "奥様には苦いんですね。残念です。",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "ちくわぶ",
            "text": "ケーキとなら飲めると飲んでますが",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "さんま",
            "text": "美味しいコーヒーならブラックで飲めます",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "ちくわぶ",
            "text": "ダイエットの意味が…",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "ちくわぶ",
            "text": "@さんま おすすめありますか？",
            "mention_to": [
                "さんま"
            ]
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "さんま",
            "text": "@ちくわぶ ないですね",
            "mention_to": [
                "ちくわぶ"
            ]
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "ちくわぶ",
            "text": "僕はこだわりなくて",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "くまのみ",
            "text": "私もこだわりありません。",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "さんま",
            "text": "コーヒー屋さんのなら",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "ちくわぶ",
            "text": "おー入れ方とかもあるのかな？",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "さんま",
            "text": "飲めました",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "くまのみ",
            "text": "でも、唯一スッパイ系は苦手です",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "さんま",
            "text": "ああ",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "ちくわぶ",
            "text": "@くまのみ すっぱいのは確かに飲めないなー",
            "mention_to": [
                "くまのみ"
            ]
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "くまのみ",
            "text": "@さんま コーヒー屋さんはおいしいですもんね",
            "mention_to": [
                "さんま"
            ]
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "ちくわぶ",
            "text": "セブンのもうまいです",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "さんま",
            "text": "最近行ってないから",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "くまのみ",
            "text": "スタバより、タリーズコーヒーかシアトルズベストコーヒーが好きです",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "ちくわぶ",
            "text": "@くまのみ ほうほう",
            "mention_to": [
                "くまのみ"
            ]
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "ちくわぶ",
            "text": "なるほど",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "さんま",
            "text": "なるほどです",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "ちくわぶ",
            "text": "勧めてみます",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "くまのみ",
            "text": "最近はコンビニもおいしいですよね",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "さんま",
            "text": "コンビニは研究してますね",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "くまのみ",
            "text": "@さんま 話戻りますが、イヤホンはどんな種類を考えてますか",
            "mention_to": [
                "さんま"
            ]
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "さんま",
            "text": "いつも安いのなので",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "くまのみ",
            "text": "私、最近、骨伝導のイヤホン買ったのですが、すごくいいです",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "ちくわぶ",
            "text": "@くまのみ わかります",
            "mention_to": [
                "くまのみ"
            ]
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "さんま",
            "text": "もうちょっといいやつがほしいです",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "ちくわぶ",
            "text": "周りの音も聞こえるし",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "くまのみ",
            "text": "@さんま 骨伝導は少し高いので対象外かな？",
            "mention_to": [
                "さんま"
            ]
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "くまのみ",
            "text": "@ちくわぶ そうなんですよね",
            "mention_to": [
                "ちくわぶ"
            ]
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "くまのみ",
            "text": "@さんま ならおすすめです",
            "mention_to": [
                "さんま"
            ]
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "さんま",
            "text": "骨伝導興味あります～",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "くまのみ",
            "text": "@ちくわぶ 骨伝導使ってるんですか？",
            "mention_to": [
                "ちくわぶ"
            ]
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "ちくわぶ",
            "text": "AirPodsもおすすめー",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "ちくわぶ",
            "text": "@くまのみ 使い分けてます！",
            "mention_to": [
                "くまのみ"
            ]
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "さんま",
            "text": "高いですよね",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "くまのみ",
            "text": "@ちくわぶ 知らないです",
            "mention_to": [
                "ちくわぶ"
            ]
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "ちくわぶ",
            "text": "高い！",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "くまのみ",
            "text": "そうなんですね",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "ちくわぶ",
            "text": "耳が小さいから取れちゃうけど",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "ちくわぶ",
            "text": "AirPodsなら取れません！",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "さんま",
            "text": "そうですよね泣",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "ちくわぶ",
            "text": "骨伝導のほうが安いですよ",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "ちくわぶ",
            "text": "AirPodsより",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "くまのみ",
            "text": "あ、そうでしたっけ",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "さんま",
            "text": "いくらくらいでしたか…？？",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "ちくわぶ",
            "text": "えーとたしかAirPodsが3万円くらい",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "ちくわぶ",
            "text": "骨伝導が2万円くらいでした",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "さんま",
            "text": "高い！",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "くまのみ",
            "text": "高い！！",
            "mention_to": []
        }
