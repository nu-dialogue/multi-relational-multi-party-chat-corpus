        "はくまい"
    ],
    "relationship": [],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "まぐろ",
            "text": "はじめまして",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "ねぎとろ",
            "text": "初めまして",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "はくまい",
            "text": "はじめまして！",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "まぐろ",
            "text": "よろしくおねがいします！",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "はくまい",
            "text": "よろしくお願いします",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "ねぎとろ",
            "text": "よろしくお願いいたします",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "まぐろ",
            "text": "ちょっと質問なのですが",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "ねぎとろ",
            "text": "はい",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "はくまい",
            "text": "なんでしょうか",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "まぐろ",
            "text": "目玉焼きに何をかけてたべますか？",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "ねぎとろ",
            "text": "塩こしょう",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "ねぎとろ",
            "text": "です",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "はくまい",
            "text": "@ねぎとろ 同じくです",
            "mention_to": [
                "ねぎとろ"
            ]
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "まぐろ",
            "text": "家族で意見が割れています",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "まぐろ",
            "text": "皆さん、塩コショウなんですね！！",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "ねぎとろ",
            "text": "@まぐろ そうなんですね",
            "mention_to": [
                "まぐろ"
            ]
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "はくまい",
            "text": "ソースか醤油か論争ありますよね",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "ねぎとろ",
            "text": "なるほど",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "まぐろ",
            "text": "@はくまい そうなんです・・",
            "mention_to": [
                "はくまい"
            ]
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "ねぎとろ",
            "text": "@はくまい ソースは、ウスターですか？",
            "mention_to": [
                "はくまい"
            ]
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "まぐろ",
            "text": "うちは、醤油、塩コショウ、ケチャップ",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "はくまい",
            "text": "@まぐろ なにかけます？",
            "mention_to": [
                "まぐろ"
            ]
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "まぐろ",
            "text": "私は・・しょうゆです！",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "はくまい",
            "text": "なんのソース何でしょうかね",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "ねぎとろ",
            "text": "@まぐろ お醤油もおいしいですよね",
            "mention_to": [
                "まぐろ"
            ]
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "まぐろ",
            "text": "そうなんです",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "はくまい",
            "text": "でもそういうのってマヨネーズが結局最強って答えになるんですよね",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "まぐろ",
            "text": "家族にマヨラーいます！",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "ねぎとろ",
            "text": "何でもマヨって人、けっこういらっしゃいますよね。",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "はくまい",
            "text": "ですよね",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "まぐろ",
            "text": "いますね～",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "ねぎとろ",
            "text": "妹は、白ご飯にマヨかけてました。",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "まぐろ",
            "text": "結構つわもの！",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "はくまい",
            "text": "@ねぎとろ 結構重度のマヨラーなんですね笑",
            "mention_to": [
                "ねぎとろ"
            ]
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "ねぎとろ",
            "text": "@まぐろ @はくまい そうなんですよ。",
            "mention_to": [
                "まぐろ",
                "はくまい"
            ]
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "ねぎとろ",
            "text": "観てる方はあまり気持ちよくない笑",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "まぐろ",
            "text": "@ねぎとろ フライものもマヨネーズですか？",
            "mention_to": [
                "ねぎとろ"
            ]
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "はくまい",
            "text": "食べてる本人は幸せでしょうね笑",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "まぐろ",
            "text": "妹さん",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "ねぎとろ",
            "text": "@まぐろ とにかく何にでもマヨ。まさにマヨラーでした。",
            "mention_to": [
                "まぐろ"
            ]
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "まぐろ",
            "text": "マヨ愛がすごいですね！",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "ねぎとろ",
            "text": "@はくまい 幸せなんでしょうね笑",
            "mention_to": [
                "はくまい"
            ]
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "はくまい",
            "text": "マヨの消費量すごそう",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "ねぎとろ",
            "text": "マヨ愛！",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "ねぎとろ",
            "text": "それがトラウマになってか、わたしはほとんどマヨ使いません。",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "はくまい",
            "text": "なるほど笑",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "まぐろ",
            "text": "わー笑",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "まぐろ",
            "text": "同じ家族でも違いますよね",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "ねぎとろ",
            "text": "違いますね。",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "はくまい",
            "text": "もうマヨは妹さん独占なんですね笑",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "ねぎとろ",
            "text": "マヨ係です",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "まぐろ",
            "text": "係・・爆笑",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "まぐろ",
            "text": "うちはケチャップをなんにでもかける家族います",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "はくまい",
            "text": "わぁ",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "まぐろ",
            "text": "目玉焼き、フライ、なんでも・・",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "はくまい",
            "text": "ケチャラーですか笑",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "まぐろ",
            "text": "そうです笑",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "ねぎとろ",
            "text": "ケチェラー笑",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "はくまい",
            "text": "私は何にでも食べるラー油をかけますね",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "まぐろ",
            "text": "その家族は、マヨ食べられません",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "まぐろ",
            "text": "@はくまい おいしいですよね！",
            "mention_to": [
                "はくまい"
            ]
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "ねぎとろ",
            "text": "@はくまい ラー油は辛みを求めてですか？",
            "mention_to": [
                "はくまい"
            ]
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "はくまい",
            "text": "小学校のときにハマって",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "まぐろ",
            "text": "お・・大人味覚！",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "はくまい",
            "text": "重度の食べるラー油中毒になってしまいました笑",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "ねぎとろ",
            "text": "@まぐろ マヨ＜ケチャですね。",
            "mention_to": [
                "まぐろ"
            ]
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "ねぎとろ",
            "text": "@はくまい 渋い味覚の小学生でしたね！",
            "mention_to": [
                "はくまい"
            ]
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "はくまい",
            "text": "ですね！",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "まぐろ",
            "text": "あんまり小学生でラー油きかないですもんね",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "ねぎとろ",
            "text": "ですね～笑",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "まぐろ",
            "text": "@ねぎとろ すきな調味料あります？",
            "mention_to": [
                "ねぎとろ"
            ]
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "まぐろ",
            "text": "推してる調味料？笑",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "ねぎとろ",
            "text": "@まぐろ 最近は、お塩やこしょうなど、シンプルなのに落ち着きました。",
            "mention_to": [
                "まぐろ"
            ]
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "ねぎとろ",
            "text": "あと、お醤油",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "はくまい",
            "text": "ほうほう",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "まぐろ",
            "text": "同じです！私はあと・・",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "はくまい",
            "text": "@ねぎとろ 前に好きだった調味料とかありますか？",
            "mention_to": [
                "ねぎとろ"
            ]
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "まぐろ",
            "text": "ポン酢です",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "ねぎとろ",
            "text": "@はくまい 前は、ウスターソース派でした。",
            "mention_to": [
                "はくまい"
            ]
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "はくまい",
            "text": "なるほどー",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "まぐろ",
            "text": "ソース派！",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "はくまい",
            "text": "おすすめの調味料がありまして",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "ねぎとろ",
            "text": "はい！",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "まぐろ",
            "text": "知りたいです！",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "ねぎとろ",
            "text": "教えて",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "はくまい",
            "text": "ふー塩っていうやつです",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "はくまい",
            "text": "知ってます？",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "ねぎとろ",
            "text": "ふー塩",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "まぐろ",
            "text": "初耳",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "ねぎとろ",
            "text": "？",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "ねぎとろ",
            "text": "どんなのですか？",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "はくまい",
            "text": "ビンで売ってあるんですけど",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "ねぎとろ",
            "text": "うんうん",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "はくまい",
            "text": "普通の塩にスパイスが入ってて",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "はくまい",
            "text": "すごく美味しいんです",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "はくまい",
            "text": "ご飯にかけても",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "まぐろ",
            "text": "メモりました",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "ねぎとろ",
            "text": "お買い物に行ったら探してみます！",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "はくまい",
            "text": "サラダにかけても",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "まぐろ",
            "text": "絶対おいしいやつだ",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "はくまい",
            "text": "とにかくなににかけても美味しいので",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "ねぎとろ",
            "text": "ありがとうございます！",
            "mention_to": []
        }
