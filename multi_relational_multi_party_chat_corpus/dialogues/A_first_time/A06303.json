    "relationship": [],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "ししとう",
            "text": "こんにちは！",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "きりん",
            "text": "こんにちは！",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "げんまい",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "ししとう",
            "text": "今日は子どもと",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "げんまい",
            "text": "うんうん",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "ししとう",
            "text": "地元のショッピングモールに行ってきました",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "きりん",
            "text": "プチお出かけですね",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "げんまい",
            "text": "ショッピングモールたのしいですね",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "ししとう",
            "text": "田舎なのでガラガラでした",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "げんまい",
            "text": "あら、",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "げんまい",
            "text": "寒いからかな？",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "きりん",
            "text": "@ししとう 何かお買い物の目的が？",
            "mention_to": [
                "ししとう"
            ]
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "げんまい",
            "text": "あ、みんな遠出をしてるのかも。",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "きりん",
            "text": "そういえば3連休でしたね",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "ししとう",
            "text": "@きりん することなくて！",
            "mention_to": [
                "きりん"
            ]
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "げんまい",
            "text": "ショッピングも十分、「すること」です！",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "げんまい",
            "text": "何か買いましたか？",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "きりん",
            "text": "@ししとう シンプルな理由！",
            "mention_to": [
                "ししとう"
            ]
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "ししとう",
            "text": "@げんまい 子どもにマンガ本を購入しました！",
            "mention_to": [
                "げんまい"
            ]
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "げんまい",
            "text": "マンガ、好きなんですね",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "きりん",
            "text": "本屋さんしばらく行ってないなぁ",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "ししとう",
            "text": "@げんまい 最近、ワンピースにハマってて。",
            "mention_to": [
                "げんまい"
            ]
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "げんまい",
            "text": "おお！ワンピース！",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "きりん",
            "text": "ワンピース、強いですね",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "ししとう",
            "text": "そうなんです！",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "きりん",
            "text": "今の子も好きなんですね",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "げんまい",
            "text": "何巻まで出てるんでしょうか。",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "ししとう",
            "text": "小学生なんで、すごくはまってます",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "ししとう",
            "text": "@げんまい 107巻だそうです",
            "mention_to": [
                "げんまい"
            ]
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "げんまい",
            "text": "へー",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "げんまい",
            "text": "わお！そんなに出てた。",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "きりん",
            "text": "100巻越えてるんですね！",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "ししとう",
            "text": "すごい冊数ですよね…",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "げんまい",
            "text": "20年以上前ですよね。",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "ししとう",
            "text": "@げんまい ですよね！",
            "mention_to": [
                "げんまい"
            ]
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "きりん",
            "text": "描き続けてるのすごいですね",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "げんまい",
            "text": "ジャンプに掲載されはじめたの。",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "ししとう",
            "text": "@きりん すごいてすよねー",
            "mention_to": [
                "きりん"
            ]
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "げんまい",
            "text": "その頃はジャンプ読んでました。",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "ししとう",
            "text": "@げんまい おぉ！そうだったんですね！",
            "mention_to": [
                "げんまい"
            ]
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "げんまい",
            "text": "@ししとう 全巻購入ですか？",
            "mention_to": [
                "ししとう"
            ]
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "ししとう",
            "text": "107巻だけです",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "げんまい",
            "text": "ですよね。",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "ししとう",
            "text": "ずっとほしかったそうで",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "ししとう",
            "text": "今日デビューです",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "きりん",
            "text": "あらあら",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "げんまい",
            "text": "そうなんだ、おめでとうございます。",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "ししとう",
            "text": "107巻からのデビュー",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "げんまい",
            "text": "もう終わるんですよね？",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "げんまい",
            "text": "ジャンプでは終わったとかなんとか。",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "きりん",
            "text": "えっ",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "ししとう",
            "text": "@げんまい 終盤とは聞いていたんですが、",
            "mention_to": [
                "げんまい"
            ]
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "きりん",
            "text": "そうなんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "ししとう",
            "text": "私は全然知らなくて。",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "げんまい",
            "text": "最近は追いかけてないので知らないけど、",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "きりん",
            "text": "終わりが近いって話は聞いた気がします",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "げんまい",
            "text": "TV、伏線回収しまくって、終わっていく感じっぽい。",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "ししとう",
            "text": "そうなんですね！",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "ししとう",
            "text": "また現役の子どもに聞いてみないと！",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "げんまい",
            "text": "そうですね。",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "きりん",
            "text": "アニメもかなり話数ありそうですね",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "ししとう",
            "text": "そうそう！",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "げんまい",
            "text": "そうですよね。",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "ししとう",
            "text": "アマプラでコツコツみてます",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "きりん",
            "text": "@ししとう おぉー見てるんですね！",
            "mention_to": [
                "ししとう"
            ]
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "げんまい",
            "text": "いっぱいあるから大変だ―",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "ししとう",
            "text": "私は横目でですが",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "げんまい",
            "text": "でも、最初のこ頃のエピソードが後半に関わってきますからね。",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "きりん",
            "text": "ほ～",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "げんまい",
            "text": "お子さんには是非、全部見てほしいです。",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "ししとう",
            "text": "おぉ！",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "げんまい",
            "text": "きっとタメになります！！ｗｗｗ",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "ししとう",
            "text": "そうなんですね！",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "ししとう",
            "text": "夢とか野望とか",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "ししとう",
            "text": "語れる大人になってほしい",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "げんまい",
            "text": "うんうん",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "げんまい",
            "text": "いいですね。",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "ししとう",
            "text": "そうそう！",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "げんまい",
            "text": "仲間を大事にして。",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "ししとう",
            "text": "ショッピングモールのパン屋で",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "げんまい",
            "text": "はい",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "ししとう",
            "text": "海軍カレーパンというカレーパンを見て",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "げんまい",
            "text": "あら、おいしそう。",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "ししとう",
            "text": "おねだりもされました",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "きりん",
            "text": "あはは",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "ししとう",
            "text": "海軍＝ワンピースみたいです",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "きりん",
            "text": "買いました？",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "げんまい",
            "text": "すっかり、はまっておられますね",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "ししとう",
            "text": "買いました！",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "げんまい",
            "text": "買ったんだＷＷ",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "ししとう",
            "text": "家帰って食べてました",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "げんまい",
            "text": "ナイスです。",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "ししとう",
            "text": "いろいろ影響うけてます",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "きりん",
            "text": "海軍カレーって何か違うんですかね？",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "げんまい",
            "text": "かわいい。",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "きりん",
            "text": "あ～でもわかります。好きな作品の影響受けるの",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "ししとう",
            "text": "@きりん カレーの味が違うみたいな",
            "mention_to": [
                "きりん"
            ]
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "ししとう",
            "text": "ポップがでてました",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "げんまい",
            "text": "作品の影響ってありますね",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "きりん",
            "text": "@ししとう ほ～　味が違うんですか",
            "mention_to": [
                "ししとう"
            ]
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "げんまい",
            "text": "へー",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "ししとう",
            "text": "まぁまぁいい値段だったんで子どものだけしか買わず",
            "mention_to": []
        }
