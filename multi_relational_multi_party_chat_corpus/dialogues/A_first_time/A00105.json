        "ねぎとろ"
    ],
    "relationship": [],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "こまつな",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "うどん",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "ねぎとろ",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "こまつな",
            "text": "寒さ対策してますか？",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "うどん",
            "text": "湯たんぽと、湯船に入るの、やってます",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "ねぎとろ",
            "text": "特にしてません。",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "こまつな",
            "text": "湯たんぽ",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "うどん",
            "text": "湯たんぽ、なんだかんだ最強な気がしてます",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "こまつな",
            "text": "ねぎとろさん寒いのへいき？",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "ねぎとろ",
            "text": "湯たんぽ、最強なんですね。",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "ねぎとろ",
            "text": "寒いのは大丈夫です。暑いのがダメ。",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "うどん",
            "text": "暑いのは暑いので大変ですよね",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "こまつな",
            "text": "夏はつらいか",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "ねぎとろ",
            "text": "昔は夏が好きだったんですけどね、、",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "ねぎとろ",
            "text": "最近は、もう暑さに耐えられません。年か。",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "こまつな",
            "text": "私はなつのほうがまし",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "うどん",
            "text": "最近ますます暑いですしね",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "こまつな",
            "text": "熱帯化してきましたね",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "ねぎとろ",
            "text": "亜熱帯気候になってますよね。",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "うどん",
            "text": "昔はもう少し過ごしやすい夏でしたね",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "こまつな",
            "text": "秋がなくなった",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "ねぎとろ",
            "text": "そうそう！",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "うどん",
            "text": "秋ない！",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "こまつな",
            "text": "夏が長いですよね",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "ねぎとろ",
            "text": "いつまでも暑くて、12月すぎにいきなり寒くなる",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "こまつな",
            "text": "紅葉があっという間",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "こまつな",
            "text": "すぐ終わる",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "うどん",
            "text": "急に気温変わりますよね",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "うどん",
            "text": "紅葉、儚いですね",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "ねぎとろ",
            "text": "紅葉の季節って短いですよね、ホント",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "こまつな",
            "text": "なんか色もよくないとか聞くので",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "うどん",
            "text": "紅葉、次の週末に見に行こうと思ったら終わってる",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "こまつな",
            "text": "日本の四季が・・・",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "ねぎとろ",
            "text": "あいまいになってる",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "うどん",
            "text": "色も落ちてるんですね",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "うどん",
            "text": "なんだかなぁ",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "ねぎとろ",
            "text": "さみしいですね",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "こまつな",
            "text": "ほんとです",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "うどん",
            "text": "綺麗な風景見れるって、当たり前じゃないんですね",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "うどん",
            "text": "貴重。",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "こまつな",
            "text": "家から紅葉とか見えたらいいけど",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "ねぎとろ",
            "text": "私の周りで目立ったものって、今は桜くらいかな。。",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "こまつな",
            "text": "実家は自然に囲まれてますけど",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "こまつな",
            "text": "ただの山の中",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "うどん",
            "text": "家の周りにあったら最高ですね",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "ねぎとろ",
            "text": "昔は、目の前のおうちの桜を借景できましたけど、今はなくなった。",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "こまつな",
            "text": "なくなった？",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "うどん",
            "text": "借景すてき。",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "ねぎとろ",
            "text": "建て替えて、桜も引っこ抜かれた。",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "ねぎとろ",
            "text": "もったいない",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "こまつな",
            "text": "あらまあ",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "うどん",
            "text": "桜が抜かれちゃうの悲しい",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "うどん",
            "text": "切ない",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "こまつな",
            "text": "桜のあるおうちいいのに",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "こまつな",
            "text": "手入れが大変か・・・",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "ねぎとろ",
            "text": "裏のおうちの庭で、もうじき梅を見られます。",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "こまつな",
            "text": "梅もいいですね",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "うどん",
            "text": "梅ちょうど季節ですね！",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "こまつな",
            "text": "香りもいいし",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "うどん",
            "text": "梅干しもつくれるし",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "ねぎとろ",
            "text": "作るんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "ねぎとろ",
            "text": "梅干し",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "こまつな",
            "text": "梅干し作るんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "うどん",
            "text": "作るんです！",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "ねぎとろ",
            "text": "すごい！",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "こまつな",
            "text": "えーすごい",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "ねぎとろ",
            "text": "梅仕事ですね",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "こまつな",
            "text": "けっこう干すの時間かかるんじゃ？",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "うどん",
            "text": "干すの時間かかりますけど、干さなくても梅漬け？として食べられます！",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "こまつな",
            "text": "そうなんですね",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "こまつな",
            "text": "梅といえば梅酒だわ",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "うどん",
            "text": "体にいいかなと思って、せっせとやってますね",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "ねぎとろ",
            "text": "爪楊枝で、へこんだところの取るとか",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "ねぎとろ",
            "text": "時間かかりそう",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "ねぎとろ",
            "text": "梅酒いいな",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "うどん",
            "text": "たしかに時間はかかるかもです",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "うどん",
            "text": "梅酒いいですね！",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "こまつな",
            "text": "もっぱら飲むほうです",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "ねぎとろ",
            "text": "わたしもです",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "こまつな",
            "text": "作りません",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "ねぎとろ",
            "text": "つくりません",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "うどん",
            "text": "梅酒ハマっちゃいました",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "こまつな",
            "text": "手仕事好きなんですね",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "うどん",
            "text": "好きです！",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "うどん",
            "text": "なんだか癒されます",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "ねぎとろ",
            "text": "ゆたかですね、そういうの。",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "こまつな",
            "text": "時間があれば・・・",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "こまつな",
            "text": "という人はやらないか",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "うどん",
            "text": "時間とるの大変ですよね",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "ねぎとろ",
            "text": "時間は作るもの、ってよく言われます。",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "こまつな",
            "text": "たしかに",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "ねぎとろ",
            "text": "いまだに母から怒られる",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "うどん",
            "text": "時間の使い方も、工夫かぁ",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "こまつな",
            "text": "もっともですね",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "こまつな",
            "text": "好きなことにならできるかも",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "ねぎとろ",
            "text": "わたしも、自分の在り方、時間との向き合い方、考えなきゃです。",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "こまつな",
            "text": "ふかい",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "うどん",
            "text": "その通りですね",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "うどん",
            "text": "勉強になる",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "ねぎとろ",
            "text": "ただ何か追われて生きてる感じ。",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "こまつな",
            "text": "私も追われてます",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "うどん",
            "text": "気持ちが焦ってきちゃって",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "こまつな",
            "text": "あっという間に一日",
            "mention_to": []
        },
        {
            "utterance_id": 103,
            "interlocutor_id": "うどん",
            "text": "そうなんです",
            "mention_to": []
        },
        {
            "utterance_id": 104,
            "interlocutor_id": "ねぎとろ",
            "text": "ですね。",
            "mention_to": []
        },
        {
            "utterance_id": 105,
            "interlocutor_id": "ねぎとろ",
            "text": "主体性がないｘ",
            "mention_to": []
        },
        {
            "utterance_id": 106,
            "interlocutor_id": "うどん",
            "text": "書き出したりすると、いいんでしょうか",
            "mention_to": []
        },
        {
            "utterance_id": 107,
            "interlocutor_id": "ねぎとろ",
            "text": "いいですね、それ。",
            "mention_to": []
        },
        {
            "utterance_id": 108,
            "interlocutor_id": "こまつな",
            "text": "やること書くの？",
            "mention_to": []
        },
        {
            "utterance_id": 109,
            "interlocutor_id": "うどん",
            "text": "やることとか、悩んでることとか",
            "mention_to": []
        },
        {
            "utterance_id": 110,
            "interlocutor_id": "ねぎとろ",
            "text": "やったことを書き出して、見直す？",
            "mention_to": []
        },
        {
            "utterance_id": 111,
            "interlocutor_id": "こまつな",
            "text": "なるほど",
            "mention_to": []
        },
        {
            "utterance_id": 112,
            "interlocutor_id": "うどん",
            "text": "ねぎとろさん、それいいですね！やってみる",
            "mention_to": []
        }
