        },
        {
            "utterance_id": 29,
            "interlocutor_id": "おでん",
            "text": "ほんと、そうですよね",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "うどん",
            "text": "春はみなさん何か旅行とかお花見とか、行かれますか？",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "はくまい",
            "text": "3月に富士急行く予定です",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "おでん",
            "text": "お花見は行きたいです",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "うどん",
            "text": "@はくまい おぉー！富士急いいですね！",
            "mention_to": [
                "はくまい"
