        },
        {
            "utterance_id": 64,
            "interlocutor_id": "はくさい",
            "text": "同じような人で",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "こぐま",
            "text": "私は、食べたいけど",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "はくさい",
            "text": "じぶんで考えられる人が",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "げんまい",
            "text": "うんうん",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "はくさい",
            "text": "レシピ載せてくれてますから",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "こぐま",
            "text": "作ろうという気がないです笑",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "げんまい",
            "text": "@こぐま 食べる専門ですね。",
            "mention_to": [
                "こぐま"
            ]
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "はくさい",
            "text": "@こぐま その時がんばりすぎて",
            "mention_to": [
                "こぐま"
            ]
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "こぐま",
            "text": "@はくさい ほんと検索すると、すぐに出てきますよね",
