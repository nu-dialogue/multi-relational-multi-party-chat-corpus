        },
        {
            "utterance_id": 6,
            "interlocutor_id": "げんまい",
            "text": "よろしくお願いします",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "こぐま",
            "text": "今日。久しぶりに",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "げんまい",
            "text": "はい。",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "こぐま",
            "text": "ケーキを買ったのですが",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "はくさい",
            "text": "お！",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "げんまい",
            "text": "ケーキ！いいなー",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "こぐま",
            "text": "高かった  泣",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "げんまい",
            "text": "どんなケーキですか？",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "こぐま",
            "text": "いちごが乗った",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "こぐま",
            "text": "タルトとか、ショートケーキ",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "げんまい",
            "text": "いちご、季節ですね。",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "こぐま",
            "text": "3個買って。2000円超えました",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "げんまい",
            "text": "え、高い。",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "はくさい",
            "text": "たかいー！！",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "げんまい",
            "text": "高級ケーキですね。",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "こぐま",
            "text": "高いですよねー",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "こぐま",
            "text": "びっくりです！",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "こぐま",
            "text": "普通のケーキ屋さんなんですけどね",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "げんまい",
            "text": "私は、高いので作派です。",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "はくさい",
            "text": "すごい！",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "こぐま",
            "text": "作るんですか！",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "げんまい",
            "text": "簡単なものだけですけどね。",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "こぐま",
            "text": "@げんまい 凄いですねー",
            "mention_to": [
                "げんまい"
            ]
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "げんまい",
            "text": "ブラウニーとか。チーズケーキとか。",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "げんまい",
            "text": "作ったほうが安いので。",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "こぐま",
            "text": "美味しそう",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "こぐま",
            "text": "確かに！作れたらいいですね",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "げんまい",
            "text": "美味しいです。自画自賛。",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "こぐま",
            "text": "私はケーキは作ったことないし",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "こぐま",
            "text": "できそうにないです笑",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "こぐま",
            "text": "@げんまい 器用なんですね",
            "mention_to": [
                "げんまい"
            ]
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "げんまい",
            "text": "いえいえ。",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "げんまい",
            "text": "食いしん坊なのです。",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "はくさい",
            "text": "私も昔はケーキ作りましたねー",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "げんまい",
            "text": "自分で作ったら、いっぱいできるでしょ。",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "こぐま",
            "text": "えー、食いしん坊なら負けないですけど笑",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "はくさい",
            "text": "食べ放題ですね",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "げんまい",
            "text": "@はくさい 今は？",
            "mention_to": [
                "はくさい"
            ]
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "こぐま",
            "text": "@はくさい 作るんですね",
            "mention_to": [
                "はくさい"
            ]
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "はくさい",
            "text": "息子が小さいとき",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "こぐま",
            "text": "どんなのを作りましたか？",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "げんまい",
            "text": "へー",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "はくさい",
            "text": "小麦と卵のアレルギーで",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "はくさい",
            "text": "市販のものが食べられなくて",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "げんまい",
            "text": "アレルギーかー。買ったもの食べさせられないですね。",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "はくさい",
            "text": "がんばって作り方検索して",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "こぐま",
            "text": "@げんまい @はくさい 器用ですね",
            "mention_to": [
                "げんまい",
                "はくさい"
            ]
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "はくさい",
            "text": "つくってましたね",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "げんまい",
            "text": "米粉を使うんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "はくさい",
            "text": "@げんまい そうなんです",
            "mention_to": [
                "げんまい"
            ]
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "はくさい",
            "text": "卵もダメなので",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "こぐま",
            "text": "今なら、作り方もすぐに調べられる？",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "げんまい",
            "text": "玉子の代替品はむずかしそう",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "はくさい",
            "text": "代わりに豆乳使いました",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "げんまい",
            "text": "なるほど。",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "こぐま",
            "text": "なるほど",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "げんまい",
            "text": "レシピサイト、充実してますよね。",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "はくさい",
