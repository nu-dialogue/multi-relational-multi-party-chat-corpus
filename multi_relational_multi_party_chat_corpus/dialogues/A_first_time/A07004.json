{
    "dialogue_id": "A07004",
    "dialogue_type": "First time",
    "interlocutors": [
        "てんぷら",
        "みたらし",
        "ししとう"
    ],
    "relationship": [],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "てんぷら",
            "text": "こんばんは！",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "みたらし",
            "text": "こんばんは",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "ししとう",
            "text": "こんばんは！",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "てんぷら",
            "text": "明日は月曜です",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "てんぷら",
            "text": "また1週間が始まります",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "ししとう",
            "text": "憂鬱な月曜日〜",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "みたらし",
            "text": "@てんぷら 憂鬱ですね",
            "mention_to": [
                "てんぷら"
            ]
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "てんぷら",
            "text": "憂鬱3人組",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "みたらし",
            "text": "来週は5日間",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "てんぷら",
            "text": "そうなんです",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "ししとう",
            "text": "ですね～！",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "てんぷら",
            "text": "うるう年",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "ししとう",
            "text": "お休みほしい！",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "みたらし",
            "text": "それだｓ",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "てんぷら",
            "text": "関係ないけど1日多い",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "みたらし",
            "text": "1日長い。。",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "ししとう",
            "text": "@てんぷら 無駄に29日",
            "mention_to": [
                "てんぷら"
            ]
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "てんぷら",
            "text": "いやほんとです",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "てんぷら",
            "text": "関係ないんですけどね",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "てんぷら",
            "text": "1週間には。",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "みたらし",
            "text": "出勤日が1日伸びる～",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "てんぷら",
            "text": "最近、仕事が変わったんです",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "ししとう",
            "text": "2月終わったー！と思ったら",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "みたらし",
            "text": "給料変わらない。。。",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "ししとう",
            "text": "あと1日の憂鬱…",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "みたらし",
            "text": "@てんぷら 転職ですか？",
            "mention_to": [
                "てんぷら"
            ]
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "てんぷら",
            "text": "異動みたいな感じで",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "ししとう",
            "text": "@てんぷら おぉ！",
            "mention_to": [
                "てんぷら"
            ]
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "みたらし",
            "text": "@ししとう それありますね",
            "mention_to": [
                "ししとう"
            ]
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "みたらし",
            "text": "@てんぷら 異動ですか？",
            "mention_to": [
                "てんぷら"
            ]
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "てんぷら",
            "text": "大学で先生のパソコンサポートみたいなのをしていたのですが",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "みたらし",
            "text": "場所もですか？",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "みたらし",
            "text": "@てんぷら おお",
            "mention_to": [
                "てんぷら"
            ]
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "てんぷら",
            "text": "サポート先が、小学校になりました",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "みたらし",
            "text": "小学校！",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "ししとう",
            "text": "@てんぷら 慣れるまで大変ですね〜",
            "mention_to": [
                "てんぷら"
            ]
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "てんぷら",
            "text": "そうなんです～",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "みたらし",
            "text": "食堂じゃなく給食ですね",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "みたらし",
            "text": "お昼は",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "てんぷら",
            "text": "給食がですね",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "てんぷら",
            "text": "パソコンサポートには、出ないんです",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "みたらし",
            "text": "なんと",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "ししとう",
            "text": "@てんぷら それはガッカリ！",
            "mention_to": [
                "てんぷら"
            ]
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "みたらし",
            "text": "それはひどいですね",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "ししとう",
            "text": "お弁当持参ですか？",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "てんぷら",
            "text": "配膳が始まりそうな空気を感じたら",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "てんぷら",
            "text": "ダッシュで逃げています",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "みたらし",
            "text": "それも憂鬱ですね",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "てんぷら",
            "text": "カフェ好きなので、外食は全然いいんですけど",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "てんぷら",
            "text": "タイミングが難しいです",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "みたらし",
            "text": "たしかに",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "ししとう",
            "text": "@てんぷら お昼のタイミングって",
            "mention_to": [
                "てんぷら"
            ]
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "ししとう",
            "text": "難しいですよね〜",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "みたらし",
            "text": "大学だとそのへん楽ですよね",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "てんぷら",
            "text": "大学、自動販売機もあるし",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "てんぷら",
            "text": "なんならコンビニもあったので",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "てんぷら",
            "text": "サポートの人間も数人いたので",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "みたらし",
            "text": "大学は楽しいですよね",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "ししとう",
            "text": "@てんぷら なるほど〜",
            "mention_to": [
                "てんぷら"
            ]
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "てんぷら",
            "text": "雑談もしましたね",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "ししとう",
            "text": "夢のキャンパスライフ！",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "みたらし",
            "text": "また大学にいけるといいですね",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "てんぷら",
            "text": "あはは、そうですね",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "みたらし",
            "text": "サポート内容は変わらないんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "みたらし",
            "text": "やっぱ小学校のほうが楽？",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "てんぷら",
            "text": "小学校は、",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "てんぷら",
            "text": "ツールを色々使ってて、そのサポートが慣れないですね",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "みたらし",
            "text": "そうなんですね",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "ししとう",
            "text": "なるほど！",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "てんぷら",
            "text": "大学は先生の端末がMacかWindowsか",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "みたらし",
            "text": "大学のほうが楽なのか",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "てんぷら",
            "text": "プロジェクターに映らないとか、それくらいなので",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "みたらし",
            "text": "ふむふむ",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "てんぷら",
            "text": "小学校は、新入生の住所入力とか",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "てんぷら",
            "text": "差し込み印刷とか、雑務が多いです",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "ししとう",
            "text": "@てんぷら 聞くだけで大変そう〜",
            "mention_to": [
                "てんぷら"
            ]
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "みたらし",
            "text": "確かに小学校のほうがめんどくさそう",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "てんぷら",
            "text": "先生たちも、大変そうです",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "てんぷら",
            "text": "休憩時間、ないんですもん",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "てんぷら",
            "text": "外にコーヒーも買いにいけない",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "みたらし",
            "text": "そうなんですね",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "ししとう",
            "text": "まさにうちは小学生の子がいて",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "ししとう",
            "text": "先日参観日だけでも大変そうだなと…",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "てんぷら",
            "text": "月給100万くらいあげたいです",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "みたらし",
            "text": "昔と時代が変わってきてる",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "ししとう",
            "text": "@てんぷら ですね！",
            "mention_to": [
                "てんぷら"
            ]
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "てんぷら",
            "text": "明日、有給もらいまーす",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "ししとう",
            "text": "@みたらし そうそう！",
            "mention_to": [
                "みたらし"
            ]
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "てんぷら",
            "text": "みたいなのが、先生って出来ないですし",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "みたらし",
            "text": "有給いいですね",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "みたらし",
            "text": "ああ",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "ししとう",
            "text": "確かに休みにくいお仕事ですよね…",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "みたらし",
            "text": "有給とれないですね",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "てんぷら",
            "text": "あ、中学生の娘が先日",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "みたらし",
            "text": "先日？",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "てんぷら",
            "text": "金曜日、担任の先生の娘さんの運動会があるから休みなんだーって",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "てんぷら",
            "text": "良い時代になったなって思いました",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "みたらし",
            "text": "それはいいことですね",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "ししとう",
            "text": "@てんぷら そうなんですね！",
            "mention_to": [
                "てんぷら"
            ]
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "てんぷら",
            "text": "昔は先生、自分の子どもの運動会で休んだり出来なかったかも",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "ししとう",
            "text": "うちは田舎なので",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "みたらし",
            "text": "有給は権利だから先生だから取れないはないかなと思ってました",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "ししとう",
            "text": "お子さんが違う学校でも",
            "mention_to": []
        }
    ],
