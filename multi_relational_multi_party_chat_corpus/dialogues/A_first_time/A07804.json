        },
        {
            "utterance_id": 25,
            "interlocutor_id": "はくまい",
            "text": "スライスチーズ食べますね",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "こぐま",
            "text": "@いそべ 目の前ならね、仕方ない笑",
            "mention_to": [
                "いそべ"
            ]
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "いそべ",
            "text": "チーズ！",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "いそべ",
            "text": "良いですね",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "こぐま",
            "text": "@はくまい チーズかぁ",
