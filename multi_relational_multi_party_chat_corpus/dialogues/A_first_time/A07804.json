        },
        {
            "utterance_id": 23,
            "interlocutor_id": "こぐま",
            "text": "もうメニュー決まってる？",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "はくまい",
            "text": "たまにご飯前お腹空いたら",
