        },
        {
            "utterance_id": 92,
            "interlocutor_id": "おでん",
            "text": "あら、色々ですね",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "はくさい",
            "text": "小説も最後まで読めなかった",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "おでん",
            "text": "長いと、時間かかるし",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "おでん",
            "text": "ですね",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "げんまい",
            "text": "ですね。なかなか時間がかかります",
            "mention_to": []
        },
        {
            "utterance_id": 97,
