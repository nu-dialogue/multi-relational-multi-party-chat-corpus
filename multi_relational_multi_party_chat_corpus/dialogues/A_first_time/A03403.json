        },
        {
            "utterance_id": 1,
            "interlocutor_id": "おでん",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "てんぷら",
            "text": "こんにちは！",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "げんまい",
            "text": "よろしくおねがいします",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "おでん",
            "text": "よろしくお願いします",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "げんまい",
            "text": "私、最近アルバイトはじめまして",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "てんぷら",
            "text": "あらあら",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "げんまい",
            "text": "お惣菜の販売なんですけど",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "おでん",
            "text": "そうなんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "おでん",
            "text": "あら、いいですね",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "げんまい",
            "text": "接客バイトが久しぶりで、パニックです",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "おでん",
            "text": "量り売りですか？",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "げんまい",
            "text": "そうです。",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "げんまい",
            "text": "量り売り",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "おでん",
            "text": "それは大変そうです",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "てんぷら",
            "text": "うん、大変そう",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "げんまい",
            "text": "測るのもたいへんなんですが、",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "おでん",
            "text": "はい",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "げんまい",
            "text": "レジが。。。",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "てんぷら",
            "text": "あー",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "てんぷら",
            "text": "なんか、いっぱいありますよね",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "てんぷら",
            "text": "支払い方法",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "おでん",
            "text": "あー色んなタイプありますよね",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "てんぷら",
            "text": "ぺいぺい、大変そう",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "げんまい",
            "text": "そう。クレジットにICにQRコードに。。。",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "おでん",
            "text": "キャッシュレスもありですか？",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "げんまい",
            "text": "ありです。",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "おでん",
            "text": "あるんですね",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "てんぷら",
            "text": "お客様のほうが知ってそう",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "おでん",
            "text": "色んなパターンがあると、",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "げんまい",
            "text": "そうなんです。お客さんのほうが知ってる",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "おでん",
            "text": "難しそう",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "げんまい",
            "text": "さらに、ポイントカードに駐車券です",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "てんぷら",
            "text": "マシンがうまく動かないとパニックになりそう",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "おでん",
            "text": "わぁ、それは",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "げんまい",
            "text": "もう、おばさん、大混乱ｗｗｗ",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "おでん",
            "text": "笑笑",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "げんまい",
            "text": "若い人がすぐ覚えてすごいなと",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "てんぷら",
            "text": "あとからポイント付けられないの？とかね",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "おでん",
            "text": "@げんまい 行きだしてどれぐらい経ちますか？",
            "mention_to": [
                "げんまい"
            ]
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "げんまい",
            "text": "えーっと４回出勤しました。",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "おでん",
            "text": "まだ、ほんとに行きだしてすぐなんですね",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "てんぷら",
            "text": "それは初心者です、安心して甘えましょう",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "てんぷら",
            "text": "若い人に。",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "げんまい",
            "text": "そうなんです。",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "げんまい",
            "text": "初心者マークをつけたいです。",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "おでん",
            "text": "私なんか、支払う客のほうで、",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "おでん",
            "text": "ポイントとか。何かでパニックてます",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "げんまい",
            "text": "@おでん 支払うのも時間かかるのわかります。",
            "mention_to": [
                "おでん"
            ]
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "てんぷら",
            "text": "昨日アフタヌーンティーでランチしたんですけど",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "てんぷら",
            "text": "ポイント3種類全部付けました",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "げんまい",
            "text": "3種類も？！",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "てんぷら",
            "text": "後ろに並んでいる人がいたのに",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "おでん",
            "text": "色んなもの提示するだけでパニクります",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "てんぷら",
            "text": "三井ショッピングなんとかカードと、",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "てんぷら",
            "text": "アフタヌーンティーメンバーと",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "おでん",
            "text": "@てんぷら 笑笑そうなんですね",
            "mention_to": [
                "てんぷら"
            ]
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "てんぷら",
            "text": "なんかもう一つ",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "げんまい",
            "text": "並んでてもできるのは、大物です！",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "てんぷら",
            "text": "もう覚えていないです、レジのお姉さんに教えてもらいました",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "げんまい",
            "text": "お店の人、すごいですね。",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "てんぷら",
            "text": "1500円そこそこの金額にポイント",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "てんぷら",
            "text": "つけるって言わなきゃよかった",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "げんまい",
            "text": "でも、権利ありますからね。つけないと、もったいなんですからね。",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "てんぷら",
            "text": "いくらくらいポイントついたんでしょう",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "おでん",
            "text": "@げんまい すぐに、慣れて、チャチャッとするようになりますね",
            "mention_to": [
                "げんまい"
            ]
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "てんぷら",
            "text": "ポイントをいっぺんに付けてくれるアプリが開発されないかしら",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "おでん",
            "text": "@てんぷら ポイントはコツコツだから、",
            "mention_to": [
                "てんぷら"
            ]
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "げんまい",
            "text": "それいいなー。",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "てんぷら",
            "text": "ポイントカード一括管理アプリ",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "げんまい",
            "text": "一気に全部やってくれるのがあったら、助かる",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "おでん",
            "text": "ウンウン、そうゆうの欲しい",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "げんまい",
            "text": "私も、コンビニで、もたつきます。",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "げんまい",
            "text": "マイバックに入れて、スマホでポイント画面だして、",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "げんまい",
            "text": "ICカードで支払って",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "てんぷら",
            "text": "マイバッグもたつきますw",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "げんまい",
            "text": "やる事多いです",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "おでん",
            "text": "ほんとあれもこれも、おばちゃんには難しいです",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "てんぷら",
            "text": "SDG",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "てんぷら",
            "text": "SDGsか",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "おでん",
            "text": "笑笑",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "てんぷら",
            "text": "もたついてしまった",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "げんまい",
            "text": "SDGSよりレジ袋にお金払うのがもったいなくて。",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "おでん",
            "text": "ウンウン",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "おでん",
            "text": "分かります",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "てんぷら",
            "text": "レジ袋の代金がエコ事業に使われるって",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "てんぷら",
            "text": "何かで聞いて",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "おでん",
            "text": "レジを終わらすだけで、冷や汗かきます",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "げんまい",
            "text": "ドキドキします。",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "てんぷら",
            "text": "もう、エコ事業にお金払います！って思います、エコバッグ忘れたときは。",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "げんまい",
            "text": "エコ事業のためと言い聞かせる感じですねｗｗｗ",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "おでん",
            "text": "@てんぷら @げんまい 私もエコバッグ持ち歩いてます",
            "mention_to": [
                "げんまい",
                "てんぷら"
            ]
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "てんぷら",
            "text": "そうです、暗示です",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "げんまい",
            "text": "でも、お家で使うゴミ袋は買うことになるんですけどね。",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "おでん",
            "text": "それね！",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "おでん",
            "text": "結局、どーなんだろなーと思ったりしますよね",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "てんぷら",
            "text": "ほんとです",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "げんまい",
            "text": "なんだかなーと思いながら、ゴミ袋を買います。",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "おでん",
            "text": "ゴミ箱にセットするゴミ袋は必要ですもんね",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "げんまい",
            "text": "そうなんですよ。ゴミ袋は必要です。",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "てんぷら",
            "text": "せめて生ごみは",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "てんぷら",
            "text": "コンポストとかできないかなと思って",
            "mention_to": []
        }
