        },
        {
            "utterance_id": 2,
            "interlocutor_id": "いそべ",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "げんまい",
            "text": "朝ごはん、食べる派ですか？",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "いそべ",
            "text": "食べる派です",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "はくさい",
            "text": "食べる派です",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "げんまい",
            "text": "私も食べる派",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "げんまい",
            "text": "パン？ごはん？",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "いそべ",
            "text": "ごはんです",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "はくさい",
            "text": "食パン",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "げんまい",
            "text": "私はパン",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "いそべ",
            "text": "パンも好きです",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "げんまい",
            "text": "ごはんと何食べます？",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "いそべ",
            "text": "結構ガッツリたべます",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "げんまい",
            "text": "ごはんの時って、おみそ汁とかおかずとかいろいろ作るのがめんどうで。。。",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "いそべ",
            "text": "昨日の残りとかカレーも",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "げんまい",
            "text": "カレー！いいですね。",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "げんまい",
            "text": "ガッツリ",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "はくさい",
            "text": "朝からカレー！",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "いそべ",
            "text": "朝カレーおいしいです！",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "はくさい",
            "text": "私の胃にはしんどいかも。。",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "げんまい",
            "text": "パンのほうが簡単で、パンにしちゃいます。",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "はくさい",
            "text": "わかります！",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "げんまい",
            "text": "子どもはピザトーストが好きです",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "いそべ",
            "text": "時間短縮できますよね",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "げんまい",
            "text": "何かのっけて、チーズのせて焼く。",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "いそべ",
            "text": "おいしいやつですね",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "はくさい",
            "text": "チーズのせたら何でもおいしくなりそう",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "げんまい",
            "text": "そうですね。おやつっぽいですね",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "げんまい",
            "text": "チーズは万能です。www",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "はくさい",
            "text": "ほんと万能",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "げんまい",
            "text": "カレーが残ってたら、パンにカレーのせてチーズです",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "いそべ",
            "text": "何にでも合いますよね",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "はくさい",
            "text": "何でも行けますね、チーズ",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "いそべ",
            "text": "うんうん。",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "げんまい",
            "text": "@はくさい 食パンに何か塗ります？",
            "mention_to": [
                "はくさい"
            ]
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "はくさい",
            "text": "私は塗らないですが、子どもと主人はあんこですね",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "いそべ",
            "text": "おいしそう",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "げんまい",
            "text": "あんこ！おいしいにきまってる",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "はくさい",
            "text": "コメダのやつ",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "げんまい",
            "text": "コメダいいですね。好きです。",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "はくさい",
            "text": "コメダのモーニングっで子どもがはまって",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "いそべ",
            "text": "すごい",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "げんまい",
            "text": "モーニングはお得。",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "いそべ",
            "text": "行ったことがまだないです",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "はくさい",
            "text": "久しぶりに言ったら",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "はくさい",
            "text": "注文の仕方が変わっててびっくりしました",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "げんまい",
            "text": "変わったんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "いそべ",
            "text": "デジタル化？",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "げんまい",
            "text": "タッチパネルとか？",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "はくさい",
            "text": "代わったというか、種類がふえてて",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "いそべ",
            "text": "ああ！",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "げんまい",
            "text": "数カ月行ってないです。増えたんですか",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "いそべ",
            "text": "うれしい変化ですね",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "げんまい",
            "text": "行きたくなってきました",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "はくさい",
            "text": "あ！私の昔は10年単位の話なので",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "はくさい",
            "text": "数か月じゃ変わらないかも。。。",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "げんまい",
            "text": "そっか。www",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "いそべ",
            "text": "なるほど笑",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "はくさい",
            "text": "紛らわしくて済みません。。",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "げんまい",
            "text": "いえいえ",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "げんまい",
            "text": "コーヒーについてくる豆が好きです。",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "はくさい",
            "text": "あ！私も好きです！",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "いそべ",
            "text": "コーヒー豆？",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "げんまい",
            "text": "コーヒーを頼むと、豆のお菓子がついてくるんです",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "いそべ",
            "text": "ああ。なるほど",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "いそべ",
            "text": "そういうものがついてくるんですね",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "げんまい",
            "text": "@いそべ ぜひ行ってみてほしいです",
            "mention_to": [
                "いそべ"
            ]
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "いそべ",
            "text": "今度必ず行きます",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "いそべ",
            "text": "楽しみです！",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "げんまい",
            "text": "コーヒーは飲まれますか？",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "いそべ",
            "text": "少し、甘目で",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "はくさい",
            "text": "毎日のんでます",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "げんまい",
            "text": "私はカフェインだめで、カフェインレスのコーヒーなのんです。",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "はくさい",
            "text": "そうなんですね",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "いそべ",
            "text": "味は同じですか？",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "いそべ",
            "text": "カフェインレスも",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "げんまい",
            "text": "やっぱり、ちょっと違いますね。",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "げんまい",
            "text": "でも、カフェインとると寝られなくなるし、",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "げんまい",
            "text": "鉄分を一緒に排出しちゃうっていうので、控えてます",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "いそべ",
            "text": "そうなんですね",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "はくさい",
            "text": "初耳です！",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "いそべ",
            "text": "私も初めて聞きました",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "げんまい",
            "text": "カフェイン取れるひと、大人っぽいです。",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "はくさい",
            "text": "私なんか、毎日３倍くらい飲んじゃってます。。。",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "はくさい",
            "text": "3杯だった",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "げんまい",
            "text": "寝られなくならないんですよね？",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "はくさい",
            "text": "全然平気なんです",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "げんまい",
            "text": "不思議。",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "いそべ",
            "text": "すごいですね",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "げんまい",
            "text": "夜、寝つけないなーと思って、",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "げんまい",
            "text": "そういえば、外出先で昼間コーヒー飲んだなーとかあります",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "はくさい",
            "text": "@げんまい カフェイン取るとそうなんですね",
            "mention_to": [
                "げんまい"
            ]
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "いそべ",
            "text": "コーヒーは朝がいいのかも",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "げんまい",
            "text": "朝ですね。",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "いそべ",
            "text": "目覚めの1杯",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "げんまい",
            "text": "朝、コーヒーはかっこいいいです",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "げんまい",
            "text": "私は朝も、カフェインレスコーヒー。",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "げんまい",
            "text": "インスタントの",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "いそべ",
            "text": "インスタントもおいしいですよね",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "げんまい",
            "text": "苦手なものがありますか？",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "げんまい",
            "text": "インスタント、簡単です。",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "いそべ",
            "text": "@げんまい 苦手な食べ物はマヨネーズです",
