    "relationship": [],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "てんぷら",
            "text": "こんにちは！",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "まりも",
            "text": "こんにちは～",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "かにたま",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "てんぷら",
            "text": "お昼ご飯食べました？",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "まりも",
            "text": "私はまだですね～",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "かにたま",
            "text": "食べました",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "まりも",
            "text": "何食べました？",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "かにたま",
            "text": "@まりも チャーシューおにぎりです！",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "てんぷら",
            "text": "10時頃に昨日のごはんの残りを…",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "かにたま",
            "text": "細かく刻んでまぜました〜",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "まりも",
            "text": "@かにたま チャーシューおにぎり！",
            "mention_to": [
                "かにたま"
            ]
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "まりも",
            "text": "なるほど",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "かにたま",
            "text": "@まりも はい！",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "かにたま",
            "text": "タレが甘くて美味しいですよ〜",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "まりも",
            "text": "@てんぷら ちょっと早めのお昼ですか",
            "mention_to": [
                "てんぷら"
            ]
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "てんぷら",
            "text": "@まりも 朝ごはん食べるの忘れていたので",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "まりも",
            "text": "おにぎりにチャーシューという発想がなかった",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "てんぷら",
            "text": "人が作った美味しいおにぎり食べたいですう",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "かにたま",
            "text": "@てんぷら そうなのですね",
            "mention_to": [
                "てんぷら"
            ]
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "まりも",
            "text": "@てんぷら 朝兼昼な感じですね",
            "mention_to": [
                "てんぷら"
            ]
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "てんぷら",
            "text": "そうですまさに。@まりも 遅めのお昼ですか？",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "かにたま",
            "text": "@てんぷら 具は何でも入れちゃいます！",
            "mention_to": [
                "てんぷら"
            ]
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "まりも",
            "text": "@てんぷら さっきまで眠くてうとうとしてしまって",
            "mention_to": [
                "てんぷら"
            ]
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "かにたま",
            "text": "@てんぷら 朝、お腹空かないですか？",
            "mention_to": [
                "てんぷら"
            ]
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "てんぷら",
            "text": "@まりも 気圧低いですからね",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "まりも",
            "text": "おにぎり食べたくなりました",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "てんぷら",
            "text": "@かにたま 今また空いています",
            "mention_to": [
                "かにたま"
            ]
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "まりも",
            "text": "今日急に寒いですけど",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "てんぷら",
            "text": "近所にインドカレーやが出来たんですよ",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "まりも",
            "text": "昨日暑かったんでなんかぐにゃぐにゃです",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "まりも",
            "text": "@てんぷら おぉ",
            "mention_to": [
                "てんぷら"
            ]
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "かにたま",
            "text": "@てんぷら カレーは香りにそそられますよね",
            "mention_to": [
                "てんぷら"
            ]
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "てんぷら",
            "text": "体調あまり良くない時でも、癒されます",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "かにたま",
            "text": "@まりも 今日寒くないですか？",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "まりも",
            "text": "うちの近くにもカレー屋さんあります",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "かにたま",
            "text": "@てんぷら わかるわかるー",
            "mention_to": [
                "てんぷら"
            ]
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "まりも",
            "text": "まだ行ったことないけど",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "てんぷら",
            "text": "@まりも お昼まだでしたっけ",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "まりも",
            "text": "@かにたま 寒いですよね　うちの辺り風がびゅーびゅーで",
            "mention_to": [
                "かにたま"
            ]
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "てんぷら",
            "text": "うちは千葉ですが、昨日から強風です",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "かにたま",
            "text": "@まりも 嵐のような風で怖いです〜",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "まりも",
            "text": "@てんぷら そうなんです　お腹へってきました",
            "mention_to": [
                "てんぷら"
            ]
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "かにたま",
            "text": "@てんぷら 同じです！",
            "mention_to": [
                "てんぷら"
            ]
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "てんぷら",
            "text": "ちなみに夕飯は決まってます？",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "まりも",
            "text": "あら　私埼玉です",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "まりも",
            "text": "夜からすごい風吹いてましたよね",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "かにたま",
            "text": "@てんぷら お夕飯は和食にします➰",
            "mention_to": [
                "てんぷら"
            ]
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "てんぷら",
            "text": "チャーシューおにぎりは中華かしら",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "まりも",
            "text": "@てんぷら 夕飯は買い物行ってお惣菜かも",
            "mention_to": [
                "てんぷら"
            ]
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "かにたま",
            "text": "@てんぷら そうです！",
            "mention_to": [
                "てんぷら"
            ]
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "てんぷら",
            "text": "あはは",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "てんぷら",
            "text": "焼うどんにしようかな",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "まりも",
            "text": "あ～またおいしそうなものを",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "まりも",
            "text": "焼きうどんもいいですね",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "かにたま",
            "text": "@てんぷら 焼きうどん美味しいですよね",
            "mention_to": [
                "てんぷら"
            ]
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "てんぷら",
            "text": "牛肉がチルドに",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "まりも",
            "text": "おにぎりとの間で揺らぐ",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "まりも",
            "text": "@かにたま かつおぶしかけたいですね",
            "mention_to": [
                "かにたま"
            ]
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "かにたま",
            "text": "すき焼きうどんとか好きです！",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "かにたま",
            "text": "@まりも いいですねー",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "まりも",
            "text": "すき焼きしばらく食べてない",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "てんぷら",
            "text": "ここで明日の朝ごはんまで決めちゃいましょうか",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "てんぷら",
            "text": "買い物の前に決めないと",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "まりも",
            "text": "おいしそうなものだらけで迷ってしまう",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "かにたま",
            "text": "@てんぷら 明日はコメダでモーニングなのです！",
            "mention_to": [
                "てんぷら"
            ]
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "てんぷら",
            "text": "最高ですねそれ！",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "まりも",
            "text": "@かにたま コメダいいですねえ",
            "mention_to": [
                "かにたま"
            ]
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "てんぷら",
            "text": "シロノワール食べたいです朝から",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "まりも",
            "text": "朝あんまり食べられなくて",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "まりも",
            "text": "モーニング行けるのうらやましい",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "かにたま",
            "text": "@まりも 辻口シェフ監修のがやっていて",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "かにたま",
            "text": "食べに行きます！",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "てんぷら",
            "text": "お、知らなかったです",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "まりも",
            "text": "ほ～　有名な方なんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "かにたま",
            "text": "@てんぷら 美味しいと評判ですよ➰",
            "mention_to": [
                "てんぷら"
            ]
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "かにたま",
            "text": "@まりも ショコラティエです",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "てんぷら",
            "text": "みなさん、近くにコメダあります？",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "まりも",
            "text": "@かにたま へええ！",
            "mention_to": [
                "かにたま"
            ]
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "まりも",
            "text": "知りまぜんでした",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "てんぷら",
            "text": "うちは自転車で10分くらいかな",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "まりも",
            "text": "コメダちょこっと遠いですね",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "かにたま",
            "text": "@てんぷら ありますよ",
            "mention_to": [
                "てんぷら"
            ]
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "まりも",
            "text": "自転車10分！",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "てんぷら",
            "text": "遠いですかね？",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "まりも",
            "text": "通っちゃいそう",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "てんぷら",
            "text": "近いんだw",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "かにたま",
            "text": "うちも電車で4分くらい",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "まりも",
            "text": "最寄りがお隣の市なんで",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "まりも",
            "text": "車で行かないと",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "てんぷら",
            "text": "電車や車よりは近いですね、自転車",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "まりも",
            "text": "まあそれでも近いほうなのかな？",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "まりも",
            "text": "ポテトとか食べたくなってきました",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "かにたま",
            "text": "近いと思います〜",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "てんぷら",
            "text": "@まりも お昼まだですもんねえ",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "まりも",
            "text": "だいぶお腹減った発言してますね",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "かにたま",
            "text": "@まりも ポテトどこが好きですか？",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "てんぷら",
            "text": "あはは、ほんとです",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "てんぷら",
            "text": "ポテトの1位はマックでしょうか・・・",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "まりも",
            "text": "@かにたま ポテト結構なんでも好きですね～",
            "mention_to": [
                "かにたま"
            ]
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "まりも",
            "text": "細いのでも太いのでも皮付きでも",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "かにたま",
            "text": "マックだったのですが、オリジンも美味しくて",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "てんぷら",
            "text": "オリジンってオリジン弁当ですか？",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "まりも",
            "text": "オリジン行ったことないです",
            "mention_to": []
        },
        {
            "utterance_id": 103,
            "interlocutor_id": "まりも",
            "text": "近くにはない気がする",
            "mention_to": []
        },
        {
            "utterance_id": 104,
            "interlocutor_id": "かにたま",
            "text": "友達がハマって差し入れに貰ってからはまりましたー",
            "mention_to": []
        },
        {
            "utterance_id": 105,
            "interlocutor_id": "てんぷら",
            "text": "うちはマックの向かいがオリジン弁当です",
            "mention_to": []
        },
        {
            "utterance_id": 106,
            "interlocutor_id": "まりも",
            "text": "うわ～気になる～",
            "mention_to": []
        },
        {
            "utterance_id": 107,
            "interlocutor_id": "かにたま",
            "text": "@てんぷら はい",
            "mention_to": [
                "てんぷら"
            ]
        },
        {
            "utterance_id": 108,
            "interlocutor_id": "てんぷら",
            "text": "今日の夕飯にオリジン寄ってみます",
            "mention_to": []
        },
        {
            "utterance_id": 109,
            "interlocutor_id": "かにたま",
            "text": "素朴で美味しいです",
            "mention_to": []
        },
        {
            "utterance_id": 110,
            "interlocutor_id": "てんぷら",
            "text": "マックを超すなんて気になります",
            "mention_to": []
        },
        {
            "utterance_id": 111,
            "interlocutor_id": "かにたま",
            "text": "@てんぷら ぜひぜひ",
            "mention_to": [
                "てんぷら"
            ]
        },
        {
            "utterance_id": 112,
            "interlocutor_id": "てんぷら",
            "text": "明日の昼ごはんは何にしようかな",
            "mention_to": []
        },
        {
