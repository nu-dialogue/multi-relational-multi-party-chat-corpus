{
    "dialogue_id": "A00405",
    "dialogue_type": "First time",
    "interlocutors": [
        "おにぎり",
        "てんぷら",
        "げんまい"
    ],
    "relationship": [],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "おにぎり",
            "text": "こんにちは。",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "てんぷら",
            "text": "こんにちはー",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "げんまい",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "おにぎり",
            "text": "花粉症、大丈夫ですか？",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "げんまい",
            "text": "花粉症です。",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "てんぷら",
            "text": "今年も気のせいで乗り切ろうとしていますが、おにぎりさんは？",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "おにぎり",
            "text": "私も花粉症で、鼻がすでにおかしいです。。",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "げんまい",
            "text": "今のところ、お薬で抑えてます。",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "おにぎり",
            "text": "気のせいで乗り切る笑",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "てんぷら",
            "text": "お薬、眠くなります？",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "げんまい",
            "text": "眠くないのを処方してもらってます",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "おにぎり",
            "text": "効きますか？",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "げんまい",
            "text": "朝、眠くならない薬、夜、眠くなるけど強いやつ",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "げんまい",
            "text": "効き目はまぁまぁですね。",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "てんぷら",
            "text": "そのリズムいいですね",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "おにぎり",
            "text": "2種類飲むのですね。",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "げんまい",
            "text": "そうです。",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "げんまい",
            "text": "飲まないよりはいいです",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "おにぎり",
            "text": "アレルギーの薬って、いまいち効果が感じられなくて。。",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "げんまい",
            "text": "病院行きますか？",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "おにぎり",
            "text": "でも、今年は飲まないと無理かもしれません。",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "おにぎり",
            "text": "病院、行っていないです。",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "てんぷら",
            "text": "げんまいさんは病院で？",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "げんまい",
            "text": "病院だと、薬の強さとか相談できていいですよ",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "げんまい",
            "text": "はい。耳鼻科",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "おにぎり",
            "text": "耳鼻科行けばいいのですね。",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "てんぷら",
            "text": "今、ラジオで今日は花粉多めと言っていました",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "げんまい",
            "text": "なんか、毎年",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "おにぎり",
            "text": "暖かくなりますもんね。。。",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "げんまい",
            "text": "多いって言ってるきが。。。",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "てんぷら",
            "text": "外に洗濯物干しちゃった",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "おにぎり",
            "text": "ほんと！",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "てんぷら",
            "text": "たまには少ないって言って欲しいです",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "げんまい",
            "text": "外に干さないと、ラチガあかないのでね。",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "おにぎり",
            "text": "洗濯、外に干したいですよね。",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "おにぎり",
            "text": "こんなに天気いいのに！",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "げんまい",
            "text": "おにぎりさん、外干しじゃないのですか？",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "てんぷら",
            "text": "乾燥機という文明の利器を",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "おにぎり",
            "text": "普通に干してます笑",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "げんまい",
            "text": "乾燥機は、うらやましいー",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "おにぎり",
            "text": "乾燥機、、お持ちですか？",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "てんぷら",
            "text": "ドライヤーで子どもの体操服乾かすことが多かったので",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "てんぷら",
            "text": "出し忘れとかで",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "げんまい",
            "text": "ドライヤーｗｗｗ",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "おにぎり",
            "text": "力技！",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "てんぷら",
            "text": "配偶者が買ってくれました",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "おにぎり",
            "text": "ガスのですか？",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "げんまい",
            "text": "花粉の時期は乾燥機がほしいと思いますね",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "てんぷら",
            "text": "ガスなのかな",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "てんぷら",
            "text": "今も回っていますがガスなのか電気なのか",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "おにぎり",
            "text": "うちは、縦型洗濯機で、一応機能はあるけど、",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "おにぎり",
            "text": "全然つかえないですね。",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "てんぷら",
            "text": "そうなんですね",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "てんぷら",
            "text": "干すのが面倒な時も乾燥機入れちゃいます",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "げんまい",
            "text": "うちのもちょっと乾燥みたいな機能ついてるけど、3時間ぐらいかかって全然乾いてない",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "おにぎり",
            "text": "乾燥機、いいですよね。",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "てんぷら",
            "text": "電気代とガス代気になりますけど",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "げんまい",
            "text": "いいなー",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "てんぷら",
            "text": "金で解決",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "てんぷら",
            "text": "家事は金で解決",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "おにぎり",
            "text": "それ大事大事！",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "おにぎり",
            "text": "母の元気が一番大事です！",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "げんまい",
            "text": "近くにコインランドリーができたので、いざと言うときは、そこで、お金でかいけつです。",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "てんぷら",
            "text": "コインランドリーが近いのいいですね",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "おにぎり",
            "text": "コインランドリー、近くにあると良いですね！",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "げんまい",
            "text": "徒歩１分です！",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "げんまい",
            "text": "うれしいです。",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "おにぎり",
            "text": "いいな！！",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "おにぎり",
            "text": "車で行くと、いちいち面倒で、家で無理やり乾かしてます。。",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "げんまい",
            "text": "布団まるごと洗えていいです",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "おにぎり",
            "text": "他にお勧め家電とかありますか？",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "てんぷら",
            "text": "食洗器も配偶者が買いましたが",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "おにぎり",
            "text": "食洗器！",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "てんぷら",
            "text": "皿を並べて入れたり、",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "おにぎり",
            "text": "配偶者さん、お優しい。。",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "てんぷら",
            "text": "洗浄後に水滴を落としたりする新しい家事が",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "てんぷら",
            "text": "その新しい家事も配偶者がやっていますね",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "げんまい",
            "text": "新しい家事ｗｗ",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "げんまい",
            "text": "うちは、食洗器、意外と面倒でやめました。",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "おにぎり",
            "text": "備え付けじゃなくて、置くタイプのですか？",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "てんぷら",
            "text": "そう、意外と面倒",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "おにぎり",
            "text": "私、食洗器大好きです笑",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "てんぷら",
            "text": "備付ですか？",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "おにぎり",
            "text": "後から、置くタイプを買いました。",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "げんまい",
            "text": "うちも置くタイプ",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "てんぷら",
            "text": "うちもです",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "おにぎり",
            "text": "買って一番良かった家電です。",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "げんまい",
            "text": "いまいち信用できなくて、予洗いをけっこうやっちゃう",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "おにぎり",
            "text": "その問題ですね。。",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "てんぷら",
            "text": "餃子の油とか、意外と残っていますね",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "げんまい",
            "text": "私には、向いてなかったです。",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "てんぷら",
            "text": "二度洗いしてもらいます、金で解決",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "げんまい",
            "text": "すばらしい！",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "おにぎり",
            "text": "二度洗いとは！",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "てんぷら",
            "text": "食洗器のメンテナンスも意外と手間ですね",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "てんぷら",
            "text": "最近やってないな",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "おにぎり",
            "text": "分かりました。",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "げんまい",
            "text": "並べるのにコツがいりますよね？",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "てんぷら",
            "text": "そうそう、重ねちゃうとアウトです",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "おにぎり",
            "text": "私、ずぼらだから、食洗器ラブなんだ。",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "てんぷら",
            "text": "新しい家事です",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "おにぎり",
            "text": "コツはいりますね～",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "てんぷら",
            "text": "逆さになったコップに水が溜まりますよね？",
            "mention_to": []
        },
        {
            "utterance_id": 103,
            "interlocutor_id": "おにぎり",
            "text": "たまります！",
            "mention_to": []
        },
        {
            "utterance_id": 104,
            "interlocutor_id": "げんまい",
            "text": "それ、ガッカリしますね",
            "mention_to": []
        },
        {
            "utterance_id": 105,
            "interlocutor_id": "おにぎり",
            "text": "横にして、しばらく放置です！",
            "mention_to": []
        }
