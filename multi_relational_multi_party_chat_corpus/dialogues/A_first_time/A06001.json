        },
        {
            "utterance_id": 76,
            "interlocutor_id": "あざらし",
            "text": "そうですよねー",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "きりん",
            "text": "たまに飲み忘れます",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "あざらし",
            "text": "色々ありますね",
            "mention_to": []
        },
        {
            "utterance_id": 79,
