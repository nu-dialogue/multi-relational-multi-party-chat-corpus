        },
        {
            "utterance_id": 96,
            "interlocutor_id": "あざらし",
            "text": "夜が眠れなくなりますね",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "こぐま",
            "text": "この時期、耳鼻科は凄い人じゃないですか",
