        },
        {
            "utterance_id": 63,
            "interlocutor_id": "こぐま",
            "text": "そんな治療法があるのですね",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "きりん",
            "text": "舌下に錠剤を1分置いてから飲むという",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "きりん",
            "text": "やつです",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "こぐま",
            "text": "まぁ、そんなものがあるんですね",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "こぐま",
            "text": "効き目の程は、",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "あざらし",
            "text": "娘も飲み薬服用してます",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "きりん",
            "text": "月1皮下注射でやるタイプのも",
