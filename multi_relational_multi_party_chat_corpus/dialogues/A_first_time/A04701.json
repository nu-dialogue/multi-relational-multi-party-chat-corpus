    "relationship": [],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "しらす",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "ねぎとろ",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "まりも",
            "text": "こんにちは～",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "しらす",
            "text": "よろしくお願いします",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "まりも",
            "text": "今日寒いですね",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "まりも",
            "text": "急に",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "ねぎとろ",
            "text": "寒いです。",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "しらす",
            "text": "寒いですね",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "しらす",
            "text": "また寒くなりました",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "まりも",
            "text": "今週ずっと天気悪くて",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "ねぎとろ",
            "text": "ですね",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "しらす",
            "text": "たしかに",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "まりも",
            "text": "お洗濯困りますね",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "ねぎとろ",
            "text": "雨ばかり",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "ねぎとろ",
            "text": "困ります",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "しらす",
            "text": "ずっと部屋干しです",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "まりも",
            "text": "そうですよね～",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "ねぎとろ",
            "text": "ですよね",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "しらす",
            "text": "しばらく晴れを見てないですね",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "ねぎとろ",
            "text": "こちらもです",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "まりも",
            "text": "買い物行かなきゃだけど",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "まりも",
            "text": "雨降って寒いと",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "まりも",
            "text": "出る気がなくなる…",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "しらす",
            "text": "分かります",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "ねぎとろ",
            "text": "わかります",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "しらす",
            "text": "気合い入れないといけないですね",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "まりも",
            "text": "私だけじゃなかった！",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "ねぎとろ",
            "text": "まさに",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "しらす",
            "text": "寒いと動くのに時間かかります",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "まりも",
            "text": "まず朝布団から出られませんね…",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "しらす",
            "text": "早く暖かくなってほしい",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "ねぎとろ",
            "text": "確かに活動的にはなりませんね。",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "まりも",
            "text": "ですね～",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "しらす",
            "text": "本当にそうです",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "ねぎとろ",
            "text": "そうですね。",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "しらす",
            "text": "私も今日布団から出られなかった",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "まりも",
            "text": "今も湯たんぽ使ってるんですが",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "しらす",
            "text": "うんうん",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "まりも",
            "text": "足が暖まらない",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "ねぎとろ",
            "text": "うんうん",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "しらす",
            "text": "分かります",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "ねぎとろ",
            "text": "わかります",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "しらす",
            "text": "うちも布団乾燥機使ってますが",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "しらす",
            "text": "効きません笑",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "まりも",
            "text": "効かないんですか～",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "まりも",
            "text": "かなしい",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "しらす",
            "text": "冷え性の方が勝っちゃってます笑",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "ねぎとろ",
            "text": "つらいですね",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "まりも",
            "text": "どうしたらいいんでしょうね？冷え性",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "しらす",
            "text": "つらいです",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "しらす",
            "text": "@まりも 何しても効かないですよね、なぜか",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "ねぎとろ",
            "text": "@まりも わたしも知りたい",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "まりも",
            "text": "最近まであんまり冷え性って自覚なかったんですけど",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "しらす",
            "text": "うんうん",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "まりも",
            "text": "@しらす どうやってもキンキンですよね？",
            "mention_to": [
                "しらす"
            ]
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "まりも",
            "text": "整体の先生からいつも冷たいねえって言われてて",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "しらす",
            "text": "@まりも 靴下履いても効かないです笑",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "まりも",
            "text": "たしかにいつも足キンキンだなぁと",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "ねぎとろ",
            "text": "@まりも わたしは手足の冷えが。",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "まりも",
            "text": "@しらす 靴下履くと逆に冷たくなりませんか？",
            "mention_to": [
                "しらす"
            ]
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "まりも",
            "text": "@ねぎとろ 手足冷えますよね～",
            "mention_to": [
                "ねぎとろ"
            ]
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "しらす",
            "text": "@まりも わかる気がします、余計冷えがわかる",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "しらす",
            "text": "今年やたら売ってるホカロンの靴下買ったのですが",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "まりも",
            "text": "@しらす 布より自分の冷たさが勝つような？",
            "mention_to": [
                "しらす"
            ]
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "しらす",
            "text": "気持ちいいけど聞きませんね笑",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "ねぎとろ",
            "text": "うーん、つらい",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "しらす",
            "text": "@まりも そうです。つらい笑",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "まりも",
            "text": "@しらす 冷えない靴下とかもダメですよね",
            "mention_to": [
                "しらす"
            ]
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "ねぎとろ",
            "text": "冷えない靴下",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "ねぎとろ",
            "text": "そんなのあるんですね。",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "しらす",
            "text": "@まりも いろいろ試したいです",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "まりも",
            "text": "@ねぎとろ ドラッグストアとかで見ますね",
            "mention_to": [
                "ねぎとろ"
            ]
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "しらす",
            "text": "履くこたつみたいな靴下ありますよね？",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "まりも",
            "text": "ヒートテック系の仕組みかなと思ってます",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "ねぎとろ",
            "text": "@まりも 今度見てみます。",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "まりも",
            "text": "履くこたつ？知らなかった",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "ねぎとろ",
            "text": "@まりも なるほど",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "まりも",
            "text": "いや見かけたことあったかな…？",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "しらす",
            "text": "@まりも それこそドラッグストアとか売ってますね",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "しらす",
            "text": "でも暖かくなったり寒くなったりで",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "まりも",
            "text": "常に足湯とかできたら…",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "しらす",
            "text": "買うのにためらう笑",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "しらす",
            "text": "@まりも 足湯!!いいですね",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "まりも",
            "text": "夏場でも足はそれなりに冷えてるみたいで",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "ねぎとろ",
            "text": "そうなんですね。",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "しらす",
            "text": "それはおつらい",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "ねぎとろ",
            "text": "それはつらい",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "まりも",
            "text": "もうちょっと気遣わないといけないのかも",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "ねぎとろ",
            "text": "そうですね。",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "まりも",
            "text": "手が冷たいと使えなくて困るから",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "まりも",
            "text": "対策するんですが",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "ねぎとろ",
            "text": "わかります",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "まりも",
            "text": "足はあんまり",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "しらす",
            "text": "分かります",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "まりも",
            "text": "でもよくないですよね",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "ねぎとろ",
            "text": "そうだと思います",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "しらす",
            "text": "そう考えると",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "しらす",
            "text": "私は冬だけなので",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "ねぎとろ",
            "text": "うんうん",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "しらす",
            "text": "夏は逆に蒸れるというか",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "ねぎとろ",
            "text": "わたしも冬だけ",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "しらす",
            "text": "裸足でいたい笑",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "ねぎとろ",
            "text": "わかります",
            "mention_to": []
        }
