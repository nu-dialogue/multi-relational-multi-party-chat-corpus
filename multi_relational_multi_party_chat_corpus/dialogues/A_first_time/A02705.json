    "relationship": [],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "こまつな",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "ししとう",
            "text": "こんにちは！",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "まりも",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "こまつな",
            "text": "スポーツとかします？",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "ししとう",
            "text": "@こまつな 全然なんです～",
            "mention_to": [
                "こまつな"
            ]
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "まりも",
            "text": "しないですね～",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "ししとう",
            "text": "@こまつな 何かしてます？？",
            "mention_to": [
                "こまつな"
            ]
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "こまつな",
            "text": "最近運動不足で",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "こまつな",
            "text": "私もしないのです",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "まりも",
            "text": "みんなしない笑",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "ししとう",
            "text": "仲間！",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "こまつな",
            "text": "見るのは？",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "ししとう",
            "text": "見るのもあんまりしないですね～",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "まりも",
            "text": "F1は入りますか？",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "まりも",
            "text": "前はフィギュアスケートよく見てました",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "ししとう",
            "text": "@まりも F1！",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "こまつな",
            "text": "@まりも 前はよく見てたF1",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "ししとう",
            "text": "かっこいい！",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "まりも",
            "text": "F1、去年から見始めて",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "こまつな",
            "text": "今日本人運転してます？",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "まりも",
            "text": "すごくおもしいですね",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "まりも",
            "text": "角田裕毅選手が",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "こまつな",
            "text": "そうなんだ！",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "こまつな",
            "text": "結構好きだったんだけど",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "まりも",
            "text": "わりとかわいらしい感じなんですけど角田選手",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "まりも",
            "text": "無線ではよくキレてますね",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "ししとう",
            "text": "@まりも へ～そうなんですね！",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "まりも",
            "text": "無線でキレてる選手結構多くて",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "こまつな",
            "text": "@まりも キレますよみんな",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "まりも",
            "text": "ピー音が",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "こまつな",
            "text": "放送できない",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "ししとう",
            "text": "@こまつな え？F1ってそういうものなんですか？",
            "mention_to": [
                "こまつな"
            ]
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "まりも",
            "text": "キレるのも無理ない状況多々ありますね",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "こまつな",
            "text": "@ししとう ピットとやり取りがねきついから",
            "mention_to": [
                "ししとう"
            ]
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "まりも",
            "text": "タイヤが用意できてない！！！とか",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "ししとう",
            "text": "@こまつな そうなんですね！",
            "mention_to": [
                "こまつな"
            ]
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "こまつな",
            "text": "それはキレる",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "ししとう",
            "text": "速さの世界ですもんね！",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "まりも",
            "text": "去年見てて実際にありましたね",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "こまつな",
            "text": "いまタイヤ交換が命だから",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "こまつな",
            "text": "前は給油があったからもっと駆け引きがあった",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "まりも",
            "text": "タイヤ交換もたもたしてるときある",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "まりも",
            "text": "給油も！へ～",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "こまつな",
            "text": "雨とか降るとね",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "ししとう",
            "text": "タイヤ交換もレース内にされるってことです？",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "こまつな",
            "text": "そうです",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "ししとう",
            "text": "本当に初心者ですいません～！",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "まりも",
            "text": "F1のタイヤって普通車と違って",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "ししとう",
            "text": "すごい世界なんですね！",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "こまつな",
            "text": "交換しないといけないんですよ数秒で！",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "まりも",
            "text": "熱で溶けることでグリップするんですよ",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "ししとう",
            "text": "運転手？選手も大変だけど、",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "こまつな",
            "text": "交換スタッフは練習してます",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "まりも",
            "text": "溶けると当然すり減っちゃう",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "ししとう",
            "text": "交換する人もやばいですね！",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "こまつな",
            "text": "何人もでチームプレイです",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "こまつな",
            "text": "一人失敗すると大変",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "ししとう",
            "text": "個人戦かと思ってましたが",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "まりも",
            "text": "レッドブルは2秒ちょいぐらいで交換しちゃいます",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "ししとう",
            "text": "団体競技なんですね！",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "こまつな",
            "text": "@ししとう そうなんですよ",
            "mention_to": [
                "ししとう"
            ]
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "ししとう",
            "text": "奥が深い！",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "まりも",
            "text": "天気とか路面状況とかコースで",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "まりも",
            "text": "すごく違いますね",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "こまつな",
            "text": "何種類かあるんですよねタイヤ",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "まりも",
            "text": "マシンの開発勝負もあるので",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "ししとう",
            "text": "@まりも わ～！運も出てきそう",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "こまつな",
            "text": "給油が出来ないからガス欠しないように走る",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "ししとう",
            "text": "レース内でいろんな人が競ってるんですね！",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "まりも",
            "text": "ですね～タイヤの硬さが3種あって",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "こまつな",
            "text": "たくさん入れると思いから最低限のガソリンではしる",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "まりも",
            "text": "必ず2種類以上使う縛りがあったり",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "こまつな",
            "text": "ああ、ありますね縛り",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "まりも",
            "text": "ガソリン、検査用に残さないといけないんだったかな",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "こまつな",
            "text": "@ししとう ちょっとたのしいので見てみて",
            "mention_to": [
                "ししとう"
            ]
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "ししとう",
            "text": "@こまつな みてみます！",
            "mention_to": [
                "こまつな"
            ]
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "こまつな",
            "text": "私も久々に見よう！",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "ししとう",
            "text": "聞いてて興味持ちました！",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "こまつな",
            "text": "音が好きです",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "まりも",
            "text": "選手も超人みたいな人がいておもしろいですよ",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "まりも",
            "text": "マンガの世界かと思うような",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "ししとう",
            "text": "@まりも 超人！！",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "ししとう",
            "text": "やっぱり筋肉とか？",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "こまつな",
            "text": "今って誰が速いのかしら？",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "まりも",
            "text": "@ししとう 去年はレッドブルのフェルスタッペンがぶっちぎりでした",
            "mention_to": [
                "ししとう"
            ]
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "こまつな",
            "text": "前に見た時の新人！",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "ししとう",
            "text": "@まりも メモメモ～！",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "まりも",
            "text": "私はアロンソが好き",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "こまつな",
            "text": "アロンソまだ走ってます？",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "ししとう",
            "text": "やっぱり外国人選手がすごいんですね！",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "まりも",
            "text": "@こまつな へ～！新人の頃のマックス…",
            "mention_to": [
                "こまつな"
            ]
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "まりも",
            "text": "@こまつな アロンソまだまだ強いです！",
            "mention_to": [
                "こまつな"
            ]
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "こまつな",
            "text": "@まりも 車も外車が多い",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "こまつな",
            "text": "トヨタはどうですか？",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "まりも",
            "text": "ホンダが何年か先に戻ってくるとかこないとか？",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "こまつな",
            "text": "ホンダだった",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "ししとう",
            "text": "自動車メーカーも参戦してるんですね！",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "ししとう",
            "text": "全然疎くて",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "まりも",
            "text": "今はエンジン提供だけでしたかねホンダ",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "こまつな",
            "text": "でもすごいお金かかるからね",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "まりも",
            "text": "私も全然知らなかったですよ～",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "こまつな",
            "text": "会社はいやがるんですよね",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "ししとう",
            "text": "なるほど～！",
            "mention_to": []
        },
        {
            "utterance_id": 103,
            "interlocutor_id": "まりも",
            "text": "知り合いにF1好きが多くて",
            "mention_to": []
        },
        {
            "utterance_id": 104,
            "interlocutor_id": "こまつな",
            "text": "1台億だから",
            "mention_to": []
        }
