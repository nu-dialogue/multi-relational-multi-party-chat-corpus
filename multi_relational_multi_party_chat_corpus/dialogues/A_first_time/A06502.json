{
    "dialogue_id": "A06502",
    "dialogue_type": "First time",
    "interlocutors": [
        "げんまい",
        "まぐろ",
        "がんも"
    ],
    "relationship": [],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "げんまい",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "まぐろ",
            "text": "こんにちは～",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "がんも",
            "text": "こんにちは。",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "げんまい",
            "text": "よろしくお願いします",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "まぐろ",
            "text": "よろしくです！",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "がんも",
            "text": "よろしくお願いします。",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "げんまい",
            "text": "天気がいまいちで、洗濯物が乾かずこまっています。",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "がんも",
            "text": "雪です",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "まぐろ",
            "text": "困りますね・・",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "げんまい",
            "text": "雪！降ってますか？",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "まぐろ",
            "text": "え！！",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "がんも",
            "text": "めっちゃ、降ってます",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "げんまい",
            "text": "わー。雪ですかー",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "げんまい",
            "text": "都内、この後雨の予報です。",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "まぐろ",
            "text": "外、見たけどこちら曇りだった",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "がんも",
            "text": "この後雪かきです。",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "げんまい",
            "text": "雪かき、お疲れ様です。",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "まぐろ",
            "text": "それは・・大変だ！",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "げんまい",
            "text": "重労働ですよね。",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "がんも",
            "text": "たいへんです。",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "げんまい",
            "text": "お家のまわりの雪をどけるんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "がんも",
            "text": "そうです。",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "まぐろ",
            "text": "屋根は？",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "がんも",
            "text": "屋根はやらないです。",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "まぐろ",
            "text": "沢山積もった時だけなのかな",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "まぐろ",
            "text": "よくわかってなくて",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "がんも",
            "text": "基本、道だけですね",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "げんまい",
            "text": "私も雪かきは数年に1回のイベントなので、よくわかってません",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "げんまい",
            "text": "雪かきをしてる最中にも積もってくると、なんか嫌ですよね",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "がんも",
            "text": "わかります。",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "まぐろ",
            "text": "雪かき無限ループですよね",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "がんも",
            "text": "ループです",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "まぐろ",
            "text": "恐ろしい・・",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "げんまい",
            "text": "高校生の息子が、雪が好きだから北海道に住みたいとか",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "げんまい",
            "text": "安易な事を言っています",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "がんも",
            "text": "笑笑。",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "げんまい",
            "text": "まだまだ子どもです",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "まぐろ",
            "text": "わはははあ笑",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "げんまい",
            "text": "わかってませんよね。",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "がんも",
            "text": "子どもですから・・・",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "まぐろ",
            "text": "気持ちわかりますけど、わかってないですね～",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "げんまい",
            "text": "都内も、先日めずらしく積もって、大喜びでした。",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "まぐろ",
            "text": "ちょっとしたイベントですよね",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "げんまい",
            "text": "そうです。",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "まぐろ",
            "text": "雪も",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "げんまい",
            "text": "イベントでえす。",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "まぐろ",
            "text": "困るのは大人だけ",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "げんまい",
            "text": "そうそう",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "がんも",
            "text": "子ども喜びますね。",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "まぐろ",
            "text": "@がんも 雪深い場所なんですか？",
            "mention_to": [
                "がんも"
            ]
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "がんも",
            "text": "長野です。",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "げんまい",
            "text": "長野は積もりますよねー",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "がんも",
            "text": "そうですね。",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "げんまい",
            "text": "スキー場もいっぱいありますもんね。",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "まぐろ",
            "text": "それは積もりますねー",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "まぐろ",
            "text": "長野いいなあ・・",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "がんも",
            "text": "スキー場多いですね",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "げんまい",
            "text": "最近YouTubeで見た小諸の温泉に行きたいです。",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "げんまい",
            "text": "小諸って長野ですよね？",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "まぐろ",
            "text": "私も今温泉考えてました",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "げんまい",
            "text": "温泉、いいですね。",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "まぐろ",
            "text": "長野の",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "まぐろ",
            "text": "いいですね",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "がんも",
            "text": "温泉も多いですね",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "まぐろ",
            "text": "上田や菅平行ったことあります",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "げんまい",
            "text": "私も、学生の頃は京都から長野までスキーに行ったりしましたよ。",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "まぐろ",
            "text": "@げんまい いいですね～",
            "mention_to": [
                "げんまい"
            ]
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "がんも",
            "text": "うんうん。",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "げんまい",
            "text": "小諸の旅館で、ロープウェイに乗って温泉に行くっていうのがおもしろそうでした",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "まぐろ",
            "text": "えー！すごそうなあ温泉",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "げんまい",
            "text": "菱野温泉だったかな。",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "げんまい",
            "text": "そうなんです。敷地内にロープウェイがあって。",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "がんも",
            "text": "楽しそう。",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "げんまい",
            "text": "サウナがあって。",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "げんまい",
            "text": "楽しそうです。",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "まぐろ",
            "text": "し・・敷地内！？",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "げんまい",
            "text": "そう。敷地内。広そうです。",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "まぐろ",
            "text": "山１つ温泉宿みたいな？",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "げんまい",
            "text": "そんな感じなのかなー",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "がんも",
            "text": "へー。",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "げんまい",
            "text": "動画だけではよくわかりませんが",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "まぐろ",
            "text": "想像をこえた施設があるもんですね～",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "げんまい",
            "text": "この前、鉄腕ダッシュでも出てました。",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "げんまい",
            "text": "その旅館",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "まぐろ",
            "text": "あ、有名なんだ！",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "がんも",
            "text": "今度見てみます。",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "げんまい",
            "text": "富士山が見える温泉の限界はどこだ？みたいなので。",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "まぐろ",
            "text": "私もー見てみます",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "まぐろ",
            "text": "よくみえそうですよね",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "げんまい",
            "text": "長野からギリギリ富士山が見えてましたね。",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "げんまい",
            "text": "カメラの望遠で。ギリギリ",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "まぐろ",
            "text": "ギリなんだ、ちょっと意外",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "げんまい",
            "text": "そう。もっと見えるかと思いました。",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "げんまい",
            "text": "肉眼では無理な感じでした。",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "まぐろ",
            "text": "まあまあ近いイメージだけど",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "まぐろ",
            "text": "長野広いから",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "げんまい",
            "text": "そうですよね。",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "げんまい",
            "text": "めちゃくちゃ広いですよね。縦に",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "がんも",
            "text": "ひろいですね。",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "がんも",
            "text": "富士山、肉眼で見えるとこ",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "がんも",
            "text": "ありますね",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "げんまい",
            "text": "そうなんですね！",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "げんまい",
            "text": "見えるんだ。",
            "mention_to": []
        },
        {
            "utterance_id": 103,
            "interlocutor_id": "まぐろ",
            "text": "お！あるんだ！",
            "mention_to": []
        }
