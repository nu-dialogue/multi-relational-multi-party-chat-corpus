    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "まりも",
            "text": "よろしくお願いします～",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "ししとう",
            "text": "よろしくお願いします！",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "かにたま",
            "text": "宜しくお願いします〜",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "ししとう",
            "text": "今日はあたたかかったですね！",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "まりも",
            "text": "ここ数日あったかいですよね",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "かにたま",
            "text": "上着いらずですね",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "まりも",
            "text": "関東は春一番吹いたって見ました",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "ししとう",
            "text": "@まりも そうなんですね！",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "かにたま",
            "text": "そうなんですかー",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "まりも",
            "text": "ちょっと前は雪が降ってたのに",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "かにたま",
            "text": "ほんとほんと",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "かにたま",
            "text": "寒暖差が激しいですよね",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "ししとう",
            "text": "今年は過ごしやすいですね！",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "ししとう",
            "text": "@かにたま 確かに！寒暖差しんどい日もありますね",
            "mention_to": [
                "かにたま"
            ]
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "まりも",
            "text": "うちは関東なんですけど",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "まりも",
            "text": "みなさんはどの辺りの地域ですか",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "かにたま",
            "text": "@ししとう 暖冬だといいですよね",
            "mention_to": [
                "ししとう"
            ]
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "ししとう",
            "text": "@まりも 私は四国です！",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "かにたま",
            "text": "@まりも 関東です",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "まりも",
            "text": "@ししとう おぉ～　四国！",
            "mention_to": [
                "ししとう"
            ]
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "ししとう",
            "text": "@まりも @かにたま 関東なんですね！都会！",
            "mention_to": [
                "まりも",
                "かにたま"
            ]
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "かにたま",
            "text": "@ししとう 四国行ったことないです",
            "mention_to": [
                "ししとう"
            ]
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "まりも",
            "text": "@ししとう @かにたま 私も四国行ったことないです",
            "mention_to": [
                "ししとう",
                "かにたま"
            ]
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "ししとう",
            "text": "@かにたま なかなか機会ないと来れないですよね",
            "mention_to": [
                "かにたま"
            ]
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "かにたま",
            "text": "そうですね",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "まりも",
            "text": "行ってみたいですね",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "まりも",
            "text": "おいしいもののイメージが",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "かにたま",
            "text": "私も〜",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "ししとう",
            "text": "@まりも 確かに！海の幸、山の幸おいしいです！",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "まりも",
            "text": "いいですね～",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "かにたま",
            "text": "@ししとう いいなぁ",
            "mention_to": [
                "ししとう"
            ]
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "まりも",
            "text": "海なし県なのでうらやましいです",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "かにたま",
            "text": "都内から中々でないので",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "かにたま",
            "text": "羨ましい！",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "ししとう",
            "text": "@まりも 海ないエリアなんですね～",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "まりも",
            "text": "@かにたま 都内なんですね、うちは北関東で",
            "mention_to": [
                "かにたま"
            ]
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "まりも",
            "text": "北陸の友人からよく海産物マウントとられます",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "かにたま",
            "text": "@まりも 物価高で悲しいです",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "ししとう",
            "text": "@まりも マウント！",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "かにたま",
            "text": "@まりも 私もとられますw",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "まりも",
            "text": "@ししとう 普通の回転寿司でも安くておいしいとか",
            "mention_to": [
                "ししとう"
            ]
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "かにたま",
            "text": "うんうん",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "かにたま",
            "text": "都内にはないだろーって言われます",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "ししとう",
            "text": "@まりも そうなんですかね？回転寿司は某チェーン店を愛用してます",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "まりも",
            "text": "@かにたま ですよね、いやほんとうらやましい",
            "mention_to": [
                "かにたま"
            ]
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "かにたま",
            "text": "ほんとほんと",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "まりも",
            "text": "@かにたま 同じチェーンでもやっぱり仕入先が違うみたいですね",
            "mention_to": [
                "かにたま"
            ]
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "ししとう",
            "text": "そうなんですね！",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "ししとう",
            "text": "知らなかったです！",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "かにたま",
            "text": "@まりも そうなんですね",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "まりも",
            "text": "おいしいホタテが食べたい…",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "ししとう",
            "text": "@まりも ホタテ美味しいですよね！",
            "mention_to": [
                "まりも"
