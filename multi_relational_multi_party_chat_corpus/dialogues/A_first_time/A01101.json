        },
        {
            "utterance_id": 53,
            "interlocutor_id": "かにたま",
            "text": "バターで食べたい！",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "まりも",
            "text": "子供の頃は生の貝類食べられなかったけど",
