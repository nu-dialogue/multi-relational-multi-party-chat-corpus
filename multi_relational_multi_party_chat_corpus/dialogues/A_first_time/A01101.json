            "mention_to": [
                "ししとう"
            ]
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "まりも",
            "text": "@かにたま そうなんですね、でも焼いてもおいしいですしね",
            "mention_to": [
                "かにたま"
            ]
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "かにたま",
            "text": "焼いたり炙ったり",
