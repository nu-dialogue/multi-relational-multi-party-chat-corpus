        },
        {
            "utterance_id": 55,
            "interlocutor_id": "まりも",
            "text": "あ～バターもいいですね！",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "ししとう",
            "text": "@かにたま ホタテバター！最高！",
            "mention_to": [
                "かにたま"
            ]
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "かにたま",
            "text": "@まりも 私、生ではたべれないのです。。",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "かにたま",
