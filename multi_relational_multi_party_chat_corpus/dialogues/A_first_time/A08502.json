{
    "dialogue_id": "A08502",
    "dialogue_type": "First time",
    "interlocutors": [
        "くまのみ",
        "ししとう",
        "みたらし"
    ],
    "relationship": [],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "くまのみ",
            "text": "こんにちは！",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "ししとう",
            "text": "こんばんは！",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "くまのみ",
            "text": "こんばんは！",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "くまのみ",
            "text": "間違いました",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "みたらし",
            "text": "こんばんは！",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "くまのみ",
            "text": "質問です",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "みたらし",
            "text": "はい",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "くまのみ",
            "text": "社会の窓があいてますよ",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "くまのみ",
            "text": "っていわれて意味がわかりますか？",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "みたらし",
            "text": "@くまのみ チャックですね",
            "mention_to": [
                "くまのみ"
            ]
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "くまのみ",
            "text": "@みたらし 正解です",
            "mention_to": [
                "みたらし"
            ]
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "ししとう",
            "text": "@くまのみ それはズボンのチャックですよね？",
            "mention_to": [
                "くまのみ"
            ]
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "くまのみ",
            "text": "@ししとう 正解です",
            "mention_to": [
                "ししとう"
            ]
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "くまのみ",
            "text": "三谷幸喜さんのコラムに書いてあったんです",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "みたらし",
            "text": "もう死後ですね",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "みたらし",
            "text": "死語",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "くまのみ",
            "text": "@みたらし そうみたいですね",
            "mention_to": [
                "みたらし"
            ]
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "ししとう",
            "text": "死語なんですか！？",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "くまのみ",
            "text": "三谷さんが若い子はわからないんじゃないかって",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "ししとう",
            "text": "知らなかった…",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "みたらし",
            "text": "使っている人見たことないです",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "ししとう",
            "text": "@みたらし そうなんですね！",
            "mention_to": [
                "みたらし"
            ]
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "くまのみ",
            "text": "三谷さんが道を歩いていて、おじいさんにいわれたそうです",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "みたらし",
            "text": "おお",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "くまのみ",
            "text": "いきなり道歩いてて言われたらどうします？",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "くまのみ",
            "text": "びっくりですよね",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "みたらし",
            "text": "三谷さん開いてたんですね",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "くまのみ",
            "text": "@みたらし そうなんです",
            "mention_to": [
                "みたらし"
            ]
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "ししとう",
            "text": "お茶目！",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "みたらし",
            "text": "@くまのみ びっくりだけどありがたいかも",
            "mention_to": [
                "くまのみ"
            ]
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "くまのみ",
            "text": "しかも２回も",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "ししとう",
            "text": "2回も！",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "みたらし",
            "text": "2回！？",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "くまのみ",
            "text": "@みたらし 好意的にうけとめるんですね",
            "mention_to": [
                "みたらし"
            ]
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "みたらし",
            "text": "@くまのみ 知っている人に見られたくないので",
            "mention_to": [
                "くまのみ"
            ]
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "くまのみ",
            "text": "おじいさんが、前もあいてましたよっていったそうです",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "みたらし",
            "text": "知らない人の指摘のほうがいいかな",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "みたらし",
            "text": "って",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "ししとう",
            "text": "@みたらし なるほど！",
            "mention_to": [
                "みたらし"
            ]
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "くまのみ",
            "text": "@みたらし お礼いいますか？",
            "mention_to": [
                "みたらし"
            ]
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "みたらし",
            "text": "いいます",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "くまのみ",
            "text": "すてきです",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "くまのみ",
            "text": "三谷さんがおじいさんと３回目にあった時",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "みたらし",
            "text": "3回目",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "くまのみ",
            "text": "ちゃんと閉まっているのを確認したら",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "くまのみ",
            "text": "おじいさんが",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "くまのみ",
            "text": "今日は大丈夫ですね",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "くまのみ",
            "text": "っていったそうです",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "みたらし",
            "text": "いいですね",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "みたらし",
            "text": "顔見知りになってますね",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "ししとう",
            "text": "社会の窓コミュニケーション！",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "くまのみ",
            "text": "三谷さんがちょっと恐怖を感じてはりました",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "みたらし",
            "text": "微笑ましく思えました",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "くまのみ",
            "text": "いつも社会の窓をチェックされてるのかと　苦笑",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "みたらし",
            "text": "確かに",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "ししとう",
            "text": "確かに〜",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "みたらし",
            "text": "まぁ",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "ししとう",
            "text": "受け取り方次第で恐怖…",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "みたらし",
            "text": "おじいさんも2回開いてるの見てたら",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "くまのみ",
            "text": "でっ、別のラジオでリスナーさんが",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "みたらし",
            "text": "3回目も見ちゃいますよね",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "くまのみ",
            "text": "@ししとう ですよね",
            "mention_to": [
                "ししとう"
            ]
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "くまのみ",
            "text": "@みたらし 毎回見ちゃいますよね",
            "mention_to": [
                "みたらし"
            ]
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "くまのみ",
            "text": "女性が電車に乗っていて",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "くまのみ",
            "text": "男子の社会の窓が開いていたので、教えてあげたら",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "くまのみ",
            "text": "激怒されたそうです",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "みたらし",
            "text": "怖い",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "ししとう",
            "text": "こわい〜",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "みたらし",
            "text": "人によりますね。",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "くまのみ",
            "text": "すいません、男子じゃなくておじさんでした",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "みたらし",
            "text": "電車だと周りに",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "みたらし",
            "text": "人がたくさんいるかもしれないし",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "ししとう",
            "text": "プライド高い系の人かな？",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "くまのみ",
            "text": "@みたらし こそっといったらしいです",
            "mention_to": [
                "みたらし"
            ]
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "くまのみ",
            "text": "@ししとう かもしれないです",
            "mention_to": [
                "ししとう"
            ]
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "みたらし",
            "text": "そうなんですね",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "くまのみ",
            "text": "異性だから恥ずかしくて怒ったのかな？",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "みたらし",
            "text": "おじさんは狭量ですね",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "ししとう",
            "text": "@くまのみ それもありそう〜",
            "mention_to": [
                "くまのみ"
            ]
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "くまのみ",
            "text": "@みたらし そうおもいます",
            "mention_to": [
                "みたらし"
            ]
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "みたらし",
            "text": "私ならすみませんって言っちゃうかも",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "くまのみ",
            "text": "@みたらし わかります！異性なら有難う　より　すみませんかも",
            "mention_to": [
                "みたらし"
            ]
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "ししとう",
            "text": "@みたらし 私も謝るかも〜",
            "mention_to": [
                "みたらし"
            ]
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "みたらし",
            "text": "ですよね。",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "みたらし",
            "text": "同性ならありがとうかな",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "くまのみ",
            "text": "こういうの聞いたら、変に人言わない方がいいのかなと思ったり",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "くまのみ",
            "text": "世知辛い世の中ですよね",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "みたらし",
            "text": "君子危うきには近寄らず",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "くまのみ",
            "text": "@みたらし かっこいい",
            "mention_to": [
                "みたらし"
            ]
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "みたらし",
            "text": "がいいのかもしれませんね",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "みたらし",
            "text": "プライド高い人だと怖いし",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "ししとう",
            "text": "確かに〜！",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "くまのみ",
            "text": "三谷さんの４回目が気になりますが、それは書いてませんでした",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "みたらし",
            "text": "いずれ出てくるかもしれないですね",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "ししとう",
            "text": "@くまのみ それって本なんですか？",
            "mention_to": [
                "くまのみ"
            ]
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "みたらし",
            "text": "別のコラムに",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "くまのみ",
            "text": "@ししとう 新聞のコラムです",
            "mention_to": [
                "ししとう"
            ]
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "ししとう",
            "text": "@くまのみ なるほど！",
            "mention_to": [
                "くまのみ"
            ]
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "くまのみ",
            "text": "チャックも若い子いうかな？って",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "くまのみ",
            "text": "ファスナーかなって書いてました",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "ししとう",
            "text": "@くまのみ 気が付かなかった！",
            "mention_to": [
                "くまのみ"
            ]
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "みたらし",
            "text": "うちの子供はチャックっていうみたいですね。",
            "mention_to": []
        }
