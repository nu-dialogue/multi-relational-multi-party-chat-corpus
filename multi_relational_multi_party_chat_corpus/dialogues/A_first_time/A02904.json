        {
            "utterance_id": 1,
            "interlocutor_id": "なると",
            "text": "こんにちは！",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "しじみ",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "しらす",
            "text": "よろしくお願いします",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "しじみ",
            "text": "よろしくお願いします！",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "なると",
            "text": "よろしくです",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "しらす",
            "text": "突然ですが",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "しじみ",
            "text": "@しらす はい！",
            "mention_to": [
                "しらす"
            ]
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "なると",
            "text": "はい",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "しらす",
            "text": "皆さん眠気対策とかどうしてます？",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "しじみ",
            "text": "あー、",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "なると",
            "text": "何でしょうね",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "しらす",
            "text": "今ものすごく眠くて",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "しじみ",
            "text": "今まさに、ミントガム噛んでました！",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "なると",
            "text": "コーヒーですかね",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "しらす",
            "text": "でも起きていないといけない用事があるので",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "しじみ",
            "text": "@なると コーヒー！確かに！",
            "mention_to": [
                "なると"
            ]
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "しらす",
            "text": "ガム!!なるほど",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "しらす",
            "text": "コーヒーはリアルに飲んでます",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "なると",
            "text": "そうなんですね",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "しじみ",
            "text": "@しらす あら、じゃあ何か試さないとなんですね！",
            "mention_to": [
                "しらす"
            ]
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "しらす",
            "text": "そうなんですよ",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "なると",
            "text": "歌を歌う",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "しじみ",
            "text": "あとはなんだろう",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "しらす",
            "text": "そういう時に限って眠くなる",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "しじみ",
            "text": "@なると かわいらしい！",
            "mention_to": [
                "なると"
            ]
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "しらす",
            "text": "歌!!いいですね",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "しじみ",
            "text": "歌を聞くのもよさそうですね！",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "しらす",
            "text": "しかし、旦那在宅で恥ずかしい笑",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "なると",
            "text": "確かに",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "しじみ",
            "text": "@しらす それは恥ずかしいかも！笑",
            "mention_to": [
                "しらす"
            ]
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "なると",
            "text": "声出すのがよさそうです",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "しじみ",
            "text": "うんうん",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "しじみ",
            "text": "つねるといいとか、聞きますけど",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "しじみ",
            "text": "痛いことわざわざしたくないですしね〜",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "しらす",
            "text": "よく言いますよね、つねる",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "なると",
            "text": "いやですね",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "しじみ",
            "text": "ちょっと外の空気を吸うとか？",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "なると",
            "text": "それもいいですね",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "しらす",
            "text": "いいですね!!",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "なると",
            "text": "ちょっと寒くするとか",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "しじみ",
            "text": "@なると 良さそう！",
            "mention_to": [
                "なると"
            ]
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "しらす",
            "text": "目が覚めそう!!",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "しじみ",
            "text": "うんうん！",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "なると",
            "text": "ですね",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "しじみ",
            "text": "眠気と食欲って、",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "しじみ",
            "text": "本当に我慢するのしんどいですよね〜",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "なると",
            "text": "辛いですね",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "しらす",
            "text": "分かります",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "なると",
            "text": "眠気の方が辛いですね",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "しらす",
            "text": "私も",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "しじみ",
            "text": "えー、どっちだろ",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "しじみ",
            "text": "お腹すくのもつらい",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "しじみ",
            "text": "でも確かに眠れないのもつらい",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "なると",
            "text": "そうですよね",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "しじみ",
            "text": "みなさん、しっかり寝る派ですか？",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "しらす",
            "text": "私、ショートスリーパーかもしれない",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "なると",
            "text": "そうですか",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "なると",
            "text": "私もかもです。",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "しじみ",
            "text": "@しらす おお！何時間睡眠ですか？",
            "mention_to": [
                "しらす"
            ]
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "しらす",
            "text": "だから眠くなるんですけど笑",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "しじみ",
            "text": "@なると あら！",
            "mention_to": [
                "なると"
            ]
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "なると",
            "text": "どれくらい寝ますか？",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "しらす",
            "text": "@しじみ 12時に寝て5時起きとかですかね",
            "mention_to": [
                "しじみ"
            ]
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "しじみ",
            "text": "@しらす 睡眠時間短いですね！",
            "mention_to": [
                "しらす"
            ]
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "しじみ",
            "text": "@なると どれくらいですか？",
            "mention_to": [
                "なると"
            ]
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "なると",
            "text": "私も5、6時間ですね",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "しじみ",
            "text": "おおー！",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "なると",
            "text": "私は長く眠る方が眠気がすごくなります",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "しじみ",
            "text": "私7、8時間くらい寝ちゃうので",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "しじみ",
            "text": "お二人がかっこよくて羨ましい！",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "しらす",
            "text": "いいと思いますよ",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "なると",
            "text": "それぐらいが健康的ですね",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "しじみ",
            "text": "勝手に目が覚めますか？",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "しらす",
            "text": "睡眠大事",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "なると",
            "text": "目が覚めると思います",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "しらす",
            "text": "いや、アラームで無理やり笑",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "しじみ",
            "text": "あら、",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "しらす",
            "text": "朝が一番誰にも邪魔されない時間帯なので",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "なると",
            "text": "なるほど",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "しじみ",
            "text": "@なると 勝手に覚めるんですね！",
            "mention_to": [
                "なると"
            ]
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "しらす",
            "text": "いろいろやりたいというか",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "しじみ",
            "text": "@しらす うんうん！",
            "mention_to": [
                "しらす"
            ]
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "なると",
            "text": "朝早いですね",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "しらす",
            "text": "集中力も1番ある時間帯なので",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "しじみ",
            "text": "朝活！素敵！",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "しらす",
            "text": "早起きしてます",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "しじみ",
            "text": "えらいぃ！",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "なると",
            "text": "朝活いいんですよね",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "しらす",
            "text": "いいですよね",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "なると",
            "text": "私は夜型なので",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "なると",
            "text": "うらやましいです",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "しらす",
            "text": "私も元々超夜型でした",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "しじみ",
            "text": "@しらす 5時頃の空気、気持ちよさそう！",
            "mention_to": [
                "しらす"
            ]
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "なると",
            "text": "そうなんですね",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "しじみ",
            "text": "そうなんですね！",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "しらす",
            "text": "@しじみ 朝日綺麗ですよ",
            "mention_to": [
                "しじみ"
            ]
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "しじみ",
            "text": "あー、いいですね！",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "なると",
            "text": "綺麗でしょうね",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "なると",
            "text": "私も朝方にしたいです",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "しらす",
            "text": "無理やり朝型にしました笑",
            "mention_to": []
        },
        {
