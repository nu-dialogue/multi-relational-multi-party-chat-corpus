{
    "dialogue_id": "A07602",
    "dialogue_type": "First time",
    "interlocutors": [
        "しらす",
        "こぐま",
        "おにぎり"
    ],
    "relationship": [],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "しらす",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "こぐま",
            "text": "こんにちわ！",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "おにぎり",
            "text": "こんにちは。",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "しらす",
            "text": "よろしくお願いします",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "しらす",
            "text": "昨日久しぶりに",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "こぐま",
            "text": "よろしくお願いします！",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "おにぎり",
            "text": "お願いいたします！",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "こぐま",
            "text": "はい",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "しらす",
            "text": "住んでいるとこで地震がありました",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "こぐま",
            "text": "大きかった？",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "おにぎり",
            "text": "あら、大丈夫でした？",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "しらす",
            "text": "震度3でしたが",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "こぐま",
            "text": "まぁまぁ揺れたんじゃない？",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "しらす",
            "text": "住んでいるところが9階なので",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "おにぎり",
            "text": "震度３でも、まぁまぁ揺れたかな。",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "しらす",
            "text": "体感5くらいありました",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "おにぎり",
            "text": "それは、揺れそうですねぇーー",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "こぐま",
            "text": "わー大きく揺れたよねー",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "しらす",
            "text": "怖かったです",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "こぐま",
            "text": "家具とか、大丈夫だった？",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "しらす",
            "text": "でも被害はなかったので良かったです",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "おにぎり",
            "text": "能登の地震の後、ちょっと各地で地震が増えてる感じですよね。",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "こぐま",
            "text": "それは良かった",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "しらす",
            "text": "こういうときいつも思うのが",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "おにぎり",
            "text": "@しらす 良かった良かった。",
            "mention_to": [
                "しらす"
            ]
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "こぐま",
            "text": "うん",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "しらす",
            "text": "全然防災グッズ準備していない",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "しらす",
            "text": "皆さん何かしていますか？",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "こぐま",
            "text": "あー、私もだー",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "おにぎり",
            "text": "防災グッズ、必要ですよね。",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "しらす",
            "text": "揃えようと思っても",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "おにぎり",
            "text": "私は、水を少し買いだめているかな。",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "こぐま",
            "text": "@しらす しとかないとと思ってるんだけど",
            "mention_to": [
                "しらす"
            ]
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "しらす",
            "text": "ついつい後回しにしてしまいます",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "こぐま",
            "text": "@おにぎり 偉いなー",
            "mention_to": [
                "おにぎり"
            ]
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "しらす",
            "text": "@こぐま 私もです",
            "mention_to": [
                "こぐま"
            ]
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "おにぎり",
            "text": "東日本の時、困ったから。。",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "しらす",
            "text": "@こぐま すごい",
            "mention_to": [
                "こぐま"
            ]
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "こぐま",
            "text": "一時期は、貴重品とかまとめたりしてたけど",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "おにぎり",
            "text": "食べ物は、買いだめても食べてしまう。。",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "こぐま",
            "text": "食べ物とかも、用意してたけど",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "しらす",
            "text": "うんうん",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "こぐま",
            "text": "いつの間にか、しなくなってる",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "おにぎり",
            "text": "でも、ほんと忘れたころにやってくるっていうし、",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "しらす",
            "text": "そうなんですよね",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "おにぎり",
            "text": "そろそろ必要かな、、って思ったりします。",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "しらす",
            "text": "慣れるというか忘れるというか",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "こぐま",
            "text": "災害が、来た時に、どーにもならないタイプ",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "こぐま",
            "text": "@おにぎり 経験あると、違うね",
            "mention_to": [
                "おにぎり"
            ]
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "おにぎり",
            "text": "こないだ、パルシステムで非常食料セット注文しました。",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "しらす",
            "text": "たしかに地震も増えてきていますもんね",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "こぐま",
            "text": "@おにぎり そんなセットあるの？",
            "mention_to": [
                "おにぎり"
            ]
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "しらす",
            "text": "@おにぎり おぉすごい",
            "mention_to": [
                "おにぎり"
            ]
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "おにぎり",
            "text": "段ボールに、レトルト食品とかが詰まっている物みたいで。",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "こぐま",
            "text": "そんなのがあるなら、",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "しらす",
            "text": "うんうん",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "おにぎり",
            "text": "私も、初めて頼んだのですが。",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "こぐま",
            "text": "頼もかなー",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "しらす",
            "text": "私も気になります",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "おにぎり",
            "text": "まとまってるから、ちょっと高くて、",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "こぐま",
            "text": "そなんだ",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "おにぎり",
            "text": "躊躇しましたけどね！",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "こぐま",
            "text": "でも、日持ちするものが揃ってた？",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "おにぎり",
            "text": "そうですね！",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "おにぎり",
            "text": "あと、おもちとか？",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "しらす",
            "text": "それはいいですね",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "こぐま",
            "text": "いいねー！",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "おにぎり",
            "text": "缶詰系が多かったかな。あと、お菓子かな！",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "しらす",
            "text": "おもち、腹持ちしそう!!",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "おにぎり",
            "text": "非常時、お菓子とかあると、ちょっと心が落ち着くとか書いてあった気が。。",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "しらす",
            "text": "たしかに！",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "こぐま",
            "text": "チョコとか、良さそうだ",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "こぐま",
            "text": "@おにぎり 東日本大震災の時、",
            "mention_to": [
                "おにぎり"
            ]
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "しらす",
            "text": "いいですね",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "こぐま",
            "text": "困ったんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "おにぎり",
            "text": "ちょっと断水したんですよね。",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "こぐま",
            "text": "水かー",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "しらす",
            "text": "なるほど",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "おにぎり",
            "text": "あと、停電もしたかな。北関東なので。。",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "こぐま",
            "text": "それは困るね",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "おにぎり",
            "text": "近かった。",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "こぐま",
            "text": "停電も！",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "こぐま",
            "text": "どっちも、すごく困るね",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "しらす",
            "text": "困りますね",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "おにぎり",
            "text": "みんな、トイレのリモコンが使えなくなって、困ったって言ってました。",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "こぐま",
            "text": "水とか、",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "こぐま",
            "text": "お風呂のお湯を捨てずにって聞くね",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "おにぎり",
            "text": "うちは、手動だったので、動きはしたけど、水が出ない！みたいな。",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "こぐま",
            "text": "流せないの困るね",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "しらす",
            "text": "お風呂の水もいつの間にかしなくなって",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "こぐま",
            "text": "私もー",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "おにぎり",
            "text": "お風呂のお湯、ためとくと良いとは思うけど、汚れとか気になりますよね。。",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "こぐま",
            "text": "すぐに湯船、洗いたくて",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "おにぎり",
            "text": "そうそう！",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "しらす",
            "text": "@おにぎり ヌルヌルしますよね",
            "mention_to": [
                "おにぎり"
            ]
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "おにぎり",
            "text": "ですよねぇーー。",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "しらす",
            "text": "洗いたくなります",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "こぐま",
            "text": "ねー、汚れは置いとくと、掃除に手間かかるし",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "しらす",
            "text": "衛生面も気になってくる",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "こぐま",
            "text": "@おにぎり 食料とかの準備だけですか？",
            "mention_to": [
                "おにぎり"
            ]
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "おにぎり",
            "text": "あとは、、、ガソリンはなるべくフルにしておくとか？",
            "mention_to": []
        }
