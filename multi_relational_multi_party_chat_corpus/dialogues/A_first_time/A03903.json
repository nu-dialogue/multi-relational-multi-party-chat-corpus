            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "かにたま",
            "text": "子供の頃信じていたものってありますか？",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "まりも",
            "text": "信じていたもの？",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "こまつな",
            "text": "えー？",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "かにたま",
            "text": "例えば",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "かにたま",
            "text": "月にうさぎが餅ついてる",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "かにたま",
            "text": "とか",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "かにたま",
            "text": "たぬきは化ける",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "かにたま",
            "text": "とか",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "まりも",
            "text": "幼稚園ぐらいまではあったのかな…",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "こまつな",
            "text": "ああー",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "まりも",
            "text": "結構早くから冷めてるタイプだった気がします",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "かにたま",
            "text": "大人だなぁー",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "こまつな",
            "text": "記憶が遠い",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "かにたま",
            "text": "@まりも そうなんですね",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "まりも",
            "text": "@かにたま 何か信じてました？",
            "mention_to": [
                "かにたま"
            ]
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "かにたま",
            "text": "わぁ、、",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "かにたま",
            "text": "さっき話したことのほかに",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "かにたま",
            "text": "あ",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "かにたま",
            "text": "おもちゃのチャチャチャ",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "かにたま",
            "text": "って曲しってますか？",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "まりも",
            "text": "はい",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "かにたま",
            "text": "あれに",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "こまつな",
            "text": "夜オモチャうごく",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "かにたま",
            "text": "夜中におもちゃが動き出す",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "かにたま",
            "text": "これ信じて",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "まりも",
            "text": "そうだったらいいな～って気持ちは",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "かにたま",
            "text": "夜中まで起きてましたー",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "まりも",
            "text": "あったかも",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "こまつな",
            "text": "すごい歌",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "まりも",
            "text": "かわいらしい",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "かにたま",
            "text": "ですよね",
            "mention_to": []
        },
        {
            "utterance_id": 34,
