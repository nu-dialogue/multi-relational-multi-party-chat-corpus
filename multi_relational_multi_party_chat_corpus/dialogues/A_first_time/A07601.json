{
    "dialogue_id": "A07601",
    "dialogue_type": "First time",
    "interlocutors": [
        "しらす",
        "おにぎり",
        "こぐま"
    ],
    "relationship": [],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "しらす",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "おにぎり",
            "text": "こんにちは。",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "こぐま",
            "text": "こんにちわ！",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "しらす",
            "text": "また寒くなりましたね",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "おにぎり",
            "text": "よろしくお願いいたします。",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "こぐま",
            "text": "今朝も寒かったー",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "おにぎり",
            "text": "寒いですね！風もすごいです。",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "しらす",
            "text": "こちらも風がすごいです",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "おにぎり",
            "text": "台風並みと、ニュースで言っていました。",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "こぐま",
            "text": "風が強い？",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "おにぎり",
            "text": "こちらは、すごい強風です！",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "こぐま",
            "text": "@おにぎり 言ってましたね",
            "mention_to": [
                "おにぎり"
            ]
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "しらす",
            "text": "@おにぎり えー？台風なみ!!",
            "mention_to": [
                "おにぎり"
            ]
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "しらす",
            "text": "音がすごいです",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "おにぎり",
            "text": "怖いくらいですよ。",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "こぐま",
            "text": "めちゃ強風！",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "しらす",
            "text": "嫌ですよね",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "おにぎり",
            "text": "電車も止まってしまったようで。。",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "こぐま",
            "text": "おー！それはすごい",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "しらす",
            "text": "@おにぎり 電車止まるくらいの強風!!",
            "mention_to": [
                "おにぎり"
            ]
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "おにぎり",
            "text": "天気が狂暴化してますよね。",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "こぐま",
            "text": "@おにぎり 電車止まって、影響ある？",
            "mention_to": [
                "おにぎり"
            ]
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "おにぎり",
            "text": "極端というか。。",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "しらす",
            "text": "たしかに極端…",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "しらす",
            "text": "先週ずっと雨だったし",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "おにぎり",
            "text": "子供が電車通学なので、このまま夜まで止まるとまずいです。。",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "こぐま",
            "text": "@おにぎり 最近の天気は怖いですね",
            "mention_to": [
                "おにぎり"
            ]
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "しらす",
            "text": "@おにぎり それは大変",
            "mention_to": [
                "おにぎり"
            ]
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "こぐま",
            "text": "@おにぎり え？行っちゃった後に止まったんですか？",
            "mention_to": [
                "おにぎり"
            ]
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "おにぎり",
            "text": "自分は、在宅なので大丈夫なのですが。。",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "おにぎり",
            "text": "@こぐま 朝は動いていたのですけどね！",
            "mention_to": [
                "こぐま"
            ]
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "こぐま",
            "text": "朝から止まってたら良かったのにね",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "しらす",
            "text": "たしかに",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "こぐま",
            "text": "帰りの足がないのは心配",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "おにぎり",
            "text": "もう一人は、自転車で40分だし、自分だけ家でぬくぬく、",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "おにぎり",
            "text": "なんか申し訳ないです笑",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "こぐま",
            "text": "風が強いと",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "しらす",
            "text": "風の日の自転車も大変ですね",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "しらす",
            "text": "前に進まない",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "こぐま",
            "text": "チャリもしんどそう",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "こぐま",
            "text": "進まないねー",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "おにぎり",
            "text": "追い風だととても速く帰れるそうです笑",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "こぐま",
            "text": "負荷が半端ない",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "こぐま",
            "text": "追い風ね！",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "おにぎり",
            "text": "自転車普段乗りますか？",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "しらす",
            "text": "追い風、早そう!!",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "こぐま",
            "text": "それならいいけど",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "しらす",
            "text": "@おにぎり 坂の街なので乗れません笑",
            "mention_to": [
                "おにぎり"
            ]
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "こぐま",
            "text": "私はあるけど、ついついクルマ",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "おにぎり",
            "text": "私はもっぱら車です。。",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "おにぎり",
            "text": "@しらす 坂は大変ですね！",
            "mention_to": [
                "しらす"
            ]
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "しらす",
            "text": "私も車です",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "こぐま",
            "text": "@しらす 坂が多いと、無理だー",
            "mention_to": [
                "しらす"
            ]
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "おにぎり",
            "text": "電動自転車とか必要なのかな。",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "しらす",
            "text": "自転車は電動アシストがないと乗れないレベルです",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "こぐま",
            "text": "前に、レンタルでアシスト自転車乗ったけど",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "おにぎり",
            "text": "おー結構な急坂っぽいですね。",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "こぐま",
            "text": "いいですねー",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "しらす",
            "text": "うんうん",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "こぐま",
            "text": "感動しました",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "しらす",
            "text": "まだ乗ったことないです",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "おにぎり",
            "text": "普通のより楽ですかね？",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "こぐま",
            "text": "アシスト自転車なら、乗るかなー？",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "しらす",
            "text": "ビュンっていきそうなイメージ",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "こぐま",
            "text": "めちゃ楽！",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "しらす",
            "text": "おぉ",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "おにぎり",
            "text": "ほぉ！",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "こぐま",
            "text": "力要らない",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "しらす",
            "text": "乗ってみたい",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "こぐま",
            "text": "でも、足よりも先に",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "おにぎり",
            "text": "ね！試してみたいです。",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "こぐま",
            "text": "お尻が痛くなった",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "しらす",
            "text": "もし子どもが自転車通学するなら",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "おにぎり",
            "text": "あらら笑",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "しらす",
            "text": "電動アシスト必須なので",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "しらす",
            "text": "出費が怖い笑",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "おにぎり",
            "text": "そうかーー。けっこう値段しますよね。。",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "こぐま",
            "text": "@しらす アシスト自転車OKな学校？",
            "mention_to": [
                "しらす"
            ]
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "おにぎり",
            "text": "禁止だったりするのかしら？！",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "こぐま",
            "text": "わかんない",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "しらす",
            "text": "@こぐま 周りの学生さん、みんな乗ってます",
            "mention_to": [
                "こぐま"
            ]
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "しらす",
            "text": "多分OKだと思いますよ",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "こぐま",
            "text": "@大丈夫なんだ",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "しらす",
            "text": "ただ坂道なので",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "こぐま",
            "text": "出てすぐの時は高い感じだったけど",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "おにぎり",
            "text": "普通の自転車もけっこう高いのに。",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "しらす",
            "text": "普通に歩いてると自転車ビュンビュンで怖いです",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "こぐま",
            "text": "今なら少し安くなってきてるかな？",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "こぐま",
            "text": "下りも怖そう",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "しらす",
            "text": "@こぐま 安くはなってますが、高いです",
            "mention_to": [
                "こぐま"
            ]
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "こぐま",
            "text": "@しらす めちゃスピード出そうだもん",
            "mention_to": [
                "しらす"
            ]
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "おにぎり",
            "text": "子供の中学は、自転車の指定はないのだけど、",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "しらす",
            "text": "うんうん",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "こぐま",
            "text": "うん",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "おにぎり",
            "text": "ほぼだいたいの子が、中学近くの自転車屋で買っていて",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "こぐま",
            "text": "うん",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "おにぎり",
            "text": "それが一台10万円近くにねあがっているらしい。",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "こぐま",
            "text": "Wow…！！",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "しらす",
            "text": "ひーっ!!",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "しらす",
            "text": "それは高い…",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "こぐま",
            "text": "それはアシスト付き？",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "おにぎり",
            "text": "買えないですよね。。。",
            "mention_to": []
        }
