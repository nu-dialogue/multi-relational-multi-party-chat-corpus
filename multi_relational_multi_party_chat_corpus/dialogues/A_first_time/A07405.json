            "interlocutor_id": "ししとう",
            "text": "@たらこ そうそう！",
            "mention_to": [
                "たらこ"
            ]
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "たらこ",
            "text": "しかも",
            "mention_to": []
        },
        {
            "utterance_id": 29,
