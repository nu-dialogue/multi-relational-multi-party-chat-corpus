        },
        {
            "utterance_id": 22,
            "interlocutor_id": "ししとう",
            "text": "そうなんです！",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "ししとう",
            "text": "痛い出費〜",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "たらこ",
            "text": "今500円とかありますもんね",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "はくまい",
            "text": "ひゃー",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "たらこ",
            "text": "@ししとう そうそう！",
            "mention_to": [
                "ししとう"
