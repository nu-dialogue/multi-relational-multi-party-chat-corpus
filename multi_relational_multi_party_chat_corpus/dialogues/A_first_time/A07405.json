        },
        {
            "utterance_id": 34,
            "interlocutor_id": "たらこ",
            "text": "@はくまい 入浴剤すきですか？",
            "mention_to": [
                "はくまい"
            ]
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "たらこ",
            "text": "@ししとう あれ困りますよね笑",
            "mention_to": [
                "ししとう"
            ]
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "はくまい",
            "text": "じゃあ普通の入浴剤におもちゃ埋め込んだ方がいいですね",
