            "interlocutor_id": "たらこ",
            "text": "@ししとう ああ！",
            "mention_to": [
                "ししとう"
            ]
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "はくまい",
            "text": "中におもちゃが入ってる",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "ししとう",
            "text": "おもちゃが出てくるのを買わされます…",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "たらこ",
            "text": "@ししとう わかります！",
            "mention_to": [
                "ししとう"
            ]
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "ししとう",
            "text": "普段使いじゃなーい",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "はくまい",
            "text": "ものが増える笑",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "たらこ",
            "text": "あれ意外に高いですよね",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "はくまい",
            "text": "いいお値段しますよね",
