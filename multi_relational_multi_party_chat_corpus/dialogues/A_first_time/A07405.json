        },
        {
            "utterance_id": 30,
            "interlocutor_id": "はくまい",
            "text": "はい",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "たらこ",
            "text": "すぐ飽きられるし",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "ししとう",
            "text": "@たらこ 飽きられる問題…まさに！",
            "mention_to": [
                "たらこ"
