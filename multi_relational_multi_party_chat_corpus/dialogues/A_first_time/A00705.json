    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "ししとう",
            "text": "こんばんは～ ",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "たらこ",
            "text": "こんばんは！ ",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "あずき",
            "text": "こんばんはー ",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "ししとう",
            "text": "私、ハムスターかってるんですが ",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "ししとう",
            "text": "なにかペットかってます？ ",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "あずき",
            "text": "@ししとう 飼っていないんです",
            "mention_to": [
                "ししとう"
            ]
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "たらこ",
            "text": "犬を買ってます！ ",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "あずき",
            "text": "@たらこ 犬ですか",
            "mention_to": [
                "たらこ"
            ]
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "ししとう",
            "text": "@たらこ 犬いいですね！",
            "mention_to": [
                "たらこ"
            ]
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "あずき",
            "text": "ペットいいですね ",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "たらこ",
            "text": "@ししとう ハムスター可愛いですね！",
            "mention_to": [
                "ししとう"
            ]
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "ししとう",
            "text": "@たらこ 犬種はなんですか？",
            "mention_to": [
                "たらこ"
            ]
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "あずき",
            "text": "大型犬ですか？ ",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "たらこ",
            "text": "@ししとう マルチーズとトイプードルです",
            "mention_to": [
                "ししとう"
            ]
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "あずき",
            "text": "2匹ですね ",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "あずき",
            "text": "すごい ",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "たらこ",
            "text": "@あずき 小型ですね！",
            "mention_to": [
                "あずき"
            ]
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "ししとう",
            "text": "@たらこ 2匹ですか！",
            "mention_to": [
                "たらこ"
            ]
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "ししとう",
            "text": "いいですね！ ",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "あずき",
            "text": "@たらこ かわいいですね",
            "mention_to": [
                "たらこ"
            ]
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "あずき",
            "text": "お散歩行きますか？ ",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "たらこ",
            "text": "めっちゃ可愛いです！ ",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "あずき",
            "text": "お出かけにも連れて行きます？ ",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "あずき",
            "text": "私も飼いたいです ",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "たらこ",
            "text": "@あずき 2日に1回しか行けてないです",
            "mention_to": [
                "あずき"
            ]
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "あずき",
            "text": "@たらこ そうなんですね",
            "mention_to": [
                "たらこ"
            ]
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "たらこ",
            "text": "マルチーズだけお出かけつれていきます！ ",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "あずき",
            "text": "家の中でも十分ですね ",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "ししとう",
            "text": "@あずき 確かに！",
            "mention_to": [
                "あずき"
            ]
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "たらこ",
            "text": "トイプードルは車酔いするんですよ ",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "あずき",
            "text": "@たらこ そうなんですね",
            "mention_to": [
                "たらこ"
            ]
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "あずき",
            "text": "犬も車酔いあるんですね ",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "ししとう",
            "text": "@たらこ そうなんですね！",
            "mention_to": [
                "たらこ"
            ]
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "あずき",
            "text": "それはかわいそう ",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "たらこ",
            "text": "@あずき 家のなかで放し飼いしてます！",
            "mention_to": [
                "あずき"
            ]
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "あずき",
            "text": "元気いっぱいですね ",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "ししとう",
            "text": "@たらこ ワンちゃんいたら片づきますね！",
            "mention_to": [
                "たらこ"
            ]
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "たらこ",
            "text": "@あずき お子さんはペットほしがりますか？",
            "mention_to": [
                "あずき"
            ]
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "あずき",
            "text": "@たらこ 欲しがります",
            "mention_to": [
                "たらこ"
            ]
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "あずき",
            "text": "でも猫アレルギーで ",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "ししとう",
            "text": "@あずき アレルギーがあるんですね！",
            "mention_to": [
                "あずき"
            ]
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "たらこ",
            "text": "@あずき 猫アレルギーですか！",
            "mention_to": [
                "あずき"
            ]
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "あずき",
            "text": "飼えません ",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "あずき",
            "text": "犬だったら大丈夫かな ",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "たらこ",
            "text": "それは確かに飼えないですね ",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "あずき",
            "text": "猫も好きなんですけどね ",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "たらこ",
            "text": "@あずき 怖いですよね！",
            "mention_to": [
                "あずき"
            ]
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "あずき",
            "text": "目が痒くなったりします ",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "あずき",
            "text": "子供がです ",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "ししとう",
            "text": "@あずき 心配ですね",
            "mention_to": [
                "あずき"
            ]
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "たらこ",
            "text": "結構ひどいんですね！ ",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "あずき",
            "text": "でも猫カフェ行きました ",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "ししとう",
            "text": "@あずき 猫カフェ！いいですね！",
            "mention_to": [
                "あずき"
            ]
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "たらこ",
            "text": "大丈夫だったんですか？ ",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "あずき",
            "text": "触りたいって言うんで ",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "あずき",
            "text": "30分くらいだったんで ",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "あずき",
            "text": "なんとか大丈夫でした ",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "ししとう",
            "text": "@あずき 大丈夫でよかったです！",
            "mention_to": [
                "あずき"
            ]
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "たらこ",
            "text": "@あずき よかったですね！",
            "mention_to": [
                "あずき"
            ]
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "あずき",
            "text": "飼うのは難しいかな ",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "たらこ",
            "text": "長い時間となると ",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "あずき",
            "text": "そうなんですよ ",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "ししとう",
            "text": "@あずき 毎日のことですもんね",
            "mention_to": [
                "あずき"
            ]
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "たらこ",
            "text": "難しいんですね ",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "あずき",
            "text": "坂上動物園のテレビはよく見てます ",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "ししとう",
            "text": "@あずき おもしろいですよね！うちもよく見ます！",
            "mention_to": [
                "あずき"
            ]
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "たらこ",
            "text": "@ししとう ハムスターのお世話は大変ですか？",
            "mention_to": [
                "ししとう"
            ]
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "ししとう",
            "text": "@たらこ 犬と比べたら全然ラクです！",
            "mention_to": [
                "たらこ"
            ]
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "あずき",
            "text": "ハムスター可愛いですよね ",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "あずき",
            "text": "そうなんですね ",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "たらこ",
            "text": "@あずき わたしもみます！",
            "mention_to": [
                "あずき"
            ]
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "あずき",
            "text": "寒いのは大丈夫ですか？ ",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "あずき",
            "text": "ハムスター ",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "たらこ",
            "text": "@ししとう そうなんですか？",
            "mention_to": [
                "ししとう"
            ]
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "たらこ",
            "text": "トイレとかは？ ",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "ししとう",
            "text": "@あずき ハムスター用の暖房をつけてます！",
            "mention_to": [
                "あずき"
            ]
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "あずき",
            "text": "@ししとう おおすごい",
            "mention_to": [
                "ししとう"
            ]
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "たらこ",
            "text": "@ししとう 専用があるんですか！",
            "mention_to": [
                "ししとう"
            ]
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "ししとう",
            "text": "@たらこ 砂を毎日交換くらいです！",
            "mention_to": [
                "たらこ"
            ]
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "たらこ",
            "text": "ハムスターは寒がり？ ",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "あずき",
            "text": "寒いのは苦手なんですね ",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "たらこ",
            "text": "@ししとう 楽そう！",
            "mention_to": [
                "ししとう"
            ]
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "あずき",
            "text": "ハムスターは何食べますか ",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "ししとう",
            "text": "あまり寒いのはよくないと聞いて ",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "ししとう",
            "text": "暖房つけてます！ ",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "たらこ",
            "text": "@ししとう すごーい！",
            "mention_to": [
                "ししとう"
            ]
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "ししとう",
            "text": "@あずき ハムスターフードをあげてます！",
            "mention_to": [
                "あずき"
            ]
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "あずき",
            "text": "@ししとう フードがあるんですね",
            "mention_to": [
                "ししとう"
            ]
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "ししとう",
            "text": "むかしはひまわりの種とかいっぱいあげてるイメージだったけど ",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "たらこ",
            "text": "@ししとう わかります！",
            "mention_to": [
                "ししとう"
            ]
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "ししとう",
            "text": "今は健康のため、たくさんはよくないらしいです。 ",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "あずき",
            "text": "@ししとう そうでした　ひまわりの種",
            "mention_to": [
                "ししとう"
            ]
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "あずき",
            "text": "そうなんですね ",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "たらこ",
            "text": "@ししとう そうなんですか！",
            "mention_to": [
                "ししとう"
            ]
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "たらこ",
            "text": "栄養のある餌なんですね！ ",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "ししとう",
            "text": "子どものときぶりに飼い始めて、時代の流れ？変化を感じます。 ",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "あずき",
            "text": "見てるだけで癒されますね ",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "あずき",
            "text": "私も自分が子供の頃飼ってました ",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "ししとう",
            "text": "@あずき 癒やされます！",
            "mention_to": [
                "あずき"
            ]
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "たらこ",
            "text": "今は餌も昔よりよくなったから ",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "たらこ",
            "text": "長生きするそうですよ！ ",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "あずき",
            "text": "@たらこ そうなんですね",
            "mention_to": [
                "たらこ"
            ]
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "あずき",
            "text": "長生きしてほしい ",
            "mention_to": []
        },
        {
            "utterance_id": 103,
            "interlocutor_id": "たらこ",
            "text": "@あずき ハムスターをですか？",
            "mention_to": [
                "あずき"
            ]
        },
        {
            "utterance_id": 104,
            "interlocutor_id": "ししとう",
            "text": "本当！長生きしてほしい！ ",
            "mention_to": []
        },
        {
            "utterance_id": 105,
            "interlocutor_id": "あずき",
            "text": "@たらこ そうです　飼ってました",
            "mention_to": [
                "たらこ"
            ]
        },
        {
            "utterance_id": 106,
            "interlocutor_id": "あずき",
            "text": "犬も亀も飼ってました ",
            "mention_to": []
        }
