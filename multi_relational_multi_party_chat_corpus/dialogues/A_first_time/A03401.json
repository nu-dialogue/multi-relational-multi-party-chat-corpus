    "relationship": [],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "げんまい",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "おでん",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "てんぷら",
            "text": "こんにちは！",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "げんまい",
            "text": "よろしくお願いします。",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "おでん",
            "text": "よろしくお願いします",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "てんぷら",
            "text": "初めまして、よろしくお願い致します",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "げんまい",
            "text": "朝食は召し上がりましたか？",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "おでん",
            "text": "はい、食べました",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "てんぷら",
            "text": "食べました！簡単スコーン作りました",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "げんまい",
            "text": "スコーン！",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "げんまい",
            "text": "朝から手作りですか？",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "おでん",
            "text": "@手作り？",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "てんぷら",
            "text": "ビニール袋で30分くらいで出来ます",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "げんまい",
            "text": "おお！30分",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "おでん",
            "text": "すごい",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "げんまい",
            "text": "でも、朝野30分はなかなか貴重ですね",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "てんぷら",
            "text": "トースターで15分焼くのを含めて30分です",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "げんまい",
            "text": "焼くの入れて？！",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "げんまい",
            "text": "それは早い。",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "てんぷら",
            "text": "前日に決めて早く起きます",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "おでん",
            "text": "@てんぷら トースターでできるんですか？",
            "mention_to": [
                "てんぷら"
            ]
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "てんぷら",
            "text": "手軽でしょ？？",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "げんまい",
            "text": "いいですねー",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "おでん",
            "text": "手軽です",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "げんまい",
            "text": "何かつけて食べるんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "てんぷら",
            "text": "ホットケーキミックスとバターと牛乳だけです",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "てんぷら",
            "text": "板チョコ割って入れるので",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "おでん",
            "text": "美味しそうです",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "てんぷら",
            "text": "何もつけなくても美味しいですが",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "おでん",
            "text": "焼きたてだと、余計に美味しいでしょうね",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "てんぷら",
            "text": "コーヒーやホットミルクに付けてもよさそうです",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "げんまい",
            "text": "板チョコ入り、おいしそう。",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "てんぷら",
            "text": "栄養的な罪悪感をぬぐうために",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "げんまい",
            "text": "ホットケーキミックスは、便利ですね",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "てんぷら",
            "text": "ネスレのミロを入れたりもします",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "てんぷら",
            "text": "鉄分とカルシウム",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "おでん",
            "text": "色んなレシピありますもんね",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "げんまい",
            "text": "ミロ、生地に練り込むんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "てんぷら",
            "text": "ミックスにミックスしちゃいます",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "おでん",
            "text": "@てんぷら なるほど～入れるもので栄養補助できますね",
            "mention_to": [
                "てんぷら"
            ]
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "おでん",
            "text": "手作り、好きなんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "げんまい",
            "text": "スコーンのバリエーション、すごく広がりますね",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "おでん",
            "text": "ウンウン",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "てんぷら",
            "text": "@おでん 朝食、何でした？",
            "mention_to": [
                "おでん"
            ]
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "おでん",
            "text": "私はベーグルとコーヒーです",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "げんまい",
            "text": "ベーグル、オシャレ",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "おでん",
            "text": "ベーグルは買ってきたものです",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "てんぷら",
            "text": "ベーグルは難しそうです、作るのは",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "おでん",
            "text": "日持ちしたので、まとめて買ってきました",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "てんぷら",
            "text": "ベーグルアンドベーグル、好きです",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "げんまい",
            "text": "作ったことあります。ベーグル",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "てんぷら",
            "text": "@げんまい 上級者です！",
            "mention_to": [
                "げんまい"
            ]
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "おでん",
            "text": "ベーグルを作ったんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "げんまい",
            "text": "焼く前にゆでる作業が発生するので、ちょっと面倒ですが、",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "げんまい",
            "text": "出来立ては美味しいです",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "てんぷら",
            "text": "ゆでるんですか！知らなかった",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "おでん",
            "text": "@げんまい そうなんですね、ひと手間かかるんですね",
            "mention_to": [
                "げんまい"
            ]
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "げんまい",
            "text": "そうなんです。",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "げんまい",
            "text": "最後の醗酵させる代わりにゆでます",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "おでん",
            "text": "@げんまい @てんぷら 凄いです！手作り！！",
            "mention_to": [
                "げんまい",
                "てんぷら"
            ]
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "てんぷら",
            "text": "友人が、指の跡が付くって言っていました、ベーグル",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "てんぷら",
            "text": "つるんとさせるのが難しいと",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "おでん",
            "text": "ネトネトするのかな？",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "げんまい",
            "text": "あー見た目にこだわるなら難しいけど",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "げんまい",
            "text": "自分と家族が食べるだけなので、気にしませんｗｗｗ",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "おでん",
            "text": "それでも、難しそうです",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "げんまい",
            "text": "買ったベーグルもおいししですよね。",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "てんぷら",
            "text": "指の跡がついたベーグル、美味しかったです",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "げんまい",
            "text": "ベーグルベーグル好きです",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "おでん",
            "text": "@げんまい 何を食べられましたか？",
            "mention_to": [
                "げんまい"
            ]
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "てんぷら",
            "text": "@げんまい そうそう、朝食召し上がりましたか？",
            "mention_to": [
                "げんまい"
            ]
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "げんまい",
            "text": "今朝は、パスコのロールパンにハムとキャベツ挟みました",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "てんぷら",
            "text": "手作り3人組ですね",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "げんまい",
            "text": "ですね。手作り仲間",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "てんぷら",
            "text": "ベーグル温めるのも手作りのうちですから",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "おでん",
            "text": "美味しそうですね",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "おでん",
            "text": "あら、ありがとうございます笑",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "てんぷら",
            "text": "コーヒー淹れるのも手作りです",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "おでん",
            "text": "朝はパンでないと食べれないです",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "げんまい",
            "text": "もちろん、コーヒーだって手間がかかりますよ",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "てんぷら",
            "text": "ほんとです",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "げんまい",
            "text": "私もパンが好きー",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "げんまい",
            "text": "子どもはピザトーストでした",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "てんぷら",
            "text": "パン食、最高です",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "てんぷら",
            "text": "ピザトーストいいですね",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "おでん",
            "text": "パン美味しいですよね",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "てんぷら",
            "text": "私もコーヒーが好きなので",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "てんぷら",
            "text": "パンだとテンション上がりますが",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "おでん",
            "text": "ウンウン",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "てんぷら",
            "text": "おにぎりでもコーヒー飲みます",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "おでん",
            "text": "同じくです",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "げんまい",
            "text": "おにぎりに？",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "てんぷら",
            "text": "せんべいでもコーヒー飲みます",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "おでん",
            "text": "私もです",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "てんぷら",
            "text": "なんならポテトチップでもコーヒー飲みます",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "げんまい",
            "text": "あんことコーヒーの組み合わせは好きです",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "おでん",
            "text": "あんこと言えば、この前、おはぎを食べました",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "げんまい",
            "text": "おはぎ、いいですねー",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "てんぷら",
            "text": "おはぎにコーヒーも良いです",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "げんまい",
            "text": "コーヒーあいそう",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "てんぷら",
            "text": "おはぎが美味しいスーパーがあります",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "てんぷら",
            "text": "なんだっけ",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "てんぷら",
            "text": "ど忘れです",
            "mention_to": []
        },
        {
            "utterance_id": 103,
            "interlocutor_id": "おでん",
            "text": "@てんぷら そうなんですか？",
            "mention_to": [
                "てんぷら"
            ]
        },
        {
            "utterance_id": 104,
            "interlocutor_id": "てんぷら",
            "text": "ヤオコーだ",
            "mention_to": []
        }
