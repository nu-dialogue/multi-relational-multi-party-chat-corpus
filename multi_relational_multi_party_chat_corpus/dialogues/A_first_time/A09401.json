{
    "dialogue_id": "A09401",
    "dialogue_type": "First time",
    "interlocutors": [
        "らっこ",
        "みたらし",
        "はまち"
    ],
    "relationship": [],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "らっこ",
            "text": "こんにちはー",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "みたらし",
            "text": "こんばんは",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "らっこ",
            "text": "よろしくおねがいします。",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "みたらし",
            "text": "よろしくです",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "はまち",
            "text": "こんばんは",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "はまち",
            "text": "よろしくおねがいします",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "らっこ",
            "text": "今日、子どもの学校の卒業式だったんですけど",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "みたらし",
            "text": "おめでとうございます。",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "らっこ",
            "text": "ありがとうございます。",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "はまち",
            "text": "@らっこ それはおめでとうございます",
            "mention_to": [
                "らっこ"
            ]
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "らっこ",
            "text": "高校生でもけっこう保護者がきて",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "らっこ",
            "text": "ビックリしました",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "はまち",
            "text": "高校卒業だったんですね",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "みたらし",
            "text": "私も行く予定ですよ",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "らっこ",
            "text": "そうなんです。高校",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "みたらし",
            "text": "高校の",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "みたらし",
            "text": "卒業式",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "らっこ",
            "text": "@みたらし　卒業式！",
            "mention_to": [
                "みたらし"
            ]
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "らっこ",
            "text": "一緒ですね",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "みたらし",
            "text": "ですね",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "はまち",
            "text": "高校卒業というと",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "らっこ",
            "text": "土曜日だったからかな。",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "はまち",
            "text": "自立への一歩というか",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "みたらし",
            "text": "@らっこ 土曜日いいですね",
            "mention_to": [
                "らっこ"
            ]
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "はまち",
            "text": "節目な気もします",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "らっこ",
            "text": "そうですねー自立",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "らっこ",
            "text": "節目ですねー",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "みたらし",
            "text": "ですね",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "はまち",
            "text": "成人年齢も引き下げられたし",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "はまち",
            "text": "大人の仲間入りですね",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "らっこ",
            "text": "そうですねー",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "らっこ",
            "text": "成人。",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "みたらし",
            "text": "でも成人式は",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "らっこ",
            "text": "でも、お酒とかタバコはそのままだし、あんまり変わらない感じですね",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "みたらし",
            "text": "二十歳なんですよね。",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "はまち",
            "text": "ですよね",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "はまち",
            "text": "混乱の制度ですよね",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "みたらし",
            "text": "@らっこ でふ",
            "mention_to": [
                "らっこ"
            ]
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "らっこ",
            "text": "こちらの地域は、20歳ではたちのつどいです",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "みたらし",
            "text": "ですね",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "はまち",
            "text": "私の地域も20歳が節目みたいです",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "みたらし",
            "text": "みんな同じですね",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "らっこ",
            "text": "20歳から大人でいいような気がします",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "はまち",
            "text": "そう思います",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "らっこ",
            "text": "この前、一人でクレジットカードを契約してきて",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "みたらし",
            "text": "私もそう思います",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "らっこ",
            "text": "ビックリしました。",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "みたらし",
            "text": "おお",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "はまち",
            "text": "クレジットカード！",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "みたらし",
            "text": "もうできるんですね",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "らっこ",
            "text": "高校生でも作れるみたいです。",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "みたらし",
            "text": "時代ですね",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "らっこ",
            "text": "アルバイトしてるからかな？",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "はまち",
            "text": "自己責任といいつつ",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "はまち",
            "text": "親は心配ですね",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "らっこ",
            "text": "ちょっと怖いです。",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "みたらし",
            "text": "わかります",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "らっこ",
            "text": "これは借金なのよ！と",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "らっこ",
            "text": "話して聞かせました",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "みたらし",
            "text": "ですね",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "はまち",
            "text": "大事ですね",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "みたらし",
            "text": "遅延すると利子取られるし",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "らっこ",
            "text": "そうそう。よけいなお金が発生しますよね",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "みたらし",
            "text": "リボだけはやらないように",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "らっこ",
            "text": "リボ！それも言いました。",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "みたらし",
            "text": "言ってあげたほうがいいですよ",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "みたらし",
            "text": "おお",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "みたらし",
            "text": "さすが",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "らっこ",
            "text": "そしたら、学校で聞いたって。授業で。",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "みたらし",
            "text": "へー",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "らっこ",
            "text": "学校、すごいです。",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "はまち",
            "text": "おぉ！",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "みたらし",
            "text": "授業でやるんですね",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "らっこ",
            "text": "そうみたいなんです。",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "はまち",
            "text": "金融教育ってやつですかね？",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "らっこ",
            "text": "18才で成人だからかも。",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "みたらし",
            "text": "大事ですね",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "らっこ",
            "text": "詐欺の話とかも聞いてきて、",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "らっこ",
            "text": "へーって思ってます",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "はまち",
            "text": "詐欺！",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "みたらし",
            "text": "金融教育大事ですね",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "はまち",
            "text": "世の中、騙されそうな甘い罠いっぱいですよね",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "みたらし",
            "text": "ホントです",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "らっこ",
            "text": "フィッシングとか、サイトクリックしちゃうやつとか、いろいろ",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "らっこ",
            "text": "ありますからねー",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "はまち",
            "text": "ありますよね",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "みたらし",
            "text": "若い人を狙った詐欺も横行してますからね",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "はまち",
            "text": "ちゃんと見極められるか心配",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "らっこ",
            "text": "以外と、学校が面倒みよくてビックリ。",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "らっこ",
            "text": "そう。自分もあやしい。",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "らっこ",
            "text": "気を付けててもね",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "はまち",
            "text": "ですよね",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "みたらし",
            "text": "公立高校ですか？私立ですか？",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "みたらし",
            "text": "金融教育は？",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "らっこ",
            "text": "公立です",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "みたらし",
            "text": "おお",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "はまち",
            "text": "公立なんですね",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "みたらし",
            "text": "公立でやってくれるのは",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "らっこ",
            "text": "そうなんです。",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "みたらし",
            "text": "いいですね",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "らっこ",
            "text": "私立だと面倒見よさそうなイメージですけどね",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "らっこ",
            "text": "公立なので、期待してなかったです",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "みたらし",
            "text": "@らっこ ですよね",
            "mention_to": [
                "らっこ"
            ]
        },
        {
            "utterance_id": 103,
            "interlocutor_id": "はまち",
            "text": "学校みたいに第三者に言われる方が",
            "mention_to": []
        },
        {
            "utterance_id": 104,
            "interlocutor_id": "はまち",
            "text": "子どもは身に入りますよね",
            "mention_to": []
        },
        {
            "utterance_id": 105,
            "interlocutor_id": "らっこ",
            "text": "それ！",
            "mention_to": []
        },
        {
            "utterance_id": 106,
            "interlocutor_id": "はまち",
            "text": "親が言うよりも",
            "mention_to": []
        },
        {
            "utterance_id": 107,
            "interlocutor_id": "みたらし",
            "text": "ですね",
            "mention_to": []
        }
