{
    "dialogue_id": "A08505",
    "dialogue_type": "First time",
    "interlocutors": [
        "くまのみ",
        "ししとう",
        "みたらし"
    ],
    "relationship": [],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "くまのみ",
            "text": "こんばんは！",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "ししとう",
            "text": "こんばんは！",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "みたらし",
            "text": "こんばんは",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "くまのみ",
            "text": "今日、子供の体操教室にいってきました",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "みたらし",
            "text": "@くまのみ いいですね",
            "mention_to": [
                "くまのみ"
            ]
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "ししとう",
            "text": "@くまのみ お子さん体操教室行ってるんですね！",
            "mention_to": [
                "くまのみ"
            ]
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "くまのみ",
            "text": "子供のコーチは大学４回なんです",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "くまのみ",
            "text": "@ししとう そうなんです。結構やってます",
            "mention_to": [
                "ししとう"
            ]
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "くまのみ",
            "text": "コーチ、４月から就職なんですけど",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "ししとう",
            "text": "@くまのみ 大学生のコーチなんですね！",
            "mention_to": [
                "くまのみ"
            ]
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "みたらし",
            "text": "離れてしまうんですね",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "くまのみ",
            "text": "@みたらし さみしいです",
            "mention_to": [
                "みたらし"
            ]
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "くまのみ",
            "text": "@ししとう 最近多いんです",
            "mention_to": [
                "ししとう"
            ]
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "くまのみ",
            "text": "コーチ、４月から小学校の先生されるそうなんです",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "みたらし",
            "text": "いい先生なら残念な感じになりますね",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "みたらし",
            "text": "おお",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "みたらし",
            "text": "先生",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "ししとう",
            "text": "@くまのみ おぉ！先生！",
            "mention_to": [
                "くまのみ"
            ]
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "くまのみ",
            "text": "@みたらし すごくいいコーチです",
            "mention_to": [
                "みたらし"
            ]
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "くまのみ",
            "text": "教え方がうまいので、いい先生になると思います",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "みたらし",
            "text": "@くまのみ さみしくなりますね",
            "mention_to": [
                "くまのみ"
            ]
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "ししとう",
            "text": "そういう出会いってお別れがツライ！",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "みたらし",
            "text": "おお",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "くまのみ",
            "text": "@みたらし そうなんです。それに残念",
            "mention_to": [
                "みたらし"
            ]
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "みたらし",
            "text": "小学校でも人気でそう",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "ししとう",
            "text": "うんうん",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "くまのみ",
            "text": "でると思います",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "くまのみ",
            "text": "学校の先生ってブラックとかいわれてる中",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "くまのみ",
            "text": "志しはった事に感激でした",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "みたらし",
            "text": "小学校って特に大変ですよね",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "ししとう",
            "text": "@くまのみ そうですよね！",
            "mention_to": [
                "くまのみ"
            ]
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "くまのみ",
            "text": "@みたらし 大変ですよねー。朝から夜まで",
            "mention_to": [
                "みたらし"
            ]
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "みたらし",
            "text": "ですよね",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "ししとう",
            "text": "先生の仕事大変って聞きます",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "くまのみ",
            "text": "教育実習の最後、泣いたそうです",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "みたらし",
            "text": "でも一番楽しいっていいますよね",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "くまのみ",
            "text": "@みたらし いってる先生もいました",
            "mention_to": [
                "みたらし"
            ]
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "くまのみ",
            "text": "やっぱり子供達がかわいいって",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "ししとう",
            "text": "やりがいあるお仕事なんですね",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "くまのみ",
            "text": "@ししとう そう思ってないと出来ないと思います",
            "mention_to": [
                "ししとう"
            ]
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "みたらし",
            "text": "小学校1年生と6年生の落差が大きいのも魅力のひとつと聞いたことあります",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "くまのみ",
            "text": "公園で子供が喧嘩してるのに、呼び出されてましたよ、以前",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "みたらし",
            "text": "@くまのみ ",
            "mention_to": [
                "くまのみ"
            ]
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "くまのみ",
            "text": "@みたらし そうなんですね",
            "mention_to": [
                "みたらし"
            ]
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "みたらし",
            "text": "えー",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "ししとう",
            "text": "@みたらし 魅力なんですね",
            "mention_to": [
                "みたらし"
            ]
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "ししとう",
            "text": "@くまのみ わーケンカ…大変！",
            "mention_to": [
                "くまのみ"
            ]
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "くまのみ",
            "text": "小1と６年じゃ全然違いますもんね",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "みたらし",
            "text": "学校外のことまで呼び出しがかかるって大変だ―",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "くまのみ",
            "text": "@ししとう 高学年だったと思います",
            "mention_to": [
                "ししとう"
            ]
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "くまのみ",
            "text": "@みたらし びっくりしました。学校の近くの公園とはいえね",
            "mention_to": [
                "みたらし"
            ]
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "みたらし",
            "text": "@くまのみ ですよね",
            "mention_to": [
                "くまのみ"
            ]
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "みたらし",
            "text": "登下校中だったのかな？",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "くまのみ",
            "text": "子供の担任の先生、放課後教室で音楽かけながら採点してました",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "くまのみ",
            "text": "@みたらし 放課後です",
            "mention_to": [
                "みたらし"
            ]
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "ししとう",
            "text": "採点も大変ですよね",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "みたらし",
            "text": "いま",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "くまのみ",
            "text": "@ししとう たくさんありますもんね",
            "mention_to": [
                "ししとう"
            ]
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "みたらし",
            "text": "サポートの先生もいるから",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "くまのみ",
            "text": "けど音楽かけながらにちょっとびっくりしました",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "みたらし",
            "text": "採点くらいはサポートの人にやってもらった方がいいと思うんだけど難しいですね",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "くまのみ",
            "text": "@みたらし ほんとそう思います",
            "mention_to": [
                "みたらし"
            ]
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "ししとう",
            "text": "@みたらし そうですよね！",
            "mention_to": [
                "みたらし"
            ]
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "みたらし",
            "text": "私の小学校では職員室で煙草を吸いながら",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "くまのみ",
            "text": "後でチェックでいいと思うんですけど",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "みたらし",
            "text": "丸付けしているのを見たことありますよ",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "くまのみ",
            "text": "@みたらし 今ですか？　今の時代ですか？",
            "mention_to": [
                "みたらし"
            ]
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "ししとう",
            "text": "@みたらし タバコを！？",
            "mention_to": [
                "みたらし"
            ]
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "みたらし",
            "text": "私の子供時代ですが",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "みたらし",
            "text": "いまは無いですよね",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "くまのみ",
            "text": "@みたらし ですよね。",
            "mention_to": [
                "みたらし"
            ]
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "みたらし",
            "text": "いい時代になりました",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "ししとう",
            "text": "今は保護者が厳しいですよね",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "くまのみ",
            "text": "校内禁煙ですもんね。",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "くまのみ",
            "text": "タバコはご法度ですもんね",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "みたらし",
            "text": "ご法度ですね",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "ししとう",
            "text": "@くまのみ なってますよね",
            "mention_to": [
                "くまのみ"
            ]
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "くまのみ",
            "text": "昔、職場でもタバコすってはりました",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "くまのみ",
            "text": "仕事をしながら",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "くまのみ",
            "text": "いつから禁煙になったんでしょうね？",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "みたらし",
            "text": "確かに職場でも吸ってた時代ありますね",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "くまのみ",
            "text": "あの時、麻痺していて煙たくなかったです",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "みたらし",
            "text": "15年位前から徐々に厳しくなりましたよね",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "ししとう",
            "text": "分煙とか進みましたよね",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "くまのみ",
            "text": "15年前かぁ",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "くまのみ",
            "text": "@ししとう すすみましたね",
            "mention_to": [
                "ししとう"
            ]
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "みたらし",
            "text": "@くまのみ 私は煙草が嫌いだったのでほんとよかったと思ってます",
            "mention_to": [
                "くまのみ"
            ]
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "くまのみ",
            "text": "愛煙家の皆さまはかたみがせまいようですね",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "くまのみ",
            "text": "@みたらし よかったですね！　嫌いな人には地獄ですよね",
            "mention_to": [
                "みたらし"
            ]
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "みたらし",
            "text": "地獄でしたがどうやって乗り換えたか忘れました",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "ししとう",
            "text": "タバコもすごく高くなったみたいですもんね",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "みたらし",
            "text": "乗り越えた",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "くまのみ",
            "text": "@ししとう 高くてびっくりしますね",
            "mention_to": [
                "ししとう"
            ]
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "くまのみ",
            "text": "@みたらし ちょっとした事って忘れますよね",
            "mention_to": [
                "みたらし"
            ]
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "くまのみ",
            "text": "@ししとう でも夫買ってます",
            "mention_to": [
                "ししとう"
            ]
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "くまのみ",
            "text": "びっくりです",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "ししとう",
            "text": "@くまのみ おぉ、必需品なんですねれ",
            "mention_to": [
                "くまのみ"
            ]
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "くまのみ",
            "text": "@ししとう そうみたいです",
            "mention_to": [
                "ししとう"
            ]
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "くまのみ",
            "text": "友達の男子は40才超えてやめた子が多いです",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "みたらし",
            "text": "中毒性高いから1度吸っちゃうとやめるの大変見たいですよね",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "みたらし",
            "text": "おお",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "くまのみ",
            "text": "そしてマラソン始める人が多い",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "みたらし",
            "text": "やめれる人いるんですね",
            "mention_to": []
        },
        {
            "utterance_id": 103,
            "interlocutor_id": "ししとう",
            "text": "@くまのみ マラソン！",
            "mention_to": [
                "くまのみ"
            ]
        },
        {
            "utterance_id": 104,
            "interlocutor_id": "みたらし",
            "text": "健康でいいですね",
            "mention_to": []
        }
    ],
