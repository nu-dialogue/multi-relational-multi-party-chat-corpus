    ],
    "relationship": [],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "なると",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "いくら",
            "text": "こんにちはー",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "こぐま",
            "text": "こんにちわ！",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "なると",
            "text": "お願いします",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "いくら",
            "text": "よろしくお願いします！",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "こぐま",
            "text": "お願いします！",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "なると",
            "text": "寝る時に横向きでしか",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "こぐま",
            "text": "はい",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "なると",
            "text": "寝れないのですが",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "いくら",
            "text": "ほほう",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "なると",
            "text": "真っすぐ寝れますか？",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "こぐま",
            "text": "@なると そうなんですね",
            "mention_to": [
                "なると"
            ]
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "なると",
            "text": "@こぐま そうなんです",
            "mention_to": [
                "こぐま"
            ]
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "いくら",
            "text": "色んな向きで寝れます！",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "なると",
            "text": "@いくら いいですね！",
            "mention_to": [
                "いくら"
            ]
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "こぐま",
            "text": "私は元々上を向いて、寝る方が多かったけど",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "いくら",
            "text": "でも仰向けで寝ると、寝癖がすごいんですよね",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "いくら",
            "text": "横向きだと特に気にならないのですが",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "こぐま",
            "text": "確かに",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "なると",
            "text": "寝癖がつくんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "いくら",
            "text": "@こぐま 最近は仰向けではないんですか？",
            "mention_to": [
                "こぐま"
            ]
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "こぐま",
            "text": "今はなるべく横向きで",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "いくら",
            "text": "@なると はい、もうなんかすごいです。",
            "mention_to": [
                "なると"
            ]
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "こぐま",
            "text": "沢山寝返りするようにしてます",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "なると",
            "text": "どちらか向きは",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "なると",
            "text": "胃腸にいいとか聞きましたが",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "なると",
            "text": "@こぐま 寝返りは私も多いですね",
            "mention_to": [
                "こぐま"
            ]
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "いくら",
            "text": "@こぐま 寝返りするといいんですか？",
            "mention_to": [
                "こぐま"
            ]
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "こぐま",
            "text": "@なると あっ、聞いたことあります",
            "mention_to": [
                "なると"
            ]
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "なると",
            "text": "ですよね",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "いくら",
            "text": "胃腸！そうなんですね！",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "こぐま",
            "text": "少し前に",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "こぐま",
            "text": "目眩が酷くて",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "なると",
            "text": "はい",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "こぐま",
            "text": "見てもらった時に",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "こぐま",
            "text": "寝返りをするようにと言われました",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "いくら",
            "text": "目眩しんどいですよね",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "なると",
            "text": "そうなんですか！",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "こぐま",
            "text": "耳石が",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "いくら",
            "text": "@こぐま 寝返りすると変わるんですね！",
            "mention_to": [
                "こぐま"
            ]
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "なると",
            "text": "うんうん",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "こぐま",
            "text": "砕けて、目眩が起こるらしくて",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "なると",
            "text": "ええー",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "こぐま",
            "text": "寝返りすることで",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "こぐま",
            "text": "それが外に出るらしいんです",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "なると",
            "text": "外に？",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "なると",
            "text": "何か出てくるんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "こぐま",
            "text": "外？なのかな？",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "なると",
            "text": "へえ",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "こぐま",
            "text": "耳石を見たことはないのですが",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "いくら",
            "text": "人の体って凄いですよね",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "こぐま",
            "text": "目眩には寝返りらしいです",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "なると",
            "text": "初めて知りました",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "こぐま",
            "text": "お利口に寝ていてはダメだと言われました笑",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "なると",
            "text": "いいのかと思ってましたが",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "いくら",
            "text": "ダメなこともあるんですね！",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "なると",
            "text": "真っすぐ寝てる人が",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "こぐま",
            "text": "笑",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "なると",
            "text": "うらやましかったんですが",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "いくら",
            "text": "寝返りたくさんしてたら、疲れがとれなそうなきがしました！",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "なると",
            "text": "そうですよね",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "いくら",
            "text": "横向きの方がなんだか可愛らしくないですか？",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "こぐま",
            "text": "どうなんでしょうね、",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "なると",
            "text": "横向きだと",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "なると",
            "text": "巻き肩がひどくなるんです",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "いくら",
            "text": "あといびきとか、、",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "こぐま",
            "text": "@いくら 可愛い笑かもですね",
            "mention_to": [
                "いくら"
            ]
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "いくら",
            "text": "旦那は仰向けだとうるさいです。",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "いくら",
            "text": "@こぐま 想像で可愛いですよね！",
            "mention_to": [
                "こぐま"
            ]
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "こぐま",
            "text": "いびきかー",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "なると",
            "text": "いびきは仰向け？",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "こぐま",
            "text": "確かに仰向けだと、いびきかきそうですね",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "いくら",
            "text": "@なると 巻き肩？猫背とかと似てますか？",
            "mention_to": [
                "なると"
            ]
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "なると",
            "text": "そうです",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "なると",
            "text": "肩が痛くて…",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "こぐま",
            "text": "枕の高さとか",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "いくら",
            "text": "@なると いびきは横向きの方が空気の流れがいいとか、",
            "mention_to": [
                "なると"
            ]
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "こぐま",
            "text": "関係あるかもですね",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "なると",
            "text": "@こぐま 枕の高さ大事ですね",
            "mention_to": [
                "こぐま"
            ]
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "こぐま",
            "text": "@いくら 私もそんな気がします",
            "mention_to": [
                "いくら"
            ]
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "なると",
            "text": "横向きの人は",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "なると",
            "text": "いびきの人いなそうですね",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "こぐま",
            "text": "ね、そんな気がしますね",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "いくら",
            "text": "仰向けで背中にタオル入れて寝転がると気持ちいいですよね！",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "なると",
            "text": "ああ",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "なると",
            "text": "良さそうですね",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "こぐま",
            "text": "@いくら 寝る時に？",
            "mention_to": [
                "いくら"
            ]
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "いくら",
            "text": "仰向けも横向きもいいとこ悪いとこありますね",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "こぐま",
            "text": "そうですよね、",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "なると",
            "text": "全部できるのが",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "なると",
            "text": "いいんですかね",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "いくら",
            "text": "@こぐま そのまま寝ると辛そうですが、寝転がるくらいだとちょうどいいです！",
            "mention_to": [
                "こぐま"
            ]
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "いくら",
            "text": "となると、寝返りをたくさんですかね！",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "こぐま",
            "text": "子供のように、一晩で1周とか",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "いくら",
            "text": "なんだか落ち着かない気がします",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "なると",
            "text": "子供すごいですよね",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "いくら",
            "text": "だから子供は肩こりとかないんですかね！",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "こぐま",
            "text": "笑笑笑",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "いくら",
            "text": "息子、立ち上がって移動してまた寝るんですよ",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "こぐま",
            "text": "ねー、あんなに寝れる？って感じです",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "なると",
            "text": "さかさまになってる時も",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "いくら",
            "text": "ほんとたくさん寝ますよね！",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "なると",
            "text": "ありましたね",
            "mention_to": []
        },
        {
            "utterance_id": 103,
            "interlocutor_id": "こぐま",
            "text": "@いくら 笑息子さん、すごい！",
            "mention_to": [
                "いくら"
            ]
        }
