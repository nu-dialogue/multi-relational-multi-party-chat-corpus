        },
        {
            "utterance_id": 81,
            "interlocutor_id": "きなこ",
            "text": "ですよね",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "きなこ",
            "text": "絵のかきこみがすごいですよね",
