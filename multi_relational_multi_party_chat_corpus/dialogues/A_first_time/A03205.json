        },
        {
            "utterance_id": 83,
            "interlocutor_id": "きなこ",
            "text": "ジョジョも",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "まりも",
            "text": "6部はなんか不評？っぽいですけど",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "まりも",
            "text": "楽しんで見ました",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "きなこ",
            "text": "私も最後まで見てみます！",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "まりも",
            "text": "@きなこ たしかに描き込み系かも！",
            "mention_to": [
                "きなこ"
            ]
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "うどん",
            "text": "絵の書き込み、見入ってしまいます",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "きなこ",
            "text": "絵が好きな人はスキかも！？",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "まりも",
            "text": "絵に興味わいてから",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "まりも",
            "text": "好きになったかも　ジョジョ",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "まりも",
            "text": "それまではちょっと怖い作品でした",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "きなこ",
            "text": "以前、美術館でジョジョ展やってましたよね",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "きなこ",
            "text": "行ってみたかった",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "まりも",
            "text": "見てみたかった～",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "まりも",
            "text": "世界で人気ですよね",
