            "interlocutor_id": "まりも",
            "text": "こんにちは～",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "うどん",
            "text": "こんにちは〜",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "きなこ",
            "text": "こんにちは！",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "まりも",
            "text": "マンガとか読みますか？",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "うどん",
            "text": "読みます！",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "きなこ",
            "text": "最近時間がなくて・・",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "きなこ",
            "text": "オススメが知りたいです",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "うどん",
            "text": "@きなこ きのう何食べた？が好き！",
            "mention_to": [
                "きなこ"
            ]
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "まりも",
            "text": "私も最近積みがちではありますが",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "まりも",
            "text": "@きなこ どういう作品が好きですか？",
            "mention_to": [
                "きなこ"
            ]
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "きなこ",
            "text": "@うどん きのう何食べた？ですね！",
            "mention_to": [
                "うどん"
            ]
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "まりも",
            "text": "ドラマやってましたっけ？",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "うどん",
            "text": "@きなこ お料理がとにかく美味しそうです！",
            "mention_to": [
                "きなこ"
            ]
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "きなこ",
            "text": "@まりも 長らく読んでいなくて・・でもどんなジャンルも行けます",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "うどん",
            "text": "@まりも ドラマやってました！！",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "うどん",
            "text": "ドラマも好きで",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "きなこ",
            "text": "料理系いいですね",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "うどん",
            "text": "お料理試したくなります！",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "きなこ",
            "text": "有名なのですね！",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "まりも",
            "text": "@うどん 絵で言ったら森薫の乙嫁語りが好きです",
            "mention_to": [
                "うどん"
            ]
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "うどん",
            "text": "@まりも 調べてみます！",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "まりも",
            "text": "森薫、エマ描いてる頃から好きで",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "まりも",
            "text": "描き込み量が尋常じゃない",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "きなこ",
            "text": "私も調べてみよう！",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "まりも",
            "text": "背景もほとんど自分で描いてるとか",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "まりも",
            "text": "あと大量の馬とか羊とか",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "まりも",
            "text": "コミックスしょっちゅう出るわけじゃないけど",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "まりも",
            "text": "見るたびすごいなぁ…って思います",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "うどん",
            "text": "すてきな絵ですね！！",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "きなこ",
            "text": "ひえ～！全部自分で描いている作家さんなんですね",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "まりも",
            "text": "すごいんですよ～",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "うどん",
            "text": "すごいなぁ〜！",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "まりも",
            "text": "キャラクターもいろんなタイプの美人さんがいて楽しいです",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "きなこ",
            "text": "ちょっとみたらすごさわかりました！！",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "まりも",
            "text": "うわ～やった嬉しい～",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "うどん",
            "text": "読んでみたい！！",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "まりも",
            "text": "刊行ペースゆっくりなので",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "きなこ",
            "text": "今も続いている作品なんですね",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "まりも",
            "text": "追いやすいはず",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "まりも",
            "text": "まだ続いてますね～",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "まりも",
            "text": "最後に出たのいつだったろう",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "まりも",
            "text": "まあでも待つのは得意です",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "まりも",
            "text": "もし気に入ったらエマもぜひ…",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "まりも",
            "text": "メイドさんと貴族の身分違いの恋という",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "うどん",
            "text": "@まりも チェックしてみますね！！",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "きなこ",
            "text": "マンガは絵から入るほうなので",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "まりも",
            "text": "超王道なお話です",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "うどん",
            "text": "@まりも 気になります！！",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "きなこ",
            "text": "私もチェックします！",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "まりも",
            "text": "ぜひぜひ！",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "うどん",
            "text": "王道も好き！！",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "まりも",
            "text": "思いがけず布教ができた…",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "きなこ",
            "text": "笑いいですね！！",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "うどん",
            "text": "@まりも 教えてくださってありがとうございます！！",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "まりも",
            "text": "原作は読めてないけどジョジョも好きです",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "きなこ",
            "text": "私・・読みました",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "きなこ",
            "text": "ジョジョ",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "うどん",
            "text": "ジョジョ！",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "まりも",
            "text": "第6部のアニメをリアルタイムで見ました",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "うどん",
            "text": "人気ですよね！",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "まりも",
            "text": "@きなこ おぉ！？全部…ではないですか？",
            "mention_to": [
                "きなこ"
            ]
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "きなこ",
            "text": "若い時に読んだのと",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "きなこ",
            "text": "最近は子供がはまって",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "まりも",
            "text": "お子さんが！",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "うどん",
            "text": "わたしも読んでみよかな！！",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "きなこ",
            "text": "コロナ禍に第１部～6部途中までアニメ見ました",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "まりも",
            "text": "アニメほぼ全部じゃないですか",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "うどん",
            "text": "すごい！",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "きなこ",
            "text": "そうかも笑",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "まりも",
            "text": "好きなエピソードとかキャラクターとか",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "まりも",
            "text": "ありました？",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "きなこ",
            "text": "3部と5部がスキです",
