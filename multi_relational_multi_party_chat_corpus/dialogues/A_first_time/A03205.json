        },
        {
            "utterance_id": 72,
            "interlocutor_id": "まりも",
            "text": "あ～　5部好きです",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "きなこ",
            "text": "描写がグロいけど",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "きなこ",
            "text": "面白いですよね",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "まりも",
            "text": "そうですね～　そこが大丈夫なら",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "きなこ",
            "text": "子供は・・目を伏せがちでした",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "まりも",
            "text": "あらあら…",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "うどん",
            "text": "グロいの子どもさんはね...",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "まりも",
            "text": "6部のほうが描写はきつかったような",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "まりも",
            "text": "私も割とアワアワしながら見てました",
