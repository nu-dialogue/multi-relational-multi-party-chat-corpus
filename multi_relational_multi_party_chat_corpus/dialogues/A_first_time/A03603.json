        },
        {
            "utterance_id": 4,
            "interlocutor_id": "くらげ",
            "text": "よろしくお願いいたします。",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "つぶあん",
            "text": "よろしくお願いします。",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "つぶあん",
            "text": "昨日子供の参観でした。",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "くらげ",
            "text": "そういう時期ですよね",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "しらす",
            "text": "あら",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "しらす",
            "text": "うちあしたです",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "くらげ",
            "text": "私は先週でした",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "つぶあん",
            "text": "おお！",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "くらげ",
            "text": "@つぶあん どうでしたか",
            "mention_to": [
                "つぶあん"
            ]
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "つぶあん",
            "text": "この時期ですね！",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "つぶあん",
            "text": "小さいときの様子から",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "しらす",
            "text": "そんな時期ですね",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "つぶあん",
            "text": "こんなに成長したよ！",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "つぶあん",
            "text": "みたいな発表でした！",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "くらげ",
            "text": "ありますね！",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "くらげ",
            "text": "そういうの",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "つぶあん",
            "text": "成長を感じます！",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "しらす",
            "text": "@つぶあん 感動しそう",
            "mention_to": [
                "つぶあん"
            ]
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "つぶあん",
            "text": "感動ですね！",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "くらげ",
            "text": "10さいですか？",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "つぶあん",
            "text": "8さいです！",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "くらげ",
            "text": "失礼しました",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "くらげ",
            "text": "10さいって、二分の一成人式っていうので",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "くらげ",
            "text": "そういう発表あるんです",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "つぶあん",
            "text": "はいはい！",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "つぶあん",
            "text": "そうですね！",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "しらす",
            "text": "@くらげ そんなのあるんですね？",
            "mention_to": [
                "くらげ"
            ]
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "くらげ",
            "text": "@しらす あるんです",
            "mention_to": [
                "しらす"
            ]
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "しらす",
            "text": "うち7歳なので",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "くらげ",
            "text": "成長＋これから＋お母さんありがとう",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "くらげ",
            "text": "って感じで",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "しらす",
            "text": "知らないことばかりです",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "つぶあん",
            "text": "泣ける！",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "しらす",
            "text": "泣けますね",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "つぶあん",
            "text": "@くらげ 学校でですか？",
            "mention_to": [
                "くらげ"
            ]
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "くらげ",
            "text": "@つぶあん 意外となけなかったんです",
            "mention_to": [
                "つぶあん"
            ]
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "くらげ",
            "text": "@つぶあん そうです",
            "mention_to": [
                "つぶあん"
            ]
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "つぶあん",
            "text": "@くらげ そうなんですか？？",
            "mention_to": [
                "くらげ"
            ]
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "しらす",
            "text": "あら",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "くらげ",
            "text": "先生しだいだと思うんですが、みんなおんなじ感じで",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "つぶあん",
            "text": "@くらげ なるほど！",
            "mention_to": [
                "くらげ"
            ]
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "つぶあん",
            "text": "その子の個性が出てないとですかね。",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "くらげ",
            "text": "幼稚園が一番なけました",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "しらす",
            "text": "@くらげ 自分の言葉じゃないみたいな？",
            "mention_to": [
                "くらげ"
            ]
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "くらげ",
            "text": "@つぶあん そのとおり",
            "mention_to": [
                "つぶあん"
            ]
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "くらげ",
            "text": "@しらす そうそう",
            "mention_to": [
                "しらす"
            ]
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "しらす",
            "text": "幼稚園、泣けますね",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "つぶあん",
            "text": "先生次第ですね！",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "くらげ",
            "text": "@つぶあん ほんとに",
            "mention_to": [
                "つぶあん"
            ]
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "つぶあん",
            "text": "幼稚園泣けます。",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "しらす",
            "text": "昨年、子より泣きました笑",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "くらげ",
            "text": "一生懸命ですもんね、幼稚園の子",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "しらす",
            "text": "卒園式で",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "くらげ",
            "text": "@しらす わかる！！！",
            "mention_to": [
                "しらす"
            ]
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "つぶあん",
            "text": "@しらす 卒園の歌とか！",
            "mention_to": [
                "しらす"
            ]
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "くらげ",
            "text": "やりました！",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "つぶあん",
            "text": "内容泣けますよね。",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "しらす",
            "text": "@くらげ 子はしらっとしてて帰りたいってひたすら言ってました笑",
            "mention_to": [
                "くらげ"
            ]
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "くらげ",
            "text": "親のサプライズの歌で親が泣いてました",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "しらす",
            "text": "@つぶあん 歌、泣けました",
            "mention_to": [
                "つぶあん"
            ]
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "つぶあん",
            "text": "おお！",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "くらげ",
            "text": "@しらす シュールですね、お子さん　笑",
            "mention_to": [
                "しらす"
            ]
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "つぶあん",
            "text": "やはり、幼稚園ってあんなに小さかったのに",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "しらす",
            "text": "@くらげ 結構ドライです笑",
            "mention_to": [
                "くらげ"
            ]
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "くらげ",
            "text": "けど、８さいならまだ感動ですね、昨日",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "つぶあん",
            "text": "ってのがありますね！",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "つぶあん",
            "text": "感動ですね！",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "くらげ",
            "text": "うちの下の娘も早くかえろっていってました、そういえば",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "しらす",
            "text": "うちの子、明日何するんだろ？",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "しらす",
            "text": "教えてくれない笑",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "つぶあん",
            "text": "ドキドキですね！",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "くらげ",
            "text": "@しらす なぜ？",
            "mention_to": [
                "しらす"
            ]
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "くらげ",
            "text": "ドライだからですか　笑",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "つぶあん",
            "text": "一年生？",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "しらす",
            "text": "@しらす そうです笑",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "しらす",
            "text": "1年生です",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "くらげ",
            "text": "@しらす かわいいですね",
            "mention_to": [
