        },
        {
            "utterance_id": 83,
            "interlocutor_id": "つぶあん",
            "text": "楽しみですね！",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "くらげ",
            "text": "そうなんだ",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "つぶあん",
            "text": "可愛いです！",
            "mention_to": []
        },
        {
            "utterance_id": 86,
