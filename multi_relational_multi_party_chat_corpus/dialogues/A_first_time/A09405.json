{
    "dialogue_id": "A09405",
    "dialogue_type": "First time",
    "interlocutors": [
        "らっこ",
        "はまち",
        "みたらし"
    ],
    "relationship": [],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "らっこ",
            "text": "こんにちはー",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "はまち",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "みたらし",
            "text": "こんにちはー",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "らっこ",
            "text": "よろしくおねがいしますー",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "みたらし",
            "text": "よろしくお願いしますー",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "はまち",
            "text": "よろしくお願いします",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "らっこ",
            "text": "最近、楽器を１つ買いまして",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "はまち",
            "text": "楽器！",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "みたらし",
            "text": "@らっこ 何買ったのですか？",
            "mention_to": [
                "らっこ"
            ]
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "らっこ",
            "text": "楽器というかおもちゃなんですけど。",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "らっこ",
            "text": "オタマトーン",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "らっこ",
            "text": "知ってます？",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "はまち",
            "text": "初耳です",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "みたらし",
            "text": "初めてききました",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "らっこ",
            "text": "明和電機というへんてこりんな兄弟音楽ユニットが発明したものです",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "はまち",
            "text": "@らっこ 気になります",
            "mention_to": [
                "らっこ"
            ]
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "らっこ",
            "text": "ふふふ",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "らっこ",
            "text": "おもちゃなんですけど、なかなかおもしろくて",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "はまち",
            "text": "それは前から買おうと思って？",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "らっこ",
            "text": "前から、ほしくて",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "らっこ",
            "text": "でも、買う程のものでもないかなとか",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "みたらし",
            "text": "機械的な楽器ですか？",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "らっこ",
            "text": "電池で動きます。",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "らっこ",
            "text": "電子楽器",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "はまち",
            "text": "ちょっと検索してみたんですが",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "らっこ",
            "text": "種類もたくさんあって、",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "みたらし",
            "text": "ほうほう",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "らっこ",
            "text": "検索、出てきました？",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "はまち",
            "text": "見た目すごくかわいいやつですか？",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "らっこ",
            "text": "そうです。おたまじゃくしの見た目です",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "はまち",
            "text": "これかー！興味そそります",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "らっこ",
            "text": "口をね、パクパクすると、わわわわぁ～ってなるんです",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "らっこ",
            "text": "伝わるかなー",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "みたらし",
            "text": "音符の形なんですね",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "らっこ",
            "text": "そうそう。音符",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "らっこ",
            "text": "八分音符ですね。音符でいうと",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "はまち",
            "text": "かわいいですね",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "みたらし",
            "text": "曲は決まってるんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "らっこ",
            "text": "演奏がめちゃくちゃむずかしくて。。。。",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "みたらし",
            "text": "自分で作曲でするんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "らっこ",
            "text": "まともに弾ける曲がありません",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "はまち",
            "text": "難易度高いんですね",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "らっこ",
            "text": "ヴァイオリンみたいに、ちょっと指をずらずと音程が変わっちゃいます。",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "みたらし",
            "text": "おお",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "らっこ",
            "text": "ギターみたいな線がなくて",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "はまち",
            "text": "@らっこ おもしろそう",
            "mention_to": [
                "らっこ"
            ]
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "みたらし",
            "text": "自分でならすんですね",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "らっこ",
            "text": "カエルの歌を演奏しようと思ったら",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "らっこ",
            "text": "めちゃくちゃ音痴で。。。",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "らっこ",
            "text": "笑っちゃいます。",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "みたらし",
            "text": "味がでそう",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "はまち",
            "text": "音痴な楽器",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "らっこ",
            "text": "味、ですね。",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "はまち",
            "text": "かわいい…",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "らっこ",
            "text": "音痴な楽器。",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "らっこ",
            "text": "かわいいですよね。",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "らっこ",
            "text": "練習しようと思います。",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "みたらし",
            "text": "ちょっとやってみたいかも",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "らっこ",
            "text": "楽器、なにか経験ありますか？",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "みたらし",
            "text": "昔トロンボーンやってたんで",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "らっこ",
            "text": "へー",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "らっこ",
            "text": "トロンボーンも音程とるの難しそう。",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "みたらし",
            "text": "ちょっと音がずれるのは分かります。",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "はまち",
            "text": "@みたらし トロンボーンすごい",
            "mention_to": [
                "みたらし"
            ]
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "みたらし",
            "text": "トロンボーン面白いですよ",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "はまち",
            "text": "私はちょこっとピアノくらい",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "らっこ",
            "text": "@はまち ピアノ！",
            "mention_to": [
                "はまち"
            ]
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "みたらし",
            "text": "だからオタマトーン興味あります",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "らっこ",
            "text": "楽しいと思います。",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "らっこ",
            "text": "オタマトーン",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "みたらし",
            "text": "ピアノいいですね",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "はまち",
            "text": "全然ひけないんですけどね",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "みたらし",
            "text": "習いたかったです",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "みたらし",
            "text": "難しいですよね",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "らっこ",
            "text": "@はまち 子どもの頃に習ってたんですか？",
            "mention_to": [
                "はまち"
            ]
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "みたらし",
            "text": "指が動かなさそう",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "はまち",
            "text": "小5から習ってたんですが",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "らっこ",
            "text": "へー",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "みたらし",
            "text": "おあ",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "みたらし",
            "text": "おお",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "はまち",
            "text": "もう少し早いほうが上達したのかなと",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "らっこ",
            "text": "小さいころからはじめますよね。ピアノは。",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "みたらし",
            "text": "音楽は幼稚園くらいからといいますよね",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "らっこ",
            "text": "私は大人になってから、ピアノはじめましたよ。",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "はまち",
            "text": "ですよね",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "はまち",
            "text": "@らっこ おお！",
            "mention_to": [
                "らっこ"
            ]
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "らっこ",
            "text": "めちゃくちゃ楽しいです。",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "みたらし",
            "text": "@らっこ いいですね",
            "mention_to": [
                "らっこ"
            ]
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "はまち",
            "text": "@らっこ すごいです",
            "mention_to": [
                "らっこ"
            ]
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "みたらし",
            "text": "指うごきますか？",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "らっこ",
            "text": "動きません。",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "みたらし",
            "text": "なかなか難しいですよね",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "らっこ",
            "text": "でも、毎日練習していれば、進歩はしています。",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "みたらし",
            "text": "ショパンひきたいけど",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "はまち",
            "text": "練習あるのみですね",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "らっこ",
            "text": "ショパン、いいですね。",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "みたらし",
            "text": "絶対無理だっおもいます",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "みたらし",
            "text": "@らっこ ショパンいいですよね",
            "mention_to": [
                "らっこ"
            ]
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "らっこ",
            "text": "大人になって、進歩を感じるってあんまりないですけど、",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "らっこ",
            "text": "ピアノは上達するのがわかるので、楽しいです",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "みたらし",
            "text": "おお",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "はまち",
            "text": "@らっこ 素敵な趣味ですね",
            "mention_to": [
                "らっこ"
            ]
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "みたらし",
            "text": "ですね",
            "mention_to": []
        },
        {
            "utterance_id": 103,
            "interlocutor_id": "らっこ",
            "text": "何を目指すでもなく、自分のための趣味っていいです。",
            "mention_to": []
        },
        {
            "utterance_id": 104,
            "interlocutor_id": "はまち",
            "text": "@らっこ 私もそんな趣味持ちたいです",
            "mention_to": [
                "らっこ"
            ]
        }
