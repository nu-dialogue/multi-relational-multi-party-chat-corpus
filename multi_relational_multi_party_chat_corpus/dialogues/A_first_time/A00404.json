    "relationship": [],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "おにぎり",
            "text": "こんにちは。",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "げんまい",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "てんぷら",
            "text": "こんにちはー",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "おにぎり",
            "text": "皆さんは、出かけるのお好きですか？",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "てんぷら",
            "text": "好きです！",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "げんまい",
            "text": "わりと好きかな",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "てんぷら",
            "text": "おにぎりさんは？",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "おにぎり",
            "text": "どこ行くの好きですか？",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "おにぎり",
            "text": "私は、お家大好きなんです笑",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "てんぷら",
            "text": "そうなんですか！",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "げんまい",
            "text": "お家も好きですw",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "げんまい",
            "text": "でかけるなら美術館！",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "てんぷら",
            "text": "お、いいですね美術館",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "おにぎり",
            "text": "美術館、素敵！",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "てんぷら",
            "text": "近くにあるんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "げんまい",
            "text": "好きすぎて美術検定3級とりました",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "てんぷら",
            "text": "すごい！",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "おにぎり",
            "text": "そんな検定があるんですか？！",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "げんまい",
            "text": "上野の美術館によく行きます",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "おにぎり",
            "text": "絵についてとか学ぶのですか？",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "げんまい",
            "text": "そうそう。",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "おにぎり",
            "text": "知らなかったです。",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "げんまい",
            "text": "誰が描いたとか、時代とか、国とか",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "げんまい",
            "text": "世界史、日本史、地理の知識も必要で、、、",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "おにぎり",
            "text": "前からお好きなんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "げんまい",
            "text": "コロナ以降ですね",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "おにぎり",
            "text": "そうですよね、絵って歴史重要ですよね。。",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "てんぷら",
            "text": "あー、密じゃなさそうです美術館",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "げんまい",
            "text": "行く場所が制限されて、でも、美術館なら黙ってみるだけだしって",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "てんぷら",
            "text": "飛沫も飛びませんね",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "おにぎり",
            "text": "確かに。",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "げんまい",
            "text": "あと、YouTubeで西洋絵画の解説動画が面白かったのもきっかけです",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "おにぎり",
            "text": "描くのも好きですか？",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "てんぷら",
            "text": "おすすめのYouTuberききたいです",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "げんまい",
            "text": "描かないです。見るだけw",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "げんまい",
            "text": "西洋絵画は、山田五郎さんの動画が面白いです",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "てんぷら",
            "text": "やまだごろうさん",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "おにぎり",
            "text": "はいはい！",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "てんぷら",
            "text": "ご存じなの？！",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "おにぎり",
            "text": "YouTubeやってるんですね！",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "げんまい",
            "text": "有名な人ですよ。テレビにも出てる",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "おにぎり",
            "text": "アド街ック天国の人ですよね？！",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "てんぷら",
            "text": "そうなの！？",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "げんまい",
            "text": "そう。アド街",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "おにぎり",
            "text": "BSでおぎやはぎとやってましたよね。",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "おにぎり",
            "text": "美術館巡りみたいな。",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "げんまい",
            "text": "おにぎりさん、よくご存じですねー",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "おにぎり",
            "text": "たまーーーーに見てました笑",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "げんまい",
            "text": "解説が上手で引き込まれるんですよー",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "おにぎり",
            "text": "話上手ですよね。",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "てんぷら",
            "text": "へー、見てみよう",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "げんまい",
            "text": "てんぷらさんも、おにぎりさんも、お時間あったら検索して見てほしい",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "おにぎり",
            "text": "してみます！",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "おにぎり",
            "text": "もうすぐ春休みなので、どこか行きたいな、、と思って。",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "げんまい",
            "text": "家族でですか？",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "おにぎり",
            "text": "はい！家族で。",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "げんまい",
            "text": "じゃ、旅行かな？",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "おにぎり",
            "text": "長い事行っていないので、旅行いいですよね。",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "げんまい",
            "text": "行ったことないけど、軽井沢とか行ってみたいなー",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "おにぎり",
            "text": "軽井沢、いいですよね～",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "げんまい",
            "text": "行ったことあります？",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "おにぎり",
            "text": "意外とそちらからも近くないですか？",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "おにぎり",
            "text": "私は、2，3回ありますが、もう５，６年前かな。",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "げんまい",
            "text": "新幹線に乗ればすぐらしいですね。",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "おにぎり",
            "text": "そうそう。",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "おにぎり",
            "text": "春の軽井沢、良いかもです！",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "てんぷら",
            "text": "数年前に行ったことがありますが、すごくお洒落な街になっていました",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "げんまい",
            "text": "いいなー。オシャレなイメージです",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "おにぎり",
            "text": "星のや効果ですかね。",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "てんぷら",
            "text": "そうそう",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "おにぎり",
            "text": "のんびりお茶でもしたりして。。",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "てんぷら",
            "text": "お洒落カフェ、多かったです",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "おにぎり",
            "text": "本当は、四国行ってみたいんですよね。",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "てんぷら",
            "text": "ベーカリーも",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "てんぷら",
            "text": "四国いったことないな",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "げんまい",
            "text": "四国、いいですね",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "おにぎり",
            "text": "私も行ったことなくて。",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "げんまい",
            "text": "松山行ったことあります。道後温泉",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "げんまい",
            "text": "昔、OL時代に社員旅行で、",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "おにぎり",
            "text": "松山！いいな。",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "てんぷら",
            "text": "社員旅行って、懐かしいですね",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "げんまい",
            "text": "今、社員旅行なんてないでしょうね",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "おにぎり",
            "text": "社員旅行で、そんな遠くに行くんですね。",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "げんまい",
            "text": "あ、京都に住んでました。",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "おにぎり",
            "text": "久々に聞いたワードです笑",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "げんまい",
            "text": "昔",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "てんぷら",
            "text": "京都もいきたい",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "おにぎり",
            "text": "京都にお住まいで！",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "げんまい",
            "text": "はい。京都。",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "おにぎり",
            "text": "あこがれます、京都。",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "おにぎり",
            "text": "長い事行っていないです。",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "げんまい",
            "text": "よく言われますが、子どもの頃は遊ぶ場所がなくて、つまらんと思ってましたw",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "げんまい",
            "text": "今は、好きです。",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "おにぎり",
            "text": "おとなっぽすぎましたかね笑",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "てんぷら",
            "text": "確かにディズニーランドとかは誘致されなさそうです",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "げんまい",
            "text": "お寺と神社しかないですからね。",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "おにぎり",
            "text": "おすすめのスポットとかありますか？",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "げんまい",
            "text": "おすすめ、いっぱいありますｗ",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "てんぷら",
            "text": "あるんですね",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "げんまい",
            "text": "清水寺が好きで何回も行ったし",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "おにぎり",
            "text": "盛りだくさんですものね。",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "げんまい",
            "text": "二条城の狩野派の絵がすばらしくて、好きだし。",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "おにぎり",
            "text": "芸術だ。",
            "mention_to": []
        }
