    "relationship": [],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "あひる",
            "text": "こんばんは！",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "がんも",
            "text": "こんばんは！",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "ししとう",
            "text": "こんばんは！",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "あひる",
            "text": "みなさん最近ハマってることありますかー？",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "がんも",
            "text": "漫画！",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "あひる",
            "text": "おぉ！",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "がんも",
            "text": "それとアニメ！",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "ししとう",
            "text": "最近、鬼滅の刃を子どもとみてます！",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "がんも",
            "text": "鬼滅",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "あひる",
            "text": "アニメ人気！！",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "ししとう",
            "text": "@がんも どんなアニメとか漫画見ます？",
            "mention_to": [
                "がんも"
            ]
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "がんも",
            "text": "いいですね。",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "あひる",
            "text": "鬼滅わたしも好きですー！！",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "がんも",
            "text": "@ししとう 最近はフリーレンや俺だけレベルアップな件など",
            "mention_to": [
                "ししとう"
            ]
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "ししとう",
            "text": "@あひる そうなんですね！仲間！",
            "mention_to": [
                "あひる"
            ]
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "あひる",
            "text": "@ししとう いぇい！！！",
            "mention_to": [
                "ししとう"
            ]
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "ししとう",
            "text": "@がんも フリーレン！見てます！",
            "mention_to": [
                "がんも"
            ]
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "あひる",
            "text": "フリーレン！！",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "あひる",
            "text": "調べてみます",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "がんも",
            "text": "@ししとう 面白いですよね。",
            "mention_to": [
                "ししとう"
            ]
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "ししとう",
            "text": "@あひる オススメです！",
            "mention_to": [
                "あひる"
            ]
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "がんも",
            "text": "@あひる お勧めです。",
            "mention_to": [
                "あひる"
            ]
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "あひる",
            "text": "教えてくださってありがとうございます！！",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "あひる",
            "text": "きれいな絵！！",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "がんも",
            "text": "薬屋のひとりごともおススメです。",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "ししとう",
            "text": "世界観もすごく綺麗です！",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "あひる",
            "text": "@がんも お！そちらも調べてみます！",
            "mention_to": [
                "がんも"
            ]
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "ししとう",
            "text": "@がんも 薬屋！！",
            "mention_to": [
                "がんも"
            ]
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "あひる",
            "text": "@ししとう そうなんですね！！気になる！！！",
            "mention_to": [
                "ししとう"
            ]
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "ししとう",
            "text": "我が家も子どもと観てます。",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "あひる",
            "text": "子どもも大人も楽しめるってすごい！",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "ししとう",
            "text": "謎解き要素があっておもしろいですね！",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "がんも",
            "text": "@ししとう うちは夫婦でみてます。",
            "mention_to": [
                "ししとう"
            ]
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "あひる",
            "text": "@がんも すてき！！",
            "mention_to": [
                "がんも"
            ]
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "ししとう",
            "text": "@がんも いいですね！",
            "mention_to": [
                "がんも"
            ]
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "あひる",
            "text": "@がんも パートナーさんもアニメお好きなんですか？",
            "mention_to": [
                "がんも"
            ]
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "がんも",
            "text": "@あひる 夫婦そろってすきですね。",
            "mention_to": [
                "あひる"
            ]
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "あひる",
            "text": "@がんも 共通の好きなものがあるっていいですね！！",
            "mention_to": [
                "がんも"
            ]
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "あひる",
            "text": "憧れる！！",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "ししとう",
            "text": "@がんも 共通の話題あるってステキです！",
            "mention_to": [
                "がんも"
            ]
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "がんも",
            "text": "アニメが面白いと",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "がんも",
            "text": "漫画大人買いしちゃいます。",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "あひる",
            "text": "わかる！！",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "ししとう",
            "text": "@がんも 大人の特権ですね！",
            "mention_to": [
                "がんも"
            ]
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "あひる",
            "text": "漫画も見たくなっちゃう",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "がんも",
            "text": "特権ですね。",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "あひる",
            "text": "アニメだとカットされてる場面もあるのかなって思ったりして",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "ししとう",
            "text": "@あひる 確かに～",
            "mention_to": [
                "あひる"
            ]
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "がんも",
            "text": "うんうん。",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "あひる",
            "text": "ね〜！",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "あひる",
            "text": "より深く楽しみたくなっちゃう〜",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "ししとう",
            "text": "@あひる 好きな漫画やアニメあります？",
            "mention_to": [
                "あひる"
            ]
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "あひる",
            "text": "@ししとう 鬼滅か、きのう何食べた？ってやつが好きです！！",
            "mention_to": [
                "ししとう"
            ]
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "あひる",
            "text": "お料理漫画",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "ししとう",
            "text": "きのう何食べたってドラマ化もしてたやつですよね？",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "あひる",
            "text": "@ししとう ですです！ドラマやってた！",
            "mention_to": [
                "ししとう"
            ]
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "あひる",
            "text": "お料理があまりにも美味しそうで...",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "ししとう",
            "text": "お料理系の作品っていいですよね！",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "あひる",
            "text": "ですよね！",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "あひる",
            "text": "真似て作ってました〜！",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "ししとう",
            "text": "@あひる おお～",
            "mention_to": [
                "あひる"
            ]
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "がんも",
            "text": "いいですね。",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "あひる",
            "text": "それにほっこり系で癒された〜！",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "あひる",
            "text": "みなさんお料理します？",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "がんも",
            "text": "休みの日のします。",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "ししとう",
            "text": "得意ではないですが、",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "あひる",
            "text": "おぉ！",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "ししとう",
            "text": "子どもがいるのでやりますね",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "あひる",
            "text": "@ししとう 素敵ですね！！",
            "mention_to": [
                "ししとう"
            ]
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "あひる",
            "text": "得意料理はありますか？",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "ししとう",
            "text": "全然なくて…。",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "ししとう",
            "text": "でもハンバーグ好きって言ってくれるので",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "あひる",
            "text": "うんうん！",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "ししとう",
            "text": "それを得意料理ってことにしておきます",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "あひる",
            "text": "好きって言ってくれるなら、十分得意なんだと思います！！！",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "あひる",
            "text": "それに、",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "あひる",
            "text": "手作りってことが、すごく意味ありますよね！！",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "がんも",
            "text": "ですね。",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "ししとう",
            "text": "ありがとうございます～",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "ししとう",
            "text": "めんどくさがってましたが",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "ししとう",
            "text": "明日から頑張ろうと思えました！",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "ししとう",
            "text": "ふふふ",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "あひる",
            "text": "ぱちぱちぱち！！！！",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "あひる",
            "text": "お子さんも喜びますね！！！！",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "あひる",
            "text": "わたしは子どもの頃冷凍食品とか多かったから、",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "あひる",
            "text": "親御さんが作ってくれるって素敵だと思います！！",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "ししとう",
            "text": "@あひる そうなんですね！",
            "mention_to": [
                "あひる"
            ]
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "あひる",
            "text": "わたしもお料理がんばります！！",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "ししとう",
            "text": "@あひる 得意料理ありますか？",
            "mention_to": [
                "あひる"
            ]
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "あひる",
            "text": "茶碗蒸しと、炊き込みご飯が得意です！！",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "がんも",
            "text": "おおー。",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "ししとう",
            "text": "@あひる すご～い！",
            "mention_to": [
                "あひる"
            ]
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "ししとう",
            "text": "季節の具材入れれそうですね！",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "がんも",
            "text": "うんうん。",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "あひる",
            "text": "@ししとう たしかに！！季節感出しやすいですね！！",
            "mention_to": [
                "ししとう"
            ]
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "あひる",
            "text": "和食の特権かも！！",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "がんも",
            "text": "たしかに。",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "あひる",
            "text": "銀杏もらったので、銀杏入れようと思います！",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "ししとう",
            "text": "おお！銀杏！",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "あひる",
            "text": "春のものかわからないけど！笑",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "あひる",
            "text": "なんとなく秋っぽい",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "ししとう",
            "text": "たしかに秋のイメージ",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "がんも",
            "text": "笑笑",
            "mention_to": []
        },
        {
            "utterance_id": 103,
