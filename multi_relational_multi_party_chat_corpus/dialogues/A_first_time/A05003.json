    ],
    "relationship": [],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "つぶあん",
            "text": "こんにちは！",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "いくら",
            "text": "こんにちは！",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "しらす",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "しらす",
            "text": "よろしくお願いします",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "つぶあん",
            "text": "動物園や博物館など行ったりしますか？",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "いくら",
            "text": "近くに、無料の動物園があって",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "いくら",
            "text": "よく行きますね！",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "しらす",
            "text": "博物館はあまり行っていませんが",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "つぶあん",
            "text": "それはいい！",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "しらす",
            "text": "動物園は好きです",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "つぶあん",
            "text": "近くに無料は最高ですね！",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "しらす",
            "text": "無料いいですね",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "いくら",
            "text": "博物館、今度息子が行事で行くようで羨ましいです",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "いくら",
            "text": "無料最高ですよね！種類はそんなに多くないですが",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "つぶあん",
            "text": "学校行事でいきますよね！",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "いくら",
            "text": "羊とかモルモットとかいるんです",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "つぶあん",
            "text": "@いくら 触れますか？",
            "mention_to": [
                "いくら"
            ]
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "いくら",
            "text": "@つぶあん しかも恐竜博物館です、、",
            "mention_to": [
                "つぶあん"
            ]
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "いくら",
            "text": "すてき、！",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "つぶあん",
            "text": "@いくら 子供は好きですよね！",
            "mention_to": [
                "いくら"
            ]
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "いくら",
            "text": "モルモットなら触れます！大きいのは怖いですね",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "つぶあん",
            "text": "恐竜博物館ですか！",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "いくら",
            "text": "そうなんです！子供も喜びますよね",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "つぶあん",
            "text": "子供が恐竜大好きです。",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "しらす",
            "text": "恐竜、子ども喜びそうです",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "つぶあん",
            "text": "子供たちそういうところはよく歩くから運動にもなります！",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "いくら",
            "text": "トリケラトプスとか無駄に詳しくなりますよね",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "しらす",
            "text": "うちの近所、子どもが喜びそうな博物館ないですね",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "つぶあん",
            "text": "近くにあるといいですよね！",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "いくら",
            "text": "@つぶあん ですね！歩数計もたせて行きたいところですね",
            "mention_to": [
                "つぶあん"
            ]
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "しらす",
            "text": "羨ましいです",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "つぶあん",
            "text": "お手軽に生きたい！",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "つぶあん",
            "text": "大人の方が疲れてしまったり。",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "いくら",
            "text": "我が家も博物館は近くなくて、",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "いくら",
            "text": "芸術性を磨きたいですね",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "いくら",
            "text": "絵がとても下手なんですよね",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "しらす",
            "text": "@いくら 私もです",
            "mention_to": [
                "いくら"
            ]
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "つぶあん",
            "text": "好きですか？",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "しらす",
            "text": "絵心ないです",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "つぶあん",
            "text": "絵をかくの？",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "いくら",
            "text": "上手にかければ好きなのですが、なんせ上手くかけなくて",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "いくら",
            "text": "@しらす 難しいですよね、どうしたらかけるのか",
            "mention_to": [
                "しらす"
            ]
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "しらす",
            "text": "イラストとかサラッと描ける人羨ましいです",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "つぶあん",
            "text": "わかります！",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "いくら",
            "text": "子供とか喜びますよね！",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "つぶあん",
            "text": "はいはい！",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "いくら",
            "text": "もうアンパンマンとかと写して書いてあげてました",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "つぶあん",
            "text": "絶対いいですよね！",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "いくら",
            "text": "そのまま書いたら怖くなるので、、",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "つぶあん",
            "text": "それでも、全然いいですよ！",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "しらす",
            "text": "写し書きでもいいですよ",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "いくら",
            "text": "いいですかね、喜んではくれますね！",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "しらす",
            "text": "私、写し書きでも曲がりそう笑",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "いくら",
            "text": "いいって言ってもらえると嬉しいです。",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "つぶあん",
            "text": "下手でも一緒に笑ってしまうのはどうでしょう！",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "いくら",
            "text": "なぞるの難しいのもわかります",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "つぶあん",
            "text": "わたしは下手でも書きます。",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "つぶあん",
            "text": "変なの！",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "つぶあん",
            "text": "って感じです。",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "しらす",
            "text": "@つぶあん すごい!!",
            "mention_to": [
                "つぶあん"
            ]
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "いくら",
            "text": "@つぶあん 笑ってくれるくらい大きくなると楽しいのですが、小さいと悲しみでおわります、、",
            "mention_to": [
                "つぶあん"
            ]
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "いくら",
            "text": "下手だと、絵しりとり楽しいですよね！！！！！",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "つぶあん",
            "text": "なるほど！",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "つぶあん",
            "text": "小さいうちは似てないと",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "つぶあん",
            "text": "きびしいですか？",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "いくら",
            "text": "子供とか絵しりとりしたんですが、ちょー難問で面白かったです",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "いくら",
            "text": "小さいうちは伝わらなかったりしますね",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "つぶあん",
            "text": "絵しりとり楽しいです！",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "しらす",
            "text": "@いくら 絵しりとり楽しそう",
            "mention_to": [
                "いくら"
            ]
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "いくら",
            "text": "それでもアンパンマンだ！と言ってくれると嬉しくなります",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "いくら",
            "text": "絵しりとりオススメです！",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "つぶあん",
            "text": "下の子は声出しちゃってます。",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "しらす",
            "text": "今度やってみます",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "いくら",
            "text": "子供も最近ゲームばかりで書くことも減ってますよね",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "しらす",
            "text": "@いくら うちもゲームばかりです",
            "mention_to": [
                "いくら"
            ]
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "いくら",
            "text": "@つぶあん 答え言っちゃう感じですか？かわいい",
            "mention_to": [
                "つぶあん"
            ]
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "つぶあん",
            "text": "そうですね。",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "いくら",
            "text": "ゲームのほうが絶対楽しいですもんね",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "つぶあん",
            "text": "@いくら 答え言っちゃってます",
            "mention_to": [
                "いくら"
            ]
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "いくら",
            "text": "可愛すぎますね、、",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "つぶあん",
            "text": "ゲームも頭使いますよね！",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "いくら",
            "text": "や、違うんだよなー、、とか言っちゃいそうですわたし",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "いくら",
            "text": "結局普通のしりとりに、、",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "いくら",
            "text": "ゲームも使いますよね！マイクラとかすごいですよね！",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "つぶあん",
            "text": "@いくら ほんと、そうです！",
            "mention_to": [
                "いくら"
            ]
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "しらす",
            "text": "ゲーム頭使いますね",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "つぶあん",
            "text": "マイクラすきです！",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "しらす",
            "text": "うちもマイクラやってます",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "いくら",
            "text": "@つぶあん さんがマイクラ好きですか？",
            "mention_to": [
                "つぶあん"
            ]
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "しらす",
            "text": "私もハマりました笑",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "いくら",
            "text": "マイクラみんなやってますよね！",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "つぶあん",
            "text": "見るだけなのですが",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "いくら",
            "text": "しらすさんも！！",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "いくら",
            "text": "みなさんSwitchでされてます？",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "つぶあん",
            "text": "やはり、はまりますか",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "いくら",
            "text": "なんか抜け出せなくなりますよねマイクラ",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "つぶあん",
            "text": "うちはもってません。",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "しらす",
            "text": "息子Switch、私iPadです笑",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "つぶあん",
            "text": "すごい！",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "いくら",
            "text": "凄いですね！！",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "いくら",
            "text": "iPhoneのマイクラもいいですよね！",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "つぶあん",
            "text": "楽しいですよね。",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "いくら",
            "text": "同じ世界に入れることを知ってからさらに楽しくなりました！",
            "mention_to": []
        },
        {
            "utterance_id": 103,
            "interlocutor_id": "しらす",
            "text": "楽しいです",
            "mention_to": []
        },
        {
            "utterance_id": 104,
            "interlocutor_id": "つぶあん",
            "text": "見てるだけでも楽しいです。",
            "mention_to": []
        },
        {
            "utterance_id": 105,
            "interlocutor_id": "いくら",
            "text": "でも教えて貰ってばかりなんですよね子供に",
            "mention_to": []
        },
        {
            "utterance_id": 106,
            "interlocutor_id": "いくら",
            "text": "見てるだけでも楽しいですよね！！",
            "mention_to": []
        },
        {
            "utterance_id": 107,
            "interlocutor_id": "つぶあん",
            "text": "おお！",
            "mention_to": []
        },
        {
            "utterance_id": 108,
            "interlocutor_id": "しらす",
            "text": "@いくら うちもです",
            "mention_to": [
                "いくら"
            ]
        },
        {
            "utterance_id": 109,
            "interlocutor_id": "しらす",
            "text": "子が先生です",
            "mention_to": []
        },
        {
            "utterance_id": 110,
            "interlocutor_id": "つぶあん",
            "text": "子供は覚えが早いですね！",
            "mention_to": []
