    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "まりも",
            "text": "こんにちは!",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "きなこ",
            "text": "こんにちは！",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "うどん",
            "text": "こんにちは！",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "まりも",
            "text": "ちょっと美術系の話をしていいですか",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "うどん",
            "text": "はい！どうぞ",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "きなこ",
            "text": "どうぞ！",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "まりも",
            "text": "好きな画家とか絵とかあります？",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "うどん",
            "text": "わたしは日本の浮世絵とか好きです",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "まりも",
            "text": "へー！",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "まりも",
            "text": "最近よさそうなの見たけど",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "きなこ",
            "text": "浮世絵かあ！",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "まりも",
            "text": "名前が…",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "うどん",
            "text": "現物みたら、表現の繊細さが際立って",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "うどん",
            "text": "現場に行ってすごくよかったです！",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "まりも",
            "text": "今度よさそうなのあったら",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "まりも",
            "text": "行ってみます！",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "うどん",
            "text": "@まりも 名前出てこなくなっちゃいますよね...！",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "まりも",
            "text": "@うどん そうなんですよ～",
            "mention_to": [
                "うどん"
            ]
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "きなこ",
            "text": "あるある",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "まりも",
            "text": "雪景色の絵だったとは思うんですけど",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "うどん",
            "text": "雪景色の絵！",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "まりも",
            "text": "ちょっと印象派の影響受けてそうな感じの",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "うどん",
            "text": "浮世絵で印象に残ったのも雪景色の絵で",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "まりも",
            "text": "おぉ",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "うどん",
            "text": "雪景色すてきですね！",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "まりも",
            "text": "モネ展も",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "まりも",
            "text": "雪の詰みわらが見たくて",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "まりも",
            "text": "行きました",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "うどん",
            "text": "おぉー！",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "うどん",
            "text": "深いですね",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "まりも",
            "text": "使われてる色が白だけじゃなくて",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "まりも",
            "text": "黄色とかピンクとかオレンジとか",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "うどん",
            "text": "些細なところも注目してみると",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "うどん",
            "text": "発見が多くて嬉しい",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "まりも",
            "text": "あの色合いがすごく好きです",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "うどん",
            "text": "あ！わかる！",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "きなこ",
            "text": "お二人、とっても詳しいですね",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "うどん",
            "text": "一色に見えても、一色じゃないというか",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "まりも",
            "text": "@きなこ これでも美術展数えるほどしか行ったことないんですよ",
            "mention_to": [
                "きなこ"
            ]
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "うどん",
            "text": "@きなこ わたしも久しく行けてないなー！",
            "mention_to": [
                "きなこ"
            ]
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "うどん",
            "text": "春だし行きたい！",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "きなこ",
            "text": "お二人ともそうなんですか！！",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "まりも",
            "text": "だいぶ大人になってから",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "まりも",
            "text": "興味もちました",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "きなこ",
            "text": "今からでも・・大丈夫かな？",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "うどん",
            "text": "大丈夫！！！！",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "まりも",
            "text": "いつでも遅くないですよ",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "きなこ",
            "text": "何か行ってみたいと思います！",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "まりも",
            "text": "おぉ～",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "まりも",
            "text": "ウスター展であってたかな～…",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "まりも",
            "text": "国立新美術館？",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "きなこ",
            "text": "ウスターは人名ですか？",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "うどん",
            "text": "@まりも チェックします！",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "まりも",
            "text": "印象派って調べれば出てくると思うんですが",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "まりも",
            "text": "ウスターは美術館だったと思います",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "きなこ",
            "text": "@まりも ありがとうございます！",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "まりも",
            "text": "海外の特定の美術館から借りて",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "まりも",
            "text": "そこの名前が題名に入ってる感じの",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "うどん",
            "text": "@まりも そういうのいいですよね！豪華！",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "まりも",
            "text": "他にもおすすめはあるんですけどまた名前が…",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "きなこ",
            "text": "なるほど！",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "まりも",
            "text": "なんだったかな",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "まりも",
            "text": "だめだ",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "うどん",
            "text": "出てこないとき出てこないですよね！",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "きなこ",
            "text": "あります、そういうとき！",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "まりも",
            "text": "だめですね～思い出したい",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "うどん",
            "text": "庭園美術館とかも素敵でした！",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "まりも",
            "text": "ジョンエヴァレットミレイだ",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "まりも",
            "text": "思い出せた",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "うどん",
            "text": "おぉ！",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "まりも",
            "text": "庭園美術館？",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "うどん",
            "text": "調べてみます！",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "うどん",
            "text": "@まりも 高知のモネの庭とか！",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "まりも",
            "text": "露に濡れたハリエニシダって絵が",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "まりも",
            "text": "とてもきれいでした",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "うどん",
            "text": "ほぉー！",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "まりも",
            "text": "@うどん 高知にあるんですか！",
            "mention_to": [
                "うどん"
            ]
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "まりも",
            "text": "モネの庭、現地に行くしかないのかと",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "うどん",
            "text": "@まりも モネの庭があるのは、フランスと高知だけとか！",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "まりも",
            "text": "高知すごい",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "きなこ",
            "text": "そうなんですね！！",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "きなこ",
            "text": "すごい！",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "うどん",
            "text": "ちょっと行きづらいですが...",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "まりも",
            "text": "そうですねえ",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "うどん",
            "text": "睡蓮の絵とか再現されてて",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "きなこ",
            "text": "確かに・・",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "うどん",
            "text": "すごくよかったです！",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "まりも",
            "text": "うわ～",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "まりも",
            "text": "気になる～",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "うどん",
            "text": "絵も添えられてて",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "うどん",
            "text": "すてき！",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "きなこ",
            "text": "再現ってすごいですね～",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "うどん",
            "text": "ふじのはなが綺麗！",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "まりも",
            "text": "高知どう行くんだろう",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "まりも",
            "text": "空路でうまいこと行ければ",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "うどん",
            "text": "飛行機が良さそう！！",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "うどん",
            "text": "龍馬空港へ！",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "まりも",
            "text": "龍馬像があるんでしょうか？",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "うどん",
            "text": "ありました！！",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "きなこ",
            "text": "さすが、高知県！",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "まりも",
            "text": "へ～！",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "うどん",
            "text": "好きな都道府県ありますか？",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "まりも",
            "text": "難しい質問ですね！？",
            "mention_to": []
        },
        {
            "utterance_id": 103,
            "interlocutor_id": "まりも",
            "text": "おすすめしたいのは滋賀県ですが",
            "mention_to": []
        },
        {
            "utterance_id": 104,
            "interlocutor_id": "うどん",
            "text": "ほー！",
            "mention_to": []
        },
        {
            "utterance_id": 105,
            "interlocutor_id": "まりも",
            "text": "気になるのは北海道です",
            "mention_to": []
        },
        {
            "utterance_id": 106,
            "interlocutor_id": "きなこ",
            "text": "なるほど！",
            "mention_to": []
        },
        {
            "utterance_id": 107,
            "interlocutor_id": "うどん",
            "text": "北海道いいですね！",
            "mention_to": []
        },
        {
            "utterance_id": 108,
            "interlocutor_id": "まりも",
            "text": "滋賀県去年遊びに行って",
            "mention_to": []
        },
        {
            "utterance_id": 109,
            "interlocutor_id": "まりも",
            "text": "琵琶湖がでかすぎて",
            "mention_to": []
        },
        {
            "utterance_id": 110,
            "interlocutor_id": "うどん",
            "text": "わたしも行きました！",
            "mention_to": []
        },
        {
            "utterance_id": 111,
            "interlocutor_id": "うどん",
            "text": "滋賀県！",
            "mention_to": []
        },
        {
            "utterance_id": 112,
            "interlocutor_id": "まりも",
            "text": "あら！",
            "mention_to": []
        },
        {
            "utterance_id": 113,
            "interlocutor_id": "まりも",
            "text": "だいたい行ったことないって言われます",
            "mention_to": []
        },
        {
            "utterance_id": 114,
            "interlocutor_id": "まりも",
            "text": "びわこバレイって",
            "mention_to": []
        },
        {
            "utterance_id": 115,
            "interlocutor_id": "うどん",
            "text": "琵琶湖のほとりのアクアポニックス農場で、ピザ焼いたりしました！",
            "mention_to": []
        },
        {
            "utterance_id": 116,
            "interlocutor_id": "まりも",
            "text": "ロープウェイで昇ったんですけど",
            "mention_to": []
        },
        {
            "utterance_id": 117,
            "interlocutor_id": "きなこ",
            "text": "良さそうですね～",
            "mention_to": []
        },
        {
            "utterance_id": 118,
            "interlocutor_id": "まりも",
            "text": "ピザ！いいですね～",
            "mention_to": []
        },
        {
            "utterance_id": 119,
            "interlocutor_id": "うどん",
            "text": "びわこバレイ！",
            "mention_to": []
        },
        {
            "utterance_id": 120,
            "interlocutor_id": "うどん",
            "text": "よさそう！",
            "mention_to": []
        },
        {
            "utterance_id": 121,
            "interlocutor_id": "まりも",
            "text": "すごく眺めがよくて",
            "mention_to": []
        },
        {
            "utterance_id": 122,
            "interlocutor_id": "まりも",
            "text": "満足度高いです",
            "mention_to": []
        },
        {
            "utterance_id": 123,
            "interlocutor_id": "うどん",
            "text": "ピザ、手作りで釜焼きでよかったです！",
            "mention_to": []
        },
        {
            "utterance_id": 124,
            "interlocutor_id": "まりも",
            "text": "ロープウェィ速すぎてちょっと怖いですが",
            "mention_to": []
        },
        {
            "utterance_id": 125,
            "interlocutor_id": "うどん",
            "text": "@まりも 行ってみます！！",
            "mention_to": [
                "まりも"
            ]
        },
        {
