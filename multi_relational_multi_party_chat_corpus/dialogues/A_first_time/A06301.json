{
    "dialogue_id": "A06301",
    "dialogue_type": "First time",
    "interlocutors": [
        "ししとう",
        "げんまい",
        "きりん"
    ],
    "relationship": [],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "ししとう",
            "text": "こんにちはー",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "げんまい",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "きりん",
            "text": "こんにちは！",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "げんまい",
            "text": "よろしくお願いします",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "きりん",
            "text": "よろしくです",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "ししとう",
            "text": "お願いしまーす",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "ししとう",
            "text": "今日は少し寒いですね",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "げんまい",
            "text": "朝から寒いですー",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "きりん",
            "text": "ですね",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "げんまい",
            "text": "足先が冷えます",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "きりん",
            "text": "昨日から急に寒くて",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "ししとう",
            "text": "@げんまい わかります",
            "mention_to": [
                "げんまい"
            ]
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "きりん",
            "text": "足ひえひえになりますね",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "ししとう",
            "text": "@きりん うんうん",
            "mention_to": [
                "きりん"
            ]
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "げんまい",
            "text": "一時期、暖かかったので",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "げんまい",
            "text": "このまま春になったらいいなーって。思いました",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "ししとう",
            "text": "@げんまい そうそう！",
            "mention_to": [
                "げんまい"
            ]
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "きりん",
            "text": "@げんまい 本当に",
            "mention_to": [
                "げんまい"
            ]
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "ししとう",
            "text": "また冬に戻りましたね",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "げんまい",
            "text": "ですね。",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "げんまい",
            "text": "まだしばらく冬です。",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "ししとう",
            "text": "寒いの苦手だから",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "げんまい",
            "text": "寒いのは嫌です。",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "きりん",
            "text": "まだ2月ですもんね",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "ししとう",
            "text": "早く暖かくならないかなー",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "げんまい",
            "text": "指先が冷えるので、暖かいマウス使ってます",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "きりん",
            "text": "暖かいマウス？",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "げんまい",
            "text": "そう。",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "げんまい",
            "text": "ＰＣのマウス",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "きりん",
            "text": "よさそうですね",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "ししとう",
            "text": "@げんまい え！あたたかいマウス！",
            "mention_to": [
                "げんまい"
            ]
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "きりん",
            "text": "初めて知りました",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "ししとう",
            "text": "興味津々です",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "げんまい",
            "text": "有線のＵＳＢで暖かいくなります。",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "ししとう",
            "text": "@げんまい おぉ！いいですね！",
            "mention_to": [
                "げんまい"
            ]
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "げんまい",
            "text": "あったかマウス　で検索です！",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "きりん",
            "text": "ちょっといいな…",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "きりん",
            "text": "後で調べてみます",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "ししとう",
            "text": "調べてみます！",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "げんまい",
            "text": "この冬、大活躍しました",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "ししとう",
            "text": "パソコンしてると",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "ししとう",
            "text": "手が冷えますもんね",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "げんまい",
            "text": "うんうん",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "げんまい",
            "text": "キーボーとか冷たいです",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "きりん",
            "text": "あったかいキーボード",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "きりん",
            "text": "ないんですかね",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "げんまい",
            "text": "それ、あったら欲しいです",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "ししとう",
            "text": "商品化希望します",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "げんまい",
            "text": "どういう仕組みか想像できないけど、あったらいいな",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "ししとう",
            "text": "寒いと仕事効率落ちちゃいますよね",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "きりん",
            "text": "キー温めるの難しそうですね",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "きりん",
            "text": "言っておいてなんですが",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "げんまい",
            "text": "暖かいマウスは、線をはずすとカイロとしても持ち運べます。",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "ししとう",
            "text": "@げんまい なんと！",
            "mention_to": [
                "げんまい"
            ]
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "げんまい",
            "text": "持ち歩いたことはないけど。",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "ししとう",
            "text": "そんな便利なアイテムなんですねー！",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "きりん",
            "text": "むしろカイロにマウスの機能がついている…？",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "げんまい",
            "text": "たぶん1時間ぐらいしかバッテリーが持たないと思います。",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "げんまい",
            "text": "カイロにマウスの機能ｗｗｗ",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "げんまい",
            "text": "そうかもしれません。",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "ししとう",
            "text": "@げんまい あー1時間ぐらいかぁ〜",
            "mention_to": [
                "げんまい"
            ]
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "きりん",
            "text": "1時間だとだいぶ短いですね",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "げんまい",
            "text": "短いです。",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "ししとう",
            "text": "ちょっとそこまで用ですね",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "きりん",
            "text": "お散歩用ですかね",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "きりん",
            "text": "マウスを持って",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "ししとう",
            "text": "@きりん ふふふ",
            "mention_to": [
                "きりん"
            ]
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "ししとう",
            "text": "想像するとかわいい。",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "きりん",
            "text": "@ししとう 微笑んじゃいますね",
            "mention_to": [
                "ししとう"
            ]
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "ししとう",
            "text": "アイデア商品いろいろありますよね！",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "ししとう",
            "text": "でも買って満足してしまうものもいっぱい〜",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "げんまい",
            "text": "ですね",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "きりん",
            "text": "何か持ってますか？便利な商品",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "ししとう",
            "text": "料理グッズになりますが",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "げんまい",
            "text": "ほう",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "ししとう",
            "text": "みじん切りができるやつとか",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "きりん",
            "text": "そういうやつですね！",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "げんまい",
            "text": "あ、テレビでみたことかあるかも",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "ししとう",
            "text": "洗い物が面倒くさくて",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "げんまい",
            "text": "家事やろうで",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "げんまい",
            "text": "あー洗うのかー",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "ししとう",
            "text": "ずっと封印してます",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "きりん",
            "text": "楽しそうですが",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "げんまい",
            "text": "楽しそうですよね",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "きりん",
            "text": "確かに洗うのは面倒そうですね",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "ししとう",
            "text": "使い終わったあとの洗い物がね",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "げんまい",
            "text": "パーツが多いのかな？",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "ししとう",
            "text": "私が持ってたのは",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "ししとう",
            "text": "切った残骸が洗いにくかったです",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "ししとう",
            "text": "こびりついちゃって",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "きりん",
            "text": "落ちにくいってことですか？",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "ししとう",
            "text": "そうそう！",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "きりん",
            "text": "あぁ…面倒ですね",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "ししとう",
            "text": "分解できないやつでした",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "げんまい",
            "text": "あら",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "ししとう",
            "text": "失敗しちゃったー",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "げんまい",
            "text": "わたし、ミートソース作る時は、フードプロセッサーを使ってますよ。",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "ししとう",
            "text": "@げんまい フードプロセッサー！",
            "mention_to": [
                "げんまい"
            ]
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "ししとう",
            "text": "便利ですね！",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "げんまい",
            "text": "洗うの面倒ですけど、一気にみじん切りができて",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "げんまい",
            "text": "便利です",
            "mention_to": []
        }
