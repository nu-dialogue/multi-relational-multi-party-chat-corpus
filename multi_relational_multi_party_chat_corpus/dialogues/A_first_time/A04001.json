        },
        {
            "utterance_id": 2,
            "interlocutor_id": "たらこ",
            "text": "こんばんは",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "あずき",
            "text": "今日は寒かったですね〜",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "うどん",
            "text": "ほんとですね！",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "たらこ",
            "text": "寒かったですねー",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "うどん",
            "text": "昨日まで暖かかったのに",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "あずき",
            "text": "思わずダウンを着ました",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "たらこ",
            "text": "温度差すごかったですね",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "うどん",
            "text": "わたしもダウン！",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "あずき",
            "text": "冬に逆戻りですね",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "うどん",
            "text": "ほんとですね",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "たらこ",
            "text": "この寒さ",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "たらこ",
            "text": "いつまで続くんですかね",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "うどん",
            "text": "風邪ひいてませんか？",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "あずき",
            "text": "@うどん 今のところ",
            "mention_to": [
                "うどん"
            ]
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "あずき",
            "text": "大丈夫です",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "うどん",
            "text": "よかった！",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "あずき",
            "text": "@うどん どうですけ？",
            "mention_to": [
                "うどん"
            ]
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "うどん",
            "text": "早く暖かくなるといいですね",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "たらこ",
            "text": "今んとこひいてないです！",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "あずき",
            "text": "@たらこ よかった！！",
            "mention_to": [
                "たらこ"
            ]
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "あずき",
            "text": "早く暖かくなってほしい",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "うどん",
            "text": "@あずき 今のところ大丈夫です！",
            "mention_to": [
                "あずき"
            ]
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "あずき",
            "text": "@うどん よかったです",
            "mention_to": [
                "うどん"
            ]
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "あずき",
            "text": "気温差が激しいので",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "あずき",
            "text": "体調管理難しいですね",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "うどん",
            "text": "そうなんですよね",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "たらこ",
            "text": "@あずき 確かに！",
            "mention_to": [
                "あずき"
            ]
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "あずき",
            "text": "何か気をつけてる",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "あずき",
            "text": "ことってあります？",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "うどん",
            "text": "湯船に浸かって、睡眠ちゃんととる！",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "うどん",
            "text": "ようにしてます",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "あずき",
            "text": "@うどん 確かに！",
            "mention_to": [
                "うどん"
            ]
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "たらこ",
            "text": "@うどん 一緒です！",
            "mention_to": [
                "うどん"
            ]
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "あずき",
            "text": "湯船に浸かるの大事ですね",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "あずき",
            "text": "温まるし",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "うどん",
            "text": "ですね！",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "あずき",
            "text": "睡眠もですね",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "あずき",
            "text": "冬は特に眠い",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "うどん",
            "text": "眠いですよね！",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "たらこ",
            "text": "@あずき わかります",
            "mention_to": [
                "あずき"
            ]
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "あずき",
            "text": "疲れた時は、まず寝ます",
