        },
        {
            "utterance_id": 43,
            "interlocutor_id": "たらこ",
            "text": "@あずき いいですね！",
            "mention_to": [
                "あずき"
            ]
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "うどん",
            "text": "疲れたら風邪ひきやすいですしね！",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "あずき",
            "text": "私は甘酒飲んでます",
