        },
        {
            "utterance_id": 48,
            "interlocutor_id": "うどん",
            "text": "@あずき 甘酒いいですね！！",
            "mention_to": [
                "あずき"
            ]
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "たらこ",
