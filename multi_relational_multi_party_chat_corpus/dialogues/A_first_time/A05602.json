    "relationship": [],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "まぐろ",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "ねぎとろ",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "はくまい",
            "text": "こんにちは！",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "はくまい",
            "text": "よろしくです",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "まぐろ",
            "text": "よろしくです！",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "ねぎとろ",
            "text": "よろしくです～",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "まぐろ",
            "text": "地震や災害の備えってしてます？？",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "まぐろ",
            "text": "やらなきゃと思って・・",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "ねぎとろ",
            "text": "ゆるーくしかしてないです。",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "はくまい",
            "text": "思ってみれば",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "ねぎとろ",
            "text": "うんうん",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "はくまい",
            "text": "あんまり準備らしきものはしてないですね",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "ねぎとろ",
            "text": "そうなんですよね。",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "まぐろ",
            "text": "やはりそうなりますよね～",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "まぐろ",
            "text": "私も何から手を付けたらよいか・・・",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "ねぎとろ",
            "text": "わかります。",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "はくまい",
            "text": "いつ来るかわからないもので",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "はくまい",
            "text": "想像しづらいんですよね",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "ねぎとろ",
            "text": "何年か前に大地震ありましたが、なんとかなったので",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "まぐろ",
            "text": "え！！",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "まぐろ",
            "text": "大変でしたね",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "ねぎとろ",
            "text": "いろいろ手助けしてくださる方達のおかげで",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "はくまい",
            "text": "@ねぎとろ なにか準備しててよかったものありますか？",
            "mention_to": [
                "ねぎとろ"
            ]
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "ねぎとろ",
            "text": "ありがたいです。",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "まぐろ",
            "text": "知りたいです",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "はくまい",
            "text": "@ねぎとろ なるほど、支援物資でなんとかなったんですか",
            "mention_to": [
                "ねぎとろ"
            ]
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "ねぎとろ",
            "text": "@はくまい やっぱりお水は大事でした。",
            "mention_to": [
                "はくまい"
            ]
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "ねぎとろ",
            "text": "いろんな意味で",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "まぐろ",
            "text": "生の声、重要！お水ですね",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "はくまい",
            "text": "やっぱり水が一番大事ですね",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "まぐろ",
            "text": "かなりたくさん必要ですよね。",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "ねぎとろ",
            "text": "あと、備えられませんが、ガソリンは困りました。",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "ねぎとろ",
            "text": "車の",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "はくまい",
            "text": "お風呂も入れるだけありがたいですもんね",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "まぐろ",
            "text": "ガソリンかあ。確かに！",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "ねぎとろ",
            "text": "供給がなくて、給油出来ませんでした。",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "まぐろ",
            "text": "本当に困りますよね・・",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "はくまい",
            "text": "車中泊なら重要ですね",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "ねぎとろ",
            "text": "そうなんです。",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "ねぎとろ",
            "text": "まさに何日か車中泊でしたので。",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "はくまい",
            "text": "ガソリンで思い出したのですが",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "ねぎとろ",
            "text": "うんうん",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "はくまい",
            "text": "スーパーカブが",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "はくまい",
            "text": "災害の時にすごく便利",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "まぐろ",
            "text": "そうなんですね！",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "ねぎとろ",
            "text": "そうなんですか！",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "はくまい",
            "text": "という話を思い出しました",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "まぐろ",
            "text": "バイクですよね",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "はくまい",
            "text": "バイクです",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "はくまい",
            "text": "燃費がいいのでガス欠になることもないし",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "ねぎとろ",
            "text": "なぜですか？優位性は？",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "ねぎとろ",
            "text": "なるほど！",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "ねぎとろ",
            "text": "燃費がいいんですね",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "まぐろ",
            "text": "なるほど～！！",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "はくまい",
            "text": "あとは",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "ねぎとろ",
            "text": "うんうん",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "はくまい",
            "text": "未舗装路でも結構走れてしまうので",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "ねぎとろ",
            "text": "すごい！",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "まぐろ",
            "text": "それは大きい！",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "はくまい",
            "text": "瓦礫があっても進めるらしいです",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "ねぎとろ",
            "text": "知らなかった",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "はくまい",
            "text": "あとは積載量もかなりあるので",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "ねぎとろ",
            "text": "なるほど",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "はくまい",
            "text": "ぜひ1家に一台！",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "ねぎとろ",
            "text": "ですね！笑",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "まぐろ",
            "text": "いいですね笑！",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "まぐろ",
            "text": "かなり参考になります！！",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "ねぎとろ",
            "text": "スーパーカブ、やりますね。",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "はくまい",
            "text": "はーい",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "まぐろ",
            "text": "そうだ",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "はくまい",
            "text": "すごいですよねー",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "ねぎとろ",
            "text": "実用性たかい！",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "はくまい",
            "text": "@まぐろ なんでしょうか",
            "mention_to": [
                "まぐろ"
            ]
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "まぐろ",
            "text": "私はトイレの心配ばかりしていて",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "ねぎとろ",
            "text": "わかります！",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "まぐろ",
            "text": "簡易トイレだけ用意してます笑",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "はくまい",
            "text": "災害時ですよね",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "まぐろ",
            "text": "そうです！",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "まぐろ",
            "text": "災害時用",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "ねぎとろ",
            "text": "地震の時、支援物資でいただきました。",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "まぐろ",
            "text": "@ねぎとろ よかったですね～！",
            "mention_to": [
                "ねぎとろ"
            ]
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "はくまい",
            "text": "ほうほう",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "ねぎとろ",
            "text": "@まぐろ まだ使ったことないんですけど。",
            "mention_to": [
                "まぐろ"
            ]
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "ねぎとろ",
            "text": "あると安心",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "まぐろ",
            "text": "使わないで済む方がいいですから！",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "ねぎとろ",
            "text": "ですね！",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "はくまい",
            "text": "ですよねー",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "まぐろ",
            "text": "とりあえず、水の用意考えてみます！",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "ねぎとろ",
            "text": "ですね。",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "ねぎとろ",
            "text": "お水だけはマメにやってます。",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "はくまい",
            "text": "どれくらいあれば足りるんでしょうかね",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "まぐろ",
            "text": "@はくまい 想像できないですよね",
            "mention_to": [
                "はくまい"
            ]
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "はくまい",
            "text": "ですよね",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "まぐろ",
            "text": "場所も取りそうだし。賞味期限も気になって・・",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "ねぎとろ",
            "text": "ポリタンク3つくらいとペットに何本か用意してます。",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "はくまい",
            "text": "家族がいたらその家族分は絶対入りますもんね",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "ねぎとろ",
            "text": "結局初期用にしかなりませんが。",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "まぐろ",
            "text": "想像より多かった！",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "はくまい",
            "text": "@ねぎとろ 物資が届くまではある分しか使えませんもんね",
            "mention_to": [
                "ねぎとろ"
            ]
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "ねぎとろ",
            "text": "ポリタンクは重いから、入れ替えとかも大変なので、ペットボトルのほうがいいかもです。",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "まぐろ",
            "text": "そっかー。入れ替えは大変そう",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "ねぎとろ",
            "text": "@はくまい そうなんですよね。",
            "mention_to": [
                "はくまい"
            ]
        }
