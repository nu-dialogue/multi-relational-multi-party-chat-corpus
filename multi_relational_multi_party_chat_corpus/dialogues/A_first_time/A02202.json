        },
        {
            "utterance_id": 63,
            "interlocutor_id": "はくさい",
            "text": "やめられない",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "げんまい",
            "text": "永遠に出来ちゃうやつですね。",
            "mention_to": []
        },
        {
            "utterance_id": 65,
