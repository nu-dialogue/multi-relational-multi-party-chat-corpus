        },
        {
            "utterance_id": 59,
            "interlocutor_id": "げんまい",
            "text": "パズル系ですか？",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "はくさい",
            "text": "@おでん 携帯ゲームも楽しいですよね",
            "mention_to": [
                "おでん"
            ]
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "おでん",
            "text": "ツムツムとか笑笑",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "はくさい",
            "text": "ちょっとした隙間時間にできちゃうから",
