        "いそべ"
    ],
    "relationship": [],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "きりん",
            "text": "こんにちは！",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "いくら",
            "text": "こんにちはー！",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "いそべ",
            "text": "こんにちは！",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "きりん",
            "text": "普段音楽聴きますか？",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "いくら",
            "text": "聴きます！",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "いそべ",
            "text": "たまに聴きますね",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "きりん",
            "text": "どんなの聴きます？",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "いくら",
            "text": "@きりん 聴かれますか？",
            "mention_to": [
                "きりん"
            ]
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "いそべ",
            "text": "ゲームの曲ですかね",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "きりん",
            "text": "@いくら お風呂入る時にスピーカー持ち込んでますね",
            "mention_to": [
                "いくら"
            ]
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "いくら",
            "text": "わたしはJPOPばかりですね！",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "いくら",
            "text": "@いそべ ゲームの曲！",
            "mention_to": [
                "いそべ"
            ]
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "きりん",
            "text": "@いそべ いいですね！最近のお気に入りなにかありますか？",
            "mention_to": [
                "いそべ"
            ]
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "いくら",
            "text": "@きりん お風呂用のですか？？",
            "mention_to": [
                "きりん"
            ]
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "いそべ",
            "text": "OMORIというゲームの曲お気に入りです！",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "きりん",
            "text": "@いくら Bluetoothの防水スピーカーです！",
            "mention_to": [
                "いくら"
            ]
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "きりん",
            "text": "@いそべ OMORI！知ってます！",
            "mention_to": [
                "いそべ"
            ]
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "いそべ",
            "text": "@きりん 防水なんてあるんですか！",
            "mention_to": [
                "きりん"
            ]
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "いくら",
            "text": "OMORI、どんなゲームなんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "いくら",
            "text": "@きりん 防水最高ですね！！",
            "mention_to": [
                "きりん"
            ]
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "いくら",
            "text": "防水のスピーカー持ってないんですよね、、",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "きりん",
            "text": "@いそべ そうなんです、意外とお手頃な価格のありますよ！",
            "mention_to": [
                "いそべ"
            ]
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "いそべ",
            "text": "買ってみようかな！",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "いくら",
            "text": "お手ごろいいですね！！",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "きりん",
            "text": "今使ってるのはsoundcoreってとこのスピーカーですね",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "いくら",
            "text": "お風呂って音楽聴きたくなりますよねー！",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "きりん",
            "text": "前はAnkerだったかな",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "いそべ",
            "text": "検索してみます！",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "きりん",
            "text": "お風呂入ってる間暇なので、音楽聴けるといいですね",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "きりん",
            "text": "スマホとかタブレットお風呂に持ち込む人もいますが",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "きりん",
            "text": "私のは防水ではなくて…",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "いそべ",
            "text": "私お風呂からすぐ出ちゃう派なんですよ",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "いそべ",
            "text": "@きりん なるほど",
            "mention_to": [
                "きりん"
            ]
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "いくら",
            "text": "お風呂暇ですよね、わかります",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "いそべ",
            "text": "スピーガー買ったら長湯するかも",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "きりん",
            "text": "@いそべ あんまり湯船に浸からないですか？",
            "mention_to": [
                "いそべ"
            ]
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "いくら",
            "text": "暖まりたいので長湯したいですよね！",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "いそべ",
            "text": "入るけれどすぐ出ちゃいます",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "いそべ",
            "text": "烏の行水です",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "きりん",
            "text": "寒い時期はしっかり温まりたいですよね",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "いくら",
            "text": "@きりん スマホをジップロックに入れて持ち込んでます！",
            "mention_to": [
                "きりん"
            ]
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "いそべ",
            "text": "寒い時期だとそうですね",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "いそべ",
            "text": "@いくら 同じです！",
            "mention_to": [
                "いくら"
            ]
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "きりん",
            "text": "@いくら ジップロック入れても操作できます？",
            "mention_to": [
                "いくら"
            ]
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "いくら",
            "text": "はい！出来ますよー！",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "いそべ",
            "text": "何故だからわからないけれど反応しますよね",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "いくら",
            "text": "@いそべ スマホ持ち込むんですね！",
            "mention_to": [
                "いそべ"
            ]
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "いそべ",
            "text": "時々持ち込みます",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "いくら",
            "text": "@いそべ ストレスなく触れますよね！",
            "mention_to": [
                "いそべ"
            ]
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "いそべ",
            "text": "そうですね！",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "きりん",
            "text": "毎日ジップロック入れて～だと面倒になりそうです",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "いそべ",
            "text": "本当にそれなんです",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "いそべ",
            "text": "めんどくさがりなので",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "いくら",
            "text": "首からぶら下げる紐付きのケースなので",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "いくら",
            "text": "それから外してジップロックにいれてって",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "いくら",
            "text": "面倒なことしてます笑",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "きりん",
            "text": "それはめんどくさいですね…",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "いそべ",
            "text": "それはめんどくさそうですね笑",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "いくら",
            "text": "ですよね、そのまま持ち込めたらいいんですけどね！",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "いくら",
            "text": "なので防水スピーカーは惹かれますね！",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "きりん",
            "text": "おすすめです！",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "きりん",
            "text": "ちなみにお湯にドボンしても大丈夫です",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "いそべ",
            "text": "えええ！",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "いそべ",
            "text": "そんなに大丈夫なんですね",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "いくら",
            "text": "ドボンしても無事はすごい",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "いくら",
            "text": "浮かべて聞きたいですね",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "いくら",
            "text": "流石に光ったりとかはしないですか？",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "きりん",
            "text": "浮かぶのか…試したことはないですね",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "きりん",
            "text": "光るもの…どうでしょう、ありそうですけどね",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "いくら",
            "text": "結構大きいですか？",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "きりん",
            "text": "安いのだと難しそう",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "いくら",
            "text": "ですね、光ったらお風呂楽しくなりそうでつい、、",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "きりん",
            "text": "片手でちょうどつかめるぐらいですかね",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "いそべ",
            "text": "割と大きいんですね",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "いくら",
            "text": "そうですね！思ったより大きめ！",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "きりん",
            "text": "あれ、小さいほうだと思ってました",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "きりん",
            "text": "ガムのボトルぐらい？",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "いそべ",
            "text": "おおお！",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "いくら",
            "text": "スピーカーでどんな曲聞いてるんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "いそべ",
            "text": "思っていたより小さかった！",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "いくら",
            "text": "ガムのボトル！わかりやすい！",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "きりん",
            "text": "もうちょっと小さいのも探せばあるかもです",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "いそべ",
            "text": "充電式なんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "きりん",
            "text": "充電式です",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "いくら",
            "text": "充電式いいですよね！",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "いそべ",
            "text": "それだとよいですね！",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "いくら",
            "text": "電池消費早い気がして",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "きりん",
            "text": "最近よく聴いてるのはSound Horizonですね",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "いくら",
            "text": "磁石とかついてるとなおいいですよね！",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "きりん",
            "text": "@いくら そんなでもないですよ、意外ともちます",
            "mention_to": [
                "いくら"
            ]
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "いくら",
            "text": "Sound Horizonってどんなのですか？",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "いそべ",
            "text": "初めて聞きました",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "きりん",
            "text": "お風呂で1日30分ぐらい使って、充電は1ヶ月に1回ぐらいかもです",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "いくら",
            "text": "おお！それはいいですねー！充電少ないの助かる！",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "いそべ",
            "text": "長持ちしますね！",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "きりん",
            "text": "@いくら 物語が音楽になってるような感じ…ですかね",
            "mention_to": [
                "いくら"
            ]
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "いそべ",
            "text": "なるほど！",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "いそべ",
            "text": "面白そうですね",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "きりん",
            "text": "セリフとかナレーションも多々入ってます",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "いくら",
            "text": "おー！面白そうですね！",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "きりん",
            "text": "物語もおもしろいですし",
            "mention_to": []
        }
