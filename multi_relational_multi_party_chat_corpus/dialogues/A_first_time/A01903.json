        {
            "utterance_id": 85,
            "interlocutor_id": "ねぎとろ",
            "text": "ちょっと話題が迷走しました。",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "ねぎとろ",
            "text": "すみません。",
            "mention_to": []
        },
