        },
        {
            "utterance_id": 26,
            "interlocutor_id": "ねぎとろ",
            "text": "上がります！",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "がんも",
            "text": "２度揚げすればけっこうサクサクですよ",
