            ]
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "うどん",
            "text": "@ねぎとろ ぶどう酒のソーダ割り",
            "mention_to": [
                "ねぎとろ"
            ]
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "うどん",
            "text": "あと梅酒も好き！",
