            "mention_to": [
                "がんも"
            ]
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "うどん",
            "text": "@がんも サクサクになりますか？",
            "mention_to": [
                "がんも"
            ]
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "がんも",
            "text": "自分がたべたいだけ",
