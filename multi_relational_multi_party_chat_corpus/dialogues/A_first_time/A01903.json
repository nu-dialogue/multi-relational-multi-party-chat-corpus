            ]
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "うどん",
            "text": "@ねぎとろ ですです！",
            "mention_to": [
                "ねぎとろ"
            ]
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "うどん",
            "text": "ぶどうは飲みやすくて",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "ねぎとろ",
            "text": "@うどん 日本酒は美味しいけど、飲み過ぎたら壊れます。",
