        },
        {
            "utterance_id": 28,
            "interlocutor_id": "ねぎとろ",
            "text": "ふむふむ",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "うどん",
            "text": "なるほど！2度揚げ！",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "うどん",
            "text": "いいですね！",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "うどん",
            "text": "やってみます！",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "ねぎとろ",
            "text": "わたしもやってみます！",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "がんも",
            "text": "ビール飲みながらよく作ってます",
