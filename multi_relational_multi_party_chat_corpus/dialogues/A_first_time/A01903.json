        },
        {
            "utterance_id": 69,
            "interlocutor_id": "うどん",
            "text": "そうなんだ！",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "うどん",
            "text": "素朴な疑問が解消できました",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "がんも",
            "text": "良かったです",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "うどん",
            "text": "わたしも焼酎楽しめるようになりたいな",
