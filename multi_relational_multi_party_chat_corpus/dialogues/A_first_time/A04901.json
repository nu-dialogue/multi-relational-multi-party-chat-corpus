            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "まりも",
            "text": "こんにちは～",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "げんまい",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "おでん",
            "text": "よろしくお願いします",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "まりも",
            "text": "寒いですね",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "げんまい",
            "text": "よろしくお願いします",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "げんまい",
            "text": "寒いですー。",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "おでん",
            "text": "寒いですねー",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "げんまい",
            "text": "寒いので、さっきお汁粉飲んでました",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "まりも",
            "text": "さっきお風呂入ったんですが",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "おでん",
            "text": "季節が戻った気がします",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "げんまい",
            "text": "ほうほう",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "まりも",
            "text": "もうお風呂効果なくなってきた",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "まりも",
            "text": "お汁粉　ほっこりしますね",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "おでん",
            "text": "@げんまい おしるこ！",
            "mention_to": [
                "げんまい"
            ]
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "げんまい",
            "text": "お風呂あがりは暖かいんですけどね",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "おでん",
            "text": "@まりも お風呂！",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "げんまい",
            "text": "お汁粉たたまります",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "おでん",
            "text": "美味しそうです",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "まりも",
            "text": "あったかいもの飲みたいですね",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "げんまい",
            "text": "フリーズドライで",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "げんまい",
            "text": "手軽です",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "まりも",
            "text": "この時期",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "まりも",
            "text": "ほ～",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "おでん",
            "text": "甘酒とかも好きです",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "げんまい",
            "text": "甘酒もいいですね",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "まりも",
            "text": "甘酒ですか",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "まりも",
            "text": "長らく飲んでない",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "おでん",
            "text": "色々あったまる飲み物ありますよね",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "げんまい",
            "text": "いろんなタイプの甘酒売ってますね",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "おでん",
            "text": "ウンウン",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "げんまい",
            "text": "ストレートとか、",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "おでん",
            "text": "私もフリーズドライのです",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "げんまい",
            "text": "お湯入れるタイプとか",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "おでん",
            "text": "そそ",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "げんまい",
            "text": "フリーズドライ便利ですよね。",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "まりも",
            "text": "フリーズドライ便利ですね",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "おでん",
            "text": "@まりも あんまり飲まないですか？",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "まりも",
            "text": "@おでん 甘酒ですか？",
            "mention_to": [
                "おでん"
            ]
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "おでん",
            "text": "はい",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "まりも",
            "text": "そうですねぇあんまり",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "まりも",
            "text": "物によっては苦手かも",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "おでん",
            "text": "おしるこは懐中しるこっていうの、飲んだりします",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "おでん",
            "text": "@まりも そーなんですね",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "まりも",
            "text": "ふんふん",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "げんまい",
            "text": "@まりも ホットドリンク、何がお好き？",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "まりも",
            "text": "最近は紅茶ですかね～",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "まりも",
            "text": "@げんまい 失敗しました",
            "mention_to": [
                "げんまい"
            ]
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "げんまい",
            "text": "紅茶派なんですねー",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "おでん",
            "text": "紅茶も色々ありますよねー",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "まりも",
            "text": "ミルクティー飲みたい時期があります",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "まりも",
            "text": "あとこの前ミロ買ったので",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "げんまい",
            "text": "ミロ",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "おでん",
            "text": "ウンウン",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "まりも",
            "text": "今週はミロ率高いです",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "げんまい",
            "text": "しばらく飲んでないけど、",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "おでん",
            "text": "ミロ！懐かしいです",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "げんまい",
            "text": "おいしいですよね",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "おでん",
            "text": "私も長らく飲んでません",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "げんまい",
            "text": "子どもが、ミロを飲んだことがないって",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "げんまい",
            "text": "この前発覚しました。",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "まりも",
            "text": "ふと飲みたくなります",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "まりも",
            "text": "へえ～",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "おでん",
            "text": "栄養ありそうです",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "げんまい",
            "text": "ビックリ",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "げんまい",
            "text": "栄養ありますよね。",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "おでん",
            "text": "ウンウン",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "まりも",
            "text": "割とあっさりしてるし",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "げんまい",
            "text": "そうそう。",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "おでん",
            "text": "私も子供が小さい頃に買ってました",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "まりも",
            "text": "好きな濃さにできるので",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "まりも",
            "text": "好きです",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "げんまい",
            "text": "ココアよりあっさりですね",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "おでん",
            "text": "ウンウン",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "おでん",
            "text": "なんか、懐かしい笑",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "げんまい",
            "text": "私は濃くしちゃうなｗｗｗ",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "まりも",
            "text": "がっつりココア飲みたいときもありますが",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "まりも",
            "text": "@げんまい 甘いの好きですか？",
            "mention_to": [
                "げんまい"
            ]
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "げんまい",
            "text": "甘いの好きです。",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "おでん",
            "text": "ほっとレモンとかも飲みます",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "げんまい",
            "text": "大人っぽい。ホットレモン",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "おでん",
            "text": "カルピスみたいに薄めるやつです",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "げんまい",
            "text": "あー、売ってますね",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "まりも",
            "text": "@おでん ホットレモンいいですね",
            "mention_to": [
                "おでん"
            ]
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "おでん",
            "text": "それそれ！",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "まりも",
            "text": "今はスープとか飲みたい気分",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "おでん",
            "text": "結構、飲む時は、そーゆーの買っちゃいますねー",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "げんまい",
            "text": "メープルシロップのお湯割りが好きだっていう人がいて、",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "げんまい",
            "text": "今度やってみようかと思ってます",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "まりも",
            "text": "へえ！初耳です",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "おでん",
            "text": "へぇー",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "げんまい",
            "text": "かなり、甘いらしいです。",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "まりも",
            "text": "メープルシロップ",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "おでん",
            "text": "美味しいだろなー",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "げんまい",
            "text": "ね、",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "げんまい",
            "text": "おいしそう",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "げんまい",
            "text": "お湯プルです。",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "おでん",
            "text": "はちみつ水ですもんね",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "まりも",
            "text": "ホットケーキにかける以外知らない",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "おでん",
            "text": "笑確かに",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "げんまい",
            "text": "聞くところによると、おみそ汁に入れてもおいしいらしいです。",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "げんまい",
            "text": "メープル",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "まりも",
            "text": "え～",
            "mention_to": []
        },
        {
            "utterance_id": 103,
            "interlocutor_id": "げんまい",
            "text": "やったことないけど",
            "mention_to": []
        }
