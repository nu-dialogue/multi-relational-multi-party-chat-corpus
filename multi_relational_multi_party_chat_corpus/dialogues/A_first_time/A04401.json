        },
        {
            "utterance_id": 1,
            "interlocutor_id": "かにたま",
            "text": "こんにちは！",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "きなこ",
            "text": "こんにちは！",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "こまつな",
            "text": "寒いです",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "こまつな",
            "text": "一日中雨で",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "かにたま",
            "text": "寒いですねー",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "きなこ",
            "text": "本当ですね",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "かにたま",
            "text": "暖房ガンガンです",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "こまつな",
            "text": "暖かい所に行きたいです",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "かにたま",
            "text": "そうですね",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "こまつな",
            "text": "暖房いりますよね",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "きなこ",
            "text": "常にかけてます",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "かにたま",
            "text": "かけずにいられないです",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "こまつな",
            "text": "常に！！",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "かにたま",
            "text": "はい",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "きなこ",
            "text": "あ、ペットがいるもので",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "こまつな",
            "text": "@かにたま 寝るときも？",
            "mention_to": [
                "かにたま"
            ]
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "かにたま",
            "text": "@きなこ ペット飼ってるんですね",
            "mention_to": [
                "きなこ"
            ]
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "かにたま",
            "text": "@こまつな 寝る時は消します",
            "mention_to": [
                "こまつな"
            ]
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "こまつな",
            "text": "ペット大事です",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "きなこ",
            "text": "@かにたま そうなんです。うさぎなので",
            "mention_to": [
                "かにたま"
            ]
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "こまつな",
            "text": "かわいい",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "きなこ",
            "text": "寒さに弱いんです",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "こまつな",
            "text": "ウサギって弱いんですね",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "かにたま",
            "text": "ペットヒーターもですか？",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "きなこ",
            "text": "ペットヒーターはつけず、エアコンをつけてます",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "きなこ",
            "text": "２０度位の設定で",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "かにたま",
            "text": "そうなのですね",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "こまつな",
            "text": "ほうほう",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "かにたま",
            "text": "インコ飼ってたときは",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "きなこ",
            "text": "必要経費なので",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "きなこ",
            "text": "痛いです",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "こまつな",
            "text": "ああ、確かに",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "かにたま",
            "text": "ペットヒーターつけてました",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "かにたま",
            "text": "@きなこ わかるわかるー",
            "mention_to": [
                "きなこ"
            ]
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "こまつな",
            "text": "電気代！！！",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "きなこ",
            "text": "@かにたま いいですね！",
            "mention_to": [
                "かにたま"
            ]
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "かにたま",
            "text": "@きなこ 真夏のようで",
            "mention_to": [
                "きなこ"
            ]
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "こまつな",
            "text": "いいなあ",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "かにたま",
            "text": "快適そうでした",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "きなこ",
            "text": "いいですね～",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "きなこ",
            "text": "電気代かかりますよね",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "かにたま",
            "text": "@きなこ 家族なので",
            "mention_to": [
                "きなこ"
            ]
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "こまつな",
            "text": "そうですよね",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "かにたま",
            "text": "仕方ないですよね",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "こまつな",
            "text": "子供には我慢させました",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "きなこ",
            "text": "そうなんです・・大切な家族なので",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "きなこ",
            "text": "@こまつな 笑わかります",
            "mention_to": [
                "こまつな"
            ]
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "かにたま",
            "text": "@きなこ わかるわかる",
            "mention_to": [
                "きなこ"
            ]
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "こまつな",
            "text": "どんどん温度をあげちゃうので",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "こまつな",
            "text": "やめてーって",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "こまつな",
            "text": "服を着なさいって",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "かにたま",
            "text": "@こまつな 寒がりなんですか？",
            "mention_to": [
                "こまつな"
            ]
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "こまつな",
            "text": "@かにたま 半袖でいるので",
            "mention_to": [
                "かにたま"
            ]
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "こまつな",
            "text": "寒いはずなんです",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "きなこ",
            "text": "こどもって臼杵ですよね",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "きなこ",
            "text": "薄着です！！",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "かにたま",
            "text": "@こまつな 子供ってそうですよねー",
            "mention_to": [
                "こまつな"
            ]
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "こまつな",
            "text": "動くからね",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "かにたま",
            "text": "うちは",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "こまつな",
            "text": "はい",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "かにたま",
            "text": "私がつけないとダメかも",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "こまつな",
            "text": "ああ",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "かにたま",
            "text": "子供は暑がりで",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "かにたま",
            "text": "消されちゃう",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "きなこ",
            "text": "そのパターンもありますね～！",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "こまつな",
            "text": "あるあるかも",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "こまつな",
            "text": "冷房とかも",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "こまつな",
            "text": "感覚が違う",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "きなこ",
            "text": "洋服で調節してくれないんですよね",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "かにたま",
            "text": "ちがいますよねー",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "こまつな",
            "text": "そうそう",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "かにたま",
            "text": "そうそう",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "かにたま",
            "text": "困ったちゃんです",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "きなこ",
            "text": "わかります・・",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "こまつな",
            "text": "あはは",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "かにたま",
            "text": "シャワーの温度もそうで",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "こまつな",
            "text": "ああ・・・",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "こまつな",
            "text": "ありがちな",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "かにたま",
            "text": "感覚違いますよね",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "きなこ",
            "text": "シャワーもなんですね！",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "かにたま",
            "text": "@きなこ そうなんです",
            "mention_to": [
                "きなこ"
            ]
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "きなこ",
            "text": "冷たくしちゃうとか？",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "こまつな",
            "text": "気を付けないと熱かったりぬるかったり",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "かにたま",
            "text": "@きなこ そうそう",
            "mention_to": [
                "きなこ"
            ]
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "きなこ",
            "text": "大人には・・辛いですね",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "かにたま",
            "text": "水みたいになっていて",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "こまつな",
            "text": "ランニングするんですうちの長男",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "かにたま",
            "text": "はい",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "こまつな",
            "text": "帰ってきてシャワー",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "こまつな",
            "text": "ぬるい",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "かにたま",
            "text": "そうかもー",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "きなこ",
            "text": "わかります～！",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "こまつな",
            "text": "かと思えば",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "こまつな",
            "text": "熱いシャワーの時もあり",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "かにたま",
            "text": "そうなんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "こまつな",
            "text": "？？？が飛びます",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "きなこ",
            "text": "ええ！",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "きなこ",
            "text": "それは困惑します",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "かにたま",
            "text": "@こまつな それは何故に？",
            "mention_to": [
                "こまつな"
            ]
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "こまつな",
            "text": "その日の気分かも",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "かにたま",
            "text": "えー！",
            "mention_to": []
        },
        {
            "utterance_id": 102,
