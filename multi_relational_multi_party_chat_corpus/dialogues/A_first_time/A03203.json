        },
        {
            "utterance_id": 10,
            "interlocutor_id": "うどん",
            "text": "@きなこ ドライブ楽しいですよね！",
            "mention_to": [
                "きなこ"
            ]
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "うどん",
            "text": "@きなこ 運転なさるんですか？",
            "mention_to": [
                "きなこ"
            ]
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "きなこ",
            "text": "楽しいです～！",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "まりも",
            "text": "@きなこ 結構遠出します？",
            "mention_to": [
                "きなこ"
            ]
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "きなこ",
            "text": "運転します！",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "きなこ",
            "text": "そして、結構遠くも行きます！",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "うどん",
            "text": "遠くすごい！",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "まりも",
            "text": "@うどん クラシックピアノですか？",
            "mention_to": [
                "うどん"
            ]
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "うどん",
            "text": "@まりも JPOPをピアノで弾いてます！",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "まりも",
            "text": "かっこい～",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "きなこ",
            "text": "いいですね～！",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "うどん",
            "text": "ストリートピアノとかあるので",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "うどん",
            "text": "最近は",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "きなこ",
            "text": "テレビで見たことあります！",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "うどん",
            "text": "モチベ維持しやすいです！",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "まりも",
            "text": "駅に置いてあるような？",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "きなこ",
            "text": "ストリートピアノ！",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "まりも",
            "text": "いやめっちゃいいですね",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "まりも",
            "text": "ピアノずっと習ってたけど",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "まりも",
            "text": "もうずっと弾いてないです",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "まりも",
            "text": "ずっとがかぶってしまった",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "きなこ",
            "text": "私も子供の時習いたかった",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "まりも",
            "text": "子供の頃はイヤイヤでした",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "うどん",
            "text": "ピアノの音きれいですよね！",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "きなこ",
            "text": "ピアノできるお友達みんなうらやましかった",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "うどん",
            "text": "@まりも 同じく！",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "うどん",
            "text": "嫌だったなぁ",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "うどん",
            "text": "子どもの頃",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "きなこ",
            "text": "そうなんですか！",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "まりも",
            "text": "@うどん そうなんですか！",
            "mention_to": [
                "うどん"
            ]
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "まりも",
            "text": "練習嫌いで",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "まりも",
            "text": "親が習わせたがって",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "まりも",
            "text": "私はスイミングやりたかったという",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "きなこ",
            "text": "確かに、友達も練習が嫌だとよく言っていました",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "まりも",
            "text": "サボって怒られるんですよね",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "うどん",
            "text": "親のやらせたい習い事になりがちですよね",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "まりも",
            "text": "ですね～",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "うどん",
            "text": "そう！怒られる！",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "まりも",
            "text": "@きなこ 習い事してました？",
            "mention_to": [
                "きなこ"
            ]
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "うどん",
            "text": "泣いて帰ってたなぁよく",
