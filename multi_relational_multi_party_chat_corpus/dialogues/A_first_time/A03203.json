        },
        {
            "utterance_id": 51,
            "interlocutor_id": "きなこ",
            "text": "古い",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "まりも",
            "text": "そんなことないですよ！",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "うどん",
            "text": "サッカーかっこいい！",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "きなこ",
            "text": "ピアノ憧れます！",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "きなこ",
            "text": "今からでも習いたい！",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "まりも",
            "text": "スポーツ系の習い事いいですよね",
