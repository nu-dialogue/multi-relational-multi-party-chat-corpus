    "relationship": [],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "てんぷら",
            "text": "こんばんはー",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "らっこ",
            "text": "こんばんはー",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "みたらし",
            "text": "こんばんは",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "てんぷら",
            "text": "先日、小学校のときの友人と",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "らっこ",
            "text": "うんうん",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "てんぷら",
            "text": "30年？",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "てんぷら",
            "text": "30年か・・・",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "みたらし",
            "text": "すごい付き合いですね",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "てんぷら",
            "text": "30年ぶりに会ってきました",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "らっこ",
            "text": "いいですねー",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "みたらし",
            "text": "@てんぷら いいですね",
            "mention_to": [
                "てんぷら"
            ]
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "らっこ",
            "text": "きっかけがあったんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "てんぷら",
            "text": "そうです、もともと良く会っていた友人が",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "みたらし",
            "text": "はい",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "てんぷら",
            "text": "同じ職場になったとかで",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "みたらし",
            "text": "おお",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "てんぷら",
            "text": "ランチ会に混ぜてもらいました",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "らっこ",
            "text": "偶然？",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "みたらし",
            "text": "偶然？",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "てんぷら",
            "text": "そう、偶然です",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "らっこ",
            "text": "すごーい",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "みたらし",
            "text": "すごい",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "てんぷら",
            "text": "良く会っていたのは女性で、",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "らっこ",
            "text": "同窓会みたいですね",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "てんぷら",
            "text": "30年ぶりにあったのは男性なんですが",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "みたらし",
            "text": "同窓会いいですよね",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "てんぷら",
            "text": "小学生のとき、2回ほど泣かしたことがあって",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "らっこ",
            "text": "へー",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "みたらし",
            "text": "おお",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "てんぷら",
            "text": "あ、私女性なんですけど",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "らっこ",
            "text": "え、男の子を？",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "てんぷら",
            "text": "そうなんです",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "らっこ",
            "text": "わー",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "てんぷら",
            "text": "彼、空手をやっていて",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "らっこ",
            "text": "うんうん",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "みたらし",
            "text": "まあありますよね",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "てんぷら",
            "text": "私が教室で教わっていて",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "てんぷら",
            "text": "6年の教室で教わっていて",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "てんぷら",
            "text": "放課後ですね",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "てんぷら",
            "text": "パンチの仕方教えてーとかいって",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "らっこ",
            "text": "あ、彼に教えてもらったんですか。空手を",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "みたらし",
            "text": "パンチ",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "てんぷら",
            "text": "そうそう",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "てんぷら",
            "text": "うっかり彼のパンチが私に当たっちゃって",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "らっこ",
            "text": "あら。",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "てんぷら",
            "text": "すっごい謝られて",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "みたらし",
            "text": "痛そうです",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "てんぷら",
            "text": "めちゃくちゃうろたえてたんです、彼が",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "らっこ",
            "text": "いい子。",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "みたらし",
            "text": "うんうん",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "てんぷら",
            "text": "私は鼻が痛くて涙が出ましたが",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "らっこ",
            "text": "悪いことしたって思いますよね。",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "てんぷら",
            "text": "いいよいいよって",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "てんぷら",
            "text": "フォローしてたんですけど",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "らっこ",
            "text": "顔にあったの？",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "みたらし",
            "text": "優しい世界",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "てんぷら",
            "text": "後日、それをネタにして",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "てんぷら",
            "text": "社会の宿題やらせようとしたんですけど",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "らっこ",
            "text": "なんか、怪しい雰囲気ですね",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "てんぷら",
            "text": "こないだ顔に当てただろ～？みたいな",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "みたらし",
            "text": "ああ",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "らっこ",
            "text": "おどしｗｗｗ",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "てんぷら",
            "text": "そしたら泣いちゃいました",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "てんぷら",
            "text": "かわいそうに！",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "らっこ",
            "text": "弱みを握っちゃったんですよね。",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "らっこ",
            "text": "わーかわいそう。ｗｗｗ",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "てんぷら",
            "text": "そんな彼も優しいおじさんになって",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "みたらし",
            "text": "仕方ないですね",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "らっこ",
            "text": "覚えてました？彼は",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "らっこ",
            "text": "その出来事。",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "てんぷら",
            "text": "その話自体はしませんでしたが",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "てんぷら",
            "text": "絶対覚えていると思います・・・",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "てんぷら",
            "text": "かなり泣いていたので",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "みたらし",
            "text": "ですね",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "らっこ",
            "text": "そうなんだ。",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "みたらし",
            "text": "トラウマものですね",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "らっこ",
            "text": "でも、笑って会えたって感じですか？",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "てんぷら",
            "text": "すごく楽しかったですよ",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "みたらし",
            "text": "良かった",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "らっこ",
            "text": "それはよかった。",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "てんぷら",
            "text": "今も3人でチャットします",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "らっこ",
            "text": "たのしそー",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "みたらし",
            "text": "いいですね！",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "てんぷら",
            "text": "今日買った漫画を挙げるだけのチャット",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "みたらし",
            "text": "おお",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "らっこ",
            "text": "小学生っぽいです。",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "てんぷら",
            "text": "確かに笑",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "みたらし",
            "text": "趣味があってるんですね",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "てんぷら",
            "text": "そうみたいです",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "らっこ",
            "text": "いい仲間ですね。",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "てんぷら",
            "text": "お二人は、古い友人と会ったりしますか？",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "らっこ",
            "text": "きっと、ずっと続くんでしょうね。",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "らっこ",
            "text": "高校の友だちと会ったりします。年に1回ぐらいだけど。",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "てんぷら",
            "text": "いいですね",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "みたらし",
            "text": "去年私も小学校の時の人たちと同窓会しました",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "らっこ",
            "text": "同窓会かー",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "てんぷら",
            "text": "同窓会やってないなー",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "らっこ",
            "text": "誰か仕切ってくれないかなー",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "てんぷら",
            "text": "それそれ",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "てんぷら",
            "text": "幹事ですよね",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "らっこ",
            "text": "幹事は大変ですよね。",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "てんぷら",
            "text": "@みたらし 誰かやってくれたんですね",
            "mention_to": [
                "みたらし"
            ]
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "みたらし",
            "text": "地元でお店やってる人がいるので",
            "mention_to": []
        },
        {
            "utterance_id": 103,
            "interlocutor_id": "てんぷら",
            "text": "なるほど",
            "mention_to": []
        },
        {
            "utterance_id": 104,
            "interlocutor_id": "らっこ",
            "text": "頼もしいですね。",
            "mention_to": []
        },
        {
            "utterance_id": 105,
            "interlocutor_id": "てんぷら",
            "text": "それいいですね",
            "mention_to": []
        },
        {
            "utterance_id": 106,
            "interlocutor_id": "みたらし",
            "text": "その人が人集めてやった",
            "mention_to": []
        },
        {
            "utterance_id": 107,
            "interlocutor_id": "みたらし",
            "text": "感じです",
            "mention_to": []
        },
        {
            "utterance_id": 108,
            "interlocutor_id": "らっこ",
            "text": "そういう人がいるといいですね。",
            "mention_to": []
        },
        {
            "utterance_id": 109,
            "interlocutor_id": "てんぷら",
            "text": "何人くらい集まりました？",
            "mention_to": []
        },
        {
            "utterance_id": 110,
            "interlocutor_id": "みたらし",
            "text": "全部で12人くらいです",
            "mention_to": []
        },
        {
            "utterance_id": 111,
            "interlocutor_id": "らっこ",
            "text": "おお！",
            "mention_to": []
        },
        {
            "utterance_id": 112,
            "interlocutor_id": "らっこ",
            "text": "なかなか連絡も取れなくなるんじゃないですか。年月すぎると。",
            "mention_to": []
        },
        {
            "utterance_id": 113,
            "interlocutor_id": "みたらし",
            "text": "そうなんですよね",
            "mention_to": []
        },
        {
            "utterance_id": 114,
            "interlocutor_id": "てんぷら",
            "text": "苗字も変わるし",
            "mention_to": []
        }
