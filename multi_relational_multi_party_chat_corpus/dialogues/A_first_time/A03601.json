{
    "dialogue_id": "A03601",
    "dialogue_type": "First time",
    "interlocutors": [
        "つぶあん",
        "くらげ",
        "しらす"
    ],
    "relationship": [],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "つぶあん",
            "text": "こんにちは！",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "くらげ",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "しらす",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "しらす",
            "text": "よろしくお願いします",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "つぶあん",
            "text": "よろしくお願いいします。",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "くらげ",
            "text": "こちらこそ",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "くらげ",
            "text": "宜しくお願いします",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "つぶあん",
            "text": "ことらは今日は雨です。",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "くらげ",
            "text": "こちらもです",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "しらす",
            "text": "こっちもしばらく雨です",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "つぶあん",
            "text": "ずっと、降るみたいです。",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "くらげ",
            "text": "月曜日からずっと雨ですね",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "つぶあん",
            "text": "昨日はすごい暖かかったのに。",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "しらす",
            "text": "しばらく雨みたいですね",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "くらげ",
            "text": "今日から寒いみたいですね",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "くらげ",
            "text": "雨と寒さ",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "つぶあん",
            "text": "月曜日からずっとですか？",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "しらす",
            "text": "ヒヤッとしますね",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "くらげ",
            "text": "風邪ひきそうですね",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "しらす",
            "text": "寒暖差にやられそう",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "つぶあん",
            "text": "寒いですね。",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "くらげ",
            "text": "@つぶあん はい、月曜からです",
            "mention_to": [
                "つぶあん"
            ]
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "くらげ",
            "text": "@しらす やられますね",
            "mention_to": [
                "しらす"
            ]
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "しらす",
            "text": "@つぶあん うちも月曜からずっとです",
            "mention_to": [
                "つぶあん"
            ]
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "つぶあん",
            "text": "@くらげ 洗濯物困りますね。",
            "mention_to": [
                "くらげ"
            ]
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "くらげ",
            "text": "@つぶあん こまります",
            "mention_to": [
                "つぶあん"
            ]
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "つぶあん",
            "text": "乾かないですよね。",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "くらげ",
            "text": "乾燥機",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "しらす",
            "text": "ずっと部屋干しです",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "くらげ",
            "text": "つかってます",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "つぶあん",
            "text": "おお！",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "つぶあん",
            "text": "いいですね！",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "しらす",
            "text": "乾燥機羨ましいです",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "くらげ",
            "text": "全部はかけれないので部屋干しもです",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "つぶあん",
            "text": "うらやましいです！",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "つぶあん",
            "text": "部屋干しですよね。",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "しらす",
            "text": "湿気が溜まってる上に",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "くらげ",
            "text": "子供が出来てからよかったと思いました",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "しらす",
            "text": "部屋干ししたくないです",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "くらげ",
            "text": "@しらす したくないですよね",
            "mention_to": [
                "しらす"
            ]
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "つぶあん",
            "text": "なかなか乾かないし。。。",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "くらげ",
            "text": "けど、うちの洗濯機はかわいそうで",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "しらす",
            "text": "@くらげ 余計ジメジメです",
            "mention_to": [
                "くらげ"
            ]
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "くらげ",
            "text": "@しらす わかる！！！",
            "mention_to": [
                "しらす"
            ]
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "くらげ",
            "text": "乾燥してる時はいいですが、湿度が高いといやですよね",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "つぶあん",
            "text": "嫌ですね。",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "つぶあん",
            "text": "なんですかね？",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "しらす",
            "text": "先週までは乾燥でこまってたのに",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "つぶあん",
            "text": "おお！",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "くらげ",
            "text": "洗濯２回した後、４時間プラス３時間乾燥機つかうんです",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "しらす",
            "text": "ええっ！",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "しらす",
            "text": "それはすごい",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "つぶあん",
            "text": "結構かかりますね！",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "くらげ",
            "text": "合計９時間、１日に",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "つぶあん",
            "text": "洗濯機頑張ってます！",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "くらげ",
            "text": "@つぶあん かかりますよ",
            "mention_to": [
                "つぶあん"
            ]
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "くらげ",
            "text": "残業状態です、人間なら",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "つぶあん",
            "text": "ほぼずっと稼働ですね！",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "くらげ",
            "text": "うったえられますね、私",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "くらげ",
            "text": "@つぶあん そうなんです",
            "mention_to": [
                "つぶあん"
            ]
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "くらげ",
            "text": "壊れないか心配です",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "しらす",
            "text": "@くらげ 洗濯機頑張ってますね",
            "mention_to": [
                "くらげ"
            ]
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "つぶあん",
            "text": "なるほど！",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "くらげ",
            "text": "部屋干しってどこでしますか？",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "つぶあん",
            "text": "雨の間だけですものね？",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "くらげ",
            "text": "はい",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "くらげ",
            "text": "晴れた日は外に！",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "しらす",
            "text": "ひとつに固めたくないので",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "しらす",
            "text": "各部屋に干してます、部屋干し",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "つぶあん",
            "text": "@くらげ 部屋の真ん中に下げてます。",
            "mention_to": [
                "くらげ"
            ]
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "くらげ",
            "text": "@しらす 大変そうですね",
            "mention_to": [
                "しらす"
            ]
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "つぶあん",
            "text": "すごい邪魔です。",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "くらげ",
            "text": "@つぶあん 笑",
            "mention_to": [
                "つぶあん"
            ]
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "くらげ",
            "text": "わかります",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "つぶあん",
            "text": "@しらす なるほど、各部屋に！",
            "mention_to": [
                "しらす"
            ]
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "つぶあん",
            "text": "湿気をためないように？",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "しらす",
            "text": "固まってると邪魔なので笑",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "くらげ",
            "text": "部屋干し用の物干し？っていうんですか",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "つぶあん",
            "text": "邪魔なんですよ！",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "しらす",
            "text": "あまり部屋が広くないのと",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "くらげ",
            "text": "たててるやつ",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "しらす",
            "text": "子どもが暴れるので笑",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "くらげ",
            "text": "@しらす わかります",
            "mention_to": [
                "しらす"
            ]
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "くらげ",
            "text": "部屋干し専用の部屋が欲しいですね",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "つぶあん",
            "text": "ほしいです！",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "しらす",
            "text": "@くらげ 分かります、ほしい",
            "mention_to": [
                "くらげ"
            ]
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "くらげ",
            "text": "ちなみに、乾燥機はガスだとすごくいいらしいですよ",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "つぶあん",
            "text": "できれば、日の当たる窓近くに！",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "くらげ",
            "text": "洗濯機とは別売りですが",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "つぶあん",
            "text": "ガスですか？",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "しらす",
            "text": "ガス乾燥機気になります!!",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "くらげ",
            "text": "@つぶあん 私も思ったんですが、夏こまるでしょう、暑くて",
            "mention_to": [
                "つぶあん"
            ]
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "くらげ",
            "text": "ガスは乾燥も早いらしいです",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "つぶあん",
            "text": "@くらげ そうですね！",
            "mention_to": [
                "くらげ"
            ]
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "つぶあん",
            "text": "夏サウナ状態？",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "くらげ",
            "text": "なりますね",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "しらす",
            "text": "暑そう笑",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "つぶあん",
            "text": "冬しか想定しませんでした。。",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "くらげ",
            "text": "@つぶあん 今、冬ですもんね",
            "mention_to": [
                "つぶあん"
            ]
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "つぶあん",
            "text": "天気が良いけれど、風が強いときとか。",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "つぶあん",
            "text": "本当は外に干したい！みたいなとき。",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "くらげ",
            "text": "お宅拝見番組で、部屋干し部屋の欠点でちょいちょい話が",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "しらす",
            "text": "なるほど",
            "mention_to": []
        }
    ],
