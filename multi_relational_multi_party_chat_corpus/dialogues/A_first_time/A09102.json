        "くまのみ"
    ],
    "relationship": [],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "はまち",
            "text": "こんばんは。",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "あざらし",
            "text": "こんばんは！",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "くまのみ",
            "text": "こんばんは！",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "はまち",
            "text": "今日の金曜ロードショー",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "くまのみ",
            "text": "なんでしたっけ？",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "はまち",
            "text": "アラジンですが",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "あざらし",
            "text": "何ですか？",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "あざらし",
            "text": "そうでした",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "はまち",
            "text": "普段映画見たりします？",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "あざらし",
            "text": "実写版",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "くまのみ",
            "text": "実写版でしたね",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "あざらし",
            "text": "全然見ないんですよ",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "はまち",
            "text": "@くまのみ そうそう！実写版！",
            "mention_to": [
                "くまのみ"
            ]
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "あざらし",
            "text": "おすすめあります？",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "くまのみ",
            "text": "最近は見れてないです",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "はまち",
            "text": "私も全然見れていなくて",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "くまのみ",
            "text": "けど、この前アマゾンでPLAN75という映画をみました",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "あざらし",
            "text": "ハイキューを見てみたいです",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "はまち",
            "text": "おすすめあったら知りたいなと思って",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "あざらし",
            "text": "面白そう",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "くまのみ",
            "text": "すいません、暗い映画です",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "はまち",
            "text": "PLAN75！洋画ですか？",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "くまのみ",
            "text": "邦画です",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "はまち",
            "text": "@あざらし ハイキュー！盛り上がってますよね。",
            "mention_to": [
                "あざらし"
            ]
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "あざらし",
            "text": "なんか聞いたことあります",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "くまのみ",
            "text": "暗いというか、気持ちがしんどくなるかも",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "あざらし",
            "text": "@くまのみ そうなんだ",
            "mention_to": [
                "くまのみ"
            ]
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "あざらし",
            "text": "７５歳までしか生きれないとか",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "あざらし",
            "text": "でしたっけ？",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "くまのみ",
            "text": "ハイキューの方が楽しいですね",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "はまち",
            "text": "そういう映画なんですね！",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "くまのみ",
            "text": "@あざらし それです",
            "mention_to": [
                "あざらし"
            ]
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "はまち",
            "text": "興味あるかも！",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "あざらし",
            "text": "色々考えさせられそうですね",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "くまのみ",
            "text": "75才になったら、安楽死を選べれるんです",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "あざらし",
            "text": "@くまのみ そうなんだ",
            "mention_to": [
                "くまのみ"
            ]
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "はまち",
            "text": "@くまのみ 私好みかもしれません。",
            "mention_to": [
                "くまのみ"
            ]
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "くまのみ",
            "text": "結局、弱者がそういう選択をせざるおえないのかなと",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "はまち",
            "text": "チェックしておきます。",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "あざらし",
            "text": "@くまのみ なるほど",
            "mention_to": [
                "くまのみ"
            ]
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "あざらし",
            "text": "チェックしてみます",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "あざらし",
            "text": "面白そう",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "くまのみ",
            "text": "@はまち それならよかった！チェックしてみてください",
            "mention_to": [
                "はまち"
            ]
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "くまのみ",
            "text": "カンヌ映画祭で賞をとりましたよ",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "あざらし",
            "text": "@くまのみ 話題だったんですね",
            "mention_to": [
                "くまのみ"
            ]
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "はまち",
            "text": "それは期待膨らむ！",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "あざらし",
            "text": "それは見てみたい",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "くまのみ",
            "text": "是枝監督ファミリーの女性監督さんです",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "くまのみ",
            "text": "倍賞千恵子さんが素敵でした",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "はまち",
            "text": "是枝監督！",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "あざらし",
            "text": "大御所ですね",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "くまのみ",
            "text": "@はまち 好きですか？",
            "mention_to": [
                "はまち"
            ]
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "あざらし",
            "text": "賠償さん",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "はまち",
            "text": "そうですね！はずれがない感じがします。",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "くまのみ",
            "text": "妹さんより綺麗でしたよ",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "あざらし",
            "text": "@はまち そうなんですね",
            "mention_to": [
                "はまち"
            ]
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "くまのみ",
            "text": "@はまち 是枝監督の、怪物みたいですね",
            "mention_to": [
                "はまち"
            ]
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "あざらし",
            "text": "ああ！ありましたね",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "くまのみ",
            "text": "「怪物」です",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "あざらし",
            "text": "怪物も話題になりましたね",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "はまち",
            "text": "そう！友人がすごくオススメしてくれたのですが",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "あざらし",
            "text": "そうなんだ",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "はまち",
            "text": "まだ見れてないです。",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "くまのみ",
            "text": "@あざらし 脚本が　なんとかゆうじん　でしたよね",
            "mention_to": [
                "あざらし"
            ]
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "くまのみ",
            "text": "すいません、名前になってませんね",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "あざらし",
            "text": "私も気になってました",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "あざらし",
            "text": "こうして考えると",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "はまち",
            "text": "坂元 裕二さんですかね？",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "くまのみ",
            "text": "これまたカンヌで賞をとってましたよね",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "あざらし",
            "text": "見たいのばっかりですね",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "くまのみ",
            "text": "@はまち それです！　有難う",
            "mention_to": [
                "はまち"
            ]
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "あざらし",
            "text": "そう、確かにカンヌでしたね",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "はまち",
            "text": "見たいものだらけです。",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "くまのみ",
            "text": "@あざらし ほんとに。時間が",
            "mention_to": [
                "あざらし"
            ]
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "あざらし",
            "text": "うん。時間がない",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "はまち",
            "text": "集中して見れないから",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "はまち",
            "text": "いつも後回し。",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "くまのみ",
            "text": "坂元さんのドラマで　マザーってしってますか\n？",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "あざらし",
            "text": "24時間じゃ足りないんですよ",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "くまのみ",
            "text": "@あざらし 同感です",
            "mention_to": [
                "あざらし"
            ]
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "くまのみ",
            "text": "映画",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "あざらし",
            "text": "マザーも聞いたことあります",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "あざらし",
            "text": "見たことはないです",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "くまのみ",
            "text": "の時間2時間追加したいですね",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "くまのみ",
            "text": "@あざらし 随分前のドラマであしだまなちゃんが子役ででてました",
            "mention_to": [
                "あざらし"
            ]
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "はまち",
            "text": "Motherってドラマですよね？",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "くまのみ",
            "text": "松雪泰子さんが主演で",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "あざらし",
            "text": "@くまのみ そうなんですね",
            "mention_to": [
                "くまのみ"
            ]
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "くまのみ",
            "text": "@はまち それです、それです",
            "mention_to": [
                "はまち"
            ]
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "はまち",
            "text": "芦田愛菜ちゃんの演技がすごくて",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "あざらし",
            "text": "どんな話ですか？",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "くまのみ",
            "text": "@はまち 私のいい加減な発言のフォロー有難うございます",
            "mention_to": [
                "はまち"
            ]
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "はまち",
            "text": "@くまのみ いえいえ",
            "mention_to": [
                "くまのみ"
            ]
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "くまのみ",
            "text": "芦田まなちゃんがネグレクトうけてて、松雪さんが連れ出すんです",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "くまのみ",
            "text": "毎回号泣でした",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "あざらし",
            "text": "@くまのみ すごい話ですね",
            "mention_to": [
                "くまのみ"
            ]
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "あざらし",
            "text": "号泣しそう",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "はまち",
            "text": "テーマが重いですよね",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "あざらし",
            "text": "うん。重たいけど",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "はまち",
            "text": "だからか引き込まれる。",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "あざらし",
            "text": "そういうのも見たい",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "くまのみ",
            "text": "松雪さんも誤解なんですけど、母親にすてられててちょっとかわいそうなんです",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "あざらし",
            "text": "そうなんだ",
            "mention_to": []
        },
        {
            "utterance_id": 103,
            "interlocutor_id": "あざらし",
            "text": "複雑ですね",
            "mention_to": []
        },
        {
            "utterance_id": 104,
            "interlocutor_id": "くまのみ",
            "text": "ネグレクトの母親にもそうなった理由があったり",
            "mention_to": []
        },
        {
            "utterance_id": 105,
            "interlocutor_id": "くまのみ",
            "text": "とにかく号泣です",
            "mention_to": []
        }
    ],
