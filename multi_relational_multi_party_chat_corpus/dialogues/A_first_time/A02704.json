        },
        {
            "utterance_id": 2,
            "interlocutor_id": "ししとう",
            "text": "こんにちは！",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "こまつな",
            "text": "近所の和菓子屋に桜餅ののぼりが",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "まりも",
            "text": "え～",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "こまつな",
            "text": "春を感じました",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "まりも",
            "text": "食べたい",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "ししとう",
            "text": "@こまつな わ～！",
            "mention_to": [
                "こまつな"
            ]
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "ししとう",
            "text": "想像しただけでおいしそう！",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "まりも",
            "text": "和菓子屋さんあるのいいですね",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "こまつな",
            "text": "で、食べちゃいました",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "ししとう",
            "text": "@こまつな おお！いいですね！",
            "mention_to": [
                "こまつな"
            ]
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "まりも",
            "text": "あらあら",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "まりも",
            "text": "いいな～",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "こまつな",
            "text": "桜餅って2種類あるけど",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "まりも",
            "text": "おいしかったですか",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "こまつな",
            "text": "好きなんですよあんこ",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "こまつな",
            "text": "おいしかったです",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "こまつな",
            "text": "道明寺を食べました",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "こまつな",
            "text": "関西風",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "まりも",
            "text": "道明寺好きです",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "こまつな",
            "text": "もちもちしていいですよね",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "まりも",
            "text": "次買い物行くときに買おうかな",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "ししとう",
            "text": "道明寺！",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "こまつな",
            "text": "@まりも ぜひぜひ",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "まりも",
            "text": "モチモチ好き",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "ししとう",
            "text": "もうひとつのは長命寺でしたっけ？",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "まりも",
            "text": "道明寺、関西風なんですね",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "まりも",
            "text": "知らなかった",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "こまつな",
            "text": "@ししとう わかんない・・・",
            "mention_to": [
                "ししとう"
            ]
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "まりも",
            "text": "もうひとつのほう存在感が薄いような",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "まりも",
            "text": "名前知らないです",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "ししとう",
            "text": "そうなんですね！",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "こまつな",
            "text": "@まりも そうそう",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "こまつな",
            "text": "どっちも食べたいです",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "まりも",
            "text": "生地の原材料が違うらしいです",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "ししとう",
            "text": "道明寺しか見たこと＆食べたことなくて",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "こまつな",
            "text": "そうなんですね",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "ししとう",
            "text": "子どもの絵本で見て気になっていました。",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "まりも",
            "text": "見たことも食べたこともあるけど",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "まりも",
            "text": "あんまり選ばないかも",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "ししとう",
            "text": "@まりも そうなんですね！",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "まりも",
            "text": "@ししとう 絵本で桜餅が！",
            "mention_to": [
                "ししとう"
            ]
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "こまつな",
            "text": "どっちかといえば道明寺！",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "こまつな",
            "text": "@ししとう どんな絵本？",
            "mention_to": [
                "ししとう"
            ]
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "ししとう",
            "text": "@まりも そう！うちでは幻の桜餅になってます！",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "まりも",
            "text": "幻の笑",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "ししとう",
            "text": "@こまつな 食べ物図鑑です！",
            "mention_to": [
                "こまつな"
            ]
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "まりも",
            "text": "図鑑ですか！",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "こまつな",
            "text": "食べ物図鑑！！",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "まりも",
            "text": "お腹すきそう～",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "こまつな",
            "text": "そうですね",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "ししとう",
            "text": "@こまつな ひたすら食べ物が載ってる図鑑です",
            "mention_to": [
                "こまつな"
            ]
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "まりも",
            "text": "絵本の挿絵とかって",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "まりも",
            "text": "やたらおいしそうじゃないですか",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "こまつな",
            "text": "@ししとう 他に珍しいものとか載ってるんですか？",
            "mention_to": [
                "ししとう"
            ]
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "ししとう",
            "text": "@まりも わかります～",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "ししとう",
            "text": "@こまつな チョコレートの製造過程とか載ってました！",
            "mention_to": [
                "こまつな"
            ]
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "こまつな",
            "text": "製造過程？",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "まりも",
            "text": "へ～！",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "まりも",
            "text": "ショコラティエ…？",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "こまつな",
            "text": "カカオから？",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "ししとう",
            "text": "カカオを機械で砕いて～とか",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "こまつな",
            "text": "そういう図鑑かあ",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "まりも",
            "text": "いいなぁ～　興味がわいてきます",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "こまつな",
            "text": "チョコレート食べたくなってきた",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "まりも",
            "text": "チョコ好きですか？",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "ししとう",
            "text": "チョコレート…おいしいですよね！",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "こまつな",
            "text": "あると食べちゃうから",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "まりも",
            "text": "自分チョコとか買ったりしました？",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "こまつな",
            "text": "バレンタイン？",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "こまつな",
            "text": "スルーでした",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "まりも",
            "text": "あら",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "ししとう",
            "text": "バレンタインは普通の板チョコもぐもぐ～",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "まりも",
            "text": "まあお値段高いですしね",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "こまつな",
            "text": "だれかくれないしら",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "こまつな",
            "text": "そうそう、家電屋さんの景品で板チョコもらいました",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "まりも",
            "text": "@ししとう 板チョコストレートで食べられる方ですか！",
            "mention_to": [
                "ししとう"
            ]
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "ししとう",
            "text": "@まりも 食べちゃいます！",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "こまつな",
            "text": "@まりも 食べますよ！",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "まりも",
            "text": "@こまつな 家電屋さんの景品？",
            "mention_to": [
                "こまつな"
            ]
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "こまつな",
            "text": "インク買ったら、どうぞーって",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "まりも",
            "text": "お二人とも！？すごい",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "ししとう",
            "text": "@こまつな 嬉しいやつだ！",
            "mention_to": [
                "こまつな"
            ]
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "こまつな",
            "text": "配ってました",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "こまつな",
            "text": "狙って言って",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "ししとう",
            "text": "@まりも 板チョコは食べないです？",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "こまつな",
            "text": "行ってないです",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "まりも",
            "text": "@ししとう 食べないですね～",
            "mention_to": [
                "ししとう"
            ]
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "まりも",
            "text": "生チョコとか、ナッツ入ってるのなら",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "ししとう",
            "text": "@まりも 生チョコ！おいしい～",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "ししとう",
            "text": "ナッツのもいいですね！",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "こまつな",
            "text": "@まりも ナッツもいいですよね",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "まりも",
            "text": "ただチョコレートバリバリはつらくなる",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "こまつな",
            "text": "@まりも 甘すぎるのかしら？",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "まりも",
            "text": "@こまつな それもあるかもです",
            "mention_to": [
                "こまつな"
            ]
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "ししとう",
            "text": "最近は高カカオチョコとかありますよね！",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "まりも",
            "text": "昔よりビター系増えてきて食べやすいですね",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "こまつな",
            "text": "苦いのは苦手です私",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "ししとう",
            "text": "@こまつな そうなんですね！",
            "mention_to": [
                "こまつな"
            ]
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "まりも",
            "text": "あんまりカカオ率高いと苦いですよね",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "ししとう",
            "text": "私はあれば何でもボリボリ食べてるかも…",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "こまつな",
            "text": "チョコじゃない気がする",
            "mention_to": []
        },
        {
            "utterance_id": 103,
            "interlocutor_id": "まりも",
            "text": "ほどほどの甘さがいい",
            "mention_to": []
        },
        {
            "utterance_id": 104,
            "interlocutor_id": "まりも",
            "text": "なんでもおいしく食べられるほうが",
            "mention_to": []
        },
        {
            "utterance_id": 105,
            "interlocutor_id": "まりも",
            "text": "オトクな気がします",
            "mention_to": []
        },
        {
            "utterance_id": 106,
            "interlocutor_id": "ししとう",
            "text": "オススメのチョコレート菓子ってあります？",
            "mention_to": []
        },
        {
            "utterance_id": 107,
            "interlocutor_id": "まりも",
            "text": "最近メルティーキッスの",
            "mention_to": []
        },
        {
            "utterance_id": 108,
            "interlocutor_id": "こまつな",
            "text": "シナモンとかナッツがたくさん入ったホワイトチョコがあったんです",
            "mention_to": []
        },
        {
            "utterance_id": 109,
            "interlocutor_id": "まりも",
            "text": "ピスタチオ味食べました",
            "mention_to": []
        },
        {
            "utterance_id": 110,
            "interlocutor_id": "ししとう",
            "text": "わわ！おいしそう！",
            "mention_to": []
        },
        {
            "utterance_id": 111,
            "interlocutor_id": "こまつな",
            "text": "今見当たらない",
            "mention_to": []
        },
        {
            "utterance_id": 112,
            "interlocutor_id": "まりも",
            "text": "あら",
            "mention_to": []
        },
        {
            "utterance_id": 113,
            "interlocutor_id": "まりも",
            "text": "なんて名前ですか？",
            "mention_to": []
        },
        {
            "utterance_id": 114,
            "interlocutor_id": "こまつな",
            "text": "クリスマスなんとかって輸入品",
            "mention_to": []
        },
        {
            "utterance_id": 115,
            "interlocutor_id": "ししとう",
            "text": "輸入品なんですね！",
            "mention_to": []
        },
        {
            "utterance_id": 116,
            "interlocutor_id": "まりも",
            "text": "復活してほしいですねえ",
            "mention_to": []
        },
        {
            "utterance_id": 117,
            "interlocutor_id": "こまつな",
            "text": "カルディとかあったかもなんだけど",
            "mention_to": []
        },
        {
            "utterance_id": 118,
            "interlocutor_id": "ししとう",
            "text": "@こまつな カルディ！今度覗いてみます！",
            "mention_to": [
                "こまつな"
            ]
        },
        {
            "utterance_id": 119,
            "interlocutor_id": "まりも",
            "text": "カルディなら近いものありそう",
            "mention_to": []
        },
        {
            "utterance_id": 120,
