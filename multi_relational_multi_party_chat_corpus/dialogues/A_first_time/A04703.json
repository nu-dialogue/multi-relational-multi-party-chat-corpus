        },
        {
            "utterance_id": 26,
            "interlocutor_id": "しらす",
            "text": "うんうん",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "まりも",
            "text": "逆転裁判のお話は弁護士が主人公なんですが",
