        },
        {
            "utterance_id": 30,
            "interlocutor_id": "ねぎとろ",
            "text": "うんうん",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "まりも",
            "text": "実際の弁護活動とは違うんだ　と",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "ねぎとろ",
            "text": "そうなんですね。",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "まりも",
            "text": "罪を犯した人と向き合って",
