    "relationship": [],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "おでん",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "げんまい",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "まりも",
            "text": "こんにちは～",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "げんまい",
            "text": "よろしくお願いします",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "おでん",
            "text": "よろしく",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "まりも",
            "text": "よろしくお願いします",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "おでん",
            "text": "お願いします",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "おでん",
            "text": "最近、",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "げんまい",
            "text": "はい",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "おでん",
            "text": "髪を切って",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "まりも",
            "text": "おぉ",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "おでん",
            "text": "ショートにしたんですが",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "げんまい",
            "text": "へー",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "おでん",
            "text": "朝が大変です",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "おでん",
            "text": "爆発です",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "まりも",
            "text": "元々長かったんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "げんまい",
            "text": "あらー",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "まりも",
            "text": "あら…",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "おでん",
            "text": "いつも髪をくくってたのですが",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "げんまい",
            "text": "うんうん",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "おでん",
            "text": "久々に短くして",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "げんまい",
            "text": "結べないくらい？",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "おでん",
            "text": "洗うのは楽なんですが",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "おでん",
            "text": "はい、結べません",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "げんまい",
            "text": "うんうん",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "まりも",
            "text": "洗う・乾かすは楽ですよね",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "おでん",
            "text": "朝が笑",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "げんまい",
            "text": "寝ぐせねー",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "おでん",
            "text": "御二方は長いですか？",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "まりも",
            "text": "@おでん 天パですか？",
            "mention_to": [
                "おでん"
            ]
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "まりも",
            "text": "私はショートですね～",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "おでん",
            "text": "すこーし、くせがあるぐらいで",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "げんまい",
            "text": "私はここ何年も短いです。",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "おでん",
            "text": "@まりも ショートなんですね",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "まりも",
            "text": "やっぱり短いと楽で",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "おでん",
            "text": "@げんまい 短いんですね",
            "mention_to": [
                "げんまい"
            ]
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "おでん",
            "text": "楽ですよね、洗う、乾かすが",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "まりも",
            "text": "伸びてきてドライヤー時間かかると",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "まりも",
            "text": "げんなりしてしまう",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "げんまい",
            "text": "髪の毛、濡れていると癖つくかも。",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "おでん",
            "text": "ウンウン",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "げんまい",
            "text": "寝ている間の汗で",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "おでん",
            "text": "@げんまい そーなんですね",
            "mention_to": [
                "げんまい"
            ]
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "げんまい",
            "text": "はい",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "おでん",
            "text": "今は、ワックスで、抑えて",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "おでん",
            "text": "ごまかしてます",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "まりも",
            "text": "寒い時期は頭まで布団かぶるので",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "げんまい",
            "text": "朝、ワックスとか面倒ですよね",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "まりも",
            "text": "起きると前髪が全部上に",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "げんまい",
            "text": "あらら",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "おでん",
            "text": "@まりも 笑頭まで布団かぶるんですね",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "まりも",
            "text": "ぶわっと",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "まりも",
            "text": "@おでん 顔が寒くて…",
            "mention_to": [
                "おでん"
            ]
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "げんまい",
            "text": "暖かくなったら布団かぶらないですか？頭",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "おでん",
            "text": "私も頭が大きく上に膨らんで",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "げんまい",
            "text": "美容師に寝ぐせがつかない髪形にして！ってオーダーしてますよ。",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "まりも",
            "text": "@げんまい 暖かくなれば大丈夫です",
            "mention_to": [
                "げんまい"
            ]
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "おでん",
            "text": "@げんまい それは叶いますか？",
            "mention_to": [
                "げんまい"
            ]
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "まりも",
            "text": "難しそう",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "げんまい",
            "text": "そしたら、良い感じです。",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "まりも",
            "text": "ほんとに！すごい",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "げんまい",
            "text": "カットでけっこうなんとかなります。",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "まりも",
            "text": "美容師さんの腕次第ですかね～",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "おでん",
            "text": "へぇー、今度言ってみよっと",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "おでん",
            "text": "美容師さんの腕って、だいじですよね",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "まりも",
            "text": "私は天パなので",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "げんまい",
            "text": "美容師、10年以上同じ人だから、私の髪をわかってくれてます。",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "まりも",
            "text": "基本的に毎日爆発するものです",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "げんまい",
            "text": "あーそっかー",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "おでん",
            "text": "@まりも 天パなんですか！",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "まりも",
            "text": "水でびしゃびしゃに濡らして",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "げんまい",
            "text": "濡らして、ドライヤー？",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "まりも",
            "text": "ドライヤーしてます",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "おでん",
            "text": "ウンウン",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "おでん",
            "text": "朝から？",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "まりも",
            "text": "それ以外に方法がないです",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "げんまい",
            "text": "朝から、大仕事ですね。",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "おでん",
            "text": "それは大変ですね",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "まりも",
            "text": "昔は寝癖直しウォーターみたいなの試したり",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "おでん",
            "text": "朝から髪を濡らしてっていのは、冬場は辛いですね",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "まりも",
            "text": "しましたが、あれは無意味です",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "おでん",
            "text": "ウンウン",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "げんまい",
            "text": "そうなんだ",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "げんまい",
            "text": "髪を伸ばしたら、重さで伸びたりしませんか？くせ。",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "まりも",
            "text": "面倒ではありますね～",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "まりも",
            "text": "しないですね",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "おでん",
            "text": "ねー、",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "げんまい",
            "text": "そっかー",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "まりも",
            "text": "横に広がっていくだけ…",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "げんまい",
            "text": "縮毛矯正っていうのもありますけどね。",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "おでん",
            "text": "横に広がるのは困りますね",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "まりも",
            "text": "冬はでもいいほうですよ",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "まりも",
            "text": "乾燥してるので",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "まりも",
            "text": "そんなにびよんびよんしない",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "げんまい",
            "text": "たしかん、乾燥していると髪は良い感じですね。",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "まりも",
            "text": "当社比",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "おでん",
            "text": "縮毛矯正は確かにストレートになるけど",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "げんまい",
            "text": "私もちょっとクセあるので。",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "げんまい",
            "text": "うんうん",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "おでん",
            "text": "高いですよね笑",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "げんまい",
            "text": "そうですね。",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "まりも",
            "text": "縮毛矯正、しばらくかけてた時期ありますが",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "まりも",
            "text": "そう、高いんですよね～",
            "mention_to": []
        },
        {
            "utterance_id": 103,
            "interlocutor_id": "おでん",
            "text": "ウンウン",
            "mention_to": []
        }
