    "relationship": [],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "いくら",
            "text": "こんにちは！",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "はくさい",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "おでん",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "いくら",
            "text": "最近旅行いかれましたか？",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "はくさい",
            "text": "この間友達家族と名古屋にいきました！",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "おでん",
            "text": "最近はいってないです",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "いくら",
            "text": "名古屋！いいですね！",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "いくら",
            "text": "わたしもあまり行けてなくて、、",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "おでん",
            "text": "名古屋！",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "いくら",
            "text": "お友達とってところが",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "いくら",
            "text": "またいいですね！",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "はくさい",
            "text": "3家族総勢17人でした笑",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "いくら",
            "text": "17人！！すごい！",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "おでん",
            "text": "すごい、団体さんですね",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "いくら",
            "text": "大人は各２名ですよね？",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "はくさい",
            "text": "ほんと、どこに行っても点呼ばかりでした",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "はくさい",
            "text": "そうです、大人は6人",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "いくら",
            "text": "点呼、安全第一ですね！",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "おでん",
            "text": "ほんとですね、置き去りにならないようにしないと",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "いくら",
            "text": "11人の子供たち、、",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "いくら",
            "text": "絶対子供達楽しいですよね笑",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "おでん",
            "text": "賑やかそうです",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "はくさい",
            "text": "間違えました、総勢13人でした",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "いくら",
            "text": "あら！笑",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "おでん",
            "text": "笑笑それでも多い",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "はくさい",
            "text": "でも、車2台でワイワイして楽しかったです",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "いくら",
            "text": "大人より子供が多いとしんどいですもんね",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "おでん",
            "text": "どこに行かれました？",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "はくさい",
            "text": "メインはレゴランドでした",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "いくら",
            "text": "レゴランド！いいですねー！",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "おでん",
            "text": "単体で、レゴランドってところがあるんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "いくら",
            "text": "単体ですよね！",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "はくさい",
            "text": "初めて行きましたけど、思った以上に楽しかったですね",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "おでん",
            "text": "へぇーそれは気になります",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "いくら",
            "text": "ほぼレゴの世界な感じで惹かれます",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "いくら",
            "text": "あとディズニーとかに比べると",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "いくら",
            "text": "空いてると聞きますが",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "いくら",
            "text": "どうでしたか？",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "はくさい",
            "text": "確かに、待ち時間はそんなになかったです",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "おでん",
            "text": "レゴって、子供向けのようで、大人向けのような気がします",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "いくら",
            "text": "そうなんですね！",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "おでん",
            "text": "どうなんでしょう？",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "いくら",
            "text": "待ち時間少ないの魅力的ですね！",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "はくさい",
            "text": "長くても30分までば乗れるかんじ",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "いくら",
            "text": "レゴ作るのって難しいですよね、、",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "おでん",
            "text": "遊園地みたいなとこなんですね",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "はくさい",
            "text": "そうそう！遊園地です！",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "おでん",
            "text": "ひたすら作るのかと笑笑",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "いくら",
            "text": "アトラクションは見たことがないので気になりますね",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "いくら",
            "text": "ひたすら作るのはしんどい笑",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "おでん",
            "text": "そんなわけないですよね笑笑",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "はくさい",
            "text": "私は創造性が無いので、つくるなら諦めます。。。",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "いくら",
            "text": "ジェットコースターとかあるんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "おでん",
            "text": "お子さんは大興奮しそうですね",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "いくら",
            "text": "レゴ、赤とか青とか目立つ色で",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "いくら",
            "text": "それだけで喜びますよね！",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "はくさい",
            "text": "ジェットコースターありました",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "いくら",
            "text": "お二方ジェットコースター好きですか？",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "おでん",
            "text": "昔は乗りましたけど、今は乗れません笑笑",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "おでん",
            "text": "はくさいさんは乗りましたか？",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "いくら",
            "text": "今はダメなんですね！",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "おでん",
            "text": "いくらさんも、まだまだ乗れそうですね",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "はくさい",
            "text": "乗りました",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "いくら",
            "text": "わたしは絶叫系好きなんです",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "はくさい",
            "text": "それほど怖いのじゃなかったので、子どもも楽しめました",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "いくら",
            "text": "身長制限とかあるんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "いくら",
            "text": "遊園地しばらく行ってないので行きたいですね",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "おでん",
            "text": "いくらさんが好きなのは、どこですか？",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "いくら",
            "text": "好きと言いましたが",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "おでん",
            "text": "近くにありますか？",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "いくら",
            "text": "ディズニーくらいしか行けてなくて、、笑",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "おでん",
            "text": "ディズニー！いいじゃないですか？",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "はくさい",
            "text": "身長制限もありましたが、あんまり困らなかったです。",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "いくら",
            "text": "いいですよね！",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "はくさい",
            "text": "ディズニー行きたいけどなかなかいけないですー",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "いくら",
            "text": "観覧車のあるような遊園地に行ってみたいですね",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "はくさい",
            "text": "誰かフリークに連れて行ってもらわないと、全然わからない。。",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "おでん",
            "text": "子供が小さいうちはあちこち行く楽しみがありますね",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "いくら",
            "text": "身長制限大丈夫だったんですね！",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "おでん",
            "text": "私は関西圏なので、USJが近いけど",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "はくさい",
            "text": "USJ行ってみたいですー",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "おでん",
            "text": "もう、子供も大きくなったので",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "おでん",
            "text": "行くことなくなりました。",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "いくら",
            "text": "USJ近いんですね！",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "はくさい",
            "text": "うちはまだまだ小さいので行ってみたいのですが",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "いくら",
            "text": "子供が大きくなるとそうなるんですね",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "はくさい",
            "text": "交通費が。。。。",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "いくら",
            "text": "寂しいですね、、",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "おでん",
            "text": "そうですよー",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "おでん",
            "text": "もう、友達と行っちゃいますから",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "いくら",
            "text": "USJ、友達がすごい高かったと言ってました笑",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "いくら",
            "text": "友達同士で行きますよね",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "おでん",
            "text": "そうなんですね、私が行ってた時より",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "はくさい",
            "text": "検索しましたけど、高そうですよね。",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "いくら",
            "text": "今のうちに行かないとですね！！！",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "おでん",
            "text": "値上がりしてると思います",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "いくら",
            "text": "マリオの時計？で遊ばせたいけど",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "いくら",
            "text": "4000円もするそうで、、",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "おでん",
            "text": "そんなのもあるんですね",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "はくさい",
            "text": "あきらめようかな。。。",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "いくら",
            "text": "スタンプラリーみたいな楽しみ方ができるみたいです！",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "おでん",
            "text": "近くに住んで、年パスとか買えたらお得なんだろな",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "いくら",
            "text": "懸賞とか当たらないかなと思ってますね笑",
            "mention_to": []
        }
