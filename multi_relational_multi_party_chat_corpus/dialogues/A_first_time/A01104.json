        },
        {
            "utterance_id": 11,
            "interlocutor_id": "かにたま",
            "text": "魔女と野獣好きです",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "まりも",
            "text": "今のところ知り合いに勧められて",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "まりも",
            "text": "スターウォーズとかマーベルとか",
