        },
        {
            "utterance_id": 14,
            "interlocutor_id": "まりも",
            "text": "見てます",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "ししとう",
            "text": "かがみの孤城がおもしろかったです！",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "まりも",
            "text": "@かにたま 魔女と野獣、初めて聞きました",
            "mention_to": [
                "かにたま"
            ]
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "まりも",
            "text": "あとで調べてみます",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "まりも",
            "text": "@ししとう 話題になってましたね！",
            "mention_to": [
                "ししとう"
            ]
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "かにたま",
            "text": "@まりも スターウォーズもディズニーなんですか？",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "かにたま",
            "text": "私も好きです",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "ししとう",
            "text": "@かにたま 私も初耳！おもしろそうですね！",
            "mention_to": [
                "かにたま"
            ]
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "まりも",
            "text": "@かにたま そうなんですよ～",
            "mention_to": [
                "かにたま"
            ]
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "まりも",
            "text": "マンダロリアンってドラマシリーズがあって",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "まりも",
            "text": "スターウォーズのスピンオフ？的な",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "まりも",
            "text": "それがディズニープラス独占配信で",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "かにたま",
            "text": "@まりも 見てみたーい",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "かにたま",
            "text": "あー",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "ししとう",
            "text": "@まりも いろいろ見れるんですね！",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "まりも",
            "text": "めちゃくちゃおもしろいです",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "かにたま",
            "text": "独占なんですね。。",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "まりも",
            "text": "@ししとう スターウォーズシリーズは全部見られるはず？",
            "mention_to": [
                "ししとう"
            ]
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "かにたま",
            "text": "@まりも ディズニープラスいいですね",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "まりも",
            "text": "そう、独占なんです…",
