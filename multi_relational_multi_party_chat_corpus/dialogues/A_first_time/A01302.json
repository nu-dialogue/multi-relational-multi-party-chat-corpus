{
    "dialogue_id": "A01302",
    "dialogue_type": "First time",
    "interlocutors": [
        "きなこ",
        "おでん",
        "まりも"
    ],
    "relationship": [],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "きなこ",
            "text": "はじめまして！",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "おでん",
            "text": "初めまして",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "まりも",
            "text": "はじめまして～",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "きなこ",
            "text": "もう夕方ですね",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "まりも",
            "text": "そうですねえ",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "おでん",
            "text": "そうですね",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "まりも",
            "text": "結構日が伸びましたよね",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "おでん",
            "text": "だいぶん日も長くなりましたね",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "きなこ",
            "text": "ですね！",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "おでん",
            "text": "気分的に、明るいのが長いと嬉しいです",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "まりも",
            "text": "夕方買い物行くことが多いので",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "きなこ",
            "text": "私もです～",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "おでん",
            "text": "そうなんですね",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "おでん",
            "text": "私は午前中が多いです",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "まりも",
            "text": "真っ暗よりはちょっと明るいほうが嬉しいですね",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "おでん",
            "text": "わかります",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "おでん",
            "text": "なんか、暗いと気が急ぎますよね",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "きなこ",
            "text": "そうなんですよね～",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "きなこ",
            "text": "春や夏が好きです！",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "まりも",
            "text": "あと朝暗いと起き上がるのがさらにつらい",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "おでん",
            "text": "わかりますー",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "まりも",
            "text": "春は花粉さえなければ…",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "おでん",
            "text": "それも分かりますー",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "きなこ",
            "text": "@まりも 朝起きるの早いですか？",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "まりも",
            "text": "@きなこ 普段はあんまり早くないです",
            "mention_to": [
                "きなこ"
            ]
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "まりも",
            "text": "たまにちゃんと起きなきゃいけない日が",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "まりも",
            "text": "とてもつらいです",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "おでん",
            "text": "ちゃんと？",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "まりも",
            "text": "普段ちゃんとしてないんです…",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "おでん",
            "text": "笑笑",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "まりも",
            "text": "すぐに生活リズム乱れてしまって",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "きなこ",
            "text": "そういうことでしたか笑",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "おでん",
            "text": "@きなこ 朝早いですか？",
            "mention_to": [
                "きなこ"
            ]
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "まりも",
            "text": "寝つきが悪く寝起きが悪く",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "きなこ",
            "text": "私は夜型です",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "きなこ",
            "text": "でも、寝つきは良いです",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "まりも",
            "text": "@きなこ 仲間…じゃなかった笑",
            "mention_to": [
                "きなこ"
            ]
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "おでん",
            "text": "私もあっという間に寝ちゃいます",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "まりも",
            "text": "うらやましい！",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "まりも",
            "text": "同じ時間に寝起きできないです",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "おでん",
            "text": "そうなんですね",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "きなこ",
            "text": "なるほど、毎日同じは難しいですよね",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "まりも",
            "text": "それだけでハンデを背負ってる気がします",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "おでん",
            "text": "それはそれで、大変そうですね",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "まりも",
            "text": "会社勤めしてるときは大変でした",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "きなこ",
            "text": "会社は辛いですね・・",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "まりも",
            "text": "ほんとにギリギリまで寝て",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "まりも",
            "text": "なんとか這い出していくという",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "おでん",
            "text": "@まりも 今は少し楽になってるんですね",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "まりも",
            "text": "@おでん 今はおうちで働いてるので",
            "mention_to": [
                "おでん"
            ]
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "まりも",
            "text": "まあまあ融通がききますね",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "おでん",
            "text": "自分のペースで生活できてるんですね",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "きなこ",
            "text": "それはいいですね！",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "まりも",
            "text": "もうたぶん勤め人には戻れないですね",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "おでん",
            "text": "@きなこ 朝はどうですか？強いですか？",
            "mention_to": [
                "きなこ"
            ]
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "きなこ",
            "text": "私も弱いです・・",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "きなこ",
            "text": "若い時よりはマシになりましたが",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "まりも",
            "text": "@きなこ そこは仲間ですね",
            "mention_to": [
                "きなこ"
            ]
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "おでん",
            "text": "私も朝早いのは苦手かなー",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "きなこ",
            "text": "みんな仲間ですね",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "おでん",
            "text": "私も仲間入れてー笑笑",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "まりも",
            "text": "学生の頃は毎日眠かった",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "まりも",
            "text": "@おでん 仲間！！",
            "mention_to": [
                "おでん"
            ]
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "おでん",
            "text": "わーい！ありがとうございます笑笑",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "きなこ",
            "text": "学生のころ授業中良く寝てた・・",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "おでん",
            "text": "学生の頃かぁ",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "きなこ",
            "text": "遠いですけど",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "まりも",
            "text": "授業中かくんかくんしてましたね",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "おでん",
            "text": "笑笑怒られますね",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "きなこ",
            "text": "ははは！",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "きなこ",
            "text": "懐かしいですよね～",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "まりも",
            "text": "学生の頃から",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "おでん",
            "text": "ほんと、教壇から見たら。バレバレなんだろうなぁ",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "まりも",
            "text": "寝付き悪かったです",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "きなこ",
            "text": "え！そうなんですか！",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "まりも",
            "text": "周りの子からもバレバレですよね",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "おでん",
            "text": "@まりも ずっと辛いですよね",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "まりも",
            "text": "@おでん いや～ほんとに",
            "mention_to": [
                "おでん"
            ]
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "おでん",
            "text": "カクンカクンって、結構目立ちそうです笑笑",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "まりも",
            "text": "夜型に優しい社会になってほしいです",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "おでん",
            "text": "人それぞれですもんね、",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "まりも",
            "text": "授業受ける気はあるんで頑張ってたんですけどね",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "きなこ",
            "text": "夜型に昼は辛いですもんね。",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "まりも",
            "text": "どこかの研究では",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "おでん",
            "text": "はい",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "まりも",
            "text": "学校の始業時間を1時間遅くする試みとかして",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "まりも",
            "text": "実際いいらしいです",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "きなこ",
            "text": "へ～！いいですね！",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "まりも",
            "text": "そのほうが集中力上がるとか",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "おでん",
            "text": "へぇ～面白いですね",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "まりも",
            "text": "1時間遅かったら私も",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "きなこ",
            "text": "満員電車も避けられるし！",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "まりも",
            "text": "もうちょっと楽だったかも",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "まりも",
            "text": "そうですね！",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "おでん",
            "text": "通学も分散されていいですよね",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "おでん",
            "text": "ほんとそうですよね",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "きなこ",
            "text": "うんうん。ついでに会社も遅くしてほしい！",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "まりも",
            "text": "思春期の頃ってだんだん夜型にズレていくらしいんですよ",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "まりも",
            "text": "会社も笑",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "まりも",
            "text": "それで帰りは早ければ",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "まりも",
            "text": "最高",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "おでん",
            "text": "うん、学生の頃って、夜中に起きてるのがカッコイイと思ってました",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "まりも",
            "text": "かっこいい？？",
            "mention_to": []
        },
        {
            "utterance_id": 103,
            "interlocutor_id": "きなこ",
            "text": "@おでん 私もちょっと思っていたかも",
            "mention_to": [
                "おでん"
            ]
        }
