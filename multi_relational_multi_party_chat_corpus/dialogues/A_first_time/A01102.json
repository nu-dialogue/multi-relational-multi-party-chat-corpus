        },
        {
            "utterance_id": 77,
            "interlocutor_id": "まりも",
            "text": "今年おでん食べてないなぁ",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "ししとう",
            "text": "大根のかわりはないですよね",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "かにたま",
            "text": "@まりも 私もです",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "まりも",
            "text": "ないですね！",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "かにたま",
