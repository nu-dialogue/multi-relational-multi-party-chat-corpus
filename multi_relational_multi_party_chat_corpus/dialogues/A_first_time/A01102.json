        },
        {
            "utterance_id": 2,
            "interlocutor_id": "かにたま",
            "text": "こんにちわ",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "まりも",
            "text": "最近どこかお出かけしましたか",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "ししとう",
            "text": "ご近所ぐらいですね",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "かにたま",
            "text": "お正月以来どこにも、、",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "ししとう",
            "text": "図書館とか",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "まりも",
            "text": "そうなんですね",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "まりも",
            "text": "私は先月国立科学博物館に行ってきました",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "まりも",
            "text": "あれ今月か",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "ししとう",
            "text": "@まりも 博物館！",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "ししとう",
            "text": "いいですね！",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "かにたま",
            "text": "@まりも いいですね",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "まりも",
            "text": "特別展で「和食」っていうのやってて",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "まりも",
            "text": "気になってたので",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "かにたま",
            "text": "わ",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "ししとう",
            "text": "@まりも すごく気になる！",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "かにたま",
            "text": "興味あるー",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "まりも",
            "text": "すごく楽しかったです！",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "まりも",
            "text": "食品サンプルとか好きで",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "かにたま",
            "text": "@まりも 私も好きです",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "ししとう",
            "text": "@まりも 食品サンプルが展示されてたんですか？",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "かにたま",
            "text": "作りましたー",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "まりも",
            "text": "いろんな食材とか料理のサンプル展示があったり",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "まりも",
            "text": "@ししとう そうなんですよ",
            "mention_to": [
                "ししとう"
            ]
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "かにたま",
            "text": "@まりも 食材もあるんですね",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "まりも",
            "text": "昔の食事再現とかも",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "かにたま",
            "text": "@まりも 興味津々",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "ししとう",
            "text": "@まりも へ～！",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "まりも",
            "text": "野菜コーナーお魚コーナーその他色々",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "かにたま",
            "text": "今とかなり違いますか？",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "まりも",
            "text": "@かにたま 食品サンプル作ったことあるんですか？",
            "mention_to": [
                "かにたま"
            ]
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "かにたま",
            "text": "@まりも はい",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "ししとう",
            "text": "@かにたま いいですね！",
            "mention_to": [
                "かにたま"
            ]
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "まりも",
            "text": "卑弥呼の食事とかもありましたよ",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "かにたま",
            "text": "作るのが大好きで",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "まりも",
            "text": "すごい！",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "ししとう",
            "text": "私もつくってみたい！",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "ししとう",
            "text": "@まりも 卑弥呼の食事！！！",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "まりも",
            "text": "体験とかじゃなくておうちで？",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "かにたま",
            "text": "@まりも 卑弥呼気になるー",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "かにたま",
            "text": "@まりも 体験とキットで！",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "まりも",
            "text": "織田信長が徳川家康をもてなした食事とか",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "かにたま",
            "text": "@まりも ますます気になるー",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "まりも",
            "text": "@かにたま 楽しそう！",
            "mention_to": [
                "かにたま"
            ]
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "ししとう",
            "text": "@かにたま キットもあるんですね！",
            "mention_to": [
                "かにたま"
            ]
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "かにたま",
            "text": "食器とかもありましたか？",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "まりも",
            "text": "食器もたしかありました！",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "かにたま",
            "text": "@ししとう あるんです！",
            "mention_to": [
                "ししとう"
            ]
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "かにたま",
            "text": "@まりも 食器も大好き",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "ししとう",
            "text": "@かにたま 探してみます！",
            "mention_to": [
                "かにたま"
            ]
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "かにたま",
            "text": "いろいろ集めちゃいます",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "まりも",
            "text": "お勉強的な要素も結構ありましたね",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "ししとう",
            "text": "@まりも 見ごたえある展覧会だったんですね！",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "かにたま",
            "text": "@ししとう作って友達に配りました➰",
            "mention_to": [
                "ししとう"
            ]
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "まりも",
            "text": "米の品種は元を辿っていくと2種類しかないとか",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "かにたま",
            "text": "@まりも 2種類！",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "まりも",
            "text": "@かにたま 例えばどんなの作ったんですか？",
            "mention_to": [
                "かにたま"
            ]
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "まりも",
            "text": "大根の品種がすごく多かったりとか",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "かにたま",
            "text": "@まりも パフェと天ぷら",
            "mention_to": [
                "まりも"
            ]
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "まりも",
            "text": "どの野菜がいつ頃日本に伝来したとか",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "ししとう",
            "text": "@かにたま パフェいいですね！",
            "mention_to": [
                "かにたま"
            ]
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "ししとう",
            "text": "てんぷらも気になる！",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "まりも",
            "text": "パフェ～いいですね",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "かにたま",
            "text": "@まりも 最近大根高いですよね",
            "mention_to": [
                "まりも"
