        },
        {
            "utterance_id": 65,
            "interlocutor_id": "まりも",
            "text": "@かにたま なんでもかんでも高いですよね",
            "mention_to": [
                "かにたま"
            ]
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "かにたま",
            "text": "パフェ好評でした〜",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "ししとう",
            "text": "@かにたま 大根！確かに高いです",
            "mention_to": [
                "かにたま"
            ]
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "かにたま",
            "text": "@ししとう @まりも 高過ぎて大根料理減りました",
            "mention_to": [
                "ししとう",
                "まりも"
            ]
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "ししとう",
            "text": "おでんに入れたいのに悩んじゃう",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "まりも",
            "text": "悲しい…",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "かにたま",
            "text": "わかるわかる",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "まりも",
            "text": "おでんには絶対入れたいですね",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "かにたま",
            "text": "でも食べたくて買っちゃいました。。",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "かにたま",
            "text": "@まりも 必須ですよね",
            "mention_to": [
                "まりも"
