{
    "dialogue_id": "A09105",
    "dialogue_type": "First time",
    "interlocutors": [
        "はまち",
        "あざらし",
        "くまのみ"
    ],
    "relationship": [],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "はまち",
            "text": "こんばんは。",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "あざらし",
            "text": "こんばんは。",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "くまのみ",
            "text": "こんばんは！",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "はまち",
            "text": "最近、メダカを飼ってるんですが",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "くまのみ",
            "text": "いいですね",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "あざらし",
            "text": "かわいいですね",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "はまち",
            "text": "何かペットかっていたりしますか？",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "くまのみ",
            "text": "かっていません",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "あざらし",
            "text": "うちも飼ってないです",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "はまち",
            "text": "そうなんですね。",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "あざらし",
            "text": "飼いたいですが",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "くまのみ",
            "text": "子供の世話でも大変なのに　といってます",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "あざらし",
            "text": "なかなか踏み切れなくて",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "はまち",
            "text": "お世話大変ですもんね。",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "あざらし",
            "text": "自分が世話することになりそう",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "くまのみ",
            "text": "@あざらし 何を飼いたいんですか？",
            "mention_to": [
                "あざらし"
            ]
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "くまのみ",
            "text": "@あざらし 絶対そうなりますよね",
            "mention_to": [
                "あざらし"
            ]
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "あざらし",
            "text": "@くまのみ 猫いいなーと",
            "mention_to": [
                "くまのみ"
            ]
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "あざらし",
            "text": "思ってます",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "はまち",
            "text": "@あざらし 猫！いいですね！",
            "mention_to": [
                "あざらし"
            ]
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "くまのみ",
            "text": "家の中でだったらいいですね",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "あざらし",
            "text": "でも壁とか爪研ぎされるらしく",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "あざらし",
            "text": "結構、すごいことになるようで",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "くまのみ",
            "text": "@はまち メダカは誰の希望ですか？",
            "mention_to": [
                "はまち"
            ]
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "はまち",
            "text": "@あざらし 聞きますね！",
            "mention_to": [
                "あざらし"
            ]
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "あざらし",
            "text": "心配でもあります",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "くまのみ",
            "text": "@あざらし それは困りますね",
            "mention_to": [
                "あざらし"
            ]
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "はまち",
            "text": "@くまのみ メダカは弟が引っ越しをするので我が家で引き取りました。",
            "mention_to": [
                "くまのみ"
            ]
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "あざらし",
            "text": "@はまち メダカは長生きしますか？",
            "mention_to": [
                "はまち"
            ]
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "くまのみ",
            "text": "家で動物買うと年中、エアコンと暖房つけていないといけないみたいです",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "あざらし",
            "text": "寒いのも平気なんですね",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "あざらし",
            "text": "@くまのみ そうですよね",
            "mention_to": [
                "くまのみ"
            ]
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "くまのみ",
            "text": "人間がいなくてもらしいです",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "はまち",
            "text": "@あざらし まだ飼い始めたばかりなので分からないのですが、長生きするっぽいことを言ってました。",
            "mention_to": [
                "あざらし"
            ]
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "くまのみ",
            "text": "@はまち 毎日水換えですか？",
            "mention_to": [
                "はまち"
            ]
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "あざらし",
            "text": "そうなんだ。メダカって強そう",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "あざらし",
            "text": "長生きするといいですね",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "はまち",
            "text": "@くまのみ 水換えは汚れてきたらとのことでまだこれからですね！",
            "mention_to": [
                "くまのみ"
            ]
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "あざらし",
            "text": "あんまり手がかからないかも",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "あざらし",
            "text": "いいですね",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "くまのみ",
            "text": "水換え頑張って下さいね。なんだか大変そうな気がします",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "はまち",
            "text": "@あざらし そうなんです！長生きしてくれると嬉しいです。",
            "mention_to": [
                "あざらし"
            ]
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "あざらし",
            "text": "昔飼っていた金魚は",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "あざらし",
            "text": "週一で水換えしてました",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "くまのみ",
            "text": "@はまち 何匹いるんですか？",
            "mention_to": [
                "はまち"
            ]
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "あざらし",
            "text": "意外と大変でしたよ",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "はまち",
            "text": "@あざらし 結構な頻度！",
            "mention_to": [
                "あざらし"
            ]
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "はまち",
            "text": "@くまのみ 今5匹います！",
            "mention_to": [
                "くまのみ"
            ]
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "くまのみ",
            "text": "@あざらし 藻とか大変そうですね",
            "mention_to": [
                "あざらし"
            ]
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "くまのみ",
            "text": "@はまち それぐらいだとかわいいですね",
            "mention_to": [
                "はまち"
            ]
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "あざらし",
            "text": "水温も気にしてたし、大変でした",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "はまち",
            "text": "@くまのみ そうそう！なんか苔対策の薬品も一緒にくれました。",
            "mention_to": [
                "くまのみ"
            ]
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "あざらし",
            "text": "子供は全然面倒見ない",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "くまのみ",
            "text": "@あざらし それは大変そうですね",
            "mention_to": [
                "あざらし"
            ]
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "はまち",
            "text": "効くのかな？",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "あざらし",
            "text": "効くといいですね",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "くまのみ",
            "text": "@あざらし ありがちですね。お子さんが言いだしっぺでしたか？",
            "mention_to": [
                "あざらし"
            ]
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "はまち",
            "text": "@あざらし 我が家も今は珍しさで眺めてますが、いつかは私のお仕事になりそう～",
            "mention_to": [
                "あざらし"
            ]
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "あざらし",
            "text": "お祭りでもらった金魚です",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "あざらし",
            "text": "子供がもらってきたんですが",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "あざらし",
            "text": "水換えも最初だけでした",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "はまち",
            "text": "@あざらし お祭り！定番ですね。",
            "mention_to": [
                "あざらし"
            ]
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "はまち",
            "text": "@あざらし 長生きしました？",
            "mention_to": [
                "あざらし"
            ]
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "あざらし",
            "text": "長生きしましたよ！",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "くまのみ",
            "text": "子供だと水換えもちゃんと出来るか不安ですしね",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "あざらし",
            "text": "冬も越したし",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "あざらし",
            "text": "私に懐いてました笑",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "くまのみ",
            "text": "@あざらし すごいですね。家庭内金魚釣りしましたか？",
            "mention_to": [
                "あざらし"
            ]
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "はまち",
            "text": "@あざらし おお！丈夫な子は丈夫ですよね！",
            "mention_to": [
                "あざらし"
            ]
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "あざらし",
            "text": "餌くれる人だと思ったみたいです",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "くまのみ",
            "text": "@あざらし なつくんや　笑",
            "mention_to": [
                "あざらし"
            ]
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "あざらし",
            "text": "寄ってきますよ",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "くまのみ",
            "text": "@あざらし 金魚ってそんなのわかるんですか？",
            "mention_to": [
                "あざらし"
            ]
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "はまち",
            "text": "@あざらし すごーい！なついてくれるとかわいい！",
            "mention_to": [
                "あざらし"
            ]
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "くまのみ",
            "text": "びっくりです",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "くまのみ",
            "text": "金魚のこと、なめてました",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "あざらし",
            "text": "死んじゃった時は悲しかったですもん",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "くまのみ",
            "text": "@あざらし そりゃそうなりますよね",
            "mention_to": [
                "あざらし"
            ]
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "あざらし",
            "text": "金魚でも悲しいから",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "あざらし",
            "text": "猫とか犬はもっとですね",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "はまち",
            "text": "そうですよね。",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "くまのみ",
            "text": "かなり辛いみたいです",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "あざらし",
            "text": "家族同然ですもんね",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "くまのみ",
            "text": "子供状態になるみたいですね",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "あざらし",
            "text": "なりますねー",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "あざらし",
            "text": "色々買ってあげたくなる",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "くまのみ",
            "text": "犬や猫なら保険にはいらないといけませんね",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "はまち",
            "text": "今いろいろあるそうですね。",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "あざらし",
            "text": "保険は必須ですね",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "くまのみ",
            "text": "保険の話を聞いてびっくりしました",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "くまのみ",
            "text": "けどほんと必須ですよね",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "はまち",
            "text": "ハムスターにも保険があるそうです。",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "くまのみ",
            "text": "ハムスターにまで！",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "あざらし",
            "text": "そうなんだ！ハムスターにも",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "あざらし",
            "text": "すごい",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "はまち",
            "text": "友達が加入悩んだと言ってました。",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "くまのみ",
            "text": "@はまち なやみますよね",
            "mention_to": [
                "はまち"
            ]
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "あざらし",
            "text": "確かに悩みそう",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "あざらし",
            "text": "もしもの時ってことですね",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "くまのみ",
            "text": "動物病院に行きそうな動物は保険に入る方がいいですね",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "あざらし",
            "text": "うんうん。入院とかすごい金額ですよ",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "あざらし",
            "text": "手術もね",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "くまのみ",
            "text": "びっくりしますよね",
            "mention_to": []
        },
        {
            "utterance_id": 103,
            "interlocutor_id": "はまち",
            "text": "わー大変！",
            "mention_to": []
        },
        {
            "utterance_id": 104,
            "interlocutor_id": "くまのみ",
            "text": "足の悪いおばあさんが、タクシーで猫を病院につれていってました",
            "mention_to": []
        },
        {
            "utterance_id": 105,
            "interlocutor_id": "あざらし",
            "text": "義実家の猫が入院で八万って言ってました",
            "mention_to": []
        }
    ],
