{
    "dialogue_id": "A06304",
    "dialogue_type": "First time",
    "interlocutors": [
        "ししとう",
        "きりん",
        "げんまい"
    ],
    "relationship": [],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "ししとう",
            "text": "こんにちは！",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "きりん",
            "text": "こんにちは！",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "げんまい",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "ししとう",
            "text": "2月もあっという間ですよね",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "きりん",
            "text": "昨夜よく眠れなくて",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "ししとう",
            "text": "@きりん おぉ！",
            "mention_to": [
                "きりん"
            ]
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "げんまい",
            "text": "あら、それはよくないですねー",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "ししとう",
            "text": "大丈夫ですか？",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "きりん",
            "text": "もうそろそろ終わりますね2月",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "げんまい",
            "text": "はい",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "きりん",
            "text": "今ちょっと眠たいです",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "ししとう",
            "text": "@きりん ありますよねー！",
            "mention_to": [
                "きりん"
            ]
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "ししとう",
            "text": "そういうとき！",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "げんまい",
            "text": "眠さって、勝てないですよね",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "きりん",
            "text": "眠気覚ましの方法って効いたためしなくて",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "げんまい",
            "text": "わかります。",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "きりん",
            "text": "@げんまい やっぱりそうですよね",
            "mention_to": [
                "げんまい"
            ]
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "ししとう",
            "text": "眠気覚ましの方法！",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "ししとう",
            "text": "私も知りたい！",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "きりん",
            "text": "眠いときは寝るしかないと思ってます",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "ししとう",
            "text": "@きりん ですよねー！",
            "mention_to": [
                "きりん"
            ]
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "げんまい",
            "text": "諦めも肝心ですね。",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "げんまい",
            "text": "寝ましょうｗｗｗ",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "ししとう",
            "text": "やっぱりそれは最強な方法ですね",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "げんまい",
            "text": "歩いたら、眠気冷めそうですけど",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "ししとう",
            "text": "@げんまい 歩くいいですね",
            "mention_to": [
                "げんまい"
            ]
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "げんまい",
            "text": "ちょっと面倒ですけどね。",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "きりん",
            "text": "眠いときって体動かしてもダメじゃないですか？",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "げんまい",
            "text": "歩きながら寝る人いないでしょうから。",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "げんまい",
            "text": "@きりん そうですか？だめですか。。。",
            "mention_to": [
                "きりん"
            ]
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "きりん",
            "text": "最近言語学習アプリやってるんですが",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "げんまい",
            "text": "お勉強！すごい。",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "ししとう",
            "text": "@きりん おぉ！",
            "mention_to": [
                "きりん"
            ]
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "きりん",
            "text": "やりながら寝ちゃうのが困ります",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "げんまい",
            "text": "それは寝ちゃうかもなー",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "ししとう",
            "text": "@きりん なるほど！",
            "mention_to": [
                "きりん"
            ]
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "げんまい",
            "text": "寝ましょう。",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "きりん",
            "text": "立ってやってみたりするんですが",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "げんまい",
            "text": "立って！眠気とすごい戦ってますね。",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "きりん",
            "text": "誤回答が明らかに増えるんです",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "げんまい",
            "text": "あらー",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "ししとう",
            "text": "@きりん うぅー！悩ましい！",
            "mention_to": [
                "きりん"
            ]
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "げんまい",
            "text": "眠いって、体からの疲れてるよーのサインかもしれませんよ。",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "きりん",
            "text": "ちゃんと目醒めてるときとは全然違いますね",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "ししとう",
            "text": "@きりん 朝勉強派ですか？",
            "mention_to": [
                "きりん"
            ]
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "きりん",
            "text": "@げんまい 睡眠サイクル乱れがちで",
            "mention_to": [
                "げんまい"
            ]
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "ししとう",
            "text": "夜勉強派ですか？",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "きりん",
            "text": "@ししとう 朝はいつまでも寝ていたい派です",
            "mention_to": [
                "ししとう"
            ]
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "げんまい",
            "text": "わかりますん。寝ていたい派",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "ししとう",
            "text": "@きりん ふふふ…わかります〜",
            "mention_to": [
                "きりん"
            ]
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "ししとう",
            "text": "睡眠サイクルの乱れ…私も課題です",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "きりん",
            "text": "朝勉強派、学生の頃もいましたけど",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "きりん",
            "text": "すごいな～と思ってました",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "げんまい",
            "text": "えー。すごい。",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "ししとう",
            "text": "すごいですよね！",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "げんまい",
            "text": "夜、寝られないってことですよね。",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "きりん",
            "text": "私は、朝起きるほうは自信ないです",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "きりん",
            "text": "夜更かしは得意です",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "ししとう",
            "text": "@きりん 私のことかと思いました",
            "mention_to": [
                "きりん"
            ]
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "ししとう",
            "text": "共感だらけ",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "きりん",
            "text": "そうですね、寝付き悪いですね",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "げんまい",
            "text": "そうなんだ。",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "げんまい",
            "text": "夜、寝られないのはしんどいですよね。",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "きりん",
            "text": "ダメな日は途中で諦めます",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "ししとう",
            "text": "気持ちのいい睡眠したいですよね",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "きりん",
            "text": "そうなんですよ",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "げんまい",
            "text": "寝ようと思うのに、寝られない時ありますよね。",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "ししとう",
            "text": "ですよねー",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "きりん",
            "text": "次の日困っちゃうんですよね",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "げんまい",
            "text": "睡眠時間が短いとパフォーマンス落ちますよね。",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "きりん",
            "text": "眠れないときはしょうがないですけど",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "ししとう",
            "text": "@きりん 次の日予定がある日に限ってとか！",
            "mention_to": [
                "きりん"
            ]
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "げんまい",
            "text": "起きる時間が決まってると焦りますよね。",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "きりん",
            "text": "@げんまい 全然頭が回らないですね",
            "mention_to": [
                "げんまい"
            ]
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "げんまい",
            "text": "私は、ここ数年で、朝派になりました。",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "ししとう",
            "text": "@げんまい おぉ！",
            "mention_to": [
                "げんまい"
            ]
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "ししとう",
            "text": "何時頃朝起きてます？",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "きりん",
            "text": "@げんまい 自然と？",
            "mention_to": [
                "げんまい"
            ]
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "げんまい",
            "text": "お弁当作るために6時起きです。",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "きりん",
            "text": "お弁当…",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "ししとう",
            "text": "@げんまい 素晴らしいです！",
            "mention_to": [
                "げんまい"
            ]
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "げんまい",
            "text": "そして、夜は23時には寝る。",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "きりん",
            "text": "偉いです",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "ししとう",
            "text": "@げんまい 拍手！！",
            "mention_to": [
                "げんまい"
            ]
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "げんまい",
            "text": "ありがとうございます。",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "ししとう",
            "text": "素晴らしいです！",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "きりん",
            "text": "素晴らしいですね",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "げんまい",
            "text": "必要に迫られて、、、ですけどね。",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "げんまい",
            "text": "高校生、毎日お弁当なので",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "ししとう",
            "text": "寝る時間、起きる時間バラバラも良くないですよね〜",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "げんまい",
            "text": "それはあるかも。バラバラ",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "きりん",
            "text": "毎日お弁当かぁ",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "ししとう",
            "text": "そっか！お弁当地獄の最中なんですね",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "げんまい",
            "text": "同じ時間に起きていると、同じ時間に眠くなります。",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "きりん",
            "text": "それが　そうならないときがたびたびあって",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "きりん",
            "text": "崩れます",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "げんまい",
            "text": "休日も6時に起きて、朝は自分の時間です。",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "ししとう",
            "text": "@げんまい 休日も！",
            "mention_to": [
                "げんまい"
            ]
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "きりん",
            "text": "いい循環ですね",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "げんまい",
            "text": "そうです。今朝も6時でした。",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "ししとう",
            "text": "すごいです！",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "ししとう",
            "text": "休日はついついゆっくり寝ちゃう",
            "mention_to": []
        }
