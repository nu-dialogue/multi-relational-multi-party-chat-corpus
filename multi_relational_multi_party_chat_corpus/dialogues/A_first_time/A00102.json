    "relationship": [],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "こまつな",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "うどん",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "ねぎとろ",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "うどん",
            "text": "お昼ごはんは決まってますか？",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "ねぎとろ",
            "text": "まだです。",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "こまつな",
            "text": "いつも簡単に",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "ねぎとろ",
            "text": "何されますか？",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "うどん",
            "text": "お肉焼こうかなと思ってます。",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "ねぎとろ",
            "text": "いいですね。",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "うどん",
            "text": "簡単がいいですね",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "こまつな",
            "text": "一人でもしっかり？",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "こまつな",
            "text": "一人じゃないのか菜",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "うどん",
            "text": "1人でもなるべく栄養揃えるようにはがんばってます！",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "こまつな",
            "text": "すごい",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "ねぎとろ",
            "text": "えらい！",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "うどん",
            "text": "なかなか難しいですね",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "うどん",
            "text": "お料理",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "こまつな",
            "text": "いつも雑炊とか",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "ねぎとろ",
            "text": "ですね。栄養面を考えるの難しいです。",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "こまつな",
            "text": "タンパク質とってないかも",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "うどん",
            "text": "お雑炊は、消化に良さそうでいいですね",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "こまつな",
            "text": "卵とか",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "ねぎとろ",
            "text": "雑炊はおなかすきませんか？",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "こまつな",
            "text": "暖かいものがいいので冬は",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "こまつな",
            "text": "おなかすきますね",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "ねぎとろ",
            "text": "ですよね、、、",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "うどん",
            "text": "体温めるの大事ですね",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "うどん",
            "text": "お雑炊、ダイエットになりそう",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "こまつな",
            "text": "おやつ食べちゃいます",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "ねぎとろ",
            "text": "そうそう！",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "こまつな",
            "text": "ダメですね",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "ねぎとろ",
            "text": "何か物足りなくて",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "うどん",
            "text": "おやつも気分転換に大事ですよね！",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "こまつな",
            "text": "でもその時はおなかいっぱいになる",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "ねぎとろ",
            "text": "たしかに！",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "こまつな",
            "text": "おやつのお勧めありますか",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "うどん",
            "text": "デーツっていうドライフルーツをよく食べます",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "こまつな",
            "text": "ドライフルーツ食べます",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "こまつな",
            "text": "この間イチジク",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "ねぎとろ",
            "text": "わたしも食べます。アプリコットが好きです。",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "こまつな",
            "text": "プルーンも定番",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "うどん",
            "text": "甘いし、お砂糖たくさん使ったものより罪悪感なくて好きです",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "ねぎとろ",
            "text": "デーツは満足感高いですよね！",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "こまつな",
            "text": "たまにチョコとか食べたくなるけど",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "ねぎとろ",
            "text": "食べないんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "こまつな",
            "text": "食べる・・・",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "うどん",
            "text": "チョコいいですね",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "うどん",
            "text": "バレンタインだし！",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "こまつな",
            "text": "バレンタインか",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "ねぎとろ",
            "text": "わたしもめちゃ食べてしまします。",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "こまつな",
            "text": "コマーシャルに影響受けるので",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "ねぎとろ",
            "text": "明日？？",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "うどん",
            "text": "明日ですね！",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "ねぎとろ",
            "text": "忘れてた。",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "こまつな",
            "text": "あしたか・・・",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "こまつな",
            "text": "何か考えねば息子に",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "うどん",
            "text": "息子さんにあげるんですね！いいですね",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "こまつな",
            "text": "自分用のついで",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "ねぎとろ",
            "text": "喜びそう",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "ねぎとろ",
            "text": "そんなぁ",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "こまつな",
            "text": "はあって感じ",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "うどん",
            "text": "コミュニケーションになるからいいですね",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "こまつな",
            "text": "ならないです",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "うどん",
            "text": "ふふ",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "うどん",
            "text": "なかなか難しいですよね",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "こまつな",
            "text": "さみしい",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "こまつな",
            "text": "あ、でも大人ですけどね",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "うどん",
            "text": "大人同士だと、余計にコミュニケーション難しそう",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "こまつな",
            "text": "気を使います",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "ねぎとろ",
            "text": "特に男の子は",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "こまつな",
            "text": "女の子は楽しそう",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "ねぎとろ",
            "text": "女の子よさそうですよね。お友だちみたいになれそう。",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "こまつな",
            "text": "うらやましいです",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "うどん",
            "text": "想像することしかできませんが、たしかに女の子楽しそうです。",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "こまつな",
            "text": "話題が合えば男子もいけますけど",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "ねぎとろ",
            "text": "一緒にお買い物したりとか",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "こまつな",
            "text": "買い物は男子は無理です",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "うどん",
            "text": "なるほど",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "うどん",
            "text": "話題探すの難しそう",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "ねぎとろ",
            "text": "ですよね、共通の話題、大事ですよね。",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "こまつな",
            "text": "次男は野球",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "こまつな",
            "text": "それだと話すんです",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "ねぎとろ",
            "text": "こまつなさん、野球はあまり、ですか？",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "こまつな",
            "text": "好きです",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "うどん",
            "text": "好きでよかった！",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "こまつな",
            "text": "息子の影響で推し球団作って",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "こまつな",
            "text": "昔一緒に見に行きました",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "ねぎとろ",
            "text": "それ、いいですね。話が盛り上がりそう！",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "うどん",
            "text": "ちゃんと息子さんに歩み寄ってるの、尊敬します",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "こまつな",
            "text": "いやいや",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "こまつな",
            "text": "子離れ出来ずです",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "ねぎとろ",
            "text": "だって、大事な息子さんだもん",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "うどん",
            "text": "息子さんもうれしいと思います",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "ねぎとろ",
            "text": "なかなか子離れできませんよ。",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "こまつな",
            "text": "そうだといいな",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "ねぎとろ",
            "text": "そうですよ！",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "こまつな",
            "text": "基本そっけないです",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "うどん",
            "text": "照れ臭さとかもあるだろうし",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "ねぎとろ",
            "text": "男の子は、どこもそんな感じみたいです。",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "ねぎとろ",
            "text": "息子持ちの友達もそう言ってました。",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "こまつな",
            "text": "なかなかみんな苦労してるかな",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "うどん",
            "text": "まだ子どもいないので、勉強になります",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "こまつな",
            "text": "赤ちゃんだった頃はかわいかったのに",
            "mention_to": []
        },
        {
            "utterance_id": 103,
            "interlocutor_id": "うどん",
            "text": "大きくなるのあっという間ですよね",
            "mention_to": []
        },
        {
            "utterance_id": 104,
            "interlocutor_id": "ねぎとろ",
            "text": "そうなのでしょうね。照れですよね、本当。",
            "mention_to": []
        },
        {
            "utterance_id": 105,
            "interlocutor_id": "こまつな",
            "text": "てれか",
            "mention_to": []
        }
