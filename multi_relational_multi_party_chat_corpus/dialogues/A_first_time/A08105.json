        },
        {
            "utterance_id": 84,
            "interlocutor_id": "しらす",
            "text": "うんうん",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "しじみ",
            "text": "豆でそんなに違うんですね！！",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "しじみ",
            "text": "@しらす いい豆苦手はちょっぴり切ないですね",
            "mention_to": [
                "しらす"
            ]
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "しらす",
            "text": "私も豆全然分かりません",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "つぶあん",
            "text": "@しらす 産地？とかですかね？？",
            "mention_to": [
                "しらす"
            ]
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "しじみ",
            "text": "なんか酸っぱい？コーヒーとかもあるんですよね？？",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "しらす",
            "text": "@しじみ 高級なものが苦手かもしれない",
            "mention_to": [
                "しじみ"
            ]
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "つぶあん",
            "text": "ふむふむ",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "しじみ",
            "text": "@しらす なんて謙虚なお身体でしょう！",
            "mention_to": [
                "しらす"
            ]
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "つぶあん",
            "text": "ですね！",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "しじみ",
            "text": "でも、私もたまに牛肉食べると",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "しじみ",
            "text": "お腹壊します！笑",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "つぶあん",
            "text": "おお！",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "しらす",
            "text": "@つぶあん あんまり気にしてないですけど、気づいたら高級なものが多い笑",
            "mention_to": [
                "つぶあん"
            ]
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "しらす",
            "text": "私もです",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "しじみ",
            "text": "あら〜",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "しらす",
            "text": "牛肉胃もたれします",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "しじみ",
            "text": "しますよね！",
            "mention_to": []
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "しらす",
            "text": "美味しいけど",
            "mention_to": []
        },
