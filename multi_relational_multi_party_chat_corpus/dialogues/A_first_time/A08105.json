            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "しじみ",
            "text": "突然ですが、",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "しらす",
            "text": "はい",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "しじみ",
            "text": "私コーラが大好きで",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "つぶあん",
            "text": "ほう！",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "しじみ",
            "text": "毎日飲みたいくらいですが、",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "しらす",
            "text": "いいですね",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "しらす",
            "text": "うんうん",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "しじみ",
            "text": "健康にいいものではないので、なるべく我慢してるんです",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "つぶあん",
            "text": "へぇ～！",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "つぶあん",
            "text": "なるほど！",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "しらす",
            "text": "なるほど",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "しじみ",
            "text": "今日は、安売りしてたペプシのレモン味を",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "しらす",
            "text": "うんうん",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "つぶあん",
            "text": "はい",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "しじみ",
            "text": "楽しみに開封してみたんですが、",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "しじみ",
            "text": "やっぱりコーラには勝てませんでした…",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "つぶあん",
            "text": "なるほど。。。",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "しじみ",
            "text": "みなさんは、何の飲み物が好きですか？？",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "しらす",
            "text": "@しじみ コカコーラ派ですか？",
            "mention_to": [
                "しじみ"
            ]
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "しじみ",
            "text": "@しらす コカコーラ派です！",
            "mention_to": [
                "しらす"
            ]
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "つぶあん",
            "text": "炭酸強いんですね！",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "しらす",
            "text": "@しじみ 分かります!!",
            "mention_to": [
                "しじみ"
            ]
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "しらす",
            "text": "私は最近お茶が好きです",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "しじみ",
            "text": "炭酸美味しいんですよねぇ",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "しらす",
            "text": "特にほうじ茶",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "しじみ",
            "text": "@しらす お茶いいですねぇ！",
            "mention_to": [
                "しらす"
            ]
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "しじみ",
            "text": "ほうじ茶、あったまりますよね",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "つぶあん",
            "text": "@しらす ほうじ茶ですか？！",
            "mention_to": [
                "しらす"
            ]
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "しらす",
            "text": "@しじみ いいです",
            "mention_to": [
                "しじみ"
            ]
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "しらす",
            "text": "あったまります",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "つぶあん",
            "text": "なるほど。",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "しらす",
            "text": "@つぶあん ほうじ茶ですね",
            "mention_to": [
                "つぶあん"
            ]
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "しじみ",
            "text": "アイスでも飲みますか？？",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "つぶあん",
            "text": "冬はホットのみたいですね",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "しじみ",
            "text": "アイスほうじ茶ってあんまり飲んだ事ないかも",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "しらす",
            "text": "@しじみ 冷たいお茶苦手です",
            "mention_to": [
                "しじみ"
            ]
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "つぶあん",
            "text": "@しじみ たしかに！",
            "mention_to": [
                "しじみ"
            ]
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "しじみ",
            "text": "@つぶあん 冬はホットがいいですよね",
            "mention_to": [
                "つぶあん"
            ]
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "しじみ",
            "text": "@しらす あら、そうなんですね！",
            "mention_to": [
                "しらす"
            ]
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "つぶあん",
            "text": "なるほど",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "しじみ",
            "text": "冷たいの飲まないのは、",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "しらす",
            "text": "常温より冷たいお茶、飲めません",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "しじみ",
            "text": "身体が冷えなさそうで",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "しじみ",
            "text": "健康に良さそう！！",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "しじみ",
            "text": "@しらす そうなんですね！",
            "mention_to": [
                "しらす"
            ]
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "しじみ",
            "text": "夏場も常温ですか？？",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "つぶあん",
            "text": "@しらす 常温ですか！",
            "mention_to": [
                "しらす"
            ]
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "しらす",
            "text": "@しじみ 夏も常温ですね",
            "mention_to": [
                "しじみ"
            ]
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "しじみ",
            "text": "おぉ〜！",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "つぶあん",
            "text": "おお！",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "しらす",
            "text": "なんかお腹痛くなるというか笑",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "しじみ",
            "text": "ほうほう！",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "つぶあん",
            "text": "はい",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "しじみ",
            "text": "お腹痛くなるのは、困りますもんねぇ",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "つぶあん",
            "text": "冷えるんですね",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "しらす",
            "text": "ほかの飲み物は冷たいのいけるのに",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "しじみ",
            "text": "あら！",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "しらす",
            "text": "なんででしょ？",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "しらす",
            "text": "笑",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "つぶあん",
            "text": "そうなんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "つぶあん",
            "text": "お茶の冷たいのがダメなんですね。",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "しじみ",
            "text": "でも、なんとなくそういう自分ルール？みたいなのってありますよね！",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "しじみ",
            "text": "これやるとお腹壊しがちとか！",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "つぶあん",
            "text": "あるかも。",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "しらす",
            "text": "@しじみ 体がそうなってるのかもしれません",
            "mention_to": [
                "しじみ"
            ]
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "しじみ",
            "text": "なるほど！",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "つぶあん",
            "text": "なるほど！",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "しらす",
            "text": "コーヒー好きだけど",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "しじみ",
            "text": "うんうん",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "つぶあん",
            "text": "はい",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "しらす",
            "text": "この豆はお腹壊すとか",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "しらす",
            "text": "あります",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "しじみ",
            "text": "へぇー！！",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "しじみ",
            "text": "私コーヒー飲めないんで、",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "つぶあん",
            "text": "体に合わないんですかね？",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "しらす",
            "text": "そして何故かいい豆とか苦手なんです笑",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "しじみ",
            "text": "豆の違いとかわからないんですが、",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "つぶあん",
            "text": "おお！",
