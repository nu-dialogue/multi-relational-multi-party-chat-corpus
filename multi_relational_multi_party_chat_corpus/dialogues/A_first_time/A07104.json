    ],
    "relationship": [],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "おにぎり",
            "text": "こんにちは。",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "ハンバーグ",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "げんまい",
            "text": "こんにちは",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "ハンバーグ",
            "text": "よろしくお願いします",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "げんまい",
            "text": "よろしくお願いします",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "おにぎり",
            "text": "みなさんは、家事とかしている時、音楽とか流しますか？",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "げんまい",
            "text": "アレクサで音楽聞くことがあります",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "おにぎり",
            "text": "在宅仕事とかしている時など。",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "おにぎり",
            "text": "アレクサお持ちなんですねー！",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "ハンバーグ",
            "text": "仕事してるときとか結構きいてます",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "ハンバーグ",
            "text": "アレクサ羨ましいです",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "げんまい",
            "text": "アレクサ、いう事聞いてくれる時と",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "おにぎり",
            "text": "無音だと、私も落ち着かなくて何か流しています。",
            "mention_to": []
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "げんまい",
            "text": "きいてくれない時があります",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "おにぎり",
            "text": "アレクサ、便利ですか？",
            "mention_to": []
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "ハンバーグ",
            "text": "なんかカッコいいですよね",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "げんまい",
            "text": "使いこなせてはいないですが、",
            "mention_to": []
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "げんまい",
            "text": "便利な時もありますね",
            "mention_to": []
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "げんまい",
            "text": "カップラーメンで3分",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "おにぎり",
            "text": "具体的に何が出来るか、詳しくないのですが。。",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "げんまい",
            "text": "測ってとか",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "ハンバーグ",
            "text": "手を離せないとき便利そう",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "おにぎり",
            "text": "タイマーは、口で指示できると便利ですね！",
            "mention_to": []
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "げんまい",
            "text": "そうそう。料理しながら",
            "mention_to": []
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "ハンバーグ",
            "text": "ですよね",
            "mention_to": []
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "げんまい",
            "text": "明日の天気は？とか",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "げんまい",
            "text": "タイマーで使うのが一番多いかも。",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "おにぎり",
            "text": "ほぉー賢い子だ。",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "ハンバーグ",
            "text": "そっかネットにつながっているんですね",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "おにぎり",
            "text": "音楽は、ネット経由って事ですか？",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "げんまい",
            "text": "そう。ネットにつながっているので検索もできます",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "ハンバーグ",
            "text": "使いこなしたら凄く便利そう",
            "mention_to": []
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "げんまい",
            "text": "そうです。ねっと経由で音楽聞けます",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "げんまい",
            "text": "でも、好きなアーティストの曲をかけてもらおうと思うのですが",
            "mention_to": []
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "ハンバーグ",
            "text": "自分で動かなくなりそうですけど",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "ハンバーグ",
            "text": "うんうん",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "げんまい",
            "text": "課金しないとかけてくれなくて、",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "おにぎり",
            "text": "今以上に怠惰に。。",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "げんまい",
            "text": "似た曲をかけます　って言われますｗｗｗ",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "ハンバーグ",
            "text": "課金ですか？",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "げんまい",
            "text": "怠惰になります",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "おにぎり",
            "text": "似た曲って！！！笑",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "げんまい",
            "text": "Spotify契約してないので",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "おにぎり",
            "text": "誰の曲なの～？！？！",
            "mention_to": []
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "ハンバーグ",
            "text": "うけます笑",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "げんまい",
            "text": "でも、だいたいいい感じに好きなアーティストの曲かけてくれます",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "おにぎり",
            "text": "アレクサ面白いなぁ！",
            "mention_to": []
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "げんまい",
            "text": "男闘呼組が好きで",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "ハンバーグ",
            "text": "本当面白い",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "げんまい",
            "text": "ご存じですか？",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "ハンバーグ",
            "text": "うんうん",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "おにぎり",
            "text": "そっか、契約が必要なんですね。",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "おにぎり",
            "text": "男闘呼組！！",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "おにぎり",
            "text": "成田しょうじ！ですっけ？！",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "げんまい",
            "text": "です！",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "おにぎり",
            "text": "好きだったかも。",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "げんまい",
            "text": "今、ロックオンソーシャルクラブっていうバンドになってるんですが",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "ハンバーグ",
            "text": "名前は知ってます",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "げんまい",
            "text": "ロックオンソーシャルクラブの曲かけてって言うと",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "おにぎり",
            "text": "名前変わったんですねー",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "ハンバーグ",
            "text": "曲聞けばわかるかもですけど",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "おにぎり",
            "text": "Time zone！！",
            "mention_to": []
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "げんまい",
            "text": "六甲おろしか、緑黄色社会がかかります",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "おにぎり",
            "text": "おしい！",
            "mention_to": []
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "ハンバーグ",
            "text": "ウケる",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "おにぎり",
            "text": "退屈しないですね笑",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "げんまい",
            "text": "おしい！ってなります。",
            "mention_to": []
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "ハンバーグ",
            "text": "でも似てるかも笑",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "おにぎり",
            "text": "今も、男闘呼組のメンバーで活動してるんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "ハンバーグ",
            "text": "爆笑です",
            "mention_to": []
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "げんまい",
            "text": "ロックオンは、男闘呼組の4人と",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "おにぎり",
            "text": "アレクサかわいいな！",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "げんまい",
            "text": "ジュンスカの寺岡呼人、ドラムの青山ヒデキ6人です",
            "mention_to": []
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "ハンバーグ",
            "text": "便利な世の中です",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "げんまい",
            "text": "男闘呼組は、去年1年限定で復活して解散しました。",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "おにぎり",
            "text": "えぇ！そうなんだ！寺岡さんは知ってるなぁ。",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "げんまい",
            "text": "アレクサ、かわいいです。",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "ハンバーグ",
            "text": "もう解散してるんですね",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "げんまい",
            "text": "そうなんです。解散しました。",
            "mention_to": []
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "おにぎり",
            "text": "なるほどー。",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "ハンバーグ",
            "text": "知りませんでした",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "げんまい",
            "text": "でも、復活の1年間、ライブいっぱいいけてよかったです。",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "おにぎり",
            "text": "ライブやってくれたんですね！",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "おにぎり",
            "text": "それは、嬉しい！",
            "mention_to": []
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "げんまい",
            "text": "いっぱいライブありました！",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "ハンバーグ",
            "text": "行けて良かったですね",
            "mention_to": []
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "おにぎり",
            "text": "@ハンバーグ 好きな歌手とかいますか？",
            "mention_to": [
                "ハンバーグ"
            ]
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "げんまい",
            "text": "はい。楽しかったです。",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "ハンバーグ",
            "text": "@おにぎり 特に最近はこれって人はいないです",
            "mention_to": [
                "おにぎり"
            ]
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "おにぎり",
            "text": "私は、元キンプリが大好きで。",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "ハンバーグ",
            "text": "ランキングでずっと流してたり",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "げんまい",
            "text": "キンプリ！",
            "mention_to": []
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "げんまい",
            "text": "元？",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "おにぎり",
            "text": "私も、普段はラジオとかで適当に聞いていることが多いかな。",
            "mention_to": []
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "おにぎり",
            "text": "脱退した3人の方が好きなもんで笑",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "げんまい",
            "text": "あ、そうなんだ。",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "ハンバーグ",
            "text": "でも最近の曲は聴いても覚えられない",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "ハンバーグ",
            "text": "聴いてるのに….",
            "mention_to": []
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "げんまい",
            "text": "最近の曲はね、",
            "mention_to": []
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "おにぎり",
            "text": "最近の曲ね、、なぜか覚えられないですねぇ。",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "ハンバーグ",
            "text": "だからカラオケとかいけないです",
            "mention_to": []
        }
