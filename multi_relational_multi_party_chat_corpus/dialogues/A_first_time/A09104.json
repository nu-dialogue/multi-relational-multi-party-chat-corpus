{
    "dialogue_id": "A09104",
    "dialogue_type": "First time",
    "interlocutors": [
        "はまち",
        "あざらし",
        "くまのみ"
    ],
    "relationship": [],
    "utterances": [
        {
            "utterance_id": 0,
            "interlocutor_id": "はまち",
            "text": "こんばんは。",
            "mention_to": []
        },
        {
            "utterance_id": 1,
            "interlocutor_id": "あざらし",
            "text": "こんばんは！",
            "mention_to": []
        },
        {
            "utterance_id": 2,
            "interlocutor_id": "くまのみ",
            "text": "こんばんは！",
            "mention_to": []
        },
        {
            "utterance_id": 3,
            "interlocutor_id": "はまち",
            "text": "最近、カメラがほしいなと思ってるんですが",
            "mention_to": []
        },
        {
            "utterance_id": 4,
            "interlocutor_id": "あざらし",
            "text": "カメラ素敵ですね",
            "mention_to": []
        },
        {
            "utterance_id": 5,
            "interlocutor_id": "くまのみ",
            "text": "いいですね、何系のカメラですか？",
            "mention_to": []
        },
        {
            "utterance_id": 6,
            "interlocutor_id": "はまち",
            "text": "一眼レフがほしいんです。",
            "mention_to": []
        },
        {
            "utterance_id": 7,
            "interlocutor_id": "くまのみ",
            "text": "本格的ですね",
            "mention_to": []
        },
        {
            "utterance_id": 8,
            "interlocutor_id": "あざらし",
            "text": "本格的だ",
            "mention_to": []
        },
        {
            "utterance_id": 9,
            "interlocutor_id": "あざらし",
            "text": "色々探してるんですね",
            "mention_to": []
        },
        {
            "utterance_id": 10,
            "interlocutor_id": "くまのみ",
            "text": "一眼レフっていってもデジタルなんですよね",
            "mention_to": []
        },
        {
            "utterance_id": 11,
            "interlocutor_id": "はまち",
            "text": "職場で仕方なく写真を撮らないといけなくなって",
            "mention_to": []
        },
        {
            "utterance_id": 12,
            "interlocutor_id": "あざらし",
            "text": "@はまち そうなんだ",
            "mention_to": [
                "はまち"
            ]
        },
        {
            "utterance_id": 13,
            "interlocutor_id": "はまち",
            "text": "撮っていたら、普段でも使ってみたくなって。",
            "mention_to": []
        },
        {
            "utterance_id": 14,
            "interlocutor_id": "くまのみ",
            "text": "@はまち 大変ですね",
            "mention_to": [
                "はまち"
            ]
        },
        {
            "utterance_id": 15,
            "interlocutor_id": "あざらし",
            "text": "でもキッカケになりましたね",
            "mention_to": []
        },
        {
            "utterance_id": 16,
            "interlocutor_id": "くまのみ",
            "text": "@はまち よい趣味みつけましたね",
            "mention_to": [
                "はまち"
            ]
        },
        {
            "utterance_id": 17,
            "interlocutor_id": "はまち",
            "text": "@くまのみ 一眼といってもデジタルです！",
            "mention_to": [
                "くまのみ"
            ]
        },
        {
            "utterance_id": 18,
            "interlocutor_id": "はまち",
            "text": "そうそう。",
            "mention_to": []
        },
        {
            "utterance_id": 19,
            "interlocutor_id": "あざらし",
            "text": "何を撮りますか？",
            "mention_to": []
        },
        {
            "utterance_id": 20,
            "interlocutor_id": "あざらし",
            "text": "お子さんとか？",
            "mention_to": []
        },
        {
            "utterance_id": 21,
            "interlocutor_id": "くまのみ",
            "text": "全く興味がないので、うらやましいです",
            "mention_to": []
        },
        {
            "utterance_id": 22,
            "interlocutor_id": "はまち",
            "text": "@あざらし 子どもの写真とか風景とか撮りたいです。",
            "mention_to": [
                "あざらし"
            ]
        },
        {
            "utterance_id": 23,
            "interlocutor_id": "あざらし",
            "text": "@はまち 素敵ですね",
            "mention_to": [
                "はまち"
            ]
        },
        {
            "utterance_id": 24,
            "interlocutor_id": "くまのみ",
            "text": "@はまち お出かけが増えそうですね",
            "mention_to": [
                "はまち"
            ]
        },
        {
            "utterance_id": 25,
            "interlocutor_id": "はまち",
            "text": "ただ、職場の備品であるカメラの値段を調べたら",
            "mention_to": []
        },
        {
            "utterance_id": 26,
            "interlocutor_id": "くまのみ",
            "text": "楽しみですね",
            "mention_to": []
        },
        {
            "utterance_id": 27,
            "interlocutor_id": "あざらし",
            "text": "確かに、色々出かけたくなりますね",
            "mention_to": []
        },
        {
            "utterance_id": 28,
            "interlocutor_id": "はまち",
            "text": "20万円超えていて…",
            "mention_to": []
        },
        {
            "utterance_id": 29,
            "interlocutor_id": "あざらし",
            "text": "それはすごい！！",
            "mention_to": []
        },
        {
            "utterance_id": 30,
            "interlocutor_id": "はまち",
            "text": "ちょっと手が出ないです。",
            "mention_to": []
        },
        {
            "utterance_id": 31,
            "interlocutor_id": "くまのみ",
            "text": "@はまち さすが会社持ち",
            "mention_to": [
                "はまち"
            ]
        },
        {
            "utterance_id": 32,
            "interlocutor_id": "あざらし",
            "text": "そんなにするんですか",
            "mention_to": []
        },
        {
            "utterance_id": 33,
            "interlocutor_id": "くまのみ",
            "text": "@はまち 出ませんね",
            "mention_to": [
                "はまち"
            ]
        },
        {
            "utterance_id": 34,
            "interlocutor_id": "はまち",
            "text": "びっくりです。",
            "mention_to": []
        },
        {
            "utterance_id": 35,
            "interlocutor_id": "あざらし",
            "text": "カメラで２０は驚き",
            "mention_to": []
        },
        {
            "utterance_id": 36,
            "interlocutor_id": "はまち",
            "text": "会社のカメラを使わないといけないとき",
            "mention_to": []
        },
        {
            "utterance_id": 37,
            "interlocutor_id": "くまのみ",
            "text": "機能がよいんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 38,
            "interlocutor_id": "はまち",
            "text": "すごくドキドキします",
            "mention_to": []
        },
        {
            "utterance_id": 39,
            "interlocutor_id": "あざらし",
            "text": "確かに、値段わかると",
            "mention_to": []
        },
        {
            "utterance_id": 40,
            "interlocutor_id": "くまのみ",
            "text": "壊したら困りますもんね",
            "mention_to": []
        },
        {
            "utterance_id": 41,
            "interlocutor_id": "あざらし",
            "text": "余計にね",
            "mention_to": []
        },
        {
            "utterance_id": 42,
            "interlocutor_id": "あざらし",
            "text": "弁償できない",
            "mention_to": []
        },
        {
            "utterance_id": 43,
            "interlocutor_id": "はまち",
            "text": "@くまのみ 私、手振れの達人なのですが、すごくきれいに撮れます！",
            "mention_to": [
                "くまのみ"
            ]
        },
        {
            "utterance_id": 44,
            "interlocutor_id": "くまのみ",
            "text": "おそろしい",
            "mention_to": []
        },
        {
            "utterance_id": 45,
            "interlocutor_id": "あざらし",
            "text": "そうなんですね",
            "mention_to": []
        },
        {
            "utterance_id": 46,
            "interlocutor_id": "くまのみ",
            "text": "@はまち すごいですね",
            "mention_to": [
                "はまち"
            ]
        },
        {
            "utterance_id": 47,
            "interlocutor_id": "あざらし",
            "text": "手振れも修正してくれるんだ",
            "mention_to": []
        },
        {
            "utterance_id": 48,
            "interlocutor_id": "あざらし",
            "text": "さすがですね",
            "mention_to": []
        },
        {
            "utterance_id": 49,
            "interlocutor_id": "くまのみ",
            "text": "20万の価値ありですね",
            "mention_to": []
        },
        {
            "utterance_id": 50,
            "interlocutor_id": "はまち",
            "text": "そうなんです。",
            "mention_to": []
        },
        {
            "utterance_id": 51,
            "interlocutor_id": "くまのみ",
            "text": "普通はどれくらいのを買うんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 52,
            "interlocutor_id": "あざらし",
            "text": "価値ありますね",
            "mention_to": []
        },
        {
            "utterance_id": 53,
            "interlocutor_id": "くまのみ",
            "text": "金額",
            "mention_to": []
        },
        {
            "utterance_id": 54,
            "interlocutor_id": "くまのみ",
            "text": "相場っていうんですか",
            "mention_to": []
        },
        {
            "utterance_id": 55,
            "interlocutor_id": "あざらし",
            "text": "私もデジタルの一眼レフ",
            "mention_to": []
        },
        {
            "utterance_id": 56,
            "interlocutor_id": "あざらし",
            "text": "持ってますが",
            "mention_to": []
        },
        {
            "utterance_id": 57,
            "interlocutor_id": "くまのみ",
            "text": "すごい",
            "mention_to": []
        },
        {
            "utterance_id": 58,
            "interlocutor_id": "あざらし",
            "text": "いくらだんたんだろ",
            "mention_to": []
        },
        {
            "utterance_id": 59,
            "interlocutor_id": "あざらし",
            "text": "覚えてなくて",
            "mention_to": []
        },
        {
            "utterance_id": 60,
            "interlocutor_id": "あざらし",
            "text": "全然使ってないんですよ",
            "mention_to": []
        },
        {
            "utterance_id": 61,
            "interlocutor_id": "くまのみ",
            "text": "@あざらし なかなか使う機会ないですもんね",
            "mention_to": [
                "あざらし"
            ]
        },
        {
            "utterance_id": 62,
            "interlocutor_id": "あざらし",
            "text": "そうなんです",
            "mention_to": []
        },
        {
            "utterance_id": 63,
            "interlocutor_id": "くまのみ",
            "text": "@あざらし お子さんの運動会用でしたか？",
            "mention_to": [
                "あざらし"
            ]
        },
        {
            "utterance_id": 64,
            "interlocutor_id": "あざらし",
            "text": "持ち歩くのが大変で",
            "mention_to": []
        },
        {
            "utterance_id": 65,
            "interlocutor_id": "あざらし",
            "text": "最初は子供を撮ろうと",
            "mention_to": []
        },
        {
            "utterance_id": 66,
            "interlocutor_id": "くまのみ",
            "text": "@あざらし 重いんですか？",
            "mention_to": [
                "あざらし"
            ]
        },
        {
            "utterance_id": 67,
            "interlocutor_id": "あざらし",
            "text": "買いました",
            "mention_to": []
        },
        {
            "utterance_id": 68,
            "interlocutor_id": "はまち",
            "text": "相場…わからないけど、5～１０万円くらいで考えてました。",
            "mention_to": []
        },
        {
            "utterance_id": 69,
            "interlocutor_id": "はまち",
            "text": "@あざらし 重たいですよね！",
            "mention_to": [
                "あざらし"
            ]
        },
        {
            "utterance_id": 70,
            "interlocutor_id": "あざらし",
            "text": "レンズが望遠なんで",
            "mention_to": []
        },
        {
            "utterance_id": 71,
            "interlocutor_id": "あざらし",
            "text": "重いですね",
            "mention_to": []
        },
        {
            "utterance_id": 72,
            "interlocutor_id": "はまち",
            "text": "@あざらし 望遠レンズなんですね。",
            "mention_to": [
                "あざらし"
            ]
        },
        {
            "utterance_id": 73,
            "interlocutor_id": "はまち",
            "text": "運動会とかで活躍しそうです。",
            "mention_to": []
        },
        {
            "utterance_id": 74,
            "interlocutor_id": "くまのみ",
            "text": "イメージ、望遠がグーンと伸びるんですけど、そんなのですか",
            "mention_to": []
        },
        {
            "utterance_id": 75,
            "interlocutor_id": "あざらし",
            "text": "10万はしてない気がします",
            "mention_to": []
        },
        {
            "utterance_id": 76,
            "interlocutor_id": "あざらし",
            "text": "そんなのです",
            "mention_to": []
        },
        {
            "utterance_id": 77,
            "interlocutor_id": "くまのみ",
            "text": "5～10万もお高いですね",
            "mention_to": []
        },
        {
            "utterance_id": 78,
            "interlocutor_id": "はまち",
            "text": "@あざらし ピンきりなんですかね！",
            "mention_to": [
                "あざらし"
            ]
        },
        {
            "utterance_id": 79,
            "interlocutor_id": "あざらし",
            "text": "ピンキリですよ",
            "mention_to": []
        },
        {
            "utterance_id": 80,
            "interlocutor_id": "あざらし",
            "text": "高いのは本当に高い",
            "mention_to": []
        },
        {
            "utterance_id": 81,
            "interlocutor_id": "くまのみ",
            "text": "悩みますね",
            "mention_to": []
        },
        {
            "utterance_id": 82,
            "interlocutor_id": "あざらし",
            "text": "でも綺麗ですよね",
            "mention_to": []
        },
        {
            "utterance_id": 83,
            "interlocutor_id": "はまち",
            "text": "@あざらし 写真の色味もスマホカメラとは違いますよね。",
            "mention_to": [
                "あざらし"
            ]
        },
        {
            "utterance_id": 84,
            "interlocutor_id": "くまのみ",
            "text": "撮った写真は現像ですか？パソコンで見るんですか？",
            "mention_to": []
        },
        {
            "utterance_id": 85,
            "interlocutor_id": "はまち",
            "text": "@くまのみ 仕事のはWebサイトに掲載したいそうでパソコンのみですが",
            "mention_to": [
                "くまのみ"
            ]
        },
        {
            "utterance_id": 86,
            "interlocutor_id": "あざらし",
            "text": "現像したことないかも",
            "mention_to": []
        },
        {
            "utterance_id": 87,
            "interlocutor_id": "はまち",
            "text": "プライベートで使うならアルバムや現像したい派です。",
            "mention_to": []
        },
        {
            "utterance_id": 88,
            "interlocutor_id": "くまのみ",
            "text": "私は見たいときに手に取りたいので現像なんですけど",
            "mention_to": []
        },
        {
            "utterance_id": 89,
            "interlocutor_id": "くまのみ",
            "text": "２年分ぐらいたまって現像できてません",
            "mention_to": []
        },
        {
            "utterance_id": 90,
            "interlocutor_id": "くまのみ",
            "text": "まずい状態です",
            "mention_to": []
        },
        {
            "utterance_id": 91,
            "interlocutor_id": "はまち",
            "text": "@くまのみ 我が家もたまってます。",
            "mention_to": [
                "くまのみ"
            ]
        },
        {
            "utterance_id": 92,
            "interlocutor_id": "あざらし",
            "text": "それは随分と溜めましたね",
            "mention_to": []
        },
        {
            "utterance_id": 93,
            "interlocutor_id": "くまのみ",
            "text": "@あざらし そうなんです",
            "mention_to": [
                "あざらし"
            ]
        },
        {
            "utterance_id": 94,
            "interlocutor_id": "あざらし",
            "text": "2年分は相当です",
            "mention_to": []
        },
        {
            "utterance_id": 95,
            "interlocutor_id": "あざらし",
            "text": "うちは全然撮らない笑",
            "mention_to": []
        },
        {
            "utterance_id": 96,
            "interlocutor_id": "くまのみ",
            "text": "３年目に突入しそうでやばいです",
            "mention_to": []
        },
        {
            "utterance_id": 97,
            "interlocutor_id": "はまち",
            "text": "@あざらし 我が家もやばいかもしれません。",
            "mention_to": [
                "あざらし"
            ]
        },
        {
            "utterance_id": 98,
            "interlocutor_id": "はまち",
            "text": "@くまのみ 写真溜まりますよね。",
            "mention_to": [
                "くまのみ"
            ]
        },
        {
            "utterance_id": 99,
            "interlocutor_id": "あざらし",
            "text": "撮った後が困りますね",
            "mention_to": []
        },
        {
            "utterance_id": 100,
            "interlocutor_id": "くまのみ",
            "text": "なので、現像だす時は数百枚単位です",
            "mention_to": []
        },
        {
            "utterance_id": 101,
            "interlocutor_id": "くまのみ",
            "text": "@はまち たまります",
            "mention_to": [
                "はまち"
            ]
        },
        {
            "utterance_id": 102,
            "interlocutor_id": "はまち",
            "text": "数百枚～！",
            "mention_to": []
        },
        {
            "utterance_id": 103,
            "interlocutor_id": "あざらし",
            "text": "すごい！見応えありますね",
            "mention_to": []
        }
    ],
